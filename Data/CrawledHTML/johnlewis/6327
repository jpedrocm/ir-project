									s.events = s.events + ",event12";
								}
							}
					
							

						</script> 
   <script type="text/javascript" id="sitecatalyst-pageload-dispatch-script">
			
								
								var fn_sc = fn_sc || {};
	
								
								fn_sc.cleanUpRefinementStringForReporting = function(str) {
									str = str.replace(/[:*\|*]/g, "_");
									str = str.replace(/\s{1,}/g, " ").replace(/^\s+|\s+$/g,"");
	
									return str;
								};
								
								
								fn_sc.getRangeFilterValue = function(op, lowerBound, upperBound) {
									var currencySymbol = "\u00A3";
									var refinementValue = "";
									
									if (lowerBound !== "") { lowerBound = Math.round(lowerBound); }
									if (upperBound !== "") { upperBound = Math.round(upperBound); }
									
									if (op == "BTWN") {
										refinementValue = currencySymbol+lowerBound+"-"+upperBound;
									} else if (op == "GTEQ") {
										refinementValue = currencySymbol+lowerBound+"-max";
									} else if (op == "LTEQ") {
										if (lowerBound === "") { lowerBound = upperBound };
										
										refinementValue = "min-"+currencySymbol+lowerBound;
									}
									
									return refinementValue;
								};
								
								
								var fn_sc_pageload = fn_sc_pageload || {};
								
								fn_sc_pageload.getNodeTextContent = function(node) {
									return node.textContent || node.innerText;
								};
								
								
								fn_sc_pageload.buildFilterList = function() { 
									var refinementsParam = "";
									
									if (document.querySelectorAll && document.querySelector) {
										// Determine selected facet values via page breadbox elements
										var selectedRefinements = [];
										var breadboxFacets = document.querySelectorAll('.facet-selection');
				
										for (var i = 0; i < breadboxFacets.length; i++) {
											var breadboxFacet = breadboxFacets[i];
											var titleElem = breadboxFacet.querySelector("h3");
											
											if (titleElem) {
												var facetName = titleElem.getAttribute("data-jl-refinement-analytics-name");	
												if (!facetName || facetName === null)
													facetName = fn_sc_pageload.getNodeTextContent(titleElem);
													
												facetName = fn_sc.cleanUpRefinementStringForReporting(facetName);
					
												var selectedRefinement = {
													name:facetName, 
													values:[]
												};
												
												var breadboxFacetValues = breadboxFacet.querySelectorAll('.selection-info'); 
		
												for (var j = 0; j < breadboxFacetValues.length; j++) {
													var breadboxFacetValue = breadboxFacetValues[j];
													var refinementValue = "";
													
													
													var rangeOp = breadboxFacetValue.parentNode.getAttribute("data-jl-operation");
													
													if (rangeOp && rangeOp !== null) {
														var lowerBound = breadboxFacetValue.parentNode.getAttribute("data-jl-lowerbound");
														var upperBound = breadboxFacetValue.parentNode.getAttribute("data-jl-upperbound");
														
														refinementValue = fn_sc.getRangeFilterValue
															(rangeOp, lowerBound, upperBound);
													} else {
														refinementValue = fn_sc_pageload.getNodeTextContent(breadboxFacetValue);
													}
													
													refinementValue = fn_sc.cleanUpRefinementStringForReporting(refinementValue);
													selectedRefinement.values.push(refinementValue);
												}
												
												selectedRefinements.push(selectedRefinement);
											}
										}
										
										// Construct piped string of refinement values
										var refinementsParam = "";
										for (var i in selectedRefinements) {
											var selectedRefinement = selectedRefinements[i];
		
											for (var j in selectedRefinement.values) {
												if (refinementsParam.length > 0)
													refinementsParam += "|";
												
												var refinementValue = selectedRefinement.values[j];
												refinementsParam += "P:"+selectedRefinement.name+":"+refinementValue;
											}
										}
		
									} // end document.querySelector check
			
									s.list1 = refinementsParam;
								};
	
								// Executes page-load analytics request
								fn_sc_pageload.dispatchAnalyticsRequest = function() {
									var s_code=s.t();
									
									if(s_code){
										document.write(s_code);
									}
								};
								
								fn_sc_pageload.buildFilterList();
								fn_sc_pageload.dispatchAnalyticsRequest();
								
							</script> 
