								s.eVar5 ='D=c5';
							}

							if (s.eVar6 != "undefined" && s.eVar6 != "" && s.eVar6 != null){
								var referrer = document.referrer;
								
								if (referrer && 
									(referrer.indexOf("/shop-by-brand") >= 0 || referrer.indexOf("/brands") >= 0)) {
