							// SN-628 fix to only use localStorage if the browser allows it - some prevent this in private browsing mode
							if (isLocalStorageSupported()) {
								s.eVar6=localStorage.getItem("typedsearch");
								s.prop5=localStorage.getItem("userSearchTerm");
								localStorage.setItem("typedsearch", "");
