         <script type="text/javascript">
    var intpageNewsContentSysWordingCheckId;
    intpageNewsContentSysWordingCheckId = setInterval("pageNewsContentCheck()", intSysWordingCheckSpeed);

    function pageNewsContentCheck() {
        if (sysWordingRead) {
            if (getWebsite() != "cn") {
                var addthis_config = {
