          </div> 
          <script>
    var intControlsSpotlightSysWordingCheckId;
    intControlsSpotlightSysWordingCheckId = setInterval("controlsSpotlightSysWordingCheck()", intSysWordingCheckSpeed);
    function controlsSpotlightSysWordingCheck() {
        if (sysWordingRead) {
            $("#product-spotlight span.item-title").html(sysWording.Spotlight);
            clearTimeout(intControlsSpotlightSysWordingCheckId);
        }
    }

