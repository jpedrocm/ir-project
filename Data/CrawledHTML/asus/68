     <script>
                $("#left-nav a").click(function () {
                    $("#left-nav a").removeClass("current");
                    $(this).addClass("current")
                    $(".rightC").hide();
                    //                            $("#" + $(this).attr("dnname")).slideToggle();
                    $("#" + $(this).attr("dnname")).show();
                });
                $("#ul_regions a").click(function () {
                    $("#ul_regions.dropdown-menu").toggle();
                    $(".rightC").hide();
                    $("#" + $(this).attr("dnname")).slideToggle();
                    $("#btn_hotline_global").html($(this).text() + "<span class=caret></span>");
                });

                $("#btn_hotline_global").click(function () {
                    $("#ul_regions.dropdown-menu").toggle();
                });            
              </script>
