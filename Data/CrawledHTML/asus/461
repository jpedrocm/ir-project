                            $(document).ready(function () {
                                $("#TB_load").css({ display: 'block' });
                                $("div.TB_Mask").show();
                                $("div.TB_Mask").css({
                                    position: 'absolute',
                                    top: '0px',
                                    left: '0px',
                                    'z-index': 10000,
                                    width: $(document).width(),
                                    height: $(document).height(),
                                    'background-color': '#000',
                                    opacity: '0.8',
                                    filter: 'Alpha(Opacity=80)'
                                });

                                topLineNavEffect();
                                function topLineNavEffect() {
                                    var resizeTimer;
                                    if ($("#top-nav-line").find(".features-menu").find(".nav-sub").find("li").length > 1) {

                                        var topLineWrap = $("#overview-top-nav");
                                        $("#top-nav-line").find(".nav").find(".has-submenu").on("click", function (e) {
                                            e.preventDefault();
                                            topLineWrap.addClass('to-submenu');
                                        })

                                        topLineWrap.find(".close-btn").on("click", function () {
                                            topLineWrap.removeClass('to-submenu');
                                        })

                                        $(window).resize(function () {
                                            clearTimeout(resizeTimer);
                                            resizeTimer = setTimeout(function () {
                                                if ($(window).width() < 721) {
                                                    topLineWrap.removeClass('to-submenu');
                                                }
                                            }, 100);
                                        });

                                    } else {

                                        $("#top-nav-line").find(".nav").find(".has-submenu").removeClass('has-submenu');
                                    }

                                    if ($("#top-nav-line").find(".nav").find(".has-submenu").length == 0) {
                                        $("#top-nav-line").find(".features-menu").find(".nav-sub").find("#liFeatures").addClass('hide');
                                    }
                                }
                                $("#top-nav-line").find(".nav-tabs li[id='li" + strContent + "']").addClass("active");
                            });
                        </script> 
