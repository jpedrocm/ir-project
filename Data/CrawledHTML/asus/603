      <script>
        var iframeDOM = "<iframe src={url} frameborder=\"0\" width={w} height={h} scrolling=\"no\"></iframe>";
        var checkMobile = function () {
            var ww = $(window).width();
            if (ww < 768 && ("createTouch" in document)) {
                return true;
            } else {
                return false;
            }
        }
        var opentutorial = function (tid, skin) {
            if (checkMobile()) {
                location.href = '/support/utilities/sbs/mobile.htm?view=mobile&tid=' + tid + "&skin=" + skin;
                return;
            }
            var url = "/support/utilities/sbs/tutorial.htm?view=iframe&tid=" + tid + "&skin=" + skin;
            var dom = iframeDOM.replace("{url}", url)
						.replace("{w}", "100%")
						.replace("{h}", "100%");

            $("#tutorial-cont").append(dom);
            $("#lightCanvas").addClass('open');
            $("body").css("overflow", "hidden");
        }
        var closetutorial = function () {
            $("#tutorial-cont").empty();
            $("#lightCanvas").removeClass('open');
            $("body").css("overflow", "visible");
        }
    </script> 
