                $(".page-menu ul>li>a").click(function () {
                    switch ($(this).data('type')) {
                        case "zenfone":
                            $("#img_title_banner").attr("src", "/support/images/zencare/zencare-banner01.jpg");
                            break;
                        case "padfone":
                            $("#img_title_banner").attr("src", "/support/images/zencare/zencare-banner02.jpg");
                            break;
                        case "fonepad":
                            $("#img_title_banner").attr("src", "/support/images/zencare/zencare-banner03.jpg");
                            break;
                        default:
                            break;
