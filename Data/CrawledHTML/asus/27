        $('#ProductVideoModal').on('hidden', function() {
            $("#ProductVideoModal h3").html('');
            $("#ProductVideoModal p").html('');
        });

        $('#ProductVideoModal').appendTo($("body"));
    });

    $("#product-info-zone img:eq(0)").one('error', function(event) {
        var temp_url = $(this).attr("src");
        $(this).attr("src", $(this).attr("src").replace(getWebsite(), "global"));
    });

    $("#ProductDescVideoZone img").one('error', function(event) {
        if ($(this).attr("alt").split("*")[0] == "1") {
            var video_path = SecureHost + "img.youtube.com/vi/" + $(this).attr("alt").split("*")[1] + "/2.jpg"
            $(this).attr("src", video_path);
        }
    });

    function setProductDescVideo() {
        $('#ProductDescVideoZone').removeClass('hide');
