      <script type="text/javascript">
    $(document).ready(function () {
        var searchKey = getUrlParameter('SearchKey');
        if (searchKey != undefined) {
            $('#searchinput2').val(decodeURIComponent(searchKey));
        }

        $('#searchinput2').keypress(function (event) {
            if (event.keyCode == 13) {
                if ($('#searchinput2').val() != '') {
					var index1 = window.location.href.toLowerCase().indexOf('?searchkey=');
					var index2 = window.location.href.toLowerCase().indexOf('&searchtype=');
					if (index2 > -1 && window.parent.document.getElementById('left-nav'))
					{					
						var href = window.location.href.replace(window.location.href.slice(index1,index2),'?SearchKey=' + encodeURI($('#searchinput2').val()));
						var index3 = href.toLowerCase().indexOf('&page=');
						if (index3 > -1 && href.slice(index3+1).indexOf('&') ==  -1)
						{
							href = href.replace(href.slice(index3), '&Page=1');
						}
						window.location.href = href;
					}
					else
					{
						window.location.href = '//' + window.location.host + window.location.pathname + '?SearchKey=' + encodeURI($('#searchinput2').val());
					}
					return false;
                }
            }
        });

        $('#search-btn2').click(function () {
            if ($('#searchinput2').val() != '') {
				var index1 = window.location.href.toLowerCase().indexOf('?searchkey=');
				var index2 = window.location.href.toLowerCase().indexOf('&searchtype=');
				if (index2 > -1 && window.parent.document.getElementById('left-nav'))
				{						
						var href = window.location.href.replace(window.location.href.slice(index1,index2),'?SearchKey=' + encodeURI($('#searchinput2').val()));
						var index3 = href.toLowerCase().indexOf('&page=');
						if (index3 > -1 && href.slice(index3+1).indexOf('&') ==  -1)
						{
							href = href.replace(href.slice(index3), '&Page=1');
						}
						window.location.href = href;				}
				else
				{
					window.location.href = '//' + window.location.host + window.location.pathname + '?SearchKey=' + encodeURI($('#searchinput2').val());
				}
                return false;
            }
            else {
                window.location.href = '//' + window.location.host + window.location.pathname + '?SearchKey=' + encodeURI($('#searchinput2').val());
                return false;
            }
        });
    });
</script> 
