    <script type="text/javascript">
        $(function () {
            //尋找refiner裡的內容, 名稱+數量 等於item-title, 加上className:active
            var activeTitle = $('#search-result-header .item-title:first').text();
            var itemCount = $('#search-result-header .blueword:first').text();
            if (activeTitle != '{type}') {
                $('li.nav-item-sub').each(function () {
                    var $itemSub = $(this);
                    if ($itemSub.find('a').text() == (activeTitle + itemCount)) {
                        $itemSub.addClass('active');
                    }
                });
            }
        });
        $(document).ready(function () {
            var first = $('.span9-line').first();
            if ($('.span9-line').length > 1) {
                var second = $('.span9-line').eq(1);
                if (first.has('#ifame_auto_size').length == 0) {
                    //if first not contains iframe, hide second
                    second.hide();
                }
            }
            var lastDivider = $('#left-nav > li.divider:last');
            if (lastDivider.length != 0) {
                if (lastDivider.next().hasClass('.nav-item-sub') == false) {
                    lastDivider.remove();
                }
            }
        });

        $(document).click(function (e) {
            if (e.target.id != "suggestions") {
                $('#suggestions').fadeOut();
            }
        });

</script> 
