     <script type="text/javascript">
    var selectproduct_wording = "";
    var wheretobuy_wording = "";

    $(document).ready(function () {
        if (getWebsite() == "cn") $("div.highlight-spec").addClass("hide");
        if (getcookiedata("isBusiness") == "BusinessType") $("#commercial_memu").addClass("active");
        else $("#products_memu").addClass("active");

        if ($("#Hid_Link").val() != "" && $("#Hid_Link").val().indexOf("Republic-Of-") > -1 && ProductLevel2Id == 2696) {
            $('#product-home-banner-box').hide();
            $('body #product-home>.row').css("margin-top", "0px");
        }

        //:2013/09/10 Angel, for migo tracking -- product line
        var currentProductLine = (isMultipleLanguageWebsite() || isApplicationPathSite()) ? window.location.pathname.split('/')[3] : window.location.pathname.split('/')[2];
        var wmx_level = (currentProductLine.indexOf('-Products') > -1) ? "wmx_L3" : "wmx_L2";
        $("#ctl00_wmx_engagement").attr("content", wmx_level);
        var wmx_type = (wmx_level == "wmx_L3") ? 9 : 12;
        ButtonTrk(wmx_level, '', wmx_type, currentProductLine);
    });

    function setBreadcrumb(categoryname) {
        window.name = "";
        create_breadcrumbs("", unescape(categoryname));
        window.name = document.URL + "," + unescape(categoryname) + "," + strID;
    }

    var intControlsAdvancedSearchPGSysWordingCheckId;
    intControlsAdvancedSearchPGSysWordingCheckId = setInterval("controlsAdvancedSearchPGSysWordingCheck()", intSysWordingCheckSpeed);
    function controlsAdvancedSearchPGSysWordingCheck() {
        if (sysWordingRead) {
            selectproduct_wording = sysWording.selectproduct;
            $("#product-level3> a.btn-asus:eq(0)").html(sysWording.selectproduct);
            wheretobuy_wording = sysWording.Wheretobuy;
            $("a.wheretobuyBtn").html(wheretobuy_wording);
            clearTimeout(intControlsAdvancedSearchPGSysWordingCheckId);
        }
    }
</script> 
