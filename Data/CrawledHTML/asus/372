        $(document).ready(function() {
            isInGroup = "1";

            $("div.category-hotproduct").each(function() {
                if ($(this).children().length < 5) {
                    $(this).find("div.span2-hot-product:last").css({ "padding-right": "12px", "border-right": "1px solid #CCCCCC" });
                    $(this).find("div.span2-hot-product:last").find(".highlight-spec").css({ "right": "12px" });
                }
            });
            $("div.row-fluid-line").each(function() {
                if ($(this).children().length < 4)
                    $(this).find("div.hot-product-unit:last").css({ "padding-right": "18px", "border-right": "1px solid #CCCCCC" });
            });
        });

        function filterCategoryProduct(categoryid) {
            $("#advanced-search-item li.isCategory").find("input[value='" + categoryid + "']").click();
            getEnableFilters("", "", categoryid);
        }

        

        
    </script> 
