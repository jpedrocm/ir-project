//popout
$(function(){
    //open pop box
    $('#internal-1').click(function(){
        $('.pop-wrapper').show();
        $('.mask').show();
        $('#pop-1').show();
    });
    //close pop box
    $('.pop-wrapper .close').click(function(){
        $('.pop-wrapper').hide();
        $('.pop-box').hide();
    });
    $('.mask').click(function(){
        $('.pop-wrapper').hide();
        $('.pop-box').hide();
    });
});
