                    "services_compact": addthis_settingConfig,
                    "ui_language": addthis_settingLanguaheConfig,
                    "ui_open_windows": true,
                    data_ga_property: sysWording.GoogleTrackingCode,
                    data_ga_social: true
                };
            }
            clearTimeout(intpageProductSysWordingCheckId);
        }
    }   
