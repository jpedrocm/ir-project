    function controlsNewsContentSysWordingCheck() {
        if (sysWordingRead) {
            $("div.news_date").html(date_format($("div.news_date").html(), sysWording.DateFormat));

            var obj = $("#ctl00_ContentPlaceHolder1_ctl00_NewsContent1_span_product_content_area2");
            var val = $("#ctl00_ContentPlaceHolder1_ctl00_NewsContent1_span_news_date").html();
            if ($("h2:eq(0)", obj).html() == null)
                $("h1:eq(0)", obj).after("<div class='news_date'>" + date_format(val, sysWording.DateFormat) + "</div>");
            else
                $("h2:eq(0)", obj).after("<div class='news_date'>" + date_format(val, sysWording.DateFormat) + "</div>");

            if ($("h1:eq(0)", obj).html() != null) document.title = $("h1:eq(0)", obj).html();

            clearTimeout(intControlsNewsContentSysWordingCheckId);
        }
    }
