    function controlsNewsListSysWordingCheck() {
        if (sysWordingRead) {
            $("span.news_list_date").each(function () { $(this).html(date_format($(this).html(), sysWording.DateFormat)); $(this).removeClass("hide"); });
            if (typeof (News_month) == "undefined") $("#news-content .dropdown-toggle").html(sysWording.AllMonths + " <span class=\"caret\"></span>");
