                            $(document).ready(function () {
                                $("#TB_load").css({ display: 'block' });
                                $("div.TB_Mask").show();
                                $("div.TB_Mask").css({
                                    position: 'absolute',
                                    top: '0px',
                                    left: '0px',
                                    'z-index': 10000,
                                    width: $(document).width(),
                                    height: $(document).height(),
                                    'background-color': '#000',
                                    opacity: '0.8',
                                    filter: 'Alpha(Opacity=80)'
