     <a data-toggle="modal" href="#supportVideoModal" id="videomodel" onclick="return false;" class="hide"></a> 
     <script type="text/javascript">
    var ShowGroup = 0;
    $('#supportVideoModal').on('hidden', function () {
        $("#supportVideoModal h3").html('');
        $("#supportVideoModal p").html('');
    });

    function receiveData(event) {
        try {
            var pass_data = JSON.parse(event.data);
            if (pass_data.post_type == "Video") {
                $("#supportVideoModal h3").html(pass_data.video_name);
                $("#supportVideoModal p").html(pass_data.video_link);
                $("#videomodel").click();
            }
            if (pass_data.post_type == "akamai_content") {
                $("#supportVideoModal h3").html(pass_data.title);
                $("#supportVideoModal p").html(pass_data.content);
                $("#videomodel").click();
            }
            if (pass_data.post_type == "Support") {
                window.location.hash = pass_data.support_hash;
            }
            //if (event.data.indexOf("support_")> -1) window.location.hash = event.data;
        }
        catch (err) { }
    }
    if (window.addEventListener) { window.addEventListener("message", receiveData, false); }
    else { window.attachEvent("onmessage", receiveData); } 
</script> 
     <script type="text/javascript">
    var hash = location.hash.replace("#", "");
    var hash_array = hash.split("_");
    var strContent = window.location.hash;
    var MDAId, GenuineWording, GenuineContent, ProductGroupHashedId;
    var isProductPage = "1";
    var ProductNewsEvent = "T";
    var url_tab = "";

    var index = strContent.indexOf("#");
    strContent = strContent.substring(index + 1);
    strContent = strContent.replace(/(^\s*)|(\s*$)/g, "");

    hash = strContent.replace(/.+?#/, "");
    hash_array = strContent.split("_");
    strContent = hash_array[0];

    if (strContent.match(/^(overview|feature|specifications|gallery|review|SuccessfulCase|wheretobuy|viewGallery)/)) {
        window.location.href = '/hk/Tower-PCs/ROG-G20CB' + '/' + strContent + '/';
    }

    if (strContent.toLowerCase() == "support") {
        if (hash_array.length > 1)
            window.location.href = '/hk/Tower-PCs/ROG-G20CB' + '/' + 'HelpDesk' + '_' + hash_array[1] + '/';
        else
            window.location.href = '/hk/Tower-PCs/ROG-G20CB' + '/' + 'HelpDesk' + '/';
    }

    strContent = current_tab;
    hash_array = strContent.split("_");
    strContent = hash_array[0];

    if (strContent.toLowerCase() == "helpdesk" || strContent.toLowerCase() == "zencare") strContent = "support";

    function setSupport(url) {
        $("#lisupport").removeClass("hide");
        if (hash_array.length == 1) {
            $('#ifame_auto_size').attr('src', url);
        }
    }

    var isBusiness;
    var intControlsSpecificationSysWordingCheckId;
    var intSpecMDAWirdingCheckId;
    var sourcePath = "";

    $(document).ready(function () {
        getSpecXML(false);
        if (strID == "r2eJwW68xEMKldzU" && getWebsite() == "de") {
            $("#liwheretobuy").remove();
        }
        if (EnableBlackVersion == "1" && strContent != "support" && strContent != "wheretobuy") {
            $("body").attr("id", "rog_black_style");
            $(".container").addClass("black-style");
            $("#af-header").addClass("aai-bg-black");
            $("#asus-api-footer").addClass("aai-bg-black");
            $(".wtb-selection-2n1, .wtb-selection-four, .pdt-title").addClass("color-w");
        }

        if (getcookiedata("isBusiness") == "BusinessType") $("#commercial_memu").addClass("active");
        else $("#products_memu").addClass("active");

        isBusiness = getcookiedata("isBusiness");
        setBusinessCookie("");
        setClickRelationIdCookie("0");

        if (strContent == "") strContent = "overview";
        if (!strContent.match(/^(feature|specifications|gallery|review|support|SuccessfulCase|wheretobuy|viewGallery)/)) {
            //strContent = "overview";
        }
        else {
            if (strContent == "support" && hash_array.length > 1) {
                if (support_root_url == '') {
                    strContent = "overview";
                    if (EnableBlackVersion == "1") {
                        $("body").attr("id", "rog_black_style");
                        $(".container").addClass("black-style");
                        $("#af-header").addClass("aai-bg-black");
                        $("#asus-api-footer").addClass("aai-bg-black");
                        $(".wtb-selection-2n1, .wtb-selection-four, .pdt-title").addClass("color-w");
                    }
                }
                else {
                    var support_url = (getWebsite() == "global") ? SecureHost + "www.asus.com/support/" : SecureHost + "www.asus.com/" + getWebsite() + "/support/";
                    if (getWebsite() == "cn") support_url = SecureHost + "www.asus.com.cn/support/";

                    support_url += hash_array[1] + "/" + support_root_url;
                    var support_temp = "";
                    for (i = 0; i < hash_array.length; i++) {
                        if (i > 1) support_temp += hash_array[i] + "/";
                    }
                    support_url += support_temp;

                    $('#ifame_auto_size').attr('src', support_url);
                    $('#support').removeClass('hide');

                    if (isBusiness == "BusinessType") $("#commercial_memu").removeClass("active");
                    else $("#products_memu").removeClass("active");

                    $("#support_memu").addClass("active");
                    if (EnableBlackVersion == "1") $("body").attr("id", "");
                }
            }
        }

        googleTrackProduct(strContent);
        checkShowFooter(strContent);

        if (strContent == "wheretobuy") $("#wheretobuyjs").attr("src", "/js/web/wheretobuy.js");
        if (strID == "") location.href = "index.aspx";
        $("#" + strContent).show();
        $("#" + strContent).removeClass("hide");
        $("#li" + strContent).addClass("active");
        $("#comparelist_button").removeClass("hide");
        $("#compare_panel").removeClass("hide");
        if (strContent == "viewGallery") {
            $("#gallery").show();
            $("#ligallery").addClass("active");
            lazyLoadImg();
            default_view_gallery();
        }

        if (strContent == "review" && (ProductNewsEvent == "T") && !($("#product_news_area").hasClass("hide"))) {
            asus.banner({
                click_back: "product_news_left_btn",
                click_next: "product_news_right_btn",
                auto_next_time: 0,
                move_time: 500,
                move_div: "product_news_area",
                in_className: "active",
                keyboard: false,
                loop: false,
                click_end_className: "no_next",
                add_padding: 50
                /*ease: 'Expo.easeInOut'*/
            });
            ProductNewsEvent = "F";
        }
        if ($("#ifProductAwards").attr("src") == "" && window.location.hash == "#Awards") {
            $("#ifProductAwards").attr("src", "ProductAward.aspx?P_ID=" + strID + "&ShowGroup=" + ShowGroup);
        }

        if (newVersionMenu == "1") asus.RecentlyView.add(strID, $(".page-title").data("modelname"), product_image_url.replace("_500", "_130"));
        else asus.Recently_panel.add(strID, $(".page-title").data("modelname"), product_image_url.replace("_500", "_130"));

        setProductTab();
        $('#supportVideoModal').appendTo($("body"));
    });

    var intPageProductSysWordingCheckId;
    intPageProductSysWordingCheckId = setInterval("pageProductSysWordingCheck()", intSysWordingCheckSpeed);

    function pageProductSysWordingCheck() {
        if (sysWordingRead) {
            //            $("#lioverview a:first").html(sysWording.Overview);
            //            $("#lispecifications a:first").html(sysWording.Specifications);
            //            $("#lifeature a:first").html(sysWording.FeatureTagTitle);
            //            $("#liSuccessfulCase a:first").html(sysWording.SuccessfulCase);
            //            $("#lireview a:first").html(sysWording.Review);
            //            $("#ligallery a:first").html(sysWording.Gallery);
            //            $("#liwheretobuy a:first").html(sysWording.Wheretobuy);
            clearTimeout(intPageProductSysWordingCheckId);
        }
    }

    function checkShowFooter(val) {
        switch (val) {
            case "overview":
            case "specifications":
            case "feature":
            case "Features":
                $("#product-footer-zone").removeClass("hide");
                break;
            case "review":
            case "gallery":
                $("#product-footer-zone").addClass("hide");
                break;
            default:
                $("#product-footer-zone").addClass("hide");
                break;
        }
    }

    function switchTempleteOverview(strtemplete) {
        switch (strtemplete) {
            case "1":
                $("#product_content_area").addClass("consumer_style");
                break;
            case "2":
                $("#product_content_area").addClass("component_style");
                break;
            case "3":
                $("#product_content_area").addClass("business_style");
                break;
            default:
                //$("#product_content_area").addClass("consumer_style");
                break;
        }
    }

    function content_item_click(cont) {
        cont = cont.replace(/(^\s*)|(\s*$)/g, "");

        hash = cont.replace(/.+?#/, "");
        hash_array = cont.split("_");
        cont = hash_array[0];

        //if ((strContent != cont) && (cont.match(/^(overview|feature|specifications|gallery|review|support|SuccessfulCase|wheretobuy|viewGallery)/))) {
        if (strContent != cont) {
            googleTrackProduct(cont);
            if (strContent == "viewGallery") strContent = "gallery";

            var oringcont = strContent;
            strContent = cont;

            var scrollmem = document.body.scrollTop;
            if (scrollmem == 0) {
                if (window.pageYOffset)
                    scrollmem = window.pageYOffset;
                else
                    scrollmem = (document.body.parentElement) ? document.body.parentElement.scrollTop : 0;
            }
            window.location.hash = cont;

            if (EnableBlackVersion == "1") {
                bodyStyle(cont);
            }

            $('html,body').scrollTop(scrollmem);
            $("#" + strContent).stop().fadeIn("slow", function () {
                if (strContent == "review" && (ProductNewsEvent == "T") && !($("#product_news_area").hasClass("hide"))) {
                    asus.banner({
                        click_back: "product_news_left_btn",
                        click_next: "product_news_right_btn",
                        auto_next_time: 0,
                        move_time: 500,
                        move_div: "product_news_area",
                        in_className: "active",
                        keyboard: false,
                        loop: false,
                        click_end_className: "no_next",
                        add_padding: 50
                        /*ease: 'Expo.easeInOut'*/
                    });
                    ProductNewsEvent = "F";
                }
                //if (strContent == "gallery") { lazyLoadImg(); }
                if (strContent == "support") {
                    if (isBusiness == "BusinessType") $("#commercial_memu").removeClass("active");
                    else $("#products_memu").removeClass("active");

                    $("#support_memu").addClass("active");
                }
                else {
                    $("#support_memu").removeClass("active");

                    if (isBusiness == "BusinessType") $("#commercial_memu").addClass("active");
                    else $("#products_memu").addClass("active");
                }
                $("#" + strContent).find(".autoHeight").each(function () {
                    if (this.contentDocument) {
                        $(this).height((this.contentDocument.body.offsetHeight + 35) + "px");
                    } else if (this.contentWindow) {
                        $(this).height((this.contentWindow.document.body.scrollHeight + 35) + "px");
                    }
                });
            });
            $("#top-nav-line ul li").removeClass("after-active");
            $("#top-nav-line ul li").removeClass("before-active");

            $("#li" + strContent).addClass("active");
            $("#li" + oringcont).removeClass("active");
            $("#" + oringcont).stop().fadeOut("slow");
            if (oringcont == "review") { $("#" + oringcont).css({ display: 'none' }); }
            if (strContent == "review") { $("#" + strContent).css({ display: 'block' }); }
            $("#li" + strContent).nextAll().not('.hide').eq(0).addClass("after-active");
            $("#li" + strContent).prevAll().not('.hide').eq(0).addClass("before-active");
            checkShowFooter(strContent);
            if (cont == "wheretobuy") {
                $("#wheretobuyjs").attr("src", "/js/web/wheretobuy.js");
                //initializeMap();
            }
            if (cont == "overview") $("#all-product-line-footer img.hdmi-icon").removeClass("hide");
            else $("#all-product-line-footer img.hdmi-icon").addClass("hide");

            ButtonTrk("wmx_L4", strID, 7, cont);
        }
        if (origin_modelname != "" && (cont.match(/^(overview|feature|specifications|gallery|review|SuccessfulCase|wheretobuy|viewGallery)/))) $("#ctl00_ContentPlaceHolder1_ctl00_span_model_name").html(origin_modelname);
    }

    function changeTabLogo(url) {
        if (origin_modelname == "") origin_modelname = $("#ctl00_ContentPlaceHolder1_ctl00_span_model_name").html();
        $("#ctl00_ContentPlaceHolder1_ctl00_span_model_name").html("<img src=\"" + url + "\">");
    }

    var bodyStyle = function (str) {
        if (str != "support" && str != "wheretobuy") {
            $("body").attr("id", "rog_black_style");
        } else {
            $("body").attr("id", "");
        }
    }

    function setProductTab() {
        $("#top-nav-line ul li").removeClass("after-active");
        $("#top-nav-line ul li").removeClass("before-active");
        $("#top-nav-line ul li.active").nextAll().not('.hide').eq(0).addClass("after-active");
        $("#top-nav-line ul li.active").prevAll().not('.hide').eq(0).addClass("before-active");
    }

    function review_click() {
        if ($("#ifProductAwards").attr("src") == "") {
            $("#ifProductAwards").attr("src", "ProductAward.aspx?P_ID=" + strID + "&ShowGroup=" + ShowGroup);
        }
        content_item_click('review');
    }

    function Shop_WhereToBuy(url) {
        $.getJSON(url, function (ResultsJsonData) {
            if (ResultsJsonData.productSearch.resultSize != "0") {
                $('#liwheretobuy').addClass('hide');
                //'True' = true;
            }
        });
    }

    function getSpecXML(isGlobal) {
        var parm = strID + ",1,0";
        if (isGlobal) parm = strID + ",1,1";
        XML_ReaderByWebservice("getProductSpecByHashedid", "specification", "param=" + parm);
    }

    function specification(xml) {
        if ($("content", xml).text() == "" && sourcePath == "") {
            intControlsSpecificationSysWordingCheckId = setInterval("controlsSpecificationSysWordingCheckNoSpec()", intSysWordingCheckSpeed);
            return false;
        }
        if (typeof ($("#overview").find("img[class='HDMI']").attr("class")) != 'undefined') {
            $("#overview").find("img[class='HDMI']").attr("src", "/media/global/icons/C95uEpFbTqPqgocG.jpg");
        }
        else {
            if (isZeusSpec != "1") { //: not zeus spec
                var HDMI_flag = true;
                $("spec_item", xml).each(function () {
                    if ($("ID", this).text() == "4" || $("ID", this).text() == "96" || $("ID", this).text() == "221" ||
                       $("ID", this).text() == "222" || $("ID", this).text() == "375" || $("ID", this).text() == "3132" ||
                       $("ID", this).text() == "3143" || $("ID", this).text() == "3144" || $("ID", this).text() == "80") {
                        if ($("spec", this).text().indexOf("HDMI") > -1 && HDMI_flag) {
                            if (EnableBlackVersion == "1" && strContent != "wheretobuy")
                                $("#all-product-line-footer ul").before("<img class='hdmi-icon' src='/media/web_img/gdml_black.jpg'/>");
                            else
                                $("#all-product-line-footer ul").before("<img class='hdmi-icon' src='/media/global/icons/C95uEpFbTqPqgocG.jpg'/>");
                            HDMI_flag = false;
                        }
                    }
                });
            }
            else {
                if ($("content", xml).text().indexOf("HDMI") > -1) {
                    if (EnableBlackVersion == "1")
                        $("#all-product-line-footer ul").before("<img class='hdmi-icon' src='/media/web_img/gdml_black.jpg'/>");
                    else
                        $("#all-product-line-footer ul").before("<img class='hdmi-icon' src='/media/global/icons/C95uEpFbTqPqgocG.jpg'/>");
                }
            }
        }
        if (!isApplicationPathSite()) {
            if (window.location.pathname.split("/")[1].toLowerCase() == "notebooks_ultrabooks") {
                $("#all-product-line-footer ul").before("<img class='hdmi-icon' src='/media/web_img/McAfee_LiveSafe_Asus_Banne.jpg' style='padding-left:10px'/>");
            }
        }
    }

    function controlsSpecificationSysWordingCheckNoSpec() {
        if (sysWordingRead) {
            sourcePath = (sysWording.SpecificationSourcePath == "") ? "global" : sysWording.SpecificationSourcePath;
            clearTimeout(intControlsSpecificationSysWordingCheckId);
            getSpecXML(true);
        }
    }
</script> 
