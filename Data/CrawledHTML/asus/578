    function ProductInformation(xml) {
        strModel = $("model_name", xml).text();

       
       
        //Change MDA Rule 2009/10/30
        if ($("MDAId", xml).text() != "") {
            MDAId = $("MDAId", xml).text()
            if ($("MDAId", xml).attr("IsGlobal") == "true") {
                XML_ReaderByWebservice("getMDA_Xml", "MDA", "isGlobal=1");
            }
            else {
                XML_ReaderByWebservice("getMDA_Xml", "MDA", "isGlobal=0");
            }

            var tempURL = parseURL(window.location.href);
            if (tempURL.segments[0].toUpperCase() != "EEE" && strID != "4cE9ClQGSXyohDpb" && strID != "gFKzPR6Nramr7Gj4" && strID != "lOXjNwr4e7zPjpjT")
                $(document).ready(function() { $("#NBSpecNotic").removeClass("hide"); });
        }

        if ($("Intel_logo ImageURL", xml).text() != "") {

            var id = "";
            var site = "";
            if ($("Intel_logo ImageLink", xml).text().indexOf("IIP_00975") != -1) {
                id = $("Intel_logo ImageLink", xml).text().split("IIP_00975")[1].split("&")[0];
            }
            else if ($("Intel_logo ImageLink", xml).text().indexOf("WTM_00975") != -1) {
                id = $("Intel_logo ImageLink", xml).text().split("WTM_00975")[1].split("&")[0];
            }
            $("#intel_logo").removeClass("hide");
            $("#cpc img").attr({ src: $("Intel_logo ImageURL", xml).text() });
            $("#cpc").click(function() {
                win = window.open($("Intel_logo ImageLink", xml).text(), id + site, 'resizable=1,scrollbars=0,width=992,height=248');
                win.focus();
                return false;
            });
            $("#textmention").html($("Intel_logo Wording", xml).text());
            $("#textmention").click(function() {
                win = window.open($("Intel_logo WordingLink", xml).text(), id + 'none', 'resizable=1,scrollbars=0,width=992,height=248');
                win.focus();
                return false;
            });
        }
        else { $("#intel_logo").remove(); }

        $("logo", xml).each(function() {
            if ($("ImageURL", this).text() != "") {
                $("#product-quick-logo").removeClass("hide");
                if ($("ImageLink", this).text() == "") {
                    $("#product-quick-logo ul").append("<li><img src='" + $("ImageURL", this).text() + "' /></li>");
                }
                else {
                    $("#product-quick-logo ul").append("<li><a href='" + $("ImageLink", this).text() + "' target='_blank'><img src='" + $("ImageURL", this).text() + "' /></a></li>");
                }
            }
        });
        if ($("MicrositeURL", xml).text() != "") {
            $("#aside-button a:eq(1)").attr("href", $("MicrositeURL", xml).text());
            $("#aside-button a:eq(1)").removeClass("hide");
        }
        
        //:[[2011/04/15 Angel, when compare cookie id is equal to 0, then hide the "add to comapre list button"]]
        if($("compareid", xml).text() == "0") $("#model_fab a.comparebutton:eq(0)").addClass("hide");

        cookie_name = 'compare_' + $("compareid", xml).text();
        add_cookie("comparecookiename", cookie_name);
        setCompareList();
    }

    function MDA(xml) {
        GenuineWording = $("SpecGenuine", xml).text();
        GenuineContent = $("SpecGenuineTipsContent", xml).text();
        setBanner_MDA_part("product");
        //: [[2011/01/04, Angel]] to set the GenuineWording in specification usercontrol 
        $(document).ready(function() {
        });
    }

    function compare_click(obj) {
        var imgtype = $("#product-info-zone>img").attr('src').search(".png") < 0 ? "jpg" : "png";
        var img_ref = $("#product-info-zone>img").attr('src').split("/")[2];
        if ($(obj).attr('checked')) {
            compare_open = "T";
            var strCookie = getcookiedata(cookie_name);
            if (itemcount(strCookie) < 5) {

                add_compare_model(cookie_name, strID, strModel, imgtype, img_ref);
                strCookie = getcookiedata(cookie_name);
            }
            else {
                $(obj).attr("checked", false);
                alert("Accept 5 models only, please remove one first.");
            }
            if (strCookie.length > 0) setCompareList();
            googleTrackProductfn('AddCompare', this);
        }
        else {
            compare_remove_click(cookie_name, strID, strModel, imgtype, img_ref);
            //checkProductCompareItem();
        }
    }

    function print_click() {
        var path = window.location.href;
        var print_path = (path.indexOf("#") > -1) ? path.substring(0, path.indexOf("#")) + "/" : path + "/";
        window.open(print_path + 'ProductPrint/' + window.location.search, 'popup', 'width=980, height=700, scrollbars=yes');
    }

