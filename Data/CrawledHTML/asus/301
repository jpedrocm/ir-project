  <script>
    $(document).ready(function () {
        if (getWebsite() == "us") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/us/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
