         <script type="text/javascript">
    var flag = "0";
    function GetProLineDealer(obj) {

        if (flag == "0") {
            $("#tbDealerList").val("");
            flag = "1";
        }

        var strarray = $("#tbDealerList").val();
        //  alert(strarray);
        var n = strarray.split(",");
        var rs = "";

        if (obj.checked) {
            n.push(obj.value);
        }

        if (!obj.checked) {
            // var index = array.indexOf(obj.value);
            // n.splice(index, 1);
            for (var i = n.length - 1; i >= 0; i--) {

                if (n[i] == obj.value) {
                    n.splice(i, 1);
                }
            }
        }
        for (var i = 0; i < n.length; i++) {
            if (n[i] != "undefined" || n[i] != "")
                rs = rs + "," + n[i];
        }

        //remove ","
        rs = rs.replace(',', ' ');
        rs = rs.replace(/(^\s*)|(\s*$)/g, "");
        rs = rs.replace(' ', ',')

        $("#tbDealerList").val(rs)
        //GetDealerList();
    }
</script> 
