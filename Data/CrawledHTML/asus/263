            <script>
    var intControlsProductSimilarSysWordingCheckId;
    intControlsProductSimilarSysWordingCheckId = setInterval("controlsProductSimilarSysWordingCheck()", intSysWordingCheckSpeed);
    function controlsProductSimilarSysWordingCheck() {
        if (sysWordingRead) {
            $("div.similar-area h2:first").html(sysWording.SimilarList);
            clearTimeout(intControlsProductSimilarSysWordingCheckId);
        }
    }
</script> 
