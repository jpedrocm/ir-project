    function create_breadcrumbs(ProductGroupNow, TargetNow) {}
    
    function controlsbreadcrumbsSysWordingCheck() {
        if (sysWordingRead) {
            $("#breadcrumbs a:first").html(sysWording.ProductNavigationHome);
            clearTimeout(intControlsbreadcrumbsSysWordingCheckId);
        }
    }

    function ProductGroupAll(xml) {
        breadcrumbsXML = xml;
        var intI;
        var cont = $("content category" + intProductCategoryLevel, breadcrumbsXML).text();
        while ($("content category" + intProductCategoryLevel, breadcrumbsXML).text().indexOf(strID) != -1) {
            intProductCategoryLevel += 1;
        }
        intProductCategoryLevel -= 1;

        if (breadcrumbStr != "" && intProductCategoryLevel > 0) {
            var productgroupname = breadcrumbStr.split(",");
            $("content category" + intProductCategoryLevel + "_name", breadcrumbsXML).each(function(e) {
                if ($("content category" + intProductCategoryLevel + "_name:eq(" + e + ")", breadcrumbsXML).text() == productgroupname[productgroupname.length - 1]) {
                    xmlProduct = $("content category" + intProductCategoryLevel + "_name:eq(" + e + ")", breadcrumbsXML);
                    //:[[2012/02/09 Angel Motherboard AMD3 AMD3+ products]]
                    for (i = intProductCategoryLevel - 1; i > 1; i--) {
                        var temp = $(this).parents("category" + i);
                        var temp2 = $(temp).parents("category" + (i - 1));
                        if ((productgroupname[i] == $("category" + i + "_name", temp).text() &&
                            productgroupname[i - 1] == $("category" + (i - 1) + "_name", temp2).text()
                           )) {
                            return false;
                        }
                    }
                    //if ($("content category" + (intProductCategoryLevel - 1) + "_name:eq(" + e + ")", breadcrumbsXML).text() == productgroupname[1]) return false;
                }
            });
            for (intI = 1; intI <= productgroupname.length; intI++) {
                var config = {
                    sensitivity: 3,
                    interval: 200,
                    over: pri_showmenu,
                    out: function() { }
                };
                $("#breadcrumbs a:eq(" + (intI + 1) + ")").hoverIntent(config);
                $("#breadcrumbs a:eq(" + (intI + 1) + ")").data('level', intI);
            }
        }
        if (intProductCategoryLevel == 0) {
            if (breadcrumbStr != "") {
                var productgroupname = breadcrumbStr.split(",");
                intProductCategoryLevel = productgroupname.length - 1;
                var inCategory2 = "N";
                $("content category1_name", breadcrumbsXML).each(function(e) {
                    if ($("content category1_name:eq(" + e + ")", breadcrumbsXML).text() == productgroupname[1]) {
                        var Level1Category = $(this).parent();
                        $("category2", $(this).parent()).each(function(i) {
                            if ($("category2_name", this).text() == productgroupname[2]) {
                                xmlProduct = $($("category3_name:eq(0)", this), breadcrumbsXML);
                                inCategory2 = "Y";
                            }
                        });
                        if (inCategory2 == "N") xmlProduct = $("category2:eq(0) category3_name:eq(0)", Level1Category);
                    }
                });
                for (intI = 1; intI <= productgroupname.length; intI++) {
                    var config = {
                        sensitivity: 3,
                        interval: 200,
                        over: pri_showmenu,
                        out: function() { }
                    };
                    if (inCategory2 == "N") {
                        if (intI < productgroupname.length - 1) {
                            $("#breadcrumbs a:eq(" + (intI + 1) + ")").hoverIntent(config);
                            $("#breadcrumbs a:eq(" + (intI + 1) + ")").data('level', intI);
                        }
                    }
                    else {
                        $("#breadcrumbs a:eq(" + (intI + 1) + ")").hoverIntent(config);
                        $("#breadcrumbs a:eq(" + (intI + 1) + ")").data('level', intI);
                    }
                }
            }
        }
        TrackerWording = TrackerWording + ">" + $(xmlProduct).parents("category" + intI).children("category" + intI + "_name").text();
    }

    function pri_showmenu() {
        showmenu($(this));
    }

    function showmenu(aObject) {
        var categoryLevel = $(aObject).data('level');
        //var intMaxWidth=0;
        $("#breadcrumbs a").removeClass("hovercolor");
        $(aObject).addClass("hovercolor");
        $("#bemnu_content ul").empty();
        $("#bmenu_return").html("&nbsp;");
        $(xmlProduct).parents("category" + categoryLevel).parent().children("category" + categoryLevel).each(function(e) {
            if ($("category" + categoryLevel + "_ProductHashedId", this).text() == "") {
                intMouseOverCategoryLevel = categoryLevel;
                var sendObject = this;
                $("#bemnu_content ul").append("<li class='havenext'><a href='#'>" + $("category" + categoryLevel + "_name", this).text() + "</a></li>");
                $("#bemnu_content ul a:eq(" + e + ")").click(function() { showmenu_onclick(sendObject, (categoryLevel + 1)); return false; });
            }
            else {
                $("#bemnu_content ul").append("<li><a href='product.aspx?P_ID=" + $("category" + categoryLevel + "_ProductHashedId", this).text() + "' onclick=\"setClickRelationIdCookie('" + $("ProductGroupRelationId", this).text() + "');\">" + $("category" + categoryLevel + "_name", this).text() + "</a></li>");
                //$("#bemnu_content ul").append("<li><a href='product.aspx?P_ID=" + $("category" + categoryLevel + "_ProductHashedId", this).text() + "'>" + $("category" + categoryLevel + "_name", this).text() + "</a></li>");
            }
        });
        $("#bmenu_outline").css({ left: $(aObject).position().left + "px" });
        $("#bmenu_outline").removeClass("hide");
        $('#scrollbarBreadcrumb').tinyscrollbar();
    }

    function showmenu_onclick(parentObject, level) {
        $("#bemnu_content ul").slideUp("slow", function() {
            $("#bemnu_content ul").empty();
            if (level == intMouseOverCategoryLevel) {
                $("#bmenu_return").html("&nbsp;");
            }
            else {
                $("#bmenu_return").html("<a href='#'>" + $(parentObject).children("category" + (level - 1) + "_name").text() + "</a>");
                $("#bmenu_return a").click(function() {
                    showmenu_onclick($(parentObject).parent(), (level - 1));
                    return false;
                });
            }
            $("category" + level, $(parentObject)).each(function(e) {
                if ($("category" + level + "_ProductHashedId", this).text() == "") {
                    var sendObject = this;
                    $("#bemnu_content ul").append("<li class='havenext'><a href='#'>" + $("category" + level + "_name", this).text() + "</a></li>");
                    $("#bemnu_content ul a:eq(" + e + ")").click(function() { showmenu_onclick(sendObject, (level + 1)); return false; });
                }
                else {
                    $("#bemnu_content ul").append("<li><a href='product.aspx?P_ID=" + $("category" + level + "_ProductHashedId", this).text() + "' onclick=\"setClickRelationIdCookie('" + $("ProductGroupRelationId", this).text() + "');\">" + $("category" + level + "_name", this).text() + "</a></li>");
                    //$("#bemnu_content ul").append("<li><a href='product.aspx?P_ID=" + $("category" + level + "_ProductHashedId", this).text() + "'>" + $("category" + level + "_name", this).text() + "</a></li>");
                }
            });
            $("#bemnu_content ul").slideDown("slow", function() { $('#scrollbarBreadcrumb').tinyscrollbar_update(); });
        });
    }

    $("#product-topinfo").mouseout(function(evt) {
        evt = evt || window.event;
        var target = evt.target || evt.srcElement;
        var relatedTarget = evt.relatedTarget || evt.toElement;
        if ($(relatedTarget).parents('#product-topinfo').attr("id") != this.id && $(relatedTarget).attr('id') != this.id) {
            $("#breadcrumbs a").removeClass("hovercolor");
            $('#bmenu_outline').addClass('hide');
        }
    })
