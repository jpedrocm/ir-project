    <script type="text/javascript">
        //open pop box
        function show_sn_pop(){
            $('.pop-wrapper').fadeIn(200);
            $('.pop-sn').fadeIn(200);
        }
        function show_ppid_pop(){
            $('.pop-wrapper').fadeIn(200);
            $('.pop-ppid').fadeIn(200);
        }
        //close pop box
        function close_sn_pop(){
            $('.pop-wrapper').fadeOut(200);
            $('.pop-sn').fadeOut(200);
        }
        function close_ppid_pop() {
            $('.pop-wrapper').fadeOut(200);
            $('.pop-ppid').fadeOut(200);
        }
        function close_mask_pop() {
            $('.pop-wrapper').fadeOut(200);
            $('.pop-sn').fadeOut(200);
            $('.pop-ppid').fadeOut(200);
        }

        function check_step3()
        {
            var g1 = 0;
            var g2 = 0;
            var g3 = 0;

            $(".step3_q1").each(function () {
                if ($(this).is(":checked"))
                    g1 += 1;
            });

            $(".step3_q2").each(function () {
                if ($(this).is(":checked"))
                    g2 += 1;
            });

            $(".step3_q3").each(function () {
                if ($(this).is(":checked"))
                    g3 += 1;
            });

            if (g1 < 1)
            {
                alert(q1_error);
                return false;
            }

            if (g2 < 1) {
                alert(q2_error);
                return false;
            }

            if (g3 < 1) {
                alert(q3_error);
                return false;
            }

            return true;
        }
    </script> 
