  <script type="text/javascript">
    function sent_to_asus_login(v0, v1, v2, v3, v4) {
        $('#form_asus_login').attr('action', v0);

        $('#hf_lang').val(v1);
        $('#hf_site').val(v2);
        $('#hf_AppID').val(v3);
        $('#hf_ReturnURL').val(v4);

        $('#form_asus_login').submit();
    }
</script> 
