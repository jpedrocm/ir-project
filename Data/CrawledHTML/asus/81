         <script type="text/javascript">
    var intControlsNewsListSysWordingCheckId;
    intControlsNewsListSysWordingCheckId = setInterval("controlsNewsListSysWordingCheck()", intSysWordingCheckSpeed);
    function controlsNewsListSysWordingCheck() {
        if (sysWordingRead) {
            $("span.news_list_date").each(function () { $(this).html(date_format($(this).html(), sysWording.DateFormat)); $(this).removeClass("hide"); });
            if (typeof (News_month) == "undefined") $("#news-content .dropdown-toggle").html(sysWording.AllMonths + " <span class=\"caret\"></span>");
            $("#news-content a.more_button").text(sysWording.more);

            var translation_News_month = (News_month == "AllMonths") ? sysWording.AllMonths : News_month;
            $("#news-content .dropdown-toggle").html(translation_News_month + " <span class=\"caret\"></span>");
            
            clearTimeout(intControlsNewsListSysWordingCheckId);
        }
    }
</script> 
