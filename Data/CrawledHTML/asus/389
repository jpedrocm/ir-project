                $(window).resize(function () {
                    SetAdditionaljCar();

                    if ($(window).width() <= 767) {
                        $("#lb_step0").hide();
                        $("#lb_zone").hide();
                        $(".channelBlack").hide();
                        $("#ul_phone_prod").hide();
                        $("#ul_phone_prod_sub").hide();
                        $("#ul_phone_seriel").hide();
                        $("#ul_phone_model").hide();
                        $("#ul_phone_final").hide();
                    }

                    if ($(".ac_results").is(":visible")) {
                        //var input = $("#" + Client_ID + "_txtSearch")[0];
                        var input = $("#txtSearch")[0];
                        var pos = findPos(input);
                        $(".ac_results").css({
                            width: $(input).width() + "px",
                            top: (pos.y + input.offsetHeight) + "px",
                            left: pos.x + "px"
                        })
                    }
                });

                //***手動選擇清單***
                function FindSerialArticle(me) {
                    me.href = $("#hd_lang_prefix").val() + "/Article/565/";
                }

                function close_popup() {
                    //TW 站--隱藏選單
                    $("#Ul1").hide();
                    $("#lb_step0").hide();
                    $("#lb_zone").hide();
                    $(".channelBlack").hide();
                    $('body').removeClass('stop-scrolling');
                }

                if ($.browser.msie) {
                    document.onmouseup = (function () { close_popup(); });
                }
                else {
                    $(window).on("mouseup", close_popup);
                }

                $("#btnManualSelect").click(function () {
                    //$("#lb_step0").toggle();
                    $("#lb_zone").show();
                    $(".channelBlack").show();
                    $(".channelBlack").height($("body").height());
                    $('body').addClass('stop-scrolling');

                    //$("#lb_zone").css("left", ($(window).width() - $("#lb_zone").width()) / 2 + $(window).scrollLeft() + "px");
                    $(".step2-zone").hide();
                    $(".product-category").show();

                    //ScrolltoSelManual_0();
                });

                $(document).on("click", "#btnManualSelect_tw", function () {
                    //$("#lb_step0").toggle();
                    $("#lb_zone").show();
                    $(".channelBlack").show();
                    $(".channelBlack").height($("body").height());
                    $('body').addClass('stop-scrolling');

                    //$("#lb_zone").css("left", ($(window).width() - $("#lb_zone").width()) / 2 + $(window).scrollLeft() + "px");
                    $(".step2-zone").hide();
                    $(".product-category").show();

                    //ScrolltoSelManual_0();
                });

                $("#lb_zone").on("mouseenter", function () {
                    if ($.browser.msie) {
                        document.onmouseup = (function () { });
                    }
                    else {
                        $(window).off("mouseup", close_popup);
                    }
                })

                $("#lb_zone").on("mouseleave", function () {
                    if ($.browser.msie) {
                        document.onmouseup = (function () {
                            close_popup();
                        });
                    } else {
                        $(window).on("mouseup", close_popup);
                    }
                })
