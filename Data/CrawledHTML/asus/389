    $(".product-category a").click(function () {
        var span = "<span class='caret'></span>";
        var space = "";
        var l1 = $(this).attr("l1_id");
        var l2 = $(this).attr("l2_id");
        var product = $(this).attr("product");
        var series = $(this).attr("series");
        $("#btn_product_name").html("<span class='txt'>" + product + "</span>" + span);
        $("#ul_model li").remove();
        $("#ul_series li").remove();

        if (l2 != "")
            $("#btn_series_name").html("<span class='txt'>" + series + "</span>" + span);

        var str_url;
        if (l1 != "" && l2 == "") {
            $("#btn_series_name").html("<span class='txt'>" + $("#hd_select_series").val() + "</span>" + span);
            $("#btn_model_name").html("<span class='txt'>" + $("#hd_select_model").val() + "</span>");
            Bind_Series($("#langNormal").val(), l1);
            goto_step2();
        }
        else if (l1 != "" && l2 != "") {
            $("#btn_model_name").html("<span class='txt'>" + $("#hd_select_model").val() + "</span>" + span);
            Bind_Series($("#langNormal").val(), l1);
            Bind_Model($("#langNormal").val(), l1, l2);
            goto_step3();
        }

        $(".img_product").attr("src", "/support/images/products/" + l1 + "/304.jpg");
        $(".product-category").hide();
        $(".step2-zone").show();
        return false;
    });

    function goto_step2() {
        if ($("#ddlSelectModel").hasClass('current')) {
            $("#ddlSelectModel").removeClass('current');
        }
        $("#ddlSelectSeries").addClass('current');

        document.getElementById("li_step1").className = "step-icon step-icon-gray1";
        document.getElementById("li_step2").className = "step-icon step-icon-blue2";
        document.getElementById("li_step3").className = "step-icon step-icon-gray3";
        $("#" + Client_ID + "_" + "pModelDesc").hide();
        $(".img_product").show();
        $(".step3-zone-model-number").hide();
        $(".step4-zone-link").hide();
    }

    function goto_step3() {
        if ($("#ddlSelectSeries").hasClass('current')) {
            $("#ddlSelectSeries").removeClass('current');
        }
        $("#ddlSelectModel").addClass('current');

        document.getElementById("li_step1").className = "step-icon step-icon-gray1";
        document.getElementById("li_step2").className = "step-icon step-icon-gray2";
        document.getElementById("li_step3").className = "step-icon step-icon-blue3";
        $("#" + Client_ID + "_" + "pModelDesc").show();
        $(".img_product").hide();
        $(".step3-zone-model-number").show();
        $(".step4-zone-link").hide();
    }

    function goto_start() {
        if ($("#ddlSelectSeries").hasClass('current')) {
            $("#ddlSelectSeries").removeClass('current');
        }
        if ($("#ddlSelectModel").hasClass('current')) {
            $("#ddlSelectModel").removeClass('current');
        }
        document.getElementById("li_step1").className = "step-icon step-icon-gray1";
        document.getElementById("li_step2").className = "step-icon step-icon-gray2";
        document.getElementById("li_step3").className = "step-icon step-icon-gray3";
        $("#" + Client_ID + "_" + "pModelDesc").hide();
        $(".img_product").hide();
        $(".step3-zone-model-number").hide();
        $(".step4-zone-link").show();
    }

    function Bind_Series(lang_id, l1) {
        var str_url = "/support/utilities/GetProducts.aspx" + "?ln=" + lang_id + "&p=" + l1;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "xml",
            success: parseXml_series
        });
        return false;
    }

    function Bind_Model(lang_id, l1, l2) {
        var str_url = "/support/utilities/GetProducts.aspx" + "?ln=" + lang_id + "&p=" + l1 + "&s=" + l2;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "xml",
            success: parseXml_model
        });
        return false;
    }

    function parseXml_series(xml) {
        $(xml).find("Table").each(function () {
            en_name = $(this).find("en_name").text();
            gname = $(this).find("gname").text();
            l1 = $(this).find("l1_id").text();
            l2 = $(this).find("id").text();
            $("#ul_series").append("<li><a href='javascript:void(0);' onclick=SeriesClick(this,'" + $("#langNormal").val() + "'," + l1 + "," + l2 + ")>" + gname + "</a></li>");
        });
    }

    function parseXml_model(xml) {
        $(xml).find("Table").each(function () {
            m_name = $(this).find("m_name").text();
            l1 = $(this).find("l1_id").text();
            l2 = $(this).find("l2_id").text();

            var lang_id = $("#langNormal").val();
            var m_new_name=m_name;

            //20131120歐洲 infinity限制
            en_name = $(this).find("en_name").text();

            $("#ul_model").append("<li><a href='javascript:void(0);' onclick=ModelClick(this,'" + $("#langNormal").val() + "'," + l1 + "," + l2 + ") en_name='" + m_name + "'>" + m_name + "</a></li>");
        });
    }

    function ProductClick(lang_id, name, l1) {
        var span = "<span class='caret'></span>";
        var space = "";
        $('#btn_product_name').html("<span class='txt'>" + name + "</span>" + space);
        $('#ul_series li').remove();
        $('#btn_series_name').html("<span class='txt'>" + $("#hd_select_series").val() + "</span>" + span);
        $('#btn_model_name').html("<span class='txt'>" + $("#hd_select_model").val() + "</span>" + span);
        Bind_Series(lang_id, l1);
        $('#ul_model li').remove();
        $(".img_product").attr("src", "/support/images/products/" + l1 + "/304.jpg");
        goto_step2();
    }

    function SeriesClick(sender, lang_id, l1, l2) {
        var name = sender.innerHTML;
        var span = "<span class='caret'></span>";
        $('#btn_series_name').html("<span class='txt'>" + name + "</span>" + span);
        $('#ul_model li').remove();
        $('#btn_model_name').html("<span class='txt'>" + $("#hd_select_model").val() + "</span>" + span);
        Bind_Model(lang_id, l1, l2);
        goto_step3();
    }

    function ModelClick(sender, lang_id, l1, l2) {
        var span = "<span class='caret'></span>";
        var name = $(sender).attr("en_name");
        var display_name = sender.innerHTML;
        $('#btn_model_name').html("<span class='txt'>" + display_name + "</span>" + span);
        $("#step4_ModelName").html(display_name);
        Get_ModelHash(name);
        Get_ProdLinks(lang_id, l1, l2, name);
        goto_start();
    }

    function Get_ModelHash(model) {
        var str_url = "/support/utilities/GetModelHashID.aspx" + "?m=" + model;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "text",
            success: function (hash) {
                $("#imgModel").attr("src", "//www.asus.com/websites/global/products/" + hash + "/P_500.jpg");
                $("#imgModel").error(function () { $("#imgModel").attr("src", $(".img_product").attr("src")); });
            }
        });
        return false;
    }

    function Get_ProdLinks(lang_id, l1, l2, model) {
        //var str_url = "/support/utilities/GetProductSupportLink.aspx" + "?ln=" + lang_id + "&p=" + l1;
        var str_url = "/support/utilities/GetProductSupportLink.aspx" + "?ln=" + lang_id + "&p=" + l1 + "&s=" + l2 + "&m=" + model;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "xml",
            success: function (xml) {
                $("#step4_List").empty();
                $(xml).find("Table1").each(function () {
                    Link_Name = $(this).find("Link_Name").text();
                    URL = $(this).find("URL").text();
                    var mkturl = $(this).find("Description").text(); 
                    //$("#step4_List").append("<li><a href='/support/" + URL + "/" + l1 + "/" + l2 + "/" + model + "'>" + Link_Name + "</a></li>");

                    //20140423
                    //$("#step4_List").append("<li><a href='" + $("#hd_lang_prefix").val() + "/Product/" + URL + "/" + modelReplace(model) + "/'>" + Link_Name + "</a></li>");
                    $("#step4_List").append("<li><a target='_blank' href='" + mkturl + "/'>" + Link_Name + "</a></li>");
                });
            }
        });
        return false;
    }

    //    function SetProdImg() {
    //        $("#imgModel").attr("src", $(".img_product").attr("src"));
    //    }

    ////Phone////

    function ScrolltoSelManual() {
        var parent = document.getElementById("btnManualSelect_phone");
        var topOffset = 0;
        while (parent != document.body) {
            //leftOffset += parent.offsetLeft;
            topOffset += parent.offsetTop;
            parent = parent.offsetParent;
        }
        //document.body.scrollTop = $(".serial-number").offset().top;        
        //if (BrowserDetect.browser == "Explorer" || BrowserDetect.browser == "Firefox") {
            document.documentElement.scrollTop = topOffset;
        //} else {
            document.body.scrollTop = topOffset;
        //}
    }

    $("#btnManualSelect_phone").click(function () {
        $("#ul_phone_prod").toggle();
        $("#ul_phone_prod_sub").hide();
        $("#ul_phone_seriel").hide();
        $("#ul_phone_model").hide();
        $("#ul_phone_final").hide();
    });

    $("#ul_phone_prod a").click(function () {
        var prod_id = $(this).attr("prod_id");
        Phone_Bind_SubProduct($("#langNormal").val(), prod_id);

        $("#ul_phone_prod").hide();
        $("#ul_phone_prod_sub").show();
        ScrolltoSelManual();
    });

    function Phone_Bind_SubProduct(lang_id, prod_id) {
        var str_url = "/support/utilities/GetProdSubCategory.aspx" + "?ln=" + lang_id + "&pid=" + prod_id;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "xml",
            success: function (xml) {
                $("#ul_phone_prod_sub").empty();
                $("#ul_phone_prod_sub").append("<li class='back-title' onclick='Phone_Back_Prod();' >" + $("#hd_menual_product").val() + "</li>");
                $(xml).find("Table").each(function () {
                    display_name = $(this).find("display_name").text().trim();
                    l1 = $(this).find("l1_id").text();
                    l2 = $(this).find("l2_id").text();
                    var para = "'" + $("#langNormal").val() + "','" + l1 + "','" + l2 + "'";
                    $("#ul_phone_prod_sub").append("<li><a href='javascript:void(0);' onclick=\"Phone_Click_ProdSub(" + para + ");\">" + display_name + "</a></li>");
                });
            }
        });
        return false;
    }

    function Phone_Bind_Series(lang_id, l1) {
        var str_url = "/support/utilities/GetProducts.aspx" + "?ln=" + lang_id + "&p=" + l1;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "xml",
            success: function (xml) {
                $("#ul_phone_seriel").empty();
                $("#ul_phone_seriel").append("<li class='back-title' onclick='Phone_Back_ProdSub();'>" + $("#hd_menual_prodcategory").val() + "</li>");
                $(xml).find("Table").each(function () {
                    gname = $(this).find("gname").text();
                    l1 = $(this).find("l1_id").text();
                    l2 = $(this).find("id").text();
                    var para = "'" + $("#langNormal").val() + "','" + l1 + "','" + l2 + "'";
                    $("#ul_phone_seriel").append("<li><a href='javascript:void(0);' onclick=\"Phone_Click_Seriel(" + para + ");\" >" + gname + "</a></li>");
                });
            }
        });
        return false;
    }

    function Phone_Bind_Model(lang_id, l1, l2) {
        var str_url = "/support/utilities/GetProducts.aspx" + "?ln=" + lang_id + "&p=" + l1 + "&s=" + l2;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "xml",
            success: function (xml) {
                $("#ul_phone_model").empty();
                $("#ul_phone_model").append("<li class='back-title' onclick='Phone_Back_Seriel();' >" + $("#hd_menual_series").val() + "</li>");
                $(xml).find("Table").each(function () {
                    m_name = $(this).find("m_name").text();
                    l1 = $(this).find("l1_id").text();
                    l2 = $(this).find("l2_id").text();
                    //var para = "'" + $("#langNormal").val() + "','" + m_name + "','" + l1 + "','" + l2 + "'";

                    //20131120歐洲 infinity限制
                    var en_name = $(this).find("en_name").text();
                    var para = "'" + $("#langNormal").val() + "','" + m_name + "','" + l1 + "','" + l2 + "','" + m_name + "'";

                    $("#ul_phone_model").append("<li><a href='javascript:void(0);' onclick=\"Phone_Click_Model(" + para + ");\" >" + m_name + "</a></li>");
                });
            }
        });
        return false;
    }

    function Phone_Bind_ModelbyL2(lang_id, l1, l2) {
        var str_url = "/support/utilities/GetProducts.aspx" + "?ln=" + lang_id + "&p=" + l1 + "&s=" + l2;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "xml",
            success: function (xml) {
                $("#ul_phone_model").empty();
                $("#ul_phone_model").append("<li class='back-title' onclick='Phone_Back_ProdSub_byModel();'>" + $("#hd_menual_prodcategory").val() + "</li>");
                $(xml).find("Table").each(function () {
                    m_name = $(this).find("m_name").text();
                    l1 = $(this).find("l1_id").text();
                    l2 = $(this).find("l2_id").text();
                    //var para = "'" + $("#langNormal").val() + "','" + m_name + "','" + l1 + "','" + l2 + "','" + m_name + "'";

                    //20131120歐洲 infinity限制
                    var en_name = $(this).find("en_name").text();
                    var para = "'" + $("#langNormal").val() + "','" + m_name + "','" + l1 + "','" + l2 + "','" + m_name + "'";
                    $("#ul_phone_model").append("<li><a href='javascript:void(0);' onclick=\"Phone_Click_Model(" + para + ");\" >" + m_name + "</a></li>");

                });
            }
        });
        return false;
    }

    function Phone_Get_ModelHash(model, l1) {
        var str_url = "/support/utilities/GetModelHashID.aspx" + "?m=" + model;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "text",
            success: function (hash) {
                $("#phone_final_Img").attr("src", "//www.asus.com/websites/global/products/" + hash + "/P_500.jpg");
                $("#phone_final_Img").error(function () { $("#phone_final_Img").attr("src", "/support/images/products/" + l1 + "/304.jpg") });
            }
        });
        return false;
    }

    function Phone_Get_ProdLinks(lang_id, l1, l2, model) {
        //var str_url = "/support/utilities/GetProductSupportLink.aspx" + "?ln=" + lang_id + "&p=" + l1;
        var str_url = "/support/utilities/GetProductSupportLink.aspx" + "?ln=" + lang_id + "&p=" + l1 + "&s=" + l2 + "&m=" + model;
        $.ajax({
            type: "GET",
            url: str_url,
            dataType: "xml",
            success: function (xml) {
                $("#ul_phone_prodlink").empty();
                $(xml).find("Table1").each(function () {
                    Link_Name = $(this).find("Link_Name").text();
                    URL = $(this).find("URL").text();
                    var mkturl = $(this).find("Description").text(); 
                    //$("#ul_phone_prodlink").append("<li><a href='/support/" + URL + "/" + l1 + "/" + l2 + "/" + model + "'>" + Link_Name + "</a></li>");
                    
                    //20140423
                    //$("#ul_phone_prodlink").append("<li><a href='" + $("#hd_lang_prefix").val() + "/Product/" + URL + "/" + modelReplace(model) + "/'>" + Link_Name + "</a></li>");
                    $("#ul_phone_prodlink").append("<li><a target='_blank' href='" + mkturl + "/'>" + Link_Name + "</a></li>");
                });
            }
        });
        return false;
    }

    function Phone_Close_Prod() { $("#ul_phone_prod").hide(); }

    function Phone_Click_ProdSub(lang_id, l1, l2) {
        if (l2 == "") {
            Phone_Bind_Series(lang_id, l1);
            $("#ul_phone_prod_sub").hide();
            $("#ul_phone_seriel").show();
        } else {
            Phone_Click_SerielbyL2(lang_id, l1, l2);
        }
        ScrolltoSelManual();
    }

    function Phone_Click_Seriel(lang_id, l1, l2) {
        Phone_Bind_Model(lang_id, l1, l2);
        $("#ul_phone_seriel").hide();
        $("#ul_phone_model").show();
        ScrolltoSelManual();
    }

    function Phone_Click_SerielbyL2(lang_id, l1, l2) {
        Phone_Bind_ModelbyL2(lang_id, l1, l2);
        $("#ul_phone_prod_sub").hide();
        $("#ul_phone_model").show();
        ScrolltoSelManual();
    }

    function Phone_Click_Model(lang_id, m_name, l1, l2, en_name) {
        Phone_Get_ModelHash(en_name, l1);
        Phone_Get_ProdLinks(lang_id, l1, l2, en_name);
        $("#ul_phone_model").hide();
        $("#ul_phone_final").show();
        $("#phone_final_ModelName").html(m_name);
        ScrolltoSelManual();
    }

    function Phone_Back_Prod() {
        $("#ul_phone_prod").show();
        $("#ul_phone_prod_sub").hide();
    }

    function Phone_Back_ProdSub() {
        $("#ul_phone_prod_sub").show();
        $("#ul_phone_seriel").hide();
    }

    function Phone_Back_ProdSub_byModel() {
        $("#ul_phone_prod_sub").show();
        $("#ul_phone_model").hide();
    }

    function Phone_Back_Seriel() {
        $("#ul_phone_seriel").show();
        $("#ul_phone_model").hide();
    }

    function Phone_Back_Model() {
        $("#ul_phone_model").show();
        $("#ul_phone_final").hide();
    }

