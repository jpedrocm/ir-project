    function SetAdditionaljCar() {
        if ($(".additional-zone li").length > 0) {
            if ($(window).width() < 959) {
                //additional to vertical
                $("#vote_right_btn").unbind();
                $("#vote_left_btn").unbind();

                $(".additional-zone").jCarouselLite({
                    btnNext: "#vote_right_btn",
                    btnPrev: "#vote_left_btn",
                    circular: false,
                    visible: 2,
                    vertical: true
                });
            } else {
                $("#vote_right_btn").unbind();
                $("#vote_left_btn").unbind();
                if ($("#hd_lsttype").val() == "additional") {
                    $(".additional-zone").jCarouselLite({
                        btnNext: "#vote_right_btn",
                        btnPrev: "#vote_left_btn",
                        circular: false,
                        visible: 2
                    });
                } else if ($("#hd_lsttype").val() == "additional-2") {
                    $(".additional-zone").jCarouselLite({
                        btnNext: "#vote_right_btn",
                        btnPrev: "#vote_left_btn",
                        circular: false,
                        visible: 4,
                        vertical: true
                    });
                }
            }
        }
    }

    function findPos(obj) {
        var curleft = obj.offsetLeft || 0;
        var curtop = obj.offsetTop || 0;
        while (obj = obj.offsetParent) {
            curleft += obj.offsetLeft
            curtop += obj.offsetTop
        }
        return { x: curleft, y: curtop };
    }

    $("#atips").click(function () {
        $("#lb_step0").toggle();
        $('#lb_zone').hide();
        $(".channelBlack").hide();
        //$("#hd_lb_zone_status").val("false");
    });

    $("#a2").click(function () {
        $("#Ul1").toggle();
        $('#lb_zone').hide();
        $(".channelBlack").hide();
    });

