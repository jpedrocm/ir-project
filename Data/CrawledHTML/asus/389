    //***手動選擇清單***

    //***Auto Complete TextBox***
    var hd_lang_prefix = $("#hd_lang_prefix").val().split('/');
    var _country = hd_lang_prefix.length > 2 ? hd_lang_prefix[1] : 'global';
    if (window.location.hostname.indexOf("asus.com.cn") >= 0) {
        _country = 'cn';
    }

    $("#txtSearch").autocomplete("//www.asus.com/searchapi/api/Suggestion",
        {
            delay: 450,
            width: 414,
            minChars: 2, //至少輸入幾個字元才開始給提示?
            matchSubset: false,
            matchContains: false,
            cacheLength: 0,
            noCache: true, //每次都重新連後端查詢
            onItemSelect: findValue,
            onFindValue: findValue,
            extraParams: { country: _country, type: "Products,Commercial", RowLimit: "10,10", eventDBRowLimit: 0 },
            formatItem: function (row) {
              //  var re = new RegExp($("#txtSearch").val(), 'gi');
                return "<div  class=\"search-result\">" + row + "</div>";
            },
            autoFill: false,
            mustMatch: false //是否允許輸入提示清單上沒有的值?
        });
    function findValue(li) {
        if (li == null) return;
        //$("#mainheader1_txtSearch").val(li.extra[0]);
    }
    //***Auto Complete TextBox***

    //Search Link
    function DoSearch() {
        var txtSearch = encodeURIComponent($.trim($("#txtSearch").val()));
        var txtGlobal = _country + '/';
        if (txtSearch.length > 0) {
            if (_country == 'cn') {
                window.location.href = 'http://www.asus.com.cn/search/results.aspx?SearchKey=' + txtSearch + '&IsSupport=True';
            } else {
                if (txtGlobal == 'global/') txtGlobal = ''
                window.location.href = 'http://www.asus.com/' + txtGlobal + 'search/results.aspx?SearchKey=' + txtSearch + '&IsSupport=True';
            }
        /*
            //check in KB
            $.post("/support/utilities/search_autocomp_check.aspx?q=" + txtSearch + "&lang=" + $("#langNormal").val(), function (data) {
                document.location.href = data;

            });*/
        }
        return false;
    }

    function modelReplace(model) {
        var re1 = new RegExp("/", "g");
        //var re2 = new RegExp("\.", "g");
        var re3 = new RegExp("-", "g");
        var re4 = new RegExp(" ", "g");
        return model.replace(re1, "").replace(/\./g, "").replace(re3, "").replace(re4, "_");
    }
