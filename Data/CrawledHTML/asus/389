    <script>

    function GetRegProd() {
        $("#pnl_sign").show();
        $("#pnl_regest").hide();
        $("#pnl_myprod").hide();

        $("#pnl_sign").block({ message: $('#loadingBox'),
            css: {
                border: 'none',
                backgroundColor: 'transparent',
                width: '50px'
            }
        }); 

        $.post("/support/utilities/fak/get_regprod.aspx?lang_id=" + $("#langNormal").val(), function (data) {
            if (data != undefined && data != null && data.length > 0) {
                if (data[0].RegCnt != undefined && data[0].RegCnt != null && data[0].RegCnt == "0") {
                    DispReg(0);
                } else {

                    for (var i = 0; i < data.length; i++) {
                        var pmodel = data[i].pmodel;
                        var img = data[i].img;
                        var detail = data[i].detail;
                        var download = data[i].download;

                        $("#ul_regprod").append("<li><div class='item prod-item'><div class='left'><img src='" + img + "' width='56px' alt=''></div>" +
            							        "<div class='entry' style='width:255px; margin-right:20px'><p class='title-name'>" + pmodel + "</p>" +
            								    "<a href='" + detail + "' target='_blank'>" + $("#hd_more").val() + "</a>" +
            								    "<a href='" + download + "' target='_blank'>" + $("#hd_download").val() + "</a>" +
            							        "</div></div></li>");

                    }
                    DispReg(data.length);
                }
            }
            else
                DispReg(-1);
        });

    }

    function DispReg(regcnt) {
        if (regcnt > 0) {
            $("#pnl_sign").hide();
            $("#pnl_regest").hide();
            $("#pnl_myprod").show();
        } else if (regcnt == 0) {
            $("#pnl_sign").hide();
            $("#pnl_regest").show();
            $("#pnl_myprod").hide();
        } else {
            $("#pnl_sign").show();
            $("#pnl_regest").hide();
            $("#pnl_myprod").hide();
        }
        SetAdditionaljCar();
        $("#pnl_sign").unblock();
    }

</script> 
    <script>
    $(document).ready(function () {
        GetRegProd();
    });
</script> 
    <script>
    function SetAdditionaljCar() {
        if ($(".additional-zone li").length > 0) {
            if ($(window).width() < 959) {
                //additional to vertical
                $("#vote_right_btn").unbind();
                $("#vote_left_btn").unbind();

                $(".additional-zone").jCarouselLite({
                    btnNext: "#vote_right_btn",
                    btnPrev: "#vote_left_btn",
                    circular: false,
                    visible: 4,
                    vertical: true
                });
            } else {
                $("#vote_right_btn").unbind();
                $("#vote_left_btn").unbind();

                $(".additional-zone").jCarouselLite({
                    btnNext: "#vote_right_btn",
                    btnPrev: "#vote_left_btn",
                    circular: false,
                    visible: 2
                });
                
            }
        }
    }

</script> 
