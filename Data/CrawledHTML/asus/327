     $(document).ready(function () {
         $("#call_message>div").removeAttr("style");

         if (isApplicationPathSite()) {
             var domail_url = "";
             switch (getWebsite()) {
                 case "ch-de":
                 case "ch-fr":
                 case "ch-it":
                     domail_url = "ch.asus.com"
                     break;
                 case "ca-fr":
                 case "ca-en":
                     domail_url = "ca.asus.com"
                     break;
                 case "ae-en":
                 case "ae-ar":
                     domail_url = "ae.asus.com"
                     break;
                 case "be-fr":
                 case "be-nl":
                     domail_url = "be.asus.com"
                     break;
                 case "af-pa":
                 case "af-da":
                     domail_url = "www.asus.com/af"
                     break; be
                 case "sa-en":
                 case "sa-ar":
                     domail_url = "www.asus.com/sa"
                     break;
                 case "nafr-ar":
                 case "nafr-fr":
                     domail_url = "www.asus.com/nafr"
                     break;
                 case "ua":
                 case "ua-ua":
                     domail_url = "www.asus.ua"
                     break;
                 case "tn-fr":
                 case "tn-ar":
                     domail_url = "www.asus.com/tn"
                     break;
                 case "ma-fr":
                 case "ma-ar":
                     domail_url = "www.asus.com/ma"
                     break;
                 case "dz-fr":
                 case "dz-ar":
                     domail_url = "www.asus.com/dz"
                     break;
             }

             $.ajax({
                 dataType: 'xml',
                 url: "/" + getWebsite() + "/GetData.asmx/getMultipleLang",
                 type: 'get',
                 cache: true,
                 success: function (xml) {
                     $("Domain", xml).each(function (e) {
                         if ($("domain_name", this).text() == domail_url) {
                             $("website", this).each(function (e) {
                                 if ($("website_code", this).text() != getWebsite()) {
                                     $("#footer_country_link a:last").after(" | <a href=\"" + $("url", this).text() + "\"><span>" + $("websiteLanguage", this).text() + "</span></a>");
                                 }
                             });
                         }
                         return;
                     });
                 },
                 error: function (XMLHttpRequest, textStatus, errorThrown) {
                     //alert(XMLHttpRequest + '\n' + textStatus + '\n' + errorThrown);
                 }
             });
         }
     });

</script> 
