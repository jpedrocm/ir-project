    isInGroup = "2";
    function setVideo(title, obj) {
        $("#ProductVideoModal h3").html(title);
        $("#ProductVideoModal p").html(unescape($(obj).find("div").text()));
    }
    function setVideoManually(title, obj) {
        $("#ProductVideoModal h3").html(title);
        $("#ProductVideoModal p").html($(obj).find("div").html());
    }

    var lang;
    var intControlsProduct_InfoSysWordingCheckId;
    intControlsProduct_InfoSysWordingCheckId = setInterval("controlsProduct_InfoSysWordingCheck()", intSysWordingCheckSpeed);

    function controlsProduct_InfoSysWordingCheck() {
        if (sysWordingRead) {
            $("#product-main-image label.checkbox input").val(sysWording.AddToList);
            $("#product-main-image label.checkbox input").after(sysWording.AddToList);
            $("#product-main-image a.btn-asus:eq(0)").html(sysWording.Shop);
            $("#aside-button a.print-btn").html(sysWording.Print);
            $("#aside-button a.link-btn").html(sysWording.MicrositeTitle);
            $("#aside-button a.doc-btn").html(sysWording.DataSheet);
            lang = sysWording.Language;
            XML_ReaderByWebservice("getProduct_Information_xml", "ProductInformation", "ProductHashedId=" + strID);
            clearTimeout(intControlsProduct_InfoSysWordingCheckId);
        }
    }

    var origin_product_img = "";
    $(document).ready(function() {
        create_breadcrumbs("", "");
        //checkProductCompareItem();
        if (getWebsite() == "cafr" || getWebsite() == "caen" || getWebsite() == "us") {
            $('.addthis_button_facebook_like').attr('fb:like:layout', 'button_count');
            $('.addthis_button_google_plusone').attr('g:plusone:size', 'small');
            $(".addthis_button_google_plusone").css("width", "50px");
