    (function () {
        var sc = document.createElement("link"); sc.type = "text/css"; sc.rel = 'stylesheet';
        sc.href = "/css/2015/alert-info.css";
        var s = document.getElementsByTagName("head")[0];
        setTimeout(function () {
            s.appendChild(sc);
        }, 100);

        var sc2 = document.createElement("script"); sc2.type = "text/javascript";
        sc2.src = "/js/2015/alert-info.js";
        var s2 = document.getElementsByTagName("script")[0]; s2.parentNode.insertBefore(sc2, s2);
    })();

</script>
 </body>
</html>