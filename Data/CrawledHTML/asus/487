    $("#ProductDescVideoZone img").one('error', function(event) {
        if ($(this).attr("alt").split("*")[0] == "1") {
            var video_path = SecureHost + "img.youtube.com/vi/" + $(this).attr("alt").split("*")[1] + "/2.jpg"
            $(this).attr("src", video_path);
        }
    });

