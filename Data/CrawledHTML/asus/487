    function create_breadcrumbs(ProductGroupNow, TargetNow) {}
    
    function controlsbreadcrumbsSysWordingCheck() {
        if (sysWordingRead) {
            $("#breadcrumbs a:first").html(sysWording.ProductNavigationHome);
            clearTimeout(intControlsbreadcrumbsSysWordingCheckId);
