            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
        if (getWebsite() == "tw") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/tw/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
        if (getWebsite() == "ph") {
