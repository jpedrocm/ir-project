    var intControlstopSysWordingCheckId;
    intControlstopSysWordingCheckId = setInterval("controlstopSysWordingCheck()", intSysWordingCheckSpeed);

    function controlstopSysWordingCheck() {
        if (sysWordingRead) {
            $("#aai-search-btn").val(sysWording.SearchTitle);
            $("#minicart_panel p.aai-item-titl").html(sysWording.ShoppingCart);

            clearTimeout(intControlstopSysWordingCheckId);
        }
    }
