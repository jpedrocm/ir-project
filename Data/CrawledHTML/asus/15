            lang = sysWording.Language;
            XML_ReaderByWebservice("getProduct_Information_xml", "ProductInformation", "ProductHashedId=" + strID);
            clearTimeout(intControlsProduct_InfoSysWordingCheckId);
        }
    }

    var origin_product_img = "";
