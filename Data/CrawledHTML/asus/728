$(function(){
    $('.pointer').click(function(){
        var $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
        $body.animate({
                scrollTop: $('.title-3').offset().top
        }, 800);
    });
});


$(function(){
    // spec表格切換
    $('.spec-tab a').click(function(){
        $(this).addClass('active');
        $(this).siblings().removeClass('active');
    });
    $('.spec-1').click(function(){
        $('.spec-table-1').show();
        $('.spec-table-2').hide();
    });
    $('.spec-2').click(function(){
        $('.spec-table-2').show();
        $('.spec-table-1').hide();
    });

    // spec表格型號顯示
    $('.control a').click(function(){
        $(this).siblings().removeClass('active');
        $(this).addClass('active');
    });
});

</script> 
