							}
						},
						responsiveLevels:[1240,1024,778,480],
						visibilityLevels:[1240,1024,778,480],
						gridwidth:[1170,1024,778,480],
						gridheight:[500,500,500,320],
						lazyType:"all",
						shadow:0,
