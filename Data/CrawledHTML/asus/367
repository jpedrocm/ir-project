  <script>
        if (window.location.href.indexOf('pick-the-best-laptop') > 1) {
            $('body').addClass('quiz');
        }
        if (window.location.href.toLowerCase().indexOf('sa-ar') > 1) {
            $('body').addClass('rtl');
