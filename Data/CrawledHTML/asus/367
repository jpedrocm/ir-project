  <script>
    //EBG section, load more
    $(function () {
        if ($("#ulEBG").children().length < 24) {
            $('#aLoadMore').hide();
        }
    });

    //Load more click event
    $("#aLoadMore").click(function () {
        var page = $("#ulEBG").children().length / 8;

        if (page >= 3) {
            $.ajax({
                type: 'GET',
                url: 'http://www.asus.com/EVENT/gifting/ID',
                data: "pageNum=" + page,
                beforeSend: function () {
                    $("#imgLoading").show();
                },
                success: function (data, textstatus) {
                    if (data != '') {
                        $("#ulEBG").append(data);
                        resizeBadGiftsThumbs();

                        //Hide load more button when there is no more feed
                        if (($("#ulEBG").children().length / 8) % 1 != 0)
                            $('#aLoadMore').hide();
                    }
                    else
                        $('#aLoadMore').hide();

                    $("#imgLoading").hide();
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    $("#imgLoading").hide();
                }
            });
        }
        else {
            $('#aLoadMore').hide();
        }
    });
</script> 
  <script>
    var uid = 'notlogon';

    //Initialize fb app
    //window.fbAsyncInit = function () {
    $(function () {
        FB.init({
            appId: '528797660608459',
            //appId: '528807870607438', //Staging App ID
            status: true, // check login status
            cookie: true, // enable cookies to allow the server to access the session
            xfbml: true,  // parse XFBML
            version: 'v2.5'
        });
        
        ////Check if user has logon/authorize the app
        //FB.getLoginStatus(function (response) {
        //    if (response.status === 'connected') {
        //        FB.api('/me', function (response) {
        //            //Get user fb id
        //            uid = response.id;
        //        });
        //    }
        //    else if (response.status === 'not_authorized') {
        //        uid = 'notauthorized';
        //    } else {
        //        uid = 'notlogon';
        //    }
        //});
    });
    //};

    //Share post to user's fb profile
    function sharePost(title, imageUrl, caption) {
        if (imageUrl != '')
        {
            if (imageUrl.indexOf('http') < 0)
                imageUrl = "http://" + imageUrl;

            //console.log('Share post with image');
            //console.log(title + ' | ' + imageUrl + '&output=embed' + ' | ' + caption)

            FB.ui({
                method: 'feed',
                link: 'http://www.asus.com/EVENT/gifting/ID',
                picture: imageUrl,
                name: title,
                description: '',
                caption: caption
            },
                function (response) {
                    if (response && !response.error) {
                        console.log(response);
                        console.log('Succeed: Share post with image');
                    }
                }
            );
        }
        else
        {
            //console.log('Share post without image');
            //console.log(title + ' | ' + imageUrl + ' | ' + caption)

            FB.ui({
                method: 'feed',
                link: 'http://www.asus.com/EVENT/gifting/ID'
            },
                function (response) {
                    if (response && !response.error) {
                        console.log(response);
                        console.log('Succeed: Share post without image');
                    }
                }
            );
        }
    }

    $('.btn-fb').click(function (e) {
        e.preventDefault;

        var type = $(this).data('type') == undefined ? '' : $(this).data('type');
        var title = $(this).data('title') == undefined ? '' : $(this).data('title');
        var imageUrl = $(this).data('imageurl') == undefined ? '' : $(this).data('imageurl');
        var caption = $(this).data('caption') == undefined ? '' : $(this).data('caption');

        sharePost(title, imageUrl, caption);
    });

    $('.btn-tweet').click(function (e) {
        e.preventDefault;
        
        var text = $(this).data("text") == undefined ? '' : $(this).data('text');
        var hashtag;

        if (text == '')
            text = window.jsTweetMessage == undefined ? '' : window.jsTweetMessage;

        if (text == '')
            text = 'Pick the Best Laptop';
            
        if ('id' == 'fr')
            hashtag = 'Pcidéal';
        else if ('id' == 'it')
            hashtag = 'regalosbagliato';
        else
            hashtag = 'bestlaptop';

        var url = 'https://twitter.com/intent/tweet?url=http://www.asus.com/EVENT/gifting/ID&hashtags=' + hashtag + '&text=' + text;

        popupWindow = window.open(
        url, 'popUpWindow', 'height=700,width=800,left=100,top=100,resizable=yes,scrollbars=yes,toolbar=yes,menubar=no,location=no,directories=no,status=yes')
    });
</script>   
