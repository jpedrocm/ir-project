     <script>
    var isInGroup = '0';
    var compare_open = "F";

    function checkShoppingCart(url) {
        //: Check Cart Quantity
        $.getJSON(url, function (cartJsonData) {
            if (!cartJsonData.shoppingCart.isEmpty) $("#num_shoppingcart").html(cartJsonData.shoppingCart.totalQuantity);
        });
    }

    //: ------------- BEGIN -------------
    //: [[Compare Model 2012/05/29 Angel]]
    function getCompareCookie() {
        var theData = "";
        var theCookie = document.cookie;
        var compareGroupName = "";
        var val = theCookie.split(";");

        for (i = 0; i < val.length; i++) {
            compareGroupName = "";
            var data = val[i].split("=");
            if (data.length > 1) {
                var index = data[0].indexOf("compare_");
                if (index > -1) {
                    var compareid = data[0].substring(index + 8);
                    if (CompareJson) {
                        $.map(CompareJson, function (obj_compare) {
                            if (obj_compare.CompareID === compareid)
                                compareGroupName = obj_compare.CompareName
                        });
                    }
                    if (data[1] != "")
                        theData += "compare_" + compareid + "." + compareGroupName.replace(/^\s+/, "") + ":" + unescape(data[1]) + ";";
                }
            }
        }
        return theData;
    }

    function getMessage() {
        //var msg_data = (getWebsite() == "cn") ? '//www.asus.com.cn/message.aspx.aspx?lang=' + strLang :
        //    '//www.asus.com/message.aspx?lang=' + strLang;
