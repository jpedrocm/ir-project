  <script type="text/javascript">
    //loading local css file
    $(document).ready(function () {
        var css_name = "";
        var lang_prefix = window.location.pathname.split("/");
        if (lang_prefix.length >= 2)
            if (lang_prefix[1] == "support")
                css_name = "en.css";
            else
                css_name = lang_prefix[1] + ".css";

        if (window.console)
            console.log("css name: " + css_name);

        if (window.location.hostname.indexOf("asus.com.cn") >= 0) {
            css_name = "cn.css";
        }

        if (window.console)
            console.log("css name: " + css_name);

        if (css_name != "") {
            var link = document.createElement('link');
            link.type = 'text/css';
            link.rel = "stylesheet";
            link.href = "/support/stylesheets/local/" + css_name;
            document.getElementsByTagName("head")[0].appendChild(link);
        }

        //video play box
        $(".vEmbedded > a").colorbox({ iframe: true, width: "70%", height: "80%" });




    });
</script> 
  <script type="text/javascript">
    window.ASUSInit = function () {
        var api_lang = '';
        var api_key = '0000000004';
        var lang_prefix = window.location.pathname.split("/");
        if (window.console) {
            console.log("pathname: " + window.location.pathname);
        }
        if (lang_prefix.length >= 2) {
            if (lang_prefix[1] == "support")
                api_lang = "";
            else
                api_lang = lang_prefix[1];
        }
        if (window.location.hostname.indexOf("asus.com.cn") >= 0) {
            api_lang = "cn";
            api_key = '0000000019';
        }
        if (window.console)
            console.log("lang: " + api_lang);
        asus_api.init({ appkey: api_key, lang: api_lang });
    };
    (function (d) {
        var js, id = 'asus-jsapi', IsDebug='False';

        if (d.getElementById("asus-jsapi")) { return; }
        js = d.createElement('script');
        js.id = id;
        js.async = true;

        if(IsDebug == 'True'){
//                js.src = "//test.asus.com/API/js/asus_api_v2.js";
                js.src = "//www.asus.com/API/js/asus_api.js";
        }else{
            if (window.location.href.indexOf("asus.com.cn") >= 0 || window.location.hostname.indexOf("asus.com.cn") >= 0)
                js.src = "//www.asus.com.cn/API/js/asus_api.js";
            else
                js.src = "//www.asus.com/API/js/asus_api.js";
        }
       
        d.getElementsByTagName('head')[0].appendChild(js);
    } (document)); 

</script> 
