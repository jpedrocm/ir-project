     <script type="text/javascript">
    var clientID = $(".hd_iqClient_ID").val();

    $("#" + clientID + "btnQuery").click(function () {
        var sRMANo = $("#txtRMANo").val();
        var sSN = $("#txtSN").val();
        if (sRMANo.length == 0 && sSN.length == 0) {
            alert($("#hd_please_enter_alert").val());
            return;
        } else {
            //var url = $("#hd_lang_prefix").val() + "/Repair-Status-Result?LangCode=" + $("#hd_lang").val() + "&RMANO=" + sRMANo + "&SN=" + sSN;
            var url = $("#hd_lang_prefix").val() + "/Repair-Status-Result?RMANO=" + sRMANo + "&SN=" + sSN + "&country=" + $("#hd_country").val();
            $("#" + clientID + "btnQuery").attr("href", url);
        }
    });
</script> 
