            <script type="text/javascript">
    var intControlsProductDesignSysWordingCheckId;
    intControlsProductDesignSysWordingCheckId = setInterval("controlsProductDesignSysWordingCheck()", intSysWordingCheckSpeed);

    function controlsProductDesignSysWordingCheck() {
        if (sysWordingRead) {
            $("#product-design-story-area h2:first").html(sysWording.BehindDesign);
            clearTimeout(intControlsProductDesignSysWordingCheckId);
        }
    }
