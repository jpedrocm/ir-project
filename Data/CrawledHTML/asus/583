  <script>
    $(document).ready(function () {
        if (getWebsite() == "us") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/us/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
        if (getWebsite() == "th") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/th/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
        if (getWebsite() == "tw") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/tw/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
        if (getWebsite() == "ph") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/ph/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
    });

    (function () {
        var sc = document.createElement("link"); sc.type = "text/css"; sc.rel = 'stylesheet';
        sc.href = "/css/2015/alert-info.css";
        var s = document.getElementsByTagName("head")[0];
        setTimeout(function () {
            s.appendChild(sc);
        }, 100);

        var sc2 = document.createElement("script"); sc2.type = "text/javascript";
        sc2.src = "/js/2015/alert-info.js";
        var s2 = document.getElementsByTagName("script")[0]; s2.parentNode.insertBefore(sc2, s2);
    })();

</script>
 </body>
</html>