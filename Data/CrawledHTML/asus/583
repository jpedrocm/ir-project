            window.location.href = '/hk/Notebooks/ASUS-ZenBook-UX305CA' + '/' + 'HelpDesk' + '_' + hash_array[1] + '/';
        else
            window.location.href = '/hk/Notebooks/ASUS-ZenBook-UX305CA' + '/' + 'HelpDesk' + '/';
    }

    strContent = current_tab;
