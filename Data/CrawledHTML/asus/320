  <script type="text/javascript">
        $("#btn_product").click(function () {
            $("#service-center-search-zone .dropdown-menu").toggle();
        });
        $(".dropdown-menu a").click(function () {

            $("#service-center-search-zone .dropdown-menu").toggle();
            $("#hd_sc_product_id").val($(this).attr("l1_id"));
            $("#hd_sc_product_name").val($(this).text());
            $("#btn_product").html("<span class=txt>" + $(this).text() + "</span><span class=caret></span>");
            RefleshScAround();
        });

        $("#search-btn2").click(function () {
            RefleshScAround($('#hd_country_en_name').val(), $('#txt_ScKeyword').val());
        });

        $("#txt_ScKeyword").keydown(function (e) {
            if (e.keyCode == 13) {
                RefleshScAround($('#hd_country_en_name').val(), $('#txt_ScKeyword').val());
            }
        });
    </script> 
  <!--Announcement --> 
  <script type="text/javascript">
            var ApiUrl = "http://demeter.asus.com/WSC/Support_API_New/SupportNewAPI/GetServiceCenterAnnouncement/";
            if (location.protocol == 'https:') {
                ApiUrl = "http://demeter.asus.com/WSC/Support_API_New/SupportNewAPI/GetServiceCenterAnnouncement/";
            }

        </script> 
  <script type="text/javascript">
        $(document).ready(function () {
            var _Local = $("#langNormal").val();
            var ServiceCenterAnnounceMent = initServiceCenterAnnounceMent(_Local);
            ServiceCenterAnnounceMent.init();

            function initServiceCenterAnnounceMent(Local) {

                var ApiOption;
                
                ApiUrl += Local;

                if (Local == "ru-ua" || Local == "ua-ua") {
                    ApiUrl += "/1/";
                }

                return {
                    init: function () {
                        $.getJSON(ApiUrl + "?callback=?", ApiOption).done(function (reuslt) {

                            if (_Local == "ru-ua" || _Local == "ua-ua") {

                                $(reuslt).insertAfter(".double-border-bottom-2");
                            } else {
                                $(reuslt).insertAfter(".double-border-bottom");
                            }

                            
                        });
                    }
                }
            }
        });
                </script>   
