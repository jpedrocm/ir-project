        <script type="text/javascript">
    var breadcrumbsXML;
    var intProductCategoryLevel = 1;
    var xmlProduct;
    var intMouseOverCategoryLevel;
    var TrackerWording = "";

    var intControlsbreadcrumbsSysWordingCheckId;

    function getMBNo1() {
