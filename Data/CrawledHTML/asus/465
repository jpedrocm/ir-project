      <script type="text/javascript">$(function(){var cD=new Date();var tMs=1476115200;$(".hook_banner").fadeIn();function timerSet(){var d=new Date();var dMs=Math.floor(d.getTime()/1000);var deltTime=tMs-dMs;var deltD=Math.floor(deltTime/86400);var deltH=Math.floor((deltTime-deltD*86400)/3600);var deltM=Math.floor((deltTime-deltD*86400-deltH*3600)/60);var deltS=deltTime-deltD*86400-deltH*3600-deltM*60;$(".number_w").removeClass(function(index,css){return(css.match(/(^|\s)n_w_\S+/g)||[]).join(' ');});$(".timer_d .number_w-1").addClass("n_w_"+Math.floor(deltD/10));$(".timer_d .number_w-2").addClass("n_w_"+(deltD-Math.floor(deltD/10)*10));$(".timer_h .number_w-1").addClass("n_w_"+Math.floor(deltH/10));$(".timer_h .number_w-2").addClass("n_w_"+(deltH-Math.floor(deltH/10)*10));$(".timer_m .number_w-1").addClass("n_w_"+Math.floor(deltM/10));$(".timer_m .number_w-2").addClass("n_w_"+(deltM-Math.floor(deltM/10)*10));$(".timer_s .number_w-1").addClass("n_w_"+Math.floor(deltS/10));$(".timer_s .number_w-2").addClass("n_w_"+(deltS-Math.floor(deltS/10)*10));if(deltTime>0){setTimeout(timerSet,1000);}else{$(".hook_banner").fadeOut(400);$(".hook_final_banner").fadeIn(800);}}
