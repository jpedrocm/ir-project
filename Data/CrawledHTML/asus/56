  <script type="text/javascript">
        var default_site = "";
        var expiresec = 60 * 60 * 24 * 365 * 10 * 1000;

        function directURL() {
            if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)) {
                var referLink = document.createElement('a');
                referLink.href = default_site;
                referLink.target = "_top";
                document.body.appendChild(referLink);
                referLink.click();
            } else {
                window.parent.location = default_site;
            }
        }

        function setDefaultLocation() {
            var exdate = new Date();
            exdate.setSeconds(exdate.getSeconds() + expiresec);

            document.cookie = "country_setting=" + escape(default_site) + ";path=/;expires=" + exdate.toGMTString() + ";domain=.asus.com;";
            directURL();
        }

        function getLanguageMapping(LangSite) {
            $.ajax({
                datatype: 'xml',
                url: 'GetData.asmx/getLanguageMapping',
                type: 'GET',
                data: { LangSite: LangSite },
                success: function (xml) {
                    $("#RememberModel div.modal-body").html($("Auto_Select_Region_Mesg", xml).text());
                    $("#RememberModel div.modal-footer a:eq(0)").html($("Auto_Select_Region_Yes", xml).text());
                    $("#RememberModel div.modal-footer a:eq(1)").html($("Auto_Select_Region_No", xml).text());
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) { }
            });
        }

        $(document).ready(function () {
            $("#RememberModel").css({ "height": 110, "width": 200 });
            $("#main-zone a").on("click", function () {
                $("#RememberModel").modal('show');
                default_site = $(this).attr("href");
                return false;
            });

            //: 2013/12/18 Angel, detect browser language as default
            var lang = (navigator.language || navigator.systemLanguage || navigator.userLanguage || 'en').toLowerCase();
            getLanguageMapping(lang);
        });

        function goSeltLocation() {
            document.cookie = "GlobalNoFlag=Y;path=/;domain=.asus.com;";
            document.cookie = "current_site=" + default_site + ";path=/;domain=.asus.com;";
            directURL();
        }

      
    </script> 
 </body>
