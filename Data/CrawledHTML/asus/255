  <script type="text/javascript">
        var qlist = [1, -1, -1, -1, -1, -1, -1, -1, -1];

        function set_q(nbr, val) {

            qlist[nbr] = val;

            $(".picker-object").each(function () {
                $(this).attr('s1', '1');
            });

            for (var i = 1; i < qlist.length; i++)
            {
                if (qlist[i] != -1)
                {
                    //alert(i);
                    $(".picker-object").each(function () {
                        if ($(this).attr('Q' + i.toString()).substr(qlist[i], 1) == '0') {
                            $(this).attr('s1', '0');
                            //alert('Q' + i.toString() + '|' + val + '|' + $(this).attr('Q' + i.toString()).substr(val, 1));
                        }
                    });
                }
            }

            show_picker_part();
            //show_picker_object();
        }

        function show_picker_object()
        {
            var is_all_hide = true;

            var show_count = 0;

            $(".picker-object").each(function () {
                $(this).hide();
                if ($(this).attr('s1') != '0' && $(this).attr('s2') != '0')
                {
                    if ($('#hf_show_all').val() == '1')
                    {
                        $(this).show();
                        is_all_hide = false;

                        show_count += 1;
                    }
                    else //if ($('#hf_show_all').val() == '0')
                    {
                        if (show_count < 25) {
                            $(this).show();
                            is_all_hide = false;
                            show_count += 1;
                        }
                    }
                }
            });

            if (is_all_hide)
                $('.picker-not-applicable').show();
                else
                $('.picker-not-applicable').hide();

            if (show_count < 25)
            {
                $('#p_show_picker_all').hide();
                $('#p_show_picker_part').hide();
            }
        }

        function clear_all_picker()
        {
            $('#p_show_picker_all').show();
            $('#p_show_picker_part').hide();

            $('.answer-input').attr('checked', false);

            qlist = [1, -1, -1, -1, -1, -1, -1, -1, -1];

            $(".picker-object").each(function () {
                $(this).attr('s1', '1');
            });

            $('.picker-not-applicable').hide();

            show_picker_object();
        }

        function show_picker_all()
        {
            $('#hf_show_all').val('1');

            $('#p_show_picker_all').hide();
            $('#p_show_picker_part').show();

            show_picker_object();
        }

        function show_picker_part()
        {
            $('#hf_show_all').val('0');

            $('#p_show_picker_all').show();
            $('#p_show_picker_part').hide();

            show_picker_object();
        }

        $(function () {
            //alert('f');
            show_picker_part();
        });

        function ga_lang(parm1, parm2, parm3, parm4, parm5)
        {
            //ga_lang('send', 'event', 'menus', 'clicked', '100series-menus-tw')
            ga(parm1, parm2, parm3, parm4, parm5 + '-' + $('#sel_language').val());
            //alert(parm5 + '-' + $('#sel_language').val());
        }
