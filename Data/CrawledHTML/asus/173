    <script>
 
        function tempFixForTallTitles() {
            var title = $(".uif_mhPageTitle").eq(0),
                titleHeight = title.height(),
                isSkinny = (($(".uif_mastHead_skinny").length > 0) || ($(".uif_mastHead_skinnyWP").length > 0)) ? true : false,
                isSkinnyPartial = ($(".uif_mastHead_skinny_control").length > 0) ? true : false,
                isBuilder = ($(".uif_mhbodyBuilder").length > 0) ? true : false;
            if ((titleHeight > 30 && !isSkinny && !isBuilder) || (titleHeight > 60 && isSkinny)) {
                if (isSkinny) {
                    $("html").eq(0).addClass("uif_altTitleSkinny");
                } else if (isBuilder) {
                    $("html").eq(0).addClass("uif_altTitleBuilder");
                } else {
                    $("html").eq(0).addClass("uif_altTitle");
                }
                title.hide();
                if (isSkinny) {
                    $(".uif_bodySubWrap").eq(0).prepend("<div class='uif_altMainFoot clearfix'>" + $(".uif_mhMainFoot").eq(0).hide().html() + "</div>");
                }
                $(".uif_bodySubWrap").eq(0).prepend("<h1 class='uif_altPageTitle gsd_pageTitle'>" + title.html() + "</h1>");
            }
            if (isSkinnyPartial) {
                $("html").eq(0).addClass("uif_altTitleSkinnyPartial");
                $(".uif_bodySubWrap").eq(0).prepend("<div class='uif_altMainFoot clearfix'>" + $(".uif_mhMainFoot").eq(0).hide().html() + "</div>");
                $(".uif_bodySubWrap").eq(0).prepend(title);
            }            
        };
        tempFixForTallTitles();
    
    </script> 
