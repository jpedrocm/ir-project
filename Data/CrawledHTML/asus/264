        window.location.href = '/fi/2-in-1-PCs/ASUS_Transformer_Book_T100HA' + '/' + strContent + '/';
    }

    if (strContent.toLowerCase() == "support") {
        if (hash_array.length > 1)
            window.location.href = '/fi/2-in-1-PCs/ASUS_Transformer_Book_T100HA' + '/' + 'HelpDesk' + '_' + hash_array[1] + '/';
