        <script type="text/javascript">
    var breadcrumbsXML;
    var intProductCategoryLevel = 1;
    var xmlProduct;
    var intMouseOverCategoryLevel;
    var TrackerWording = "";

    var intControlsbreadcrumbsSysWordingCheckId;

    function getMBNo1() {
        $("#MDA").html("<img src='/media/" + getWebsite() + "/images/no1.jpg' border='0' onerror='this.src=\"media/global/images/no1.jpg\";' />");
