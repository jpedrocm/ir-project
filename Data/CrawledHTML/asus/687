      //top banner
      var topAd = new Swiper('.ad-slide', {
          pagination: '.swiper-pagination',
          nextButton: '.ad-btn-next',
          prevButton: '.ad-btn-prev',
          slidesPerView: 1,
          paginationClickable: true,
