      $('#btnMeritLeft').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#meritRight,.mask').hide();
        $('#meritLeft,.mask').show();
      });
      $('#btnMeritRight').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#meritLeft,.mask').hide();
        $('#meritRight,.mask').show();
      });
      $('#btnMeritCloseL').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#meritLeft,.mask').hide();
      });
      $('#btnMeritCloseR').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#meritRight,.mask').hide();
      });

      //upgrade your ingame fn menu 1
      $("#crosshairToggle").on("click", function() {
        $("#crosshairToggle,#timerToggle,#fpsToggle").removeClass("on");
        $("#crosshairToggle").addClass("on");
        $("#timerLayer,#fpsLayer").hide();
        $("#crosshairLayer").show();
      });
      $("#timerToggle").on("click", function() {
        $("#crosshairToggle,#timerToggle,#fpsToggle").removeClass("on");
        $("#timerToggle").addClass("on");
        $("#crosshairLayer,#fpsLayer").hide();
      $("#timerLayer").show();
      });
      $("#fpsToggle").on("click", function() {
        $("#crosshairToggle,#timerToggle,#fpsToggle").removeClass("on");
        $("#fpsToggle").addClass("on");
        $("#crosshairLayer,#timerLayer").hide();
        $("#fpsLayer").show();
      });
      //upgrade your ingame fn menu 2
      $(".gamevisual-btns li a").click(function () {
          $(".gamevisual-btns li a").removeClass("active");
          $(this).addClass("active");
      });
      $(".gamevisual-btn-01").on("click", function() {
          $(".gamevisual-g").hide();
          $("#Gamevisual-FPS").show();
          
      });
      $(".gamevisual-btn-02").on("click", function() {
          $(".gamevisual-g").hide();
          $("#Gamevisual-RTS").show();
      });
      $(".gamevisual-btn-03").on("click", function() {
          $(".gamevisual-g").hide();
          $("#Gamevisual-Racing").show();
      });
      $(".gamevisual-btn-04").on("click", function() {
          $(".gamevisual-g").hide();
          $("#Gamevisual-Cinema").show();
      });
      $(".gamevisual-btn-05").on("click", function() {
          $(".gamevisual-g").hide();
          $("#Gamevisual-Scenery").show();
      });
      $(".gamevisual-btn-06").on("click", function() {
          $(".gamevisual-g").hide();
          $("#Gamevisual-sRGB").show();
      });
      //upgrade your ingame fn menu 3
      $(".ergonomic-btns li a").click(function () {
          $(".ergonomic-btns li a").removeClass("active");
          $(this).addClass("active");
      });
      $("#ergonomic-btn-00").on("click", function() {
          $(".ergonomic-pics").hide();
          $("#pics-00").show();
      });
      $("#ergonomic-btn-01").on("click", function() {
          $(".ergonomic-pics").hide();
          $("#pics-01").show();
      });
      $("#ergonomic-btn-02").on("click", function() {
          $(".ergonomic-pics").hide();
          $("#pics-02").show();
      });
      $("#ergonomic-btn-03").on("click", function() {
          $(".ergonomic-pics").hide();
          $("#pics-03").show();
      });
      $("#ergonomic-btn-04").on("click", function() {
          $(".ergonomic-pics").hide();
          $("#pics-04").show();
      });

      //open player
      $('#btnOpenPlayer01').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#user01,#user02,#user03,#user04,.mask').hide();
        $('#user01,.mask').fadeIn();
      });
      $('#btnOpenPlayer02').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#user01,#user02,#user03,#user04,.mask').hide();
        $('#user02,.mask').fadeIn();
      });
      $('#btnOpenPlayer03').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#user01,#user02,#user03,#user04,.mask').hide();
        $('#user03,.mask').fadeIn();
      });
      $('#btnOpenPlayer04').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#user01,#user02,#user03,#user04,.mask').hide();
        $('#user04,.mask').fadeIn();
      });
      $('#bk_Close1,#bk_Close2,#bk_Close3,#bk_Close4').on('click',function(){
        $('body').css({overflow:"scroll"});
        $("#user01,#user02,#user03,#user04,.mask").fadeOut();
      });

      //open album
      $('#btnOpenAlbum').on('click',function(){
        $('.gallery-box').fadeOut(function(){
          $('.album-list-box').fadeIn();
        });
      });
      $('#btnBackAlbum').on('click',function(){
        $('.album-list-box').fadeOut(function(){
          $('.gallery-box').fadeIn();
        });
      });

      //open album list
      $('#album01').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#albumList01').show();
        galleryTop.update();
        galleryThumbs.update();
        
      });
      $('#album02').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#albumList02').show();
        galleryTop2.update();
        galleryThumbs2.update();
      });
      $('#album03').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#albumList03').show();
        galleryTop3.update();
      galleryThumbs3.update();
      });

      $('#btnCloseAlbum01').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#albumList01').hide();
      });
      $('#btnCloseAlbum02').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#albumList02').hide();
      });
      $('#btnCloseAlbum03').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#albumList03').hide();
      });

      
      //photo list 1
      var galleryTop = new Swiper('.gallery-top', {
          nextButton: '.swiper-button-next',
          prevButton: '.swiper-button-prev',
          spaceBetween:0
      });
      var galleryThumbs = new Swiper('.gallery-thumbs', {
          spaceBetween: 10,
          centeredSlides: true,
          slidesPerView: 'auto',
          touchRatio: 0.2,
          slideToClickedSlide: true
      });
      galleryTop.params.control = galleryThumbs;
      galleryThumbs.params.control = galleryTop;

      //photo list 2
      var galleryTop2 = new Swiper('.gallery-top2', {
          nextButton: '.photo-one-next2',
          prevButton: '.photo-one-prev2',
          spaceBetween:0
      });
      var galleryThumbs2 = new Swiper('.gallery-thumbs2', {
          spaceBetween: 10,
          centeredSlides: true,
          slidesPerView: 'auto',
          touchRatio: 0.2,
          slideToClickedSlide: true
      });
      galleryTop2.params.control = galleryThumbs2;
      galleryThumbs2.params.control = galleryTop2;

      //photo list 3
      var galleryTop3 = new Swiper('.gallery-top3', {
          nextButton: '.photo-one-next3',
          prevButton: '.photo-one-prev3',
          spaceBetween:0
      });
      var galleryThumbs3 = new Swiper('.gallery-thumbs3', {
          spaceBetween: 10,
          centeredSlides: true,
          slidesPerView: 'auto',
          touchRatio: 0.2,
          slideToClickedSlide: true
      });
      galleryTop3.params.control = galleryThumbs3;
      galleryThumbs3.params.control = galleryTop3;

      
      
    });
