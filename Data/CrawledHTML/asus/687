  <script>
    $(function(){
      var w = $(window).width();
      if(w >= 881){
        $('.menu').removeAttr('style');
      }
      $(window).resize(function(){
        var wR = $(window).width();
        if(wR >= 881){
          $('.menu').removeAttr('style');
        }
      })
      var toggles = document.querySelectorAll(".c-hamburger");
      for (var i = toggles.length - 1; i >= 0; i--) {
          var toggle = toggles[i];
          toggleHandler(toggle);
      };
      function toggleHandler(toggle) {
          toggle.addEventListener( "click", function(e) {
              e.preventDefault();
              if (this.classList.contains("is-active") === true) {
                  this.classList.remove("is-active");
                  $(".menu").slideUp();
              }
              else {
                  this.classList.add("is-active");
                  $(".menu").slideDown();
              }
          });
      }

      //nav select
      $(".nav_li_01").zxxAnchor();
      $(".nav_li_02").zxxAnchor();
      $(".nav_li_03").zxxAnchor();
      $(".nav_li_04").zxxAnchor();
      $(".nav_li_05").zxxAnchor();
      $("#goMerit").zxxAnchor();
      $("#goRecommend").zxxAnchor();
      $("#goDownload").zxxAnchor();
      $("#goGallery").zxxAnchor();



      // $('.owl-carousel').owlCarousel({
      //   loop:true,
      //   margin:0,
      //   nav:true,
      //   // dots:true,
      //   dotsEach:true,
      //   items:1,
      //   center: true
      // })

      // var pgwSlideshow = $('.pgwSlideshow').pgwSlideshow();
      // pgwSlideshow.reload({
      //   transitionEffect : 'fading',
      //   adaptiveDuration : 300
      // });

      //menu open products
      $('#prod01').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#prodCont01').show();
      });
      $('#prod02').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#prodCont02').show();
      });
      $('#prod03').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#prodCont03').show();
      });
      $('#prod04').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#prodCont04').show();
      });
      $('#prod05').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#prodCont05').show();
      });
      $('#prod06').on('click',function(){
        $('body').css({overflow:"hidden"});
        $('#prodCont06').show();
      });

      $('#prodCont01 .btn-close-photo').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#prodCont01').hide();
      });
      $('#prodCont02 .btn-close-photo').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#prodCont02').hide();
      });
      $('#prodCont03 .btn-close-photo').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#prodCont03').hide();
      });
      $('#prodCont04 .btn-close-photo').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#prodCont04').hide();
      });
      $('#prodCont05 .btn-close-photo').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#prodCont05').hide();
      });
      $('#prodCont06 .btn-close-photo').on('click',function(){
        $('body').css({overflow:"scroll"});
        $('#prodCont06').hide();
      });
      //full prod lightbox
      $('#btnLeftProd01').on('click',function(){
        $('#prodLeftCont01,.mask').show();
      });
      $('#closeLeft01').on('click',function(){
        $('#prodLeftCont01,.mask').hide();
      });
      $('#btnLeftProd02').on('click',function(){
        $('#prodLeftCont02,.mask').show();
      });
      $('#closeLeft02').on('click',function(){
        $('#prodLeftCont02,.mask').hide();
      });
      $('#btnLeftProd03').on('click',function(){
        $('#prodLeftCont03,.mask').show();
