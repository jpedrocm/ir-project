  var top_banner_resize = function() {
      if ($(window).width() < 720) {
          $(".product-home-top-banner").css('width', '100%');
          $(".product-home-top-banner").css('height', parseInt(($(".product-home-top-banner").width() / 985) * 460));
      } else {
          $(".product-home-top-banner").css('width', '');
          $(".product-home-top-banner").css('height', '');
      }

  }
  $(window).resize(function() {
      top_banner_resize();
  });
