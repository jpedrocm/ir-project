       <script>
    $(window).on("resize", window_resize);
    $(document).ready(function () {
        if ($(window).width() < 768) {
            setMobileView()
        }
    });
    
    function window_resize() {
        if ($(window).width() < 768) {
            setMobileView();
        } else if ($(window).width() >= 768) {
            $("#div_pc").show();
            $("#div_mobile").hide();
            
        }
    }
    function setMobileView() {
        $("#div_pc").hide();
        $("#div_mobile").show();
        var zm = ($(window).width() / $(".mobile-phone").attr("data-imgw"));
        $(".mobile-phone").css("zoom", zm * 0.97);
    }


