    <div class="search-bar"> 
     <input type="text" class="input-search" id="top-search-bar" autocomplete="off" placeholder="Search out products" onkeyup="lookup(this.value, event);"> 
     <button class="btn btn-search-submit" onclick="Search();return false;"> <i class="icon icon-search"></i> </button> 
     <div id="suggestions" class="input-medium search-query"></div> 
    </div> 
    <div class="link_home"> 
     <a href="http://www.asus.com">Return to Homepage <img src="/images/arr.png"></a>
    </div> 
   </div> 
  </div>  
  <script language="javascript" type="text/javascript">

    function Search() {
        var domain = "//www.asus.com";
        var keyword = $("#top-search-bar").val();
        var local = ("/" + getWebsite()).replace("/global", "");
        var APIurl = domain + local + "/search/results.aspx?SearchKey=" + encodeURI(keyword);
        window.location.href = APIurl;
    }

    $("#top-search-bar").keypress(function (event) {
        if (event.keyCode == 13) { Search(); }
    });

    var currentIndex = -1;
    var firstStr = "";

    function lookup(inputString, e) {
        
        var url = "https://www.asus.com/searchapi/api/Suggestion";
        var options = {
            queryText: inputString.trim() + "*",
            country: getWebsite(),
            type: "Products,Commercial",
            RowLimit: "10,10"
        };
        var KeyCode = window.event ? e.keyCode : e.which;
        $("#searchresults a").removeClass("highlight");
        if (KeyCode != 38 && KeyCode != 40) {
            if (inputString.length == 0) {
                $('#suggestions').fadeOut();
            } else {
                if (KeyCode != 13) {
                    ajaxCallJsonp(url + "?", options);
                    $('#suggestions').fadeIn();
                }
            }
        }
        else {

            var $results = $("#searchresults"), currentItem;
            var total = $results.find("a").length;
            var maxIndex = total - 1;
            //向上鍵
            if ((event.keyCode == 38) && ($("#suggestions").html() != "")) {
                currentIndex = currentIndex <= -1 ? maxIndex : currentIndex - 1;
                $results.find("a").removeClass("highlight");
                if (currentIndex == (-1)) {
                    //currentIndex == -1, 顯示使用者輸入的搜尋字串
                    $("#searchinput2").focus();
                    $("#searchinput2").val(firstStr.trim());
                }
                else {
                    currentItem = $results.find("a").eq(currentIndex);
                    currentItem.addClass("highlight");
                    $("#searchinput2").val(currentItem.text().trim());
                }
            }
            //向下鍵
            if ((event.keyCode == 40) && ($("#suggestions").html() != "")) {
                currentIndex = currentIndex >= maxIndex ? -1 : currentIndex + 1;
                $results.find("a").removeClass("highlight");
                if (currentIndex == (-1)) {
                    //currentIndex == -1, 顯示使用者輸入的搜尋字串
                    $("#searchinput2").focus();
                    $("#searchinput2").val(firstStr.trim());
                }
                else {
                    currentItem = $results.find("a").eq(currentIndex);
                    currentItem.addClass("highlight");
                    $("#searchinput2").val(currentItem.text().trim());
                }
            }
        }


        function ajaxCallJsonp(target, options) {

            var data = $.getJSON(target, options);

            //成功得到資料
            data.success(function (msg) {
                msg = MarkHighLight(inputString, msg);
                $("#suggestions").empty();
                $("#suggestions").append(msg);
                $('#searchresults a').each(function () {
                    var href = $(this).attr('href');
                    $(this).attr('onclick', "window.location='" + href + "?SearchKey=" + inputString + "/'").removeAttr('href');
                });
                $('#searchresults a').mouseover(function () {
                    var $parent = $(this).parent();
                    $parent.find('a').removeClass('highlight');
                    $(this).not('.MoreTag').addClass('highlight');
                }).mouseout(function () {
                    $(this).removeClass('highlight');
                });

                //起始從-1開始，並記一開始輸入查詢
                currentIndex = -1;
                firstStr = $("#searchinput2").val();
                //replace span to hr
                var $span = $("#suggestions span.category");
                $span.replaceWith(function () {
                    return $('<hr/>', {
                        html: this.innerHTML,
                        style: 'color: #F8F8F8'
                    });
                });
                var left = document.getElementById('searchinput2').getBoundingClientRect().left;
                var width = $('.span7').css('width');
                $('#suggestions').css({ 'width': width, 'left': left });
                $('p#searchresults').css({ 'width': width });
            });

            //取得資料失敗
            data.error(function (XMLHttpRequest, textStatus, errorThrown) {
                //$("#suggestions").html("fail getting data: " + errorThrown);
            });

        }

    }

   function MarkHighLight(inputString, msg) {
        var inputStringTrim = inputString.trim();
        var html = $.parseHTML(msg);
        if (msg.length > 0) {
            $(html).find('span').each(function () {
                //find highlight word
                var word = $(this).text();
                var index = word.toLowerCase().indexOf(inputStringTrim.toLowerCase());
                if (index > -1) {
                    var highlight = word.slice(index, index + inputStringTrim.length);
                    //surround highlight word with <span class='keyword'></span>
                    var keyword = '<span class="keyword">' + highlight + '</span>';
                    //replace span.text() in msg surrounded word
                    var regex = new RegExp(highlight, 'g');
                    var surroundedWord = word.replace(regex, keyword);
                    $(this).html(surroundedWord);
                }
            });
        }
        return html;
    }

    
</script>  
 </body>
</html>