             <!-- section__10 --> 
             <section id="section__touch" class="wd_section section__10 theme__text-white"> 
              <div class="section_content"> 
               <div class="row"> 
                <div class="col l12 m12 s12"> 
                 <div class="wd__content large__text-center"> 
                  <div class="content__slogan">
                   Bezpečný dotyk
                  </div> 
                  <div class="content__title">
                   Čítačka odtlačkov prstov 
                   <span class="no__wrap">a oveľa viac</span>
