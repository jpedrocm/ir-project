	$(document).ready(function() {
		$('#footerEmailSignup').bind( "click", function() {
			$('#innerSignUp').toggle();
			if($('#innerSignUp').is(":visible")){
				$('#footerEmailSignup').addClass('signUpActive');
			}else{
				$('#footerEmailSignup').removeClass('signUpActive');
