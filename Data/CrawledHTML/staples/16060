  <script type="text/javascript">
	
  if(propertyValues.powerFrontProChatSwitch ===  'ON' || propertyValues.powerFrontGlobalChatSwitch ===  'ON'){
	STAPLES.Onload.addWindowEvent(function(){
	   
            var s = document.createElement("script");
            s.type = "text/javascript";
            s.src = '//assets.staples-static.com/NDM2/sbdpas/20161013151911/js/powerfront/powerfrontchat.js';
