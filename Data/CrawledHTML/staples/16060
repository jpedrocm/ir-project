  <script type="text/javascript">
	STAPLES.Onload.addWindowEvent(function(){
		function loadjscssfile(filename, filetype){
			var fileref = "";
			var prefix = "//assets.staples-static.com/NDM2/sbdpas/20161013151911/onlineopinionV5/";

	 		if (filetype=="js"){
	 			fileref = document.createElement('script');
	  			fileref.setAttribute("type","text/javascript");
	  			fileref.setAttribute("src", prefix + filename);
					fileref.async = true;
			}
	 		else if (filetype=="css"){
	  			fileref = document.createElement("link");
	  			fileref.setAttribute("rel", "stylesheet");
	  			fileref.setAttribute("type", "text/css");
	  			fileref.setAttribute("href", prefix + filename);
	 		}
	 		document.getElementsByTagName("head")[0].appendChild(fileref);
	 	}

		loadjscssfile("oo_style_english.css", "css");
		loadjscssfile("oo_tab_ux2.js", "js")
	});
</script> 
  <script type="text/javascript">
        function showCartPopup(url,id,partNumber,catentryId,quantity,yourOrderOverlayFlag) {
            STAPLES.Main.closeoverlay();
            STAPLES.Main.addToCartWithOverlay(url,id,partNumber,catentryId,quantity,yourOrderOverlayFlag); 
        }
        //IMO, it is absolutely unacceptable that this is required on pages - MV
        (function(window){
          var protocol = window.location.protocol;
          var addToCartUrlSuffix = STAPLES.Utilities.getServerName() + '/office/supplies/StaplesAddToCart?catalogId=10051&storeId=10001&ST_viewFrom=' + pageId
                  + '&URL=yourorder';
          if (protocol === 'https:') {
              window.initialAddToCartURL = "https://" + addToCartUrlSuffix;
          } else {
              window.initialAddToCartURL = "http://" + addToCartUrlSuffix;
          }
        })(window);
    </script> 
