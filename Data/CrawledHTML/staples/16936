	function loadJSFileAsync(path, callbackFunction){
		var gads = document.createElement('script');
	    gads.async = true;
	    gads.type = 'text/javascript';
	    gads.src = path;
	    var node = document.getElementsByTagName('script')[0];
	    if(callbackFunction){
	    	gads.addEventListener('load', callbackFunction, false);
	    }
	    node.parentNode.insertBefore(gads, node);
	}
	
	// load Staples AdSense Integration Code; using 'document.readyState' to make sure the AdSense Integration code will loaded with/out Akamai FEO 
	if(document.readyState === 'complete' || document.readyState === 'loaded'){
		//If document is already completed loading, we can load the AdSense related code now
		loadJSFileAsync(resourcesAssetPrefix + '/search/resources'+ resourcesStaticVersion + '/js/gad/adsenseIntegration.js', null);
	}
	else if(document.readyState === 'loading' || document.readyState === 'interactive'){
		//If document is not loaded fully yet, adding onload callback to load AdSense related code after page load
		if (window.addEventListener) // W3C standard
    	{
    	  window.addEventListener('load', function(){loadJSFileAsync(resourcesAssetPrefix + '/search/resources'+ resourcesStaticVersion + '/js/gad/adsenseIntegration.js', null);}, false);
    	} 
    	else if (window.attachEvent) // Microsoft
    	{
    	  window.attachEvent('onload', function(){loadJSFileAsync(resourcesAssetPrefix + '/search/resources'+ resourcesStaticVersion +'/js/gad/adsenseIntegration.js', null);});
    	}
	}
}
