    function dloScriptFilter() {
    	try {
	    	Analytics.tracking['categories'] = [{"id":"149651","value":"Brand"},{"id":"4224927150","value":"Deals"},{"id":"4224941382","value":"Bluetooth Compatibility"},{"id":"4224995593","value":"Color Family"},{"id":"4224994184","value":"Tablet Use"},{"id":"4225009117","value":"Battery Life"},{"id":"4225009099","value":"Built-in WiFi"},{"id":"4225008564","value":"Operating Platform"},{"id":"4225009812","value":"Product Condition"},{"id":"4225029446","value":"Tablet Memory Capcity (GB)"},{"id":"4225029459","value":"Tablet Screen Size (in.)"},{"id":"4225009340","value":"Tablet Weight"},{"id":"4225034500","value":"iPad Models"},{"id":"4225035492","value":"Samsung Galaxy Models"},{"id":"4224981892","value":"Customer Recommended"},{"id":"4225006450","value":"New Arrivals"},{"id":"149485","value":"Environmental"},{"id":"149757","value":"Rating"},{"id":"4225038978","value":"ShopRunner"},{"id":"56648","value":"Price"}];
