  <script type="text/javascript">
	$('#custSvcList').html('<a title="Customer Service" class="footerSlideLink">Customer Service</a>').append($('.helpContent').html());
	$('#corpCustList').html('<a title="Corporate Customers" class="footerSlideLink">Corporate Customers</a>').append($('.corpCustomersContent').html());
