  <script type="text/javascript">
STAPLES.apiRequest = (function _xhr(location, callback, that, _method, _async, data, headers) {
    function _xhr(location, callback, that, _method, _async, data, headers) {
        var url;
        if (!/^(inner|outer|top|bottom|before|after)$/.test(location)) {
            url = location;
        }

        if (typeof callback != "function") {
            throw new Error('no callback');
            return;
        }

        var req;
        if (typeof window.XMLHttpRequest === 'function' || typeof window.XMLHttpRequest === 'object') {
            req = new XMLHttpRequest();
        } else {
            req = new ActiveXObject('Microsoft.XMLHTTP');
        }
        var method = _method || 'get', async = _async || true, params = data || null, key;

        req.open(method, url, async);
        req.setRequestHeader('X-Requested-With', 'XMLHttpRequest');

        if (method.toLowerCase() == 'post') {
            req.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
        }

        for (key in headers) {
            if (headers.hasOwnProperty(key)) {
                req.setRequestHeader(key, headers[key]);
            }
        }
        handleResp = function() {
            callback(that, req.responseText);
        };
        handleError = function() {
            callback(that, '');
        };

        function hdl() {
            if (req.readyState == 4) {
                if ((/^[20]/).test(req.status)) {
                    handleResp();
                } else if ((/^[45]/).test(req.status)) {
                    handleError();
                }
            }
        }

        if (async) {
            req.onreadystatechange = hdl;
            this.xmlHttpRequest = req;
        }
        req.send(params);

        if (!async) {
            hdl();
        }
    }
    return {
        get : function(location, callback, that, _method, _async, data, headers) {
            _xhr(location, callback, that, _method, _async, data, headers);
        }
    }
})();

(function() {
    var product = STAPLES.angularData.selectedSKU;
    var streetDate = product.metadata.street_date;
    if (typeof streetDate === 'string') {
        try {
            streetDate = new Date(streetDate);
        } catch (e) {
            streetDate = null;
        }
    }
    if (streetDate === null || typeof streetDate === 'undefined') {
        streetDate = new Date();
    }
    // console.log('price component street date', streetDate);
    var priceAPIURL = STAPLES.angularData.priceURL + '?offer_flag=true&warranty_flag=true&coming_soon=' + product.metadata.coming_soon_flag + '&price_in_cart='
            + product.metadata.price_in_cart_flag + '&productDocKey=' + product.prod_doc_key + '&product_type_id=' + product.metadata.product_type.id
            + '&preorder_flag=' + product.metadata.preorder_flag + '&street_date=' + streetDate.getTime() + '&channel_availability_for_id='
            + product.metadata.channel_availability_for.id + '&backOrderFlag=' + product.metadata.backorder_flag;

    var makeApiCall = function(fallback){
        STAPLES.apiRequest.get(priceAPIURL, function(object, data) {
          try {
              STAPLES.priceData = JSON.parse(data);
              // console.log('price data', STAPLES.priceData);
              var priceElement = document.getElementById('getPrice');
              if (typeof angular !== 'undefined' && typeof priceElement === 'object' && priceElement !== null) {
                  var priceScope = angular.element(priceElement).scope();
                  // console.log('type of price scope',typeof priceScope);
                  if (typeof priceScope === 'object' && priceScope !== null) {
                      priceScope.parseData(STAPLES.priceData);
                      priceScope.$digest();
                      STAPLES.SKU.scrollToReviewSectionOnload();
                  }
              }
          } catch (e) {
              console.error('failed to parse price data', e);
              if(!fallback) {
                makeApiCall(true);
              }
          }
      });
    };
    makeApiCall();

})();
</script> 
