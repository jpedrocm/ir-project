  <script type="text/javascript">
	STAPLES.Onload.addWindowEvent(function(){
		function loadjscssfile(filename, filetype){
			var fileref = "";
			var prefix = "//assets.staples-static.com/NDM/sbdpas/20161008151138/onlineopinionV5/";

	 		if (filetype=="js"){
	 			fileref = document.createElement('script');
	  			fileref.setAttribute("type","text/javascript");
	  			fileref.setAttribute("src", prefix + filename);
					fileref.async = true;
			}
	 		else if (filetype=="css"){
	  			fileref = document.createElement("link");
	  			fileref.setAttribute("rel", "stylesheet");
	  			fileref.setAttribute("type", "text/css");
	  			fileref.setAttribute("href", prefix + filename);
	 		}
	 		document.getElementsByTagName("head")[0].appendChild(fileref);
	 	}

		loadjscssfile("oo_style_english.css", "css");
		loadjscssfile("oo_tab_ux2.js", "js")
	});
</script> 
