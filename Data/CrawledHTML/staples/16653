            if(hash && hash.indexOf('dropdown_') != -1){
                hash = hash.substr(hash.indexOf('dropdown_')+9,10);
                hash = hash.substr(0,hash.indexOf('\''));
            }
            if(hash && hash.indexOf('swatch_') != -1){
                hash = hash.substr(hash.indexOf('swatch_')+7,10);
                hash = hash.substr(0,hash.indexOf('\''));
            }
            if(hash && hash.indexOf('collection_') != -1){
                hash = hash.substr(hash.indexOf('collection_')+11,10);
                hash = hash.substr(0,hash.indexOf('\''));
            }
            if(hash){
                if(products[parentSKU]){
                    var selectedChild = products[parentSKU].child_product;
                    for(var i = 0;i < selectedChild.length;i++){
                        if(selectedChild[i].partnumber == hash)
                        {
                            selectedSKU = selectedChild[i].prod_doc_key;
                        }
                    }
                }
            }
            var isChildLoaded;
            if(!!parentSKU) {
                isChildLoaded =  (selectedSKU  !=  parentSKU);
            } else {
                isChildLoaded = false;
            }
            return {
                products: function(sku) {
                    if(!!sku) {
                        return products[sku] || null;
                    } else {
                        return products || null;
                    }
                },
                parentSKU: products[parentSKU]||null,
                selectedSKU: products[selectedSKU]||products[parentSKU],
                isChildLoaded: isChildLoaded,
