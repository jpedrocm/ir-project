	    	Analytics.tracking['categories'] = [{"id":"149651","value":"Brand"},{"id":"4224927150","value":"Deals"},{"id":"4224995593","value":"Color Family"},{"id":"4225006848","value":"Paper Towel Type"},{"id":"4225007628","value":"Thickness (Ply)"},{"id":"4225040487","value":"Paper Towel Pack Size"},{"id":"4224981892","value":"Customer Recommended"},{"id":"4225006450","value":"New Arrivals"},{"id":"4225030578","value":"Auto Restock"},{"id":"149485","value":"Environmental"},{"id":"149757","value":"Rating"},{"id":"4225038978","value":"ShopRunner"},{"id":"56648","value":"Price"}];
Analytics.tracking.filters = [];

	    	console.log("dloScriptFilter()");
	    	
	    	if(typeof classDFP != "undefined" && typeof classDFP.processFilterForClassDFP === "function"){
				// Call class page dfp service.
		    	classDFP.processFilterForClassDFP(Analytics.tracking.filters);
	    	}
			if(typeof searchDFP != "undefined" && typeof searchDFP.processFilterForSearchDFP === "function"){
				// Call search page dfp service.
		    	searchDFP.processFilterForSearchDFP(Analytics.tracking.filters);
		    } 
	    } catch(e) {
			console.log("dloScriptFilter() failed");
		}
    }
    dloScriptFilter();
    </script> 
