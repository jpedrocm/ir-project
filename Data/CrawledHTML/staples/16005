  <script type="text/javascript">
	/* ************ DO NOT ALTER ANYTHING BELOW THIS LINE ! ************* */
	// store all data in the s object into a temp variable so it doesn't get overridden when s_code.js loads.
	var sc_temp = s;
	
	// add the tracking tag to the page
	function s_omniPageLoadTrack() {
		// pass s code data stored in temp to s_code.js
		for (var i in sc_temp){
			s[i] = sc_temp[i];
		}
		// add tracking tag to page
		try {
			var s_code=s.t();
			if(s_code)$("body").append(s_code);
		} catch(err){
		
		}
	}
	// check if scode is supposed be loaded
	if(propertyValues.scodeSwitch == 'ON'){
		// load the scode.js file asynchronously
		$.getScript('/sbdpas/js/omniture/s_code.js',function(){ 
			// After successful load of s_code and DOM is ready
			$(function(){
				s_omniPageLoadTrack();
			});
		});
	}

</script>
