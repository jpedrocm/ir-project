  $(document).ready(function() {
    resizeGuidedNav();
  });
  function resizeGuidedNav() {
    if ($("#filtersDiv").height() < $("#resultsDiv").height()) { 
    $("#selectors").css("height",$("#resultsDiv").height()-110); }
  }
