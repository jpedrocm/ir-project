    <script type="text/javascript" src="/js/third-party/RSH0.6FINAL/rsh.js"></script> 
    <script type="text/javascript" src="/js/third-party/jQuery/v1.3.1/jquery.json-1.3.min.js"></script> 
    <script type="text/javascript">
/*instantiate our history object*/
window.dhtmlHistory.create({
	debugMode: false,//set this to false, or just don't pass in an options bundle, to see real-world, non-debug conditions
    toJSON: function(o) {
            return $.toJSON(o);
        },
    fromJSON: function(s) {
            return $.evalJSON(s);
        }
});

function resetRefinement(nValue, otherParams, hValue){
    disable_all();
    historyManager('laptopFinderFilters.jsp', 'laptopFinderResults.jsp', nValue, hValue, otherParams, true);
}

//This function will pass non-endeca based refinement in query string.
//this will be used to determine which refinement is selected if refinement info is not availabel in endeca (if all results have same propery e.g. same processor)
function modifyNonEndecaRefinements(action, attribute, refinementValue, nValue, otherParams, hValue){
    var existingRefinements = document.getElementById(attribute).value;
    var modifiedRefinement = existingRefinements;
    if ("add"==action){
        modifiedRefinement +=  "|" + refinementValue;
    } else if ("remove" == action){
        var refinementsArray = existingRefinements.split("|");
        var selectedRefinementCount = existingRefinements.split("|").length;
        modifiedRefinement = ""; //reset modifiedExtras for rebuilding
        //generate a new string of selected extras
        for (var i=0; i < selectedRefinementCount; i++){
            var refinementToCheck = refinementsArray[i];
            if (refinementToCheck != refinementValue ){
                modifiedRefinement += "|" + refinementToCheck;
            }
        }
    }

    if (modifiedRefinement.indexOf('|') == 0)
            modifiedRefinement = modifiedRefinement.substring(1,modifiedRefinement.length);

    document.getElementById(attribute).value = modifiedRefinement;
    
    //remove existing RefinementValue from otherParams
    if (otherParams.indexOf(attribute+"=") >= 0){
        var otherParamsArray = otherParams.split("&");
        var temp = '';
        for (var i=0; i < otherParamsArray.length; i++){
            if (otherParamsArray[i].indexOf(attribute) == -1)
                temp += otherParamsArray[i] + "&";
        }
        if (temp.length > 0)
            temp = temp.substring(0, temp.length-1);
        otherParams = temp;
    }

    //dont put refinmentvalue if "No preference" is selected.
    if ("remove" != action && "ALL" != refinementValue){
        if(otherParams.indexOf(attribute+"=") == -1 ) {
                otherParams = otherParams + "&" + attribute+ "=" + modifiedRefinement;
        }
    }
    selectRefinement(nValue, otherParams, hValue);
}

function selectRefinement(nValue, otherParams, hValue){
    disable_all();

    if(otherParams.indexOf("compareSkus=") == -1 && document.resultsForm.compareSkus &&  document.resultsForm.compareSkus.value) {
            otherParams = otherParams + "&compareSkus=" + replaceAllOccurances(document.resultsForm.compareSkus.value,"&","^^^");
    }

    if(otherParams.indexOf("Ns=") == -1 && document.getElementById("Ns") &&  document.getElementById("Ns").value) {
            otherParams = otherParams + "&Ns=" + document.getElementById("Ns").value;
    }

    if(otherParams.indexOf("resPerPage=") == -1 && document.getElementById("view") && document.getElementById("view").options[document.getElementById("view").selectedIndex].value) {
            var view = document.getElementById("view").options[document.getElementById("view").selectedIndex].value;
            otherParams+="&resPerPage="+view;
    }

    historyManager('laptopFinderFilters.jsp', 'laptopFinderResults.jsp', nValue, hValue, otherParams, true);
}

function changeSortOrder(nValue, otherParams, hValue){
    var order = document.getElementById("sortOrder").options[document.getElementById("sortOrder").selectedIndex].value;
    document.getElementById("Ns").value = order;
    otherParams+="&Ns="+order;
    if(otherParams.indexOf("resPerPage=") == -1 && document.getElementById("view") && document.getElementById("view").options[document.getElementById("view").selectedIndex].value) {
            var view = document.getElementById("view").options[document.getElementById("view").selectedIndex].value;
            otherParams+="&resPerPage="+view;
    }
    if(document.resultsForm.compareSkus && document.resultsForm.compareSkus.value.length) {
            otherParams = otherParams + "&compareSkus=" + replaceAllOccurances(document.resultsForm.compareSkus.value,"&","^^^");
     }
    historyManager('laptopFinderFilters.jsp','laptopFinderResults.jsp', nValue, hValue, otherParams, false);
}

function changeView(nValue, otherParams, hValue){
    var view = document.getElementById("view").options[document.getElementById("view").selectedIndex].value;
    document.getElementById("resPerPage").value = view;
    otherParams+="&resPerPage="+view;
    if(document.resultsForm.compareSkus && document.resultsForm.compareSkus.value.length) {
            otherParams = otherParams + "&compareSkus=" + replaceAllOccurances(document.resultsForm.compareSkus.value,"&","^^^");
     }
    historyManager('laptopFinderFilters.jsp','laptopFinderResults.jsp', nValue, hValue, otherParams, false);
}

 function paginate(nValue, otherParams, hValue) {
     if(document.resultsForm.compareSkus && document.resultsForm.compareSkus.value) {
            otherParams = otherParams + "&compareSkus=" + replaceAllOccurances(document.resultsForm.compareSkus.value,"&","^^^");
     }
     if(otherParams.indexOf("resPerPage=") == -1 && document.getElementById("view") && document.getElementById("view").options[document.getElementById("view").selectedIndex].value) {
            var view = document.getElementById("view").options[document.getElementById("view").selectedIndex].value;
            otherParams+="&resPerPage="+view;
    }
     historyManager('laptopFinderFilters.jsp','laptopFinderResults.jsp', nValue, hValue, otherParams, false);
 }

 function historyManager(filersUrl,resultsUrl, nValue, hValue, otherParams, loadFilters){
     updateResults(resultsUrl, otherParams,  'resultsDiv', nValue, hValue);
     if(loadFilters) {
        updateFilters(filersUrl, otherParams,  'filtersDiv', nValue, hValue);
     }

    var newHashName = nValue.replace(/ /gi, "%20"); // new Date().getTime();

    var hashValue = nValue + "~" + hValue + "~" + otherParams;
    dhtmlHistory.add(newHashName, hashValue);
 }

function historyManagerUpdate(nValue, hValue, otherParams){
    var newHashName = document.location.hash;
    if(!newHashName) {
        newHashName = nValue.replace(/ /gi, "%20");// new Date().getTime();
        document.location.hash = newHashName;
    }
     var hashValue = nValue + "~" + hValue + "~" + otherParams;
     dhtmlHistory.add(newHashName, hashValue);
 }

function historyChange(newLocation, historyData, firstLoad) {
        var historyMsg = (typeof historyData == "object" && historyData != null	? historyStorage.toJSON(historyData) : historyData);
        if(newLocation && historyMsg) {
            var params = historyMsg.split("~");
            if(params.length >=3) {
                updateFilters('laptopFinderFilters.jsp', params[2],  'filtersDiv', params[0], params[1]);
                updateResults('laptopFinderResults.jsp', params[2],  'resultsDiv', params[0], params[1]);
            }
        } else if (newLocation.substring(0,1) != "." ) {
            if(firstLoad) {
                updateFilters('laptopFinderFilters.jsp', 'target=laptop-finder',  'filtersDiv', newLocation, '0');
                updateResults('laptopFinderResults.jsp', 'target=laptop-finder',  'resultsDiv', newLocation, '0');
            }
        } else {
            updateFilters('laptopFinderFilters.jsp', 'target=laptop-finder',  'filtersDiv', '4294965746 4294965347', '0');
            updateResults('laptopFinderResults.jsp', 'target=laptop-finder',  'resultsDiv', '4294965746 4294965347', '0');
        }
}


//create url and make ajax request to update the filters section
function updateFilters(url, otherParams, divElement, nValue, hValue) {

    if(otherParams && otherParams.charAt(otherParams.length-1) != '&') {
        otherParams = otherParams + "&";
    }
    otherParams = otherParams + "timestamp=" + new Date().getTime();
    if(otherParams && otherParams.charAt(0) != '&') {
        otherParams = '&' + otherParams;
    }
    var get_url =  "/showcase/explore/jsp/catalog/laptopfinder/laptopFinderFilters.jsp"  + '?N=' + nValue + (hValue ? '&H=' + hValue : '') + otherParams;

    new ajax().update(get_url,
           divElement,
           {
               method    : "GET",   // N is in URL so we must use GET method
               onError   : AjaxError
           }
        );

}

//create url and make ajax request to update the results section
    function updateResults(url, otherParams, divElement, nValue, hValue) {
        if(otherParams && otherParams.charAt(otherParams.length-1) != '&') {
            otherParams = otherParams + "&";
        }
        otherParams = otherParams + "timestamp=" + new Date().getTime();
        if(otherParams && otherParams.charAt(0) != '&') {
            otherParams = '&' + otherParams;
        }
        var params = 'N=' + nValue + (hValue ? '&H=' + hValue : '') + otherParams;
        var get_url = "/showcase/explore/jsp/catalog/laptopfinder/laptopFinderResults.jsp" + '?' + params;
        new ajax().update(get_url,
			   divElement,
			   {
			       onError   : function(request) { if(request.instance.status == 404) { getElementById(divElement).innerHTML = "<!-- Page not found -->"; } },
                   onPostSuccess: function() {try {resetCompareSkusAndRenderCart();setFeatureSpecData();scrollToResults();updateshareurl();resizeGuidedNav();} catch(e){}}
                                                    }
        );
    }

function resetCompareSkusAndRenderCart(){
    resetCompareSkus();
    renderCompareCartImages('Laptop');
}

function replaceAllOccurances (source,stringToFind,stringToReplace){
    if (source == null)
        return "";
  var temp = source;
    var index = temp.indexOf(stringToFind);
        while(index != -1){
            temp = temp.replace(stringToFind,stringToReplace);
            index = temp.indexOf(stringToFind);
        }
        return temp;
}

function AjaxError(request) {
		if(request.instance.status == 404) {
			getElementById(divElement).innerHTML = "<!-- Page not found -->";
		}
	}

//This function will be called when checkbox to compare sku is clicked
function updateCompareSkus(isChecked, subbrand, skuName, shortProductTitle, modelNumber, productLine, nValue, hValue, otherParams) {
        var maxSkusToCompare = 3;
        var skusToCompare = readCookie('explore.catalog.compare'+'Laptop'+'Cart'); // document.resultsForm.compareSkus.value;
        if (skusToCompare.length > 1 && skusToCompare.charAt( skusToCompare.length-1 ) != ',' ){  //safari doesn't return last ',' used as separator
            skusToCompare += ',';
        }
        skusToCompare = replaceAllOccurances (skusToCompare,"#@#",";");
        var noOfCheckedSkus = skusToCompare.split(",").length - 1;

        //if 3 skus are already checked for comparision, show overlay to replace
        if (noOfCheckedSkus == maxSkusToCompare && isChecked){
            //uncheck the newly selected sku
            if (document.getElementById(skuName)) {
                document.getElementById(skuName).checked = false;
            }

            showReplaceSkuOverlay(subbrand, skuName, shortProductTitle, modelNumber);

            return false;
        }

        //since 3 skus are not selected for comparision, add/remove the sku depending upon isChecked flag.
        skuName = subbrand + "|" + skuName + "|" + shortProductTitle + "|" + modelNumber;
        if(skusToCompare.indexOf(skuName + ",") != -1 && !isChecked) {
            //found in skusToCompare but not checked.. removing
            var idxOfCurrSku = skusToCompare.indexOf(skuName+",");
            skusToCompare = skusToCompare.substr(0,idxOfCurrSku) + skusToCompare.substr(idxOfCurrSku + (skuName+",").length);
        } else if (skusToCompare.indexOf(skuName + ",") == -1 && isChecked){
            //not found in skusToCompare but checked.. adding
            skusToCompare = skusToCompare + skuName + ",";
        }
        document.resultsForm.compareSkus.value = skusToCompare;
        skusToCompare =  replaceAllOccurances (skusToCompare,";","#@#");  //replace any semi-colon with #@# to allow in cookie. We'll revert it at lookup time

        var cookieName = 'explore.catalog.compare'+productLine+'Cart';
        setCookie( cookieName, skusToCompare, '1', '/', '' , '');

        renderCompareCartImages(productLine);
        if(otherParams.indexOf("compareSkus=") == -1 && document.resultsForm.compareSkus.value) {
            otherParams = otherParams + "&compareSkus=" + replaceAllOccurances(document.resultsForm.compareSkus.value,"&","^^^");
        }
        historyManagerUpdate(nValue, hValue, otherParams);
}

//This function will be called when compare link is clicked.
function compare(subbrand, skuName, shortProductTitle, modelNumber, productLine, nValue, hValue, otherParams){
    var maxSkusToCompare = 3;
    var nameToAdd = subbrand + "|" + skuName + "|" + shortProductTitle + "|" + modelNumber;
    var skusToCompare = document.resultsForm.compareSkus.value;
    var noOfCheckedSkus = skusToCompare.split(",").length - 1;

    //If no other sku is already selected, show error overlay.  If one sku is selected, and it's same the the this sku, show error overlay as well.
    if (noOfCheckedSkus == 0 || (noOfCheckedSkus == 1 && isSkuInCompareCart(nameToAdd))){
        showMinumumSkuOverlay();
        updateCompareSkus(true, subbrand, skuName, shortProductTitle, modelNumber, productLine, nValue, hValue, otherParams);
        if (document.getElementById(skuName)) {
                document.getElementById(skuName).checked = true;
        }
        return false;
    }
    //if 3 skus are already checked and this sku is not part of it, for comparision, show overlay to replace
    if (noOfCheckedSkus == maxSkusToCompare && !isSkuInCompareCart(nameToAdd)){
        showReplaceSkuOverlay(subbrand, skuName, shortProductTitle, modelNumber);
        return false;
    }


    if (!isSkuInCompareCart(nameToAdd)){
        updateCompareSkus(true, subbrand, skuName, shortProductTitle, modelNumber, productLine, nValue, hValue, otherParams);
        //skusToCompare = skusToCompare + nameToAdd + ",";

        if (document.getElementById(skuName)) {
                document.getElementById(skuName).checked = true;
        }

    }
    setTimeout("goToComparePage();",200);
}

function  goToComparePage(){
    //remove subbrand name from compareSkus parameter so make url cleaner.
    var maxSkusToCompare = 3;
    var skusToCompare = document.resultsForm.compareSkus.value;
    var noOfCheckedSkus = skusToCompare.split(",").length - 1;
    var comparePageUrl = "/computers/laptop-finder/compare";

    //check if there are atleast 2 skus to compare. 
    if (noOfCheckedSkus < 2){
        showMinumumSkuOverlay();
        return false;
    }
    
    var skuList = skusToCompare.split(",");
    var skuListForComparePage = "";
    for (var i = 0; i < noOfCheckedSkus; i++) {
        if (i >= maxSkusToCompare) //show maximum of 3
            break;

        var nameToParse = skuList[i];
    
         var selectedSku = nameToParse.split("|")[1];
    
        skuListForComparePage += selectedSku + ",";
    }
    document.compareForm.skus.value = skuListForComparePage;
    skuListForComparePage = skuListForComparePage.substr(0,skuListForComparePage.length-1);  //removing last ","
    var currPageUrl = window.location;
    setCookie("finderPageHashUrl", currPageUrl, '1', '/', '' , '');
    document.location.href = comparePageUrl+'?skus='+skuListForComparePage;
}


function isSkuInCompareCart(skuName){
    var skusToCompare = document.resultsForm.compareSkus.value;
    skuName = replaceAllOccurances (skuName,";","#@#");
    if (skusToCompare.indexOf(skuName + ",") == -1)
        return false;
    else
        return true;
}

function showMinumumSkuOverlay(){
    position_overlay('select-minO');
    document.getElementById("select-minO").style.visibility = "visible";
    
}

function closeMinumumSkuOverlay(){
    document.getElementById("select-minO").style.visibility = "hidden";    
}

function showReplaceSkuOverlay(subbrand, skuName, shortProductTitle, modelNumber) {
    var maxSkusToCompare = 3;
    var skusToCompare = document.resultsForm.compareSkus.value;
    skusToCompare = replaceAllOccurances (skusToCompare,"#@#",";");
    var noOfCheckedSkus = skusToCompare.split(",").length - 1;
    var productLine = 'Laptop';

    var skuList = skusToCompare.split(",");


    var html = '<p class="replace text">';
    html += '<strong class="red-text-sm">Replace:</strong><br />';
    for (var i = 0; i < noOfCheckedSkus; i++) {
        if (i >= maxSkusToCompare) //show maximum of 3
            break;

        var nameToParse = skuList[i];

        var existingSubbrand = nameToParse.split("|")[0];
        var existingSku = nameToParse.split("|")[1];
        var existingSkuShortProductTitle = nameToParse.split("|")[2];
        var existingModelNumber = nameToParse.split("|")[3];
        existingSkuShortProductTitle = existingSkuShortProductTitle.replace("\"","&quot;");
        shortProductTitle = shortProductTitle.replace("\"","&quot;");
        if (productLine.toUpperCase() == 'LAPTOP'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSubbrand + ' ' + existingSku +'</label><br />';
        } else if (productLine.toUpperCase() == 'TV'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSkuShortProductTitle+'</label><br />';
        } else if (productLine.toUpperCase() == 'BRDVD'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSkuShortProductTitle+'</label><br />';
        } else if (productLine.toUpperCase() == 'DISCPLAYER'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSkuShortProductTitle+'</label><br />';
        }  else if (productLine.toUpperCase() == 'CAMCORDER'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSkuShortProductTitle+'</label><br />';
        }  else if (productLine.toUpperCase() == 'TABLET'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSkuShortProductTitle+'</label><br />';
        }  else if (productLine.toUpperCase() == 'DESKTOP'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSkuShortProductTitle+'</label><br />';
        } else if (productLine.toUpperCase() == 'STORAGE'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSkuShortProductTitle+'</label><br />';
        } else if (productLine.toUpperCase() == 'SOUNDBAR'){
            html += '<label><input type="radio" onclick="javascript:enable_replace();" id="'+ existingSubbrand + '|' + existingSku + '|' + existingSkuShortProductTitle + '|' + existingModelNumber + '_'+subbrand + '|' + skuName + '|' + shortProductTitle + '|' + modelNumber +'" name="replaceSku" /> '+ existingSkuShortProductTitle+'</label><br />';
        }
    }
    html += '</p>';
    html += '<p class="new-selection text">';
    html += '<strong class="red-text-sm">New Selection:</strong><br />';
    if (productLine.toUpperCase() == 'LAPTOP'){
        html += '<label> '+subbrand + ' ' +  skuName +'</label>';
    } else if (productLine.toUpperCase() == 'TV'){
        html += '<label> '+shortProductTitle+'</label>';
    } else if (productLine.toUpperCase() == 'BRDVD'){
        html += '<label> '+shortProductTitle+'</label>';
    } else if (productLine.toUpperCase() == 'DISCPLAYER'){
        html += '<label> '+shortProductTitle+'</label>';
    } else if (productLine.toUpperCase() == 'CAMCORDER'){
        html += '<label> '+shortProductTitle+'</label>';
    } else if (productLine.toUpperCase() == 'TABLET'){
        html += '<label> '+shortProductTitle+'</label>';
    } else if (productLine.toUpperCase() == 'DESKTOP'){
        html += '<label> '+shortProductTitle+'</label>';
    } else if (productLine.toUpperCase() == 'STORAGE'){
        html += '<label> '+shortProductTitle+'</label>';
    } else if (productLine.toUpperCase() == 'SOUNDBAR'){
        html += '<label> '+shortProductTitle+'</label>';
    }
    html += '</p>';

    document.getElementById("replace-laptop-text").innerHTML = html
    position_overlay('replace-laptopO');
    document.getElementById("replace-laptopO").style.visibility = "visible";
}


function replaceCompareSku(productLine, nValue, hValue, otherParams){
    var isSkuSelected = false;
    for (var i=0; i < document.resultsForm.replaceSku.length; i++) {
       if (document.resultsForm.replaceSku[i].checked) {
          var val = document.resultsForm.replaceSku[i].id;
           val = replaceAllOccurances (val,"\"","&quot;")
           isSkuSelected = true;
           break;
        }
    }

    //if no sku is selected to replace, don't proceed.
    if (!isSkuSelected)
        return false;
    
    var replaceOldValue = val.split("_")[0];
    var replaceNewValue = val.split("_")[1];
    //remove old value from compareSku variable
    
    removeFromCompareCart(replaceOldValue, productLine, nValue, hValue, otherParams);

    // add new value to compareSku variable
    updateCompareSkus(true, replaceNewValue.split("|")[0], replaceNewValue.split("|")[1], replaceNewValue.split("|")[2], replaceNewValue.split("|")[3], productLine, nValue, hValue, otherParams);
    
    //go to compare page
    setTimeout("goToComparePage();",200);
}
    


function closeReplaceSkuOverlay() {
    document.getElementById("replace-laptopO").style.visibility = "hidden";    
}




function resetCompareSkus(){
    var fullCookieValue = replaceAllOccurances(readCookie('explore.catalog.compare'+'Laptop'+'Cart'), "\"","&quot;");
    if (fullCookieValue.length > 1 && fullCookieValue.charAt( fullCookieValue.length-1 ) != ',' ){  //safari doesn't return last ',' used as separator
        fullCookieValue += ',';
    }
    //alert("in resetCompareSkus:"+fullCookieValue);
    document.resultsForm.compareSkus.value = fullCookieValue;
}

function renderCompareCartImages(productLine){
    var maxSkusToCompare = 3;
    //first clear all 3 images
    for (var i=1; i<=maxSkusToCompare; i++){
        if (document.getElementById("sku"+i+"_compare"))
            document.getElementById("sku"+i+"_compare").innerHTML = "";   
    }

    //render image blocks using compareSkus variable
    if(document.resultsForm.compareSkus && document.resultsForm.compareSkus.value) {
        var skusToCompare = document.resultsForm.compareSkus.value;
        skusToCompare = replaceAllOccurances (skusToCompare,"#@#",";");
        //alert("in renderCompareCart:"+skusToCompare);
        var noOfCheckedSkus = skusToCompare.split(",").length - 1;
        var skuList = skusToCompare.split(",");
        for (var i=0; i < noOfCheckedSkus; i++){
            if (i >= maxSkusToCompare) //show maximum of 3
                break;

            var nameToParse = skuList[i];

            var subbrandName = nameToParse.split("|")[0];
            var skuName = nameToParse.split("|")[1];
            //check sku's compare box
            
            if (document.getElementById(skuName))
                document.getElementById(skuName).checked = true;
            
            var shortProductTitle = nameToParse.split("|")[2];
            var modelNumber = nameToParse.split("|")[3];
            var imageId = subbrandName + "|" + skuName + "|" + shortProductTitle;
            imageId = imageId.replace('"','');
            shortProductTitle = shortProductTitle.replace('"', '&quot;');
            var image = "";
            var altText = "";
            if (productLine.toUpperCase() == 'LAPTOP'){
                image = "/images/showcase/products/laptop-finder/"+subbrandName.toLowerCase().replace(" ","-")+"-"+skuName.toLowerCase()+"-laptop.png";
                altText = subbrandName + " " + skuName + " Laptop";
            } else if (productLine.toUpperCase() == 'TV') {
                image = "/images/showcase/products/tv/finder/"+skuName.toLowerCase()+".png";
                altText = shortProductTitle;
            } else if (productLine.toUpperCase() == 'BRDVD') {
                if (subbrandName.toUpperCase() == 'DVD-PLAYERS')
                    image = "/images/showcase/products/dvd-players/finder/"+skuName.toLowerCase()+".png";
                else if (subbrandName.toUpperCase() == 'DVD-RECORDERS')
                    image = "/images/showcase/products/dvd-recorders/finder/"+skuName.toLowerCase()+".png";
                else if (subbrandName.toUpperCase() == 'BLU-RAY-PLAYERS')
                    image = "/images/showcase/products/blu-ray-players/finder/"+skuName.toLowerCase()+".png";
                altText = shortProductTitle;
            }  else if (productLine.toUpperCase() == 'DISCPLAYER') {
                image = "/images/showcase/products/disc-players/finder/"+skuName.toLowerCase()+".png";
                altText = shortProductTitle;
            }  else if (productLine.toUpperCase() == 'CAMCORDER') {
                image = "/images/showcase/products/camcorders/finder/"+skuName.toLowerCase()+".png";
                altText = shortProductTitle;
            }  else if (productLine.toUpperCase() == 'TABLET') {
                image = "/images/showcase/products/tablets/finder/"+subbrandName.toLowerCase() + "_" + skuName.toLowerCase()+".png";
                altText = shortProductTitle;
            }  else if (productLine.toUpperCase() == 'DESKTOP') {
                image = "/images/showcase/products/desktops/finder/"+skuName.toLowerCase()+".png";
                altText = shortProductTitle;
            }  else if (productLine.toUpperCase() == 'STORAGE') {
                image = "/images/showcase/products/storage-finder/"+modelNumber.toLowerCase()+".png";
                altText = shortProductTitle;
            } else if (productLine.toUpperCase() == 'SOUNDBAR') {
                image = "/images/showcase/products/sound-bars/finder/"+modelNumber.toLowerCase()+".png";
                altText = shortProductTitle;
            }
            var nValue = document.resultsForm.nValue.value;
            var paramsOtherThanN = document.resultsForm.paramsOtherThanN.value;
            var skuNameToRemove = subbrandName + "|" + skuName + "|" + shortProductTitle + "|" + modelNumber;
            document.getElementById("sku"+(i+1)+"_compare").innerHTML = '<a><img id="'+imageId+'" src="'+ image +'" title="'+altText+'" alt="'+ altText +'" class="laptop-img nofix" /> <img src="/images/showcase/ui/closeBox.gif" alt="'+altText+'" title="'+altText+'" onclick="javascript:removeFromCompareCart(\''+ skuNameToRemove.replace("&","&amp;") +'\',\''+ productLine+'\',\''+ nValue +'\',\'\',\''+ paramsOtherThanN +'\'); return false;" class="close-box" /></a>';
        }
    }
}

function removeFromCompareCart(skuName, productLine, nValue, hValue, otherParams){
    var modifiedCompareSkus = "";
    if(document.resultsForm.compareSkus && document.resultsForm.compareSkus.value) {
        var skusToCompare = readCookie('explore.catalog.compare'+'Laptop'+'Cart');  //document.resultsForm.compareSkus.value;
        if (skusToCompare.length > 1 && skusToCompare.charAt( skusToCompare.length-1 ) != ',' ){  //safari doesn't return last ',' used as separator
            skusToCompare += ',';
        }
        skusToCompare = replaceAllOccurances (skusToCompare,"#@#",";");
        var noOfCheckedSkus = skusToCompare.split(",").length - 1;
        var skuList = skusToCompare.split(",");
        //generate a new string of skus to compare
        for (var i=0; i < noOfCheckedSkus; i++){
            var nameToCheck = skuList[i];
            nameToCheck = replaceAllOccurances (nameToCheck,"\"","&quot;")
            skuName = replaceAllOccurances (skuName,"\"","&quot;")
            if (nameToCheck != skuName && nameToCheck.replace("|"," ") != skuName){    //replace "|" with " " in sku name so we can compare full name or pipe separated.
                modifiedCompareSkus += nameToCheck+",";
            }
        }
        document.resultsForm.compareSkus.value = modifiedCompareSkus;
        modifiedCompareSkus =  replaceAllOccurances (modifiedCompareSkus,";","#@#");
        var cookieName =  'explore.catalog.compare'+productLine+'Cart';
        setCookie(cookieName, modifiedCompareSkus, '1', '/', '' , '');

        //uncheck the compare checkbox in sku section
        
        if (document.getElementById(skuName.split("|")[1])) {
            document.getElementById(skuName.split("|")[1]).checked = false;
        }
        
        if(otherParams.indexOf("compareSkus=") == -1 && document.resultsForm.compareSkus.value) {
            otherParams = otherParams + "&compareSkus=" + replaceAllOccurances(document.resultsForm.compareSkus.value,"&","^^^");
        }
                
        renderCompareCartImages(productLine);
        historyManagerUpdate(nValue, hValue, otherParams);
    }
}


function getCompareCartCookie(name){
    if (document.cookie.length>0) {
        var startPos=document.cookie.indexOf(name + "=");
        if (startPos!=-1) {
            startPos=startPos + name.length+1;
            var endPos=document.cookie.indexOf(";",startPos);
            if (endPos==-1)
                endPos=document.cookie.length;
            return (document.cookie.substring(startPos,endPos));
        }
    }
    return "";
}


 function setFeatureSpecData() {
        $(".built-for-business, .extraSpecs").click(function(e){
            if(!e) var e= window.event;
            var text = $(this).html();
            //remove <img> tag if present. So we only get text of the feature
            var count = text.split("<").length-1;
            for (var i=0; i<=count; i++){
                var startIndex = text.indexOf("<");
                if (startIndex >= 0){
                    var endIndex = text.indexOf(">");
                    text = text.substr(0,startIndex) + text.substr(endIndex+1);
                }
            }
            showSpecOverlay(text, e.pageX, e.pageY);
            return false;
        });        
    }


function hideSpecDataOverlay() {
    var specDataOverlay = document.getElementById('specDataOverlay');
    if (specDataOverlay) {
        specDataOverlay.style.display = "none";
    }
}

function showSpecOverlay(specDataName, xPos, yPos) {
        var specDataOverlay = document.getElementById('specDataOverlay');
        if (specDataOverlay) {
            var specDataImg = document.getElementById(specDataName + "_img");
            var specDataDesc = document.getElementById(specDataName + "_desc");
            var specDataNameTarget = document.getElementById("specDataName");
            var specDataImgTarget = document.getElementById("specDataImg");
            var specDataDescTarget = document.getElementById("specDataDesc");
            xPos = xPos - 150;
            if (xPos < 0) xPos = 0;
            //for the same spec data id, do not redo, the html copy just position it at new x and y
            if (specDataName && specDataNameTarget && specDataNameTarget.innerHTML == specDataName.innerHTML && Math.abs(specDataOverlay.style.left - xPos) > 200) {
                specDataOverlay.style.left = xPos + "px";
                specDataOverlay.style.top = yPos + "px";
                return;
            }

            //show overlay only if it has description or image
            if ((specDataDesc && specDataDesc.innerHTML && specDataDesc.innerHTML != "null") ||
                 (specDataImg && specDataImg.innerHTML && specDataImg.innerHTML != "null")) {
                if (specDataImgTarget) {
                    if (specDataImg && specDataImg.innerHTML && specDataImg.innerHTML != "null") {
                        specDataImgTarget.src = specDataImg.innerHTML;
                        fixPng(specDataImgTarget);
                    } else {
                        specDataImgTarget.src = "/images/ui5/spacer.gif";
                    }
                }
                if (specDataNameTarget) {
                    if (specDataName) {
                        specDataNameTarget.innerHTML = specDataName;
                    } else {
                        specDataNameTarget.innerHTML = "&nbsp";
                    }
                }
                if (specDataDescTarget) {
                    if (specDataDesc && specDataDesc.innerHTML && specDataDesc.innerHTML != "null") {
                        specDataDescTarget.innerHTML = specDataDesc.innerHTML;
                    } else {
                        specDataDescTarget.innerHTML = "&nbsp";
                    }
                }

                specDataOverlay.style.left = xPos + "px";
                specDataOverlay.style.top = yPos + "px";
                specDataOverlay.style.display = "";
//                $("#specDataName").css("margin-bottom", 50 - $("#specDataName").height());
            }
        }
    }

function fixPng(imgEle) {
        try {
            if(imgEle && imgEle.src && !pngSupported && imgEle.src.indexOf("png") != -1 ) {
                var imgPath = imgEle.src;
                imgEle.src = '/images/assets/blank.gif';
                imgEle.runtimeStyle.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true,src='" + imgPath + "',sizingMethod='image')";
            }
        } catch(e) {

        }
    }

jQuery(function($) {
  position_overlay = function(id) {
    var obj = $('#' + id), container = $('#lf-container'),
      top = document.documentElement.scrollTop - container.offset().top + (($(window).height() - obj.outerHeight()) / 2);
    obj.css({ top: top, left: (container.outerWidth() - obj.outerWidth()) / 2 });
  }

  disable_all = function() {
    $('.group label', '#selectors').addClass('disabled').children('input, select').attr('disabled', 'disabled');
  }

  // if disabled, do nothing
  $('.disabled').click(function(e) { if ($(this).hasClass('disabled')) e.preventDefault() });

  // enable buttons on click
  enable_replace = function() {
    $('#continue-replace').removeClass('disabled');
  }

  // set button to disabled after click
  $('a', '#replace-laptopO .actions').click(function() {
    $('#continue-replace').addClass('disabled');
  });

});


    $(document).ready( function() {
        //initialize our DHTML history
        dhtmlHistory.initialize();

        //subscribe to DHTML history change events
        dhtmlHistory.addListener(historyChange);

        //setTimeout("positionCompare()", 1000);

        setFeatureSpecData();
        if(document.location.hash != null && document.location.hash.length > 0) {
            historyChange(document.location.hash.substr(1), '', true);
        }

    });


function doDetailReporting() {
        try {
            omtr = s_gi(s_account);
            omtr.linkTrackVars = "eVar31,eVar32,hier1,events";
            omtr.linkTrackEvents = "event28";
            omtr.eVar31 = $("#searchDimensions").html();
            omtr.eVar32 = 'laptop-finder';
            omtr.hier1 = omtr.eVar32 + ":" + omtr.eVar31.replace(/\|/g, ":");
            omtr.events = "event28"
            omtr.tl(this, 'o', "Search");
            omtr.events = omtr.hier1 = omtr.eVar31 = omtr.eVar32 = "";
        } catch (e) {
        }
    }

function scrollToResults(){
    scroll(0, $('#results').offset().top) 
}

 var all_features_show = function () {
    $('.features').find('.hidden').slideDown(500);
    $('#show-all-features-link').hide();
    $('#show-all-features-arrow').hide();
    $('#show-less-features-link').show();
    $('#show-less-features-arrow').show();
    return false;
};

 var less_features_show = function () {
    $('.features').find('.hidden').slideUp(500);
    $('#show-less-features-link').hide();
    $('#show-less-features-arrow').hide();
    $('#show-all-features-link').show();
    $('#show-all-features-arrow').show();
    return false;
};

</script> 
