  <script type="text/javascript">

    var prodClickedOn;


    function selectRefinement(nValue, otherParams, rpp, nsValue) {
        disableFilters();
        historyManager('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', '/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', nValue, rpp, 0 /* when refinements change move back to first page*/, nsValue, otherParams, true);
    }

    function paginate(nValue, otherParams, rpp, noValue, nsValue) {
        if(document.resultsForm.poids && document.resultsForm.poids.value) {
            otherParams = otherParams + "&poids=" + document.resultsForm.poids.value;
        }
        historyManager('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', '/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', nValue, rpp, noValue, nsValue, otherParams, false);
    }

    function sortChange(nValue, otherParamsWithoutNs, selectEle, rpp) {
        sort(nValue, otherParamsWithoutNs, rpp, 0/*sorting changes it back to first page*/, selectEle.value);
    }

    function sort(nValue, otherParams, rpp, noValue, nsValue) {
        if(document.resultsForm.poids && document.resultsForm.poids.value) {
            otherParams = otherParams + "&poids=" + document.resultsForm.poids.value;
        }
        historyManager('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', '/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', nValue, rpp, noValue, nsValue, otherParams, false);
    }

    function changeRpp(nValue, otherParamsWithoutRpp, nsValue, rppEle) {
        var otherParams = otherParamsWithoutRpp;
        if(document.resultsForm.poids && document.resultsForm.poids.value) {
            otherParams = otherParams + "&poids=" + document.resultsForm.poids.value;
        }
        historyManager('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', '/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', nValue, rppEle.value, 0/*change to first page*/, nsValue, otherParams, false);
    }

    function toggleFilter(dimId) {
        if(jQuery("#dimHandle" + dimId) != null && jQuery("#dimHandle" + dimId).length > 0) {
            var show = jQuery("#dim" + dimId).is(":hidden");
            if(show) {
                jQuery("#dimHandle" + dimId).html("-");
                jQuery("#dim" + dimId).show();
            } else {
                jQuery("#dimHandle" + dimId).html("+");
                jQuery("#dim" + dimId).hide();
            }
        }
    }

    

    //this method disables all filters. meant to be used before submitting the AJAX request upon user action
    //this prevents successive requests before the response from the first request comes in
    function disableFilters() {
        if(document.navForm && document.navForm.filterOption) {
            var filterOptions = document.navForm.filterOption;
            if(filterOptions.length) {
                for(var i=0; i<filterOptions.length; i++) {
                    filterOptions[i].disabled = true;
                }
            } else {
                filterOptions.disabled = true;
            }
        }
    }

    function collapseRefinement(refIdx) {
        var ref = getElementById('ref'+refIdx);
        if(ref && ref.style) {
            ref.style.display = "none";
        }
        var expImg = getElementById('exp'+refIdx);
        if(expImg && expImg.style) {
            expImg.style.display = "none";
        }
        var colImg = getElementById('col'+refIdx);
        if(colImg && colImg.style) {
            colImg.style.display = "block";
        }
    }

    function expandRefinement(refIdx) {
        var ref = getElementById('ref' + refIdx);
        if(ref && ref.style) {
            ref.style.display = "";
        }
        var colImg = getElementById('col'+refIdx);
        if(colImg && colImg.style) {
            colImg.style.display = "none";
        }
        var expImg = getElementById('exp'+refIdx);
        if(expImg && expImg.style) {
            expImg.style.display = "block";
        }
    }



    function checkRedirect() {
        var redirectEle = getElementById('redirectUrl');
        if(redirectEle) {
            var url_to_go = redirectEle.innerText;
            //firefox does not support innerText property
            if(!url_to_go) {
                url_to_go = redirectEle.textContent;
            }
            if(url_to_go) {
                document.location.href = url_to_go ;
            }
        }
    }

	function guidedAjaxError(request) {
		if(request.instance.status == 404) {
			getElementById(divElement).innerHTML = "<!-- Page not found -->";
		}
	}

	//create url and make ajax request to update the results section
    function updateFilters(url, otherParams, divElement, nValue, rpp, noValue, nsValue) {
        if(otherParams && otherParams.charAt(otherParams.length-1) != '&') {
            otherParams = otherParams + "&";
        }
        otherParams = otherParams + "timestamp=" + new Date().getTime();
        if(otherParams && otherParams.charAt(0) != '&') {
            otherParams = '&' + otherParams;
        }
        var get_url = url + '?N=' + nValue + '&rpp=' + rpp + '&No=' + noValue + '&Ns=' + nsValue + otherParams;
		new ajax().update(get_url,
			   divElement,
			   {
				   method    : "GET",   // N is in URL so we must use GET method
                   onError   : guidedAjaxError
               }
            );
    }

    function getUIParams() {
        var openDimIds = "";
        var closeDimIds = "";
        var uiParams = "";
        try {
            if(jQuery(".filterList") != null && jQuery(".filterList").length > 0) {
                var filterLists = jQuery(".filterList");
                for(var v=0, vMax = filterLists.length; v < vMax; v++) {
                    var dimId = filterLists[v].id != null && filterLists[v].id.length > 3 ? filterLists[v].id.substr(3) : null; //take the id value after "dim"
                    if(dimId == null) continue;
                    if(jQuery("#dim" + dimId).is(":hidden")) {
                        closeDimIds += "|" + dimId;
                    } else {
                        openDimIds += "|" + dimId;
                    }
                }
            }
            if(openDimIds.length > 0){
                uiParams = uiParams + "&openDimIds=" + openDimIds.substr(1);
            }
            if(closeDimIds.length > 0){
                uiParams = uiParams + "&closeDimIds=" + closeDimIds.substr(1);
            }
        } catch(ignore){}
        return uiParams;
    }

    //create url and make ajax request to update the results section
    function updateResults(url, otherParams, divElement, nValue, rpp, noValue, nsValue) {

        if(otherParams && otherParams.charAt(otherParams.length-1) != '&') {
            otherParams = otherParams + "&";
        }
        otherParams = otherParams + "timestamp=" + new Date().getTime();
        if(otherParams && otherParams.charAt(0) != '&') {
            otherParams = '&' + otherParams;
        }
        otherParams = otherParams + getUIParams();
        if (nsValue) {
            //alert("noValue is present");
            var params = 'N=' + nValue + '&rpp=' + rpp + '&No=' + noValue + "&Ns=" + nsValue + otherParams;
        } else if (!rpp == 'undefined' && rpp!="") {
            //alert("noValue is not present");
            var params = '&rpp=' + rpp + otherParams;
        } else {
            var params = otherParams;
        }

        var get_url = url + '?' + params;
        new ajax().update(get_url,
			   divElement,
			   {
                   onPostSuccess: function() {
                       

                       //scrollToTop();
                       //setTimeout("positionCompare()", 1000);
                       //setExtraSpecData();
                       //shareThisActivate();
                       initFinderPage();
                       addthis.toolbox(".addthis_toolbox");
                       $("#Results #CLP-Promo").show();
                   },
			       onError   : function(request) { if(request.instance.status == 404) { getElementById(divElement).innerHTML = "<!-- Page not found -->"; } }
               }
            );

        if(jQuery("#finderGreyDiv") != null && jQuery("#finderGreyDiv").length > 0) {
            setTimeout('jQuery("#finderGreyDiv").css("height",jQuery("#resultsDiv").height());', 1000);
        }
    }

    function scrollToTop() {
        window.scrollTo(0,0);
    }



	function selectCategory(afinConfig) {
        if(document.getElementById("stt2")) {
            document.getElementById("stt2").value = "All";
        }
        if(document.getElementById("sttSeries")) {
            document.getElementById("sttSeries").value = "All";
        }
        if(document.getElementById("stt3")) {
            document.getElementById("stt3").value = "All";
        }
        updateAfinResults(true, document.getElementById("stt1"));
	}

	function selectFamily(afinConfig) {
        var stt1 = document.getElementById("stt1");
        var stt2 = document.getElementById("stt2");
        if(document.getElementById("sttSeries")) {
            document.getElementById("sttSeries").value = "All";
        }
        if(document.getElementById("stt3")) {
            document.getElementById("stt3").value = "All";
        }
        updateAfinResults(true, stt1, stt2);
	}

        function selectSeries() {
            var stt1 = document.getElementById("stt1");
            var stt2 = document.getElementById("stt2");
            var sttSeries = document.getElementById("sttSeries");
            if (document.getElementById("stt3")) {
                document.getElementById("stt3").value = "All";
            }
            updateAfinResults(true, stt1, stt2, sttSeries);
        }

        function selectModel() {
            var stt1 = document.getElementById("stt1");
            var stt2 = document.getElementById("stt2");
            var sttSeries = document.getElementById("sttSeries");
            var stt3 = document.getElementById("stt3");
            updateAfinResults(true, stt1, stt2, sttSeries, stt3);
        }

        function selectType() {
            var stt1 = document.getElementById("stt1");
            var stt2 = document.getElementById("stt2");
            var sttSeries = document.getElementById("sttSeries");
            var stt3 = document.getElementById("stt3");
            updateAfinResults(false, stt1, stt2, sttSeries, stt3);
        }

        function selectPartType() {
            var part = document.getElementById("part");
            var stt4 = document.getElementById("stt4");
            updateAfinPartResults(false, part, stt4);
        }

        function filterByPartType() {
             var part = document.getElementById("part");
                 filterAfinPartResults( part);
         }

        var findersAjaxCallMade = false;

        function updateAfinPartResults(clearAccTypesFlag, part, acctype) {
            var ntkValue = "p_CompatiblePart";
            var nttValue = part.value;

             // moreParams used by guided nav framework in jspf include files.  See Ntt and Ntk for Endeca searches.
            var otherParams = "Ntk=" + ntkValue.toString() + "&Ntt=" + nttValue.toString() + "&Ns=" + nsValue + '&target=afin.to';

            if (document.getElementById("finderParams") && document.getElementById("finderParams").value) {
                otherParams += document.getElementById("finderParams").value;
            }
            // Clear type list only when category, family, or model was changed.  List is rebuilt by updateAfinFilters()
            // on callback from Ajax call.
            try {
                if (clearAccTypesFlag)
                    clearAccTypes();
                var stt4 = document.getElementById("stt4");
                       var nValue = stt4 && stt4.value != "4294963323 4294963314 4294963323 4294963157" ? stt4.value : '4294963323 4294963314 4294963323 4294963157';
            } catch (error) {
            }

            var rpp = document.getElementById("View-By") != null ? document.getElementById("View-By").value : "5";
            var noValue = "0";

            var nsValue = document.getElementById("sortEle") != null ? document.getElementById("sortEle").value : "p_Featured";

            // Update results and filters using common functions defined in guidedNav.jspf
            findersAjaxCallMade = true;
            historyManager('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', '/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', nValue, rpp, noValue, nsValue, otherParams, true);
        }

    function filterAfinPartResults(part) {
        var ntkValue = "p_CompatiblePart";
         var nttValue = part.value;

        // moreParams used by guided nav framework in jspf include files.  See Ntt and Ntk for Endeca searches.
		var otherParams = "Ntk=" + ntkValue.toString()
                + "&Ntt=" + nttValue.toString()
                + "&Ns=" + nsValue
                    + "&part=" + part.value
                + '&target=afin.to';

        if(document.getElementById("finderParams") && document.getElementById("finderParams").value ) {
            otherParams += document.getElementById("finderParams").value;
        }

        var rpp = document.getElementById("View-By") != null ? document.getElementById("View-By").value : "5";
        var noValue = "0";

        var nsValue = document.getElementById("sortEle") != null ? document.getElementById("sortEle").value : "p_Featured";
        var stt4 = document.getElementById("stt4");
                 var nValue = stt4 && stt4.value != "4294963323 4294963314 4294963323 4294963157" ? stt4.value : '4294963323 4294963314 4294963323 4294963157';
        // Update results and filters using common functions defined in guidedNav.jspf
        findersAjaxCallMade = true;
        historyManager('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', '/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', nValue, rpp, noValue, nsValue, otherParams, true);
    }

        function updateAfinResults(clearAccTypesFlag, stt1, stt2, sttSeries, stt3) {
       		var ntkValue = "";
       		var nttValue = "";
       		// Category (stt1) is property search in p_CompatibleLine in Endeca.  Include if value is not "All"
       		if (stt1 && stt1.value != 'All') {
       			ntkValue = "p_CompatibleLine";
                   if(stt1.value.indexOf("&") >=0)
                              	stt1.value = stt1.value.replace("&", "%26");
       			nttValue = stt1.value;
       		}

       		// Type (stt2) is property search within p_CompatibleFamily in Endeca.    Include if value is not "All"
       		if (stt2 && stt2.value != 'All') {
       			if (ntkValue.length > 0)
       				ntkValue += "|";
       			ntkValue += "p_CompatibleFamily";
       			if (nttValue.length > 0)
       				nttValue +="|";
       			nttValue += stt2.value;
       		}

               // Series (sttSeries) is property search within p_CompatibleSeries in Endeca.    Include if value is not "All"
               if (sttSeries && sttSeries.value != 'All') {
                   if (ntkValue.length > 0)
                       ntkValue += "|";
                   ntkValue += "p_CompatibleSeries";
                   if (nttValue.length > 0)
                       nttValue +="|";
                   nttValue += sttSeries.value;
               }

       		// Model (stt3) is property search within p_CompatibleModel in Endeca.    Include if value is not "All"
       		if (stt3 && stt3.value != 'All') {
       			if (ntkValue.length > 0)
       				ntkValue += "|";
       			ntkValue += "p_CompatibleModel";
       			if (nttValue.length > 0)
       				nttValue +="|";
       			nttValue += stt3.value;
       		}

               if (stt1 && stt1.value == 'All') {
       			ntkValue = "All";
                   nttValue = stt1.value;
       		}
               // moreParams used by guided nav framework in jspf include files.  See Ntt and Ntk for Endeca searches.
       		var otherParams = "Ntk=" + ntkValue.toString() + "&Ntt=" + nttValue.toString() + "&Ns=" + nsValue + '&target=afin.to';

               if(document.getElementById("finderParams") && document.getElementById("finderParams").value ) {
                   otherParams += document.getElementById("finderParams").value;
               }
            // Clear type list only when category, family, or model was changed.  List is rebuilt by updateAfinFilters()
            // on callback from Ajax call.
            try {
                if (clearAccTypesFlag)
                    clearAccTypes();

                var stt4 = document.getElementById("stt4");
                var nValue = stt4 && stt4.value != "4294963323 4294963314 4294963323 4294963157" ? stt4.value : '4294963323 4294963314 4294963323 4294963157';
               } catch(error){}

               var rpp = document.getElementById("View-By") != null ? document.getElementById("View-By").value : "5";
               var noValue = "0";

               var nsValue = document.getElementById("sortEle") != null ? document.getElementById("sortEle").value : "p_Featured";

               // Update results and filters using common functions defined in guidedNav.jspf
               findersAjaxCallMade = true;
               historyManager('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', '/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', nValue, rpp, noValue, nsValue, otherParams, true);
           }

    function clearAccTypes() {
		var stt4 = document.getElementById("stt4");
		removeOptions(stt4);
		stt4.options[0] = new Option("All", "4294963323 4294963314 4294963323 4294963157");
		stt4.value = "4294963323 4294963314 4294963323 4294963157";
	}

    function positionCompare() {
        $(".showingResults > tbody > tr > td").each(function() {
            $("div", this).css("margin-top", (
                 $(this).height() - (12 +
                     $("table", this).eq(0).height() +
                     $("table", this).eq(1).height() +
                     $("div", this).height()))
                 + "px").show();
         });
    }

    function fixPng(imgEle) {
        try {
            if(imgEle && imgEle.src && !pngSupported && imgEle.src.indexOf("png") != -1 ) {
                var imgPath = imgEle.src;
                imgEle.src = '/images/assets/blank.gif';
                imgEle.runtimeStyle.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true,src='" + imgPath + "',sizingMethod='image')";
            }
        } catch(e) {

        }
    }

</script> 
