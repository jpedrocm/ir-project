  <script type="text/javascript">
  $(function() {
    $('a[href=#genuine]').click(function(e){
      e.preventDefault();
      popWindow('ebtextpop.to?page=genuineWindows',false,false, 500, 400);
    });

    $('a[href=#free-shipping]').click(function(e){
      e.preventDefault();
      popWindow('ebtextpop.to?page=FreeShipping499Up',false,false, 500, 250);
    });

    $('a[type=popup],a[href*="ebtextpop.to?"]').click(function(e){e.preventDefault(); popWindow(this.href, ($(this).attr('scroll'))?'yes':'yes',($(this).attr('resize'))?'yes':'no', ($(this).attr('width'))?$(this).attr('width'):500,($(this).attr('height'))?$(this).attr('height'):500);})
      });

</script> 
