  <script type="text/javascript">
    var lastHashName = "";
    var historyChangeCalled = false;
    function historyManager(filtersUrl, resultsUrl, nValue, rpp, noValue, nsValue, otherParams, loadFilters){
        
        if(loadFilters) {
            updateFilters(filtersUrl, otherParams,  'filtersDiv', nValue, rpp, noValue, nsValue);
        }
        
        updateResults(resultsUrl, otherParams,  'resDiv', nValue, rpp, noValue, nsValue);
        var newHashName = nValue + "~" + rpp + "~" + noValue + "~" + nsValue;
        var hashValue = nValue + "~" + rpp + "~" + noValue + "~" +  nsValue + "~" + otherParams;
        dhtmlHistory.add(escape(newHashName), hashValue);
        lastHashName = newHashName;
    }

    function historyChange(newLocation, historyData) {
        if(findersAjaxCallMade) {
            return;
        }
        historyChangeCalled = true;
        var historyMsg = (typeof historyData == "object" && historyData != null	? historyStorage.toJSON(historyData) : historyData);
        if(newLocation && typeof(historyMsg) != "undefined" && historyMsg != null && historyMsg.length > 2) {
            var paramVals = historyMsg.split("~");
            if(paramVals.length >=5) {
                updateFilters('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', paramVals[4],  'filtersDiv', paramVals[0], paramVals[1], paramVals[2], params[3]/*Ns*/);
                updateResults('/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', paramVals[4],  'resDiv', paramVals[0], paramVals[1], paramVals[2], paramVals[3]);
            }
        } else if (newLocation && typeof(newLocation) != "undefined") {
            if(isValidHash(newLocation) && newLocation != lastHashName
                    && newLocation != escape(lastHashName) && escape(newLocation) != lastHashName) {
                var params = newLocation.split("~");
                if(params.length >=4) {
                    updateFilters('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', 'Nty=1&Ntx=mode%2bmatchall&Ntk=p_CompatibleLine&stt3=All&target=afin.to&stt1=All&sttSeries=All&stt2=All&part=+Enter+Laptop+or+Tablet+Part+Number+Here&accFinderCat=All&Ns=pRating|1&Ntk=p_CompatibleLine&Ntt=All',  'filtersDiv', params[0]/*N*/, params[1]/*rpp*/, params[2]/*No*/, params[3]/*Ns*/); 
                    updateResults('/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', 'Nty=1&Ntx=mode%2bmatchall&Ntk=p_CompatibleLine&stt3=All&target=afin.to&stt1=All&sttSeries=All&stt2=All&part=+Enter+Laptop+or+Tablet+Part+Number+Here&accFinderCat=All&Ns=pRating|1&Ntk=p_CompatibleLine&Ntt=All',  'resDiv', params[0]/*N*/, params[1]/*rpp*/, params[2]/*No*/, params[3]/*Ns*/);
                }
            }
        } else {
            updateFilters('/showcase/explore/jsp/catalog/accessoryFinder/afinFiltersAjax.jsp', 'Nty=1&Ntx=mode%2bmatchall&Ntk=p_CompatibleLine&stt3=All&target=afin.to&stt1=All&sttSeries=All&stt2=All&part=+Enter+Laptop+or+Tablet+Part+Number+Here&accFinderCat=All&Ns=pRating|1&Ntk=p_CompatibleLine&Ntt=All',  'filtersDiv', '4294963323 4294963314 4294963323 4294963157', 10, 0, 'pRating|1'); 
            updateResults('/showcase/explore/jsp/catalog/accessoryFinder/resultsAjax.jsp', 'Nty=1&Ntx=mode%2bmatchall&Ntk=p_CompatibleLine&stt3=All&target=afin.to&stt1=All&sttSeries=All&stt2=All&part=+Enter+Laptop+or+Tablet+Part+Number+Here&accFinderCat=All&Ns=pRating|1&Ntk=p_CompatibleLine&Ntt=All',  'resDiv', '4294963323 4294963314 4294963323 4294963157',  10, 0, 'pRating|1');
        }
    }

    function isValidHash(inputText) {
        var validChars = "0123456789 ~%+|_abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";
        for(var i=0, iMax=inputText.length; i< iMax; i++) {
            if(validChars.indexOf(inputText.charAt(i)) == -1)
                return false;
        }
        return true;
    }

    $(document).ready( function() {

        //initialize our DHTML history
        dhtmlHistory.initialize();

        //subscribe to DHTML history change events
        dhtmlHistory.addListener(historyChange);

        initFinderPage();
        //setExtraSpecData();
        //shareThisActivate();
        if(jQuery("#finderGreyDiv") != null && jQuery("#finderGreyDiv").length > 0) {
            jQuery("#finderGreyDiv").height(jQuery("#resultsDiv").height());
        }

    });

        if(window.location.hash != null && window.location.hash.length >= 2 && isValidHash(window.location.hash.substr(1)) && !historyChangeCalled) {
            historyChange(window.location.hash.substr(1));
        }
</script> 
  <!-- END of page content --> 
