   <script type="text/javascript">
    var afinConfig = new AfinConfig();
    afinConfig.catPrefix = "Accessories for ";
    afinConfig.typeFirstOptionText = "Choose Family";
    afinConfig.seriesFirstOptionText = "Choose Series";
    afinConfig.modelFirstOptionText = "Choose Model";
    afinLoad(afinConfig);
    
    jQuery("#stt1").val(stt1Value);    
    if (stt1Value != '')
        afinOpt(afinConfig, stt1Value);

    jQuery("#stt2").val(stt2Value);
    if (stt2Value != '')
        afinOpt(afinConfig, stt1Value, stt2Value);

    jQuery("#stt3").val(stt3Value);
    if (stt3Value != '')
        afinOpt(afinConfig, stt1Value, stt2Value, stt3Value);

    jQuery("#sttSeries").val(sttSeriesValue);
    if (sttSeriesValue != '')
        afinOpt(afinConfig, stt1Value, stt2Value, stt3Value, sttSeriesValue);

</script> 
   <script type="text/javascript">
  
  function checkSubmitButton(){
    var partFieldVal = trimAll(jQuery("#part").val());
    var familyVal = jQuery("#stt2").val();
    var seriesVal = jQuery("#sttSeries").val();
    var isSeriesRendered = jQuery("#sttSeriesBlock").is(':visible');
    if (false == isSeriesRendered){
      seriesVal = familyVal;
    }
    if (("" != partFieldVal  && "Enter Laptop or Tablet Part Number Here" != partFieldVal) || "All" != seriesVal )
      return enableSubmitButton();
    else
      return disableSubmitButton();
  }
  
  function enableSubmitButton(){
    jQuery("#submitButton").removeAttr("disabled");
    jQuery("#submitButton").attr("src","/images/shop/buttons/find-accessories.png");
  }
  
  function disableSubmitButton(){
    jQuery("#submitButton").attr("disabled", "disabled");
    jQuery("#submitButton").attr("src","/images/shop/buttons/find-accessories-disabled.png");
  }
  
  
  function doAccessoryMenuReporting() {
    try {
      omtr = s_gi(s_account);
      omtr.linkTrackVars = "eVar9,eVar31,eVar32,hier1,events";
      omtr.linkTrackEvents = "event27,event28";
      omtr.eVar9 = $("#searchKeywords").html();
      omtr.eVar31 = $("#accFinderCat option:selected").text() + "|" + $("#part").val() + "|" + $("#stt1").val() + "|" +
        $("#stt2").val() + "|";
      if ($("#sttSeries").val()) omtr.eVar31 += $("#sttSeries").val().replace(/.*\^/, "") + "|";
      omtr.eVar31 += $("#stt3").val();
      omtr.eVar32 = "Accessory Finder";
      omtr.hier1 = omtr.eVar32 + ":" + omtr.eVar31.replace(/\|/g, ":");
      omtr.events = "event27,event28"
        omtr.tl(this, 'o', "Search");
      omtr.events = omtr.hier1 = omtr.eVar9 = omtr.eVar31 = omtr.eVar32 = "";
    } catch (e) {
    }
  }
  
  function resetPartField(){
    $("#part").val(" Enter Laptop or Tablet Part Number Here");   
  }            
  
</script> 
   <script>checkSubmitButton();</script> 
