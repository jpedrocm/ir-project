			$(document).ready(function() {
	                var stickyNavTop = $('.shell-category-header').offset().top;
	                var stickyNav = function(){
		                var scrollTop = $(window).scrollTop();
		                if (scrollTop > stickyNavTop) { 
		                      $('.shell-category-header').addClass('category-sticky');
		                } else {
		                      $('.shell-category-header').removeClass('category-sticky'); 
		                }
	                };
	                stickyNav();
	                $(window).scroll(function() {
	                      stickyNav();
	                });
					});
		</script>
