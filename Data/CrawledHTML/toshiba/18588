  <script>
        $(document).ready(function () {
            $("#reseller-locator-overlay-wrapper").overlay({
                mask: '#222222',
                effect: 'apple',
                opacity: '.5',
                fixed: false
            });
            $("a:contains('Tour')").click(function(){
                try{
                    reportTool("Product Tour");
                } catch(e) {}
            });
            var directPurchase = "null";
            var availableAtCDW = "true";
            var cdwUrl = 'www.cdw.com';

            if (directPurchase == 1 && "true"== availableAtCDW){
                var itemPartNumber = $("#SKU-PartNumber").val();
                var itemName = encodeURIComponent($("#SKU-Title").val());
                var itemQty = 1;
                var itemImg = $("#sku-image img").attr("src");
                var itemLink = window.location.href;
                updateCookie("add", itemPartNumber, itemName, itemQty,
                        itemImg, itemLink);
                $("#Overlay-Mask, #CDW-Interstitial-2").fadeIn(200).delay('5000').fadeOut(200);
                setTimeout(callBuyNow, '5000');
            }

            function callBuyNow(){
                buyNow(cdwUrl);
            }
        });
    </script> 
