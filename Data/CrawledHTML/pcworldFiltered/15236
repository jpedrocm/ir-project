  <script type="text/javascript">


    //todo WE REALLY SHOULD NOT HAVE SCRIPTS LIKE THIS IN TPL FILES, PLEASE CREATE JS CONFIG WITH THINGS LIKE aTagData
    //WHICH WE CAN THEN LOAD STUFF WITH

    /**
     * ie < 8 safe XMLHttpRequest
     * @returns {*}
     */
    function get_XmlHttp() {
        var xmlHttp = null;

        if(window.XMLHttpRequest) {        // Firefox, Chrome, IE7+, Opera, Safari, ...
            xmlHttp = new XMLHttpRequest();
        }
        else if(window.ActiveXObject) {    //  IE6, IE5
            xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
        }

        return xmlHttp;
    }

    /**
     * gets the opentag scripts from amazon cloudfare
     */
    function load_OpenTagScript()
    {
        var refNode = document.getElementById('ots').nextSibling;
        var script = document.createElement('script');

        script.type = 'text/javascript';
        script.src = '//d3c3cq33003psk.cloudfront.net/opentag-75667-654400.js';

        refNode.parentNode.insertBefore(script,refNode);
    }

    /**
     * gets the json representation of the universal_variable which we parse
     */
    function get_UniversalVariableByAjax()
    {
        var xmlhttp =  get_XmlHttp();

        xmlhttp.onreadystatechange=function() {
            var headObject = document.getElementsByTagName('head')[0];
            if (xmlhttp.readyState==4 && xmlhttp.status==200) {
                if(window.universal_variable) {
                    var analytics = window.universal_variable.analytics;
                }
                window.universal_variable=JSON.parse(xmlhttp.responseText);
                if(analytics){
                    //mcd-1526, for some reason in safari with cleared cache the JSON parse happens after we set up analytics
                    //causing an error
                    window.universal_variable.analytics = analytics;
                }
                load_OpenTagScript();
            }
        }

        // get universal variable json
        xmlhttp.open("GET","http://www.pcworld.co.uk/gb/uk/opentagscript/ajaxContext/%7B%22sFuseaction%22%3A%22item_details%22%2C%22sCountry%22%3A%22gb%22%2C%22sLanguage%22%3A%22uk%22%2C%22bSecure%22%3Afalse%2C%22iUniverseId%22%3A72%2C%22iCategoryId%22%3A703%2C%22iMarketId%22%3A7006%2C%22iSegmentId%22%3A70006%2C%22iBrandId%22%3A1731%2C%22iThemeId%22%3Anull%2C%22sProductId%22%3A%2210134864%22%2C%22iEquivalenceId%22%3Anull%2C%22bProductCanBeHomeDelivered%22%3Afalse%2C%22bProductCanBeReservedAndCollected%22%3Afalse%2C%22bProductIsOutOfStock%22%3Atrue%2C%22bProductIsHomeAndRC%22%3Afalse%2C%22bProductIsDownload%22%3Afalse%7D/ajax.html",true);
        xmlhttp.send();
    }
</script> 
  <script id="ots" type="text/javascript">
