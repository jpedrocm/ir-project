         <script type="text/javascript">    function search(){      if(jQuery(".store-search-keywords").val() && jQuery(".store-search-keywords").val().trim().length > 0){      	window.location = "http://www.newegg.com/Product/ProductList.aspx?Submit=ENE&DEPA=0&Order=BESTMATCH&N=100010428&isNodeId=1&Description=" + jQuery(".store-search-keywords").val();      }    }    jQuery(".store-search-keywords").keydown(function(e) {      if(e.keyCode == 13) {        search();      }    });</script> 
