            //'sellerRatings': true,
            'longerHeadlines' : false,
            'number' : '5',
            'width' : 'auto',
            //'longerHeadlines' : false,
            //'instantPreviews' : true,
            //'colorAdSeparator' : '#E3E6E9',
            //'colorBorder' : '#E3E6E9',
            'adLoadedCallback' : function(containerName, adsLoaded) {
      	      if (adsLoaded) {
      	      try {
      	        jQuery("#csa1Header").show();
      			jQuery("#csa1Feedback").show();
      			jQuery("#csa1").addClass("ad_box wide_ads");
      	      } catch (e) {
      			if(jQuery("#productListVersion").val() == 2016){
      				if(rightside.hasClass("has-side-banner")){
      					if(rightside.find(".row-side") && rightside.find(".row-side").height() > 60){
      						jQuery(".row-side-banners #narrow_ad_unit").parent().hide();
      					}
      					else{
                  jQuery(".row-side-banners #narrow_ad_unit").parent().hide();
      						rightside.removeClass("has-side-banner");
      					}
      				}
      			}
      		  }}
      		  else{
      			if(jQuery("#productListVersion").val() == 2016){
      				if(rightside.hasClass("has-side-banner")){
      					if(rightside.find(".row-side") && rightside.find(".row-side").height() > 60){
      						jQuery(".row-side-banners #narrow_ad_unit").parent().hide();
      					}
      					else{
                  jQuery(".row-side-banners #narrow_ad_unit").parent().hide();
      						rightside.removeClass("has-side-banner");
      					}
      				}
      			}
      		  }
      		  }
            };
            var adblock1 = {
            'siteLinks': false,
            'sellerRatings': false,
            'longerHeadlines': true,
            'instantPreviews': false,
            'number' : '4',
            'lines' : '2',
            //'verticalSpacing':5,
            'container' : 'csa2',
            'fontFamily' : 'arial',
            'fontSizeTitle' : '13px',
            'fontSizeDescription' : '13px',
            'fontSizeDomainLink' : '13px',
            'rolloverLinkColor' : '#0066FF',
            'colorText' : '#444444',
            'rolloverLinkUnderline' : false,
            'colorTitleLink' : '#335F9C',
            'colorDomainLink' : '#FF9900',
            'width' : 'auto',
            //'colorAdSeparator' : '#E3E6E9',
            'adLoadedCallback' : function(containerName, adsLoaded) {
      	      if (adsLoaded) {
      		  try {
      			jQuery("#csa2Header").show();
      			jQuery("#csa2").addClass("ad_box wide_ads");
      			} catch (e) {
      			}}	
      		}
            };
	
new google.ads.search.Ads(pageOptions, adblock2);

