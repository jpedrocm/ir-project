     <script type="text/javascript">
         usingNamespace("Biz.Store")["Seller"] = {
             isCrossDomain: 0,
             loaded: false,
             init: false,
             sellerId: "A7EX",
             storeType:"12",
             currentObj : null,
             collapsed: true,
             moreText:"Show More",
             lessText:"Show Less",
             getRequestUrl: function () {
                 var requestUrl;
                 if (this.isCrossDomain) {
                     requestUrl = Web.Config.Environment.Url.Content + "Common/Ajax/GetStoreNavigationList.aspx";
                 }
                 else {
                     requestUrl = Web.UI.ResourceManager.Url.www("Common/Ajax/GetStoreNavigationList.aspx");
                 }
                 return requestUrl;
             },
             displayMore: function (control, moreText, lessText) {
                 if (this.init && !this.loaded) {
                     return;
                 }
                 this.currentObj = control;
                 this.moreText = moreText;
                 this.lessText = lessText;
                 if (this.loaded) {
                     var _moredd =jQuery(this.currentObj).parent();
                     var _moreLabel = _moredd.children("label");
                     this.collapsed = !this.collapsed;
                     if(this.collapsed){
                         _moreLabel.html(this.moreText);
                         _moredd.addClass("collapsed").parent().children(".moreList").css("display","none");
                     }else{
                         _moreLabel.html(this.lessText);
                         _moredd.removeClass("collapsed").parent().children(".moreList").css("display","");
                     }
                     
                 } else {
                     this.init = true;
                     this.collapsed = false;
                     jQuery(this.currentObj).parent().children(".loader").toggle();
                     var _url = this.getRequestUrl() + "?StoreType="+this.storeType+"&SellerID=" + this.sellerId;
                     if (this.isCrossDomain) {
                         _url = _url + "&action=Biz.Store.Seller.renderData";
                         var script = document.createElement("script");
                         script.type = "text/javascript";
                         script.id = 'SellerStoreNavLimitAjaxReq';
                         script.src = _url;
                         document.getElementsByTagName('head')[0].appendChild(script);
                     } else {
                         jQuery.ajax({
                             type: "GET",
                             url: _url,
                             cache: false,
                             success: function (data) {
                                 Biz.Store.Seller.renderData(data);
                             }, 
                             error: function (xmlHttpRequest, error) {
                                 Biz.Store.Seller.loaded = false;
                                 Biz.Store.Seller.init = false;
                                 Biz.Store.Seller.collapsed = true;
                             }
                         });
                     }
                 }
             },
             renderData: function (data) {
                 this.loaded = true;
                 this.collapsed = false;
                 var _moredd = jQuery(this.currentObj).parent();
                 var _moreLabel = _moredd.children("label");
                 _moreLabel.html(this.lessText);
                 _moredd.toggleClass("collapsed");
                 _moredd.children(".loader").toggle();
                 _moredd.parent().children(".moreList").append(data).css("display","block");
             }
         };
    </script> 
