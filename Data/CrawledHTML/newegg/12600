       <script type="text/javascript">

    NEG.run(function (require) {
        var $ = require("Utility.jQuery");
        var switcher = require("Widget.Switch");
        var tab = document.getElementById('heroModule');
        var tabItems = $("#heroModule .wrap_banner");
        var tabTriggers = $("#heroNav li");
        var ctrl = switcher(tab, {
            contents: tabItems,
            interval: 6000,
            triggers: tabTriggers,
            autoPlay: true,
            switchAfter: function () {
                var currentIndex = this.activeIndex + 1;
                var style = $(tab).attr("class");
                style = style.replace(/show_[0-9]/, "show_" + currentIndex);
                $(tab).attr("class", style);
                
                NEG(tabItems[this.activeIndex]).trigger("Switch_Activate");
            }
        });

    });
</script> 
