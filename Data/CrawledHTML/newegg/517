        pagesize:36,
	    itemCount:4041,
	    pageCount:113,
	    langResource:{ShowingRecord:'Showing  {0}-{1} of {2} Products',Page:'Page',Of:'of'},
	    order:8,
	    url:"http://www.newegg.com/Product/ProductList.aspx",
	    subCategoryUrl:"",
	    baseQueryString:new Web.QueryStringBuilder("Submit=ENE\&N=50001080\&IsNodeId=1\&Manufactory=1080\&bop=And\&SpeTabStoreType=0"),					
	    sortQueryString:new Web.QueryStringBuilder(""),
	    multiAction:true,
	    DetailedView:{
	        orderMapping:{1:'RATING',2:'PRICE',3:'PRICED',4:'REVIEWS',6:'DESCRIPTION',7:'DESCRIPDOWN',8:'BESTMATCH',9:'FEATURED',10:'SMARTSORT',12:'STOREPM',14:'RELEASE',15:'BESTSELLING'},
		    currentOrder:""
		},
	    PageUrlAlias:{
	        compare:"http://www.newegg.com/Product/Productcompare.aspx",
		    wishlist:"https://secure.newegg.com/WishList/WishCartPlus.aspx",
		    shoppingCart:"https://secure.newegg.com/Shopping/AddToCart.aspx"
		},
	    ItemType:{
	        Normal:"0",
		    Preorder:"1"
		},
	    CustomizeOptions:{
	        descriptionValue4:"13",
		    descriptionValue8:"20"
		},
	    Manufactory:"",
	    SubCategoryId:"-1",
	    MasterComboId:"-1",
	    ModuleID: "",
	    PageID: "",
	    BaseOnItemNumber: "",
	    BaseOnSubCategoryID: "",
	    BaseOnKeyword: "",
	    DisplayedItems: "",
	    Navigation: "",
	    HooklogicAds: {
	        enable:true,
		    tagPosition:9,
		    tagPositionDefault:9
		    }
	};
    var cfg=Biz.ProductList.Config;
    Biz.ProductList.Common.cmpItemListInQueryString=cfg.baseQueryString.get("CompareItemList")?cfg.baseQueryString.get("CompareItemList").split(","):[];
    Biz.ProductList.Common.initialize();
    window.attachEvent("onload",(function(){
        var bpc=Biz.ProductList.Config;
        Biz.ProductList.Common.fillSelect($("select_top"),bpc.pagesize);
        jQuery("#select_page_size").text(function() {
            return jQuery("#select_top").val();
        });
        Biz.ProductList.Common.fillSelect($("Order_top"),bpc.order);
        jQuery("#product_sort_type").text(function() {
            return jQuery("#Order_top").find("option:selected").text();
        });
    }));	    
