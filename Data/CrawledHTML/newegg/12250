            <!--common facet body --> 
            <dd class="filter-box-body"> 
             <!-- body core --> 
             <ul class="filter-box-list"> 
              <!-- common facet --> 
              <li> 
               <!-- checkbox and lable --> 
               <div class="filter-box-label"> 
                <label class="form-checkbox" for="601193723"> <input type="checkbox" id="601193723" name="600038458" neg-n-value="100011693 601193723" autocomplete="off"> <span class="form-checkbox-title">1.50mm</span> </label> 
               </div> 
               <!-- item count--> <span class="filter-box-num">(2)</span> </li> 
              <!-- range facet --> 
              <!-- common facet --> 
              <li> 
               <!-- checkbox and lable --> 
               <div class="filter-box-label"> 
                <label class="form-checkbox" for="601107968"> <input type="checkbox" id="601107968" name="600038458" neg-n-value="100011693 601107968" autocomplete="off"> <span class="form-checkbox-title">2.23mm</span> </label> 
               </div> 
               <!-- item count--> <span class="filter-box-num">(1)</span> </li> 
              <!-- range facet --> 
              <!-- common facet --> 
              <li> 
               <!-- checkbox and lable --> 
               <div class="filter-box-label"> 
                <label class="form-checkbox" for="600556022"> <input type="checkbox" id="600556022" name="600038458" neg-n-value="100011693 600556022" autocomplete="off"> <span class="form-checkbox-title">2.30mm</span> </label> 
               </div> 
               <!-- item count--> <span class="filter-box-num">(3)</span> </li> 
              <!-- range facet --> 
              <!-- common facet --> 
              <li> 
               <!-- checkbox and lable --> 
               <div class="filter-box-label"> 
                <label class="form-checkbox" for="601114385"> <input type="checkbox" id="601114385" name="600038458" neg-n-value="100011693 601114385" autocomplete="off"> <span class="form-checkbox-title">2.38mm</span> </label> 
               </div> 
               <!-- item count--> <span class="filter-box-num">(3)</span> </li> 
              <!-- range facet --> 
              <!-- common facet --> 
