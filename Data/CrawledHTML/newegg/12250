               </div> 
               <!-- item count--> <span class="filter-box-num">(1)</span> </li> 
              <!-- range facet --> 
              <!-- common facet --> 
              <li> 
               <!-- checkbox and lable --> 
               <div class="filter-box-label"> 
                <label class="form-checkbox" for="600374186"> <input type="checkbox" id="600374186" name="600038458" neg-n-value="100011693 600374186" autocomplete="off"> <span class="form-checkbox-title">7.00mm</span> </label> 
               </div> 
               <!-- item count--> <span class="filter-box-num">(34)</span> </li> 
              <!-- range facet --> 
             </ul> 
             <!-- alone apply button--> 
             <div class="filter-box-bottom"> 
              <button class="btn btn-tool btn-mini filter-box-apply" style="display: none" id="fab_600038458" name="fabGroup">Apply</button> 
             </div> 
             <!-- show more area --> 
            </dd> 
           </dl> 
           <dl class="filter-box has-menu" id="facet600108253"> 
            <!--common facet title --> 
            <dt class="filter-box-title">
             Used For
            </dt> 
            <input type="hidden" name="hidNSValue" value=""> 
            <!--common facet body --> 
            <dd class="filter-box-body"> 
             <!-- body core --> 
             <ul class="filter-box-list"> 
              <!-- common facet --> 
              <li> 
               <!-- checkbox and lable --> 
               <div class="filter-box-label"> 
                <label class="form-checkbox" for="600110116"> <input type="checkbox" id="600110116" name="600108253" neg-n-value="100011693 600110116" autocomplete="off"> <span class="form-checkbox-title">Enterprise</span> </label> 
               </div> 
               <!-- item count--> <span class="filter-box-num">(2)</span> </li> 
              <!-- range facet --> 
              <!-- common facet --> 
              <li> 
               <!-- checkbox and lable --> 
               <div class="filter-box-label"> 
                <label class="form-checkbox" for="600110115"> <input type="checkbox" id="600110115" name="600108253" neg-n-value="100011693 600110115" autocomplete="off"> <span class="form-checkbox-title">Consumer</span> </label> 
               </div> 
