            var bannerSwiper = new Swiper('#heroModule', {
                prevButton: '#heroModule .swiper-button-prev',
                nextButton: '#heroModule .swiper-button-next',
                centeredSlides: true,
                loop: true,
                autoplay: 5000
            });

