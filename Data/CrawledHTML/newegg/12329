   <script>
			jQuery('.guide .view-more').click(function(){
				var $self = jQuery(this);
				if ($self.siblings('.section-inner').children('.more-guide')[0]) {
					$self.siblings('.section-inner').children('.more-guide').stop().slideToggle();
					$self.parents('.guide').toggleClass('is-active');
					if ($self.parents('.guide.is-active')[0]) {
						$self.children('.view-more-label').text('Load Less');
					}else {
						$self.children('.view-more-label').text('Load More');
					}
				}
			});
		</script> 
