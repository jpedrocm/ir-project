jQuery(window).load(function(){
	//v2
	//banner
	var _bannerSwiper_1 = new Swiper('#Banner_v2_1', {
		pagination: '#Banner_v2_1 .thumbnail',
		loop: true,
		speed: 250,
		autoplay: 5000,
		paginationClickable: true
	});
	var _bannerSwiper_2 = new Swiper('#Banner_v2_2', {
		pagination: '#Banner_v2_2 .thumbnail',
		loop: true,
		speed: 250,
		//autoplay: 5000,
		paginationClickable: true
	});
	
	jQuery('#Category_v2').hover(function(){
		jQuery(this).find('.category_arrow').show();
	}, function(){
		jQuery(this).find('.category_arrow').hide();
	}).find('li:not([class="category_arrow"])').each(function( index ){
		var arrow = jQuery(this).siblings('.category_arrow');
		var width = jQuery(this).width();
		jQuery(this).hover(function(){
			arrow.stop();
			Pointer(arrow, index, width);
		}, null);
	});
	
});
function Pointer( arrow, index, width ) {
	arrow.animate({ left: index * width + index }, 100);
}
