             <!-- begin: item list --> 
             <script type="text/javascript">
    usingNamespace("Biz.Product")["CompareConfig"] = {
        baseQueryString: new Web.QueryStringBuilder(""),
        PageUrlAlias: {
            compare: "http://www.newegg.com/Product/Productcompare.aspx"
        },
        MasterComboId: "-1",
        SubCategoryId: "10",
        compareItems: [],
        Thumbs: []
    };
</script> 
             <div class="items-view is-grid"> 
              <div class="item-container "> 
               <!--product image--> 
               <a href="http://www.newegg.com/Product/Product.aspx?Item=9SIA0ZX4NT8767" class="item-img"> <img alt="Lenovo Desktop Computer Y900-34ISZ (90DD000TUS) Intel Core i7 6700 (3.4 GHz) 8 GB DDR4 2 TB HDD + 8 GB SSHD + 120 GB SSD NVIDIA GeForce GTX 970 4 GB Windows 10 Home 64-Bit" title="Lenovo Desktop Computer Y900-34ISZ (90DD000TUS) Intel Core i7 6700 (3.4 GHz) 8 GB DDR4 2 TB HDD + 8 GB SSHD + 120 GB SSD NVIDIA GeForce GTX 970 4 GB Windows 10 Home 64-Bit" src="http://images10.newegg.com/ProductImageCompressAll300/83-798-522-20.jpg"> </a> 
               <div class="item-info"> 
                <!--brand info--> 
                <div class="item-branding"> 
                 <a href="http://www.newegg.com/Lenovo/BrandStore/ID-10418" class="item-brand"> <img src="http://images10.newegg.com/Brandimage_70x28/Brand10418.gif" alt="Lenovo" title="Lenovo"></a> 
                 <!--rating info--> 
                 <a title="Rating + 4" href="http://www.newegg.com/Product/ProductReview.aspx?Item=9SIA0ZX4NT8767" class="item-rating"><i class="rating rating-4"></i><span class="item-rating-num">(3)</span></a> 
                </div> 
                <!--description info--> 
                <a href="http://www.newegg.com/Product/Product.aspx?Item=9SIA0ZX4NT8767" class="item-title" title="View Details">Lenovo Desktop Computer Y900-34ISZ (90DD000TUS) Intel Core i7 6700 (3.4 GHz) 8 GB DDR4 2 TB HDD + 8 GB SSHD + 120 GB SSD NVIDIA GeForce GTX 970 4 GB Windows 10 Home 64-Bit</a> 
                <!--promption info--> 
                <p class="item-promo"></p> 
                <!--feature--> 
                <ul class="item-features"> 
                 <li><strong>Type:</strong> Gaming &amp; Entertainment</li>
                 <li><strong>Form Factor:</strong> Tower</li>
