           <li> 
            <!-- egg style lable --> 
            <div class="filter-box-label"> 
             <label class="form-checkbox" for="4111"> <input type="checkbox" id="4111" name="4110" autocomplete="off" title="Ratings" neg-n-value="100161775 4111"> <span class="form-checkbox-title"> <i class="rating rating-1"></i> </span> </label> 
