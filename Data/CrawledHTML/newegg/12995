            <div class="centerPopup-title">
             <h2></h2>
            </div> 
            <!--popup-header--> 
            <div class="complete-sections-wrap"> 
             <div class="complete-section-filter" style="display: none;"> 
              <ul class="complete-section-filter-alphabet"> 
               <li style="display: none;"><a href="javascript:void(0)"></a></li> 
               <li style="display: none;"><a href="javascript:void(0)"></a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">#</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">A</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">B</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">C</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">D</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">E</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">F</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">G</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">H</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">I</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">J</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">K</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">L</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">M</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">N</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">O</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">P</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">Q</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">R</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">S</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">T</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">U</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">V</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">W</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">X</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">Y</a></li> 
               <li class="is-disabled"><a href="javascript:void(0)">Z</a></li> 
              </ul> 
             </div> 
             <ul class="filter-box-list complete-section-list"> 
             </ul> 
            </div> 
            <!--popup-body--> 
            <div class="centerPopup-bottom"> 
             <div class="centerPopup-button-area"> 
              <button type="button" class="btn btn-tool centerPopup-trigger-close" onclick="Biz.SearchPanel2016.NavigationListLimit.applyFiltersEvent()">Apply Filters <i class="fa fa-caret-right"></i> <input type="hidden" id="hidSelectedNList"> </button> 
             </div> 
