   <section class="page-section"> 
    <div class="page-section-inner"> 
     <div class="row has-side-left"> 
      <!-- begin: left side --> 
      <div class="row-side"> 
       <div class="left-nav"> 
        <!-- common checkbox Scenes --> 
        <!--Search Panel Core--> 
        <script type="text/javascript">
    Biz.SearchPanel2016.NavigationListLimit.Config={RequestContent:{Submit:'ENE',sstore:'',havesstore:'',IsNodeId:'1',StoreType:'NonStore',Depa:'133',Category:'-1',SubCategory:'-1',Brand:'-1',Ntk:'',SecondSearch:'False',Description:"",Configurator:'',CFG:'',ActiveSearchResult:'',FirstFromAsia:'False',PageSize:'0',page:'1',name:"Writing \x26 Correction Supplies",OEMMark:"",N:'100093462 4824',IsShowItemCount:true}}
</script> 
        <!--powerserarch head--> 
        <!--facet--> 
        <dl class="filter-box is-category has-menu is-active"> 
         <dt class="filter-box-title">
          Writing &amp; Correction Supplies
         </dt> 
         <dd class="filter-box-body"> 
          <input type="hidden" name="hidNSValue" value="100093521"> 
          <!-- common department--> 
          <!-- subcatgory --> 
          <ul class="filter-box-list"> 
           <li> 
            <!-- link --> <a class="filter-box-label" href="http://www.newegg.com/Product/ProductList.aspx?Submit=ENE&amp;N=100093521%204824&amp;IsNodeId=1&amp;name=Pens%2c%20Pencils%2c%20%26%20Markers&amp;SpeTabStoreType=356&amp;isdeptsrh=1" title="Pens, Pencils, &amp; Markers"> Pens, Pencils, &amp; Markers <span style="display: none;">100093521 4824</span> </a> 
