           <input id="haQuickSearchBox" name="Description" tabindex="1" title="Search Site" maxlength="268" value="Keywords, Model # or Item #" type="search" autocomplete="off" onblur="Biz.Common.QuickSearch.inputFocus(false)" onwebkitspeechchange="this.value=(this.value).replace(/Keywords, Model # or Item #/,'')" onfocus="Biz.Common.QuickSearch.inputFocus(true)"> 
           <script type="text/javascript">Biz.Common.QuickSearch.chromeSpeechSearch(); </script> 
