         <script type="text/javascript">
jQuery(window).load(function(){
	//v12
	//banner
	var banner_current = 0;
	var _width = jQuery('#Banner_v12 .thumbnail li').width();
	SetArrow(_width, banner_current, 0);
	jQuery('#Banner_v12 .thumbnail li').each(function( index ){
		var arrow = jQuery('#Banner_v12 .arrow');
		var title = jQuery('#Banner_v12 .title');
		jQuery(this).hover(function(){
			var width = jQuery(this).width();
			var text = jQuery(this).find('a').attr('title');
			title.text(text);
			arrow.stop();
			banner_current = index;
			var full = jQuery('#Banner_Text').width();
			var margin = (full - width * 3) / 2;
			Pointer(arrow, banner_current, width + margin);
		}, null);
	});
	jQuery(window).resize(function(){
		var width = jQuery('#Banner_v12 .thumbnail li').width();
		var full = jQuery('#Banner_Text').width();
		var margin = (full - width * 3) / 2;
		SetArrow(width, banner_current, margin);
	});
});
function SetArrow(width, banner_current, margin){
	var arrow = jQuery('#Banner_v12 .arrow');
	arrow.css({
		width: width,
		display: 'block'
	});
	arrow.stop();
	Pointer(arrow, banner_current, width + margin);
}
function Pointer( arrow, index, width ) {
	arrow.animate({ left: index * width }, 100);
}
</script> 
