          <!-- begin: center side --> 
          <div class="row-body"> 
           <div class="row-body-inner"> 
            <!-- begin: Config Script For RSS --> 
            <script type="text/javascript">				
    usingNamespace("Biz.ProductList")["Config"]={
        pageIdex:1,
        pagesize:36,
	    itemCount:41,
	    pageCount:2,
	    langResource:{ShowingRecord:'Showing  {0}-{1} of {2} Products',Page:'Page',Of:'of'},
	    order:8,
	    url:"http://www.newegg.com/Product/ProductList.aspx",
