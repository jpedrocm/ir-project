    <script type="text/javascript">
				
					function setupLanding() {
										
						$('#ow-content .hero-menu li').each(function(index) {
							$(this).bind('click', function() {
                				if($(document).width() >= 480) {
								  $('#ow-content .hero-menu li').removeClass("hover");
								  $(this).addClass("hover");
								  $('#ow-content .hero-img a').css('display', 'none');
								  $('#ow-content #hero-link-' + (index + 1)).css('display', 'block');
				                } else {
				                  window.location.href = $("#ow-content .hero-img a").eq(index).attr("href");
				                }
							});
						});
	
        				if($(document).width() >= 480) {
						  $('#ow-content #hero-1').trigger('click');
            			}
					}
				
			</script>
