				function emailThisPage() {
					var u = "//shop.lenovo.com//shop.lenovo.com/SEUILibrary/controller/e/usstudents/StdAffinityPortal/en_US/seutil.workflow:ShowEmailPage?width=500&Email.PageUrl=";
