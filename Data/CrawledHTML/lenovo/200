   <script type="text/javascript">

function bvOverlay(itemoid, bvproductid){
	var _showInOverLay = false;
	<!-- BVHandlerRWD.html -->
	if ($("#bv-results-wrapper").length == 0) {
			var results = $("<div id='bv-results-wrapper'>");
			results.append("<div id='bvreviewsWrap'>");
			var buttonWrapper = $("<div id='bv-results-buttons'>");
			var button = $("<button class='button-standard-alt'>CLOSE</button>");

			button.click(
				function (e) {
					var $_continer = $("div.contentContainer");

					//If there are facet result items are there then no need to remove the browsing class
					if($(".facetedResults").length == 0){
						$_continer.removeClass("browsing");
					}
					$("#bv-results-wrapper").hide();
					$('html, body').animate({scrollTop: $_continer.offset().top}, 500);
					e.preventDefault();
				}
			);
			buttonWrapper.append(button);
			results.append(buttonWrapper);
			if($("#results-area").length != 0)
				results.insertAfter("#results-area");
			else{
				//if there is no parent item to display the review then use overlay to show them
				_showInOverLay = true;
			}
	}
	if ($("a[href='#tab-reviews']").length != 0) {
		//if its Sub series then show the reviews in the tab
		$("a[href='#tab-reviews']").trigger('click');
		return;
	}

	$.post("//shop.lenovo.com/SEUILibrary/controller/e/usstudents/StdAffinityPortal/en_US/wci.workflow:load?page=/WW/site/templates/bazaarvoice/owv2/BVTemplate.html", {
	'oid' : itemoid,
	'bvoid' : bvproductid
	}, function (bvcontent) {

		if(!_showInOverLay){
			$("#bvreviewsWrap").html(bvcontent);
			$("#bv-results-wrapper").show();
			$("div.contentContainer").addClass("browsing");
		}else{
			$("#bvOverlay").fancybox({
				'content': $("<div id='bvreviewsWrap'>").append(bvcontent),
				'autoScale': true,
				'transitionIn': 'elastic',
				'transitionOut': 'elastic',
				'speedIn': 500,
				'speedOut': 300,
				'autoDimensions': true,
				'centerOnScroll': true,
			}).click();
		}
	});

	if(!_showInOverLay){
		var _r_wrapper = $("#bvreviewsWrap").offset();
		$('html, body').animate({scrollTop: (typeof _r_wrapper != 'undefined') ? _r_wrapper.top: ''}, 500);
	}
}

$('.contentContainer').on('click','.bv_reviews',function(e){
	bvOverlay($(this).data('oid'), $(this).data('pid'));
	e.preventDefault();
});
</script> 
