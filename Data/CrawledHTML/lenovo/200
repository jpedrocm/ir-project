         <script type="text/javascript">
	//Desktop Version	
    $('.search_menu').prepend('<form role="search" class="searchbox" action="https://search.lenovo.com/search" method="get" name="site_search" onsubmit="return ajaxsearchsubmit(this)"><input class="input-text" id="sli_search_1" autocomplete="off" type="search" name="w" placeholder="Search" data-provide="rac"><input type="hidden" name="cc" value="www"><input type="hidden" name="fsearch" value="yes"><input value="en" type="hidden" name="lang"></form>');
	
	//Mobile Version
	$('.search_menu .link-wrapper').html('<form role="search" class="searchInput" method="get" name="site_search" onsubmit="return ajaxsearchsubmit(this)"><input id="sli_search_1" class="searchInput-text" autocomplete="off" type="search" name="w" placeholder="Enter a search term" data-provide="rac"><input type="hidden" name="cc" value="us"><input value="en" type="hidden" name="lang"><input class="button-standard searchInput-button" value="Search" type="submit"></form>');
	var matchString = ["monitor","battery","charger","dock","lenovo 500 bluetooth speaker","stack","memory","ram","warranties","warranty"];
	var redirect_URL = ["/usstudents/us/en/accessories/monitors/","/usstudents/us/en/accessories/batteries-power-adapters/?page-size=40","/usstudents/us/en/accessories/batteries-power-adapters/","/usstudents/us/en/accessories/dock-port-replicators/","/usstudents/us/en/accessories/lenovobluetoothspeaker","/usstudents/us/en/accessories/buythinkpadstack/","/usstudents/us/en/accessories/memory/","/usstudents/us/en/accessories/memory/","//shop.lenovo.com/SEUILibrary/controller/e/usstudents/StdAffinityPortal/en_US/config.workflow:StartWarranty","//shop.lenovo.com/SEUILibrary/controller/e/usstudents/StdAffinityPortal/en_US/config.workflow:StartWarranty"];
	function ajaxsearchsubmit(form) {
	var search = encodeURIComponent(form.w.value);
	document.activeElement.blur();
	  for (var iLoop = 0; iLoop < matchString.length; iLoop++){
		 if (search.trim().toLowerCase().indexOf(matchString[iLoop]) != -1)
         {
           window.location = redirect_URL[iLoop];
           return false;
         }
      }
	window.location = "http://search.lenovo.com/search#w=" + search + "&fsearch=1";
	return false;
    }	
	</script> 
