#rollovercart #rollovercartContents #rollovercartContentsItems > li > a .rollovercartItemImg {max-width:50px; width: 100%; top: 50% !important; left: 50% !important; -webkit-transform: translate(-50%,-50%) !important; -ms-transform: translate(-50%,-50%) !important; -moz-transform: translate(-50%,-50%) !important; transform: translate(-50%,-50%) !important;}
div.rolloverCartDesc{width:80%; float:left;}
div.rolloverCartDesc >a{width:100%; float:left; font-size:.8em; font-weight:bold; color: #009dd9; line-height:1.2em;}

form.updateQty{font-size: .8em; width: auto; float: left; margin-left: 3px; clear:left; margin-top: .6em;}
form.updateQty label{margin-right: 3px; color: #454545;}
