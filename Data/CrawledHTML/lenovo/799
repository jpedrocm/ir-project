try
{
		if(navigator && navigator.userAgent) {
			var ua = navigator.userAgent.toLowerCase();    
		var iOS = /ipad|iphone|ipod/.test(ua);
		var android = /android/.test(ua);	
		if(iOS){
		$(document.body).addClass('device-iOS');
		}    
			else if(android) { 
				$(document.body).addClass('device-android');
			}
		}
}
catch(detecterr)
{
	console.error(detecterr);
}
</script> 
