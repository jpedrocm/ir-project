					var ua = navigator.userAgent.toLowerCase();
					var isAndroid = ua.indexOf("android") > -1;
					if(isAndroid) {
						footerjson.icons[x].url = "wtai://wp/mc;" + footerjson.icons[x].phoneNumber;
					}	
					else{		
						footerjson.icons[x].url = "tel://" + footerjson.icons[x].phoneNumber;
					}
				}
				else{
					footerjson.icons[x].url = "";
				}
			}
			
			if(typeof footerjson.icons[x].url != "undefined" && footerjson.icons[x].url != ""){
				navHtml += '<a href="' + footerjson.icons[x].url + '"';
				if(footerjson.icons[x].newTab){
					navHtml += ' target="_blank" ';
				}
								
				navHtml += '"><img src="' + footerjson.icons[x].icon + '"/><span>' + footerjson.icons[x].label + '</span></a>'
				
			}			
		}
		
		navHtml += '<div id="MobileChatSalesContainer"><div id="MobileChatSalesLink"><div id="MobileChatSalesLinkInfo"><img src="//www.lenovo.com/shop/americas/content/img_lib/mobile/dock-icon-chat-v2.png"/></div></div></div>'
		
		navHtml += '</nav>';
		
		//hookup builder listener
		$(document).ajaxComplete(function(e, xhr, settings){		
			if(settings.url.indexOf("workflow:LoadBuilderTemplate") != -1){
				$(".dockNav").hide();
			}
		});				
	
		$("document").ready(function(){
			$("div.mainFooter").addClass("stickyDock");		
		  $("body").prepend(navHtml);
		});		
	}	
	



