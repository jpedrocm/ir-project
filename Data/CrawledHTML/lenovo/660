   <script type="text/javascript">
  $(".footer-secondary-navigation-links-title").on("click", function() {
    $(this).next("ol").slideToggle();
    if($(this).hasClass("footer-nav-list-open"))
    {
      $(this).removeClass("footer-nav-list-open").addClass("footer-nav-list-collapsed");
    }
    else
    {
      $(this).removeClass("footer-nav-list-collapsed").addClass("footer-nav-list-open");
    }
  });
</script> 
