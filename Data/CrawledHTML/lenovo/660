	  }
	  $('.bar_3-logos .left').show();
	  
	  //Email Sign-UP  
	  //$('li.email_menu > a').attr('href','https://webform.wd.lenovo.com/webforms/Signup/COMEMS15?country_iso2=ca&ldsrc=emlsu1a&site_ssc={SITE}');
	$(function(){
		$('li.email_menu > a').fancybox({
			'type':'iframe',
			onComplete: function(){
			   $('#fancybox-content').css({'height':'650px','background':'#fff'});
			   $('#fancybox-close').css({'right':'-30px'});
			}
		});
		$('.newsletterInput').submit(function() {
		     var cus_email = $('.newsletterInput .newsletterInput-text').val();		
            var url = $(this).attr("action") + "&email=" + (encodeURIComponent(cus_email));			
            $.fancybox({
                href: url,
                'type': 'iframe',
				onComplete: function(){
				$('#fancybox-content').css({'height':'650px','background':'#fff'});
				$('#fancybox-close').css({'right':'-30px'});
				}
            }); 
			return false;			
        });
	});
	  
</script> 
