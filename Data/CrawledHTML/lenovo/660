function SubmitWebForm(path, command)
{
  


  
	persistRtfData();
  swapAction('https://shop.lenovo.com/SEUILibrary/controller/e/webca/LenovoPortal/en_CA/webforms.workflow:EmailWebForm?UniqueURLID=1476683795359&statedata_id_string=&WebFormId=emailForm');
}
var rftEditorObjects = new Array();
function addRtfEditor(obj)
{
	rftEditorObjects[rftEditorObjects.length] = obj;
}
function persistRtfData()
{
	for(var i=0;i<rftEditorObjects.length;i++)
	{
		rftEditorObjects[i].saveContent();
	}
}

function ResetWebForm()
{
  document.getElementById('mainForm').reset();
}

function DeleteWebForm()
{
  
  if( isChanged() ){
  	alert('Form values are changed. Delete Disabled.');
    return;
  }
  swapAction('https://shop.lenovo.com/SEUILibrary/controller/e/webca/LenovoPortal/en_CA/webforms.workflow:DeleteWebForm?UniqueURLID=1476683795360&statedata_id_string=&WebFormId=emailForm');
}

function swapAction(newAction)
{
  var frm = document.mainForm;
  frm.action = newAction;
  frm.submit();
  return false;
}

function refreshPage(){
  swapAction('https://shop.lenovo.com/SEUILibrary/controller/e/webca/LenovoPortal/en_CA/webforms.workflow:StartWebForms?UniqueURLID=1476683795360&statedata_id_string=&WebFormId=emailForm&capture=true');
}

function executeCommand(url, param){
  swapAction(url + '&' + 'WebFormId=emailForm&capture=true' + '&' + param);
}

function downloadFile(attachmentId, fileName){

  swapAction('//shop.lenovo.com/SEUILibrary/controller/e/webca/LenovoPortal/en_CA/webforms.workflow:DownloadAttachment?UniqueURLID=1476683795361&statedata_id_string=' + '&AttachmentId='+ attachmentId  + '&FileName='+ fileName);
}

function content_onload()
{
  

  if(typeof(nicEditor) != "undefined")
  {
    var rtfWrappers = document.getElementsByTagName("TEXTAREA");
    for(var i=0;rtfWrappers != null && i < rtfWrappers.length;i++)
    {
      if(rtfWrappers[i].className == "fieldRtf")
      {
        var panelName = rtfWrappers[i].name;
        new nicEditor({buttonList : ['bold','italic','underline','left','center','right','justify','ol','ul','fontSize','fontFamily','indent','outdent','image','upload','link','unlink','forecolor','bgcolor'], iconsPath : '/ISS_Static/site/rtf/rtficons.gif'}).panelInstance(panelName);
        var editor = nicEditors.findEditor(panelName);
        editor.setContent(document.getElementById(panelName).value);
        addRtfEditor(editor);       
      }
    }
  }

  setCrumbTail('Email Us');
  
}

function disableForm(theform) {
//	alert("Disabling Form");
	if (document.all || document.getElementById) {
		for (i = 0; i < theform.length; i++) {
			var formElement = theform.elements[i];
			formElement.disabled = true;
		}
	}
}

function removeCR(eleName) {
//	alert("Before = " + document.getElementById(eleName).value);
	var str = document.getElementById(eleName).value;
	document.getElementById(eleName).value=str.replace("\n", "");
//	alert("After = " + document.getElementById(eleName).value);
}

function handleChecked(fieldName, onclickevent)
{
	var j = 0;
	var check_values = new Array();
	var the_form = document.getElementById('mainForm');
	var colonVal = "";
	for(var i=0; i<the_form.length; i++)
    {
      if (the_form.elements[i].name != fieldName + "CheckBox"){
      	continue;
      }
      var temp = the_form.elements[i].type;
      if((temp == "checkbox") && (the_form.elements[i].checked)) {
		check_values[j] = the_form.elements[i].value;
		j++;
      }
    }
	for(var k=0; k < check_values.length; k++)
    {
      k>0?colonVal+=":"+check_values[k] : colonVal+=check_values[k]; //get as colon delimited string
    }
	document.getElementById(fieldName).value = colonVal; //assign to hidden
//	alert(document.getElementById(fieldName).value);

//	alert("onclickevent = " + onclickevent);
	if (onclickevent != null && onclickevent == "Refresh"){
		refreshPage();
	}
}

function handleRemoveFile(fieldName, file)
{
//	alert("fieldName = " + fieldName);
//	alert("file = " + file);

	var the_form = document.getElementById('mainForm');
	var fileField = "";
	var fileFieldHidden = "";
	var fileRemoveField = "";
	var fileDeletedField = "";
	var param = file.split(":", 2);

	for(var i=0; i<the_form.length; i++)
    {
      if (the_form.elements[i].name != (fieldName + param[0] + "CheckBox")){
      	continue;
      }
      fileRemoveField = the_form.elements[i];
    }
	for(var i=0; i<the_form.length; i++)
    {
      if (the_form.elements[i].name != fieldName){
      	continue;
      }
      fileField = the_form.elements[i];
    }
	for(var i=0; i<the_form.length; i++)
    {
      if (the_form.elements[i].name != (fieldName + "Hidden")){
      	continue;
      }
      fileFieldHidden = the_form.elements[i];
    }
    var fileFieldValue = fileFieldHidden.value;
//  alert("fileFieldValue0 = " + fileFieldValue);

	for(var i=0; i<the_form.length; i++)
    {
      if (the_form.elements[i].name != (fieldName + "Deleted_Hidden")){
      	continue;
      }
      fileDeletedField = the_form.elements[i];
    }
    var fileDeletedFieldValue = fileDeletedField.value;
//  alert("fileDeletedFieldValue0 = " + fileDeletedFieldValue);

    if (fileRemoveField.checked)
    {
      //Delete file - Remove deleted file from the file list.
      fileFieldValue = fileFieldValue.replace(file, "");
      fileFieldValue = fileFieldValue.replace("||", "|");
      if (fileFieldValue.indexOf("|") == 0){
      	fileFieldValue = fileFieldValue.substring(fileFieldValue.indexOf("|") + 1);
      }
      if (fileFieldValue.lastIndexOf("|") == (fileFieldValue.length - 1)){
      	fileFieldValue = fileFieldValue.substring(0, fileFieldValue.length - 1);
      }

      //Add the file to deleted file list
      if (fileDeletedFieldValue.length > 0){
      	fileDeletedFieldValue = fileDeletedFieldValue + "|" + file;
      }else{
        fileDeletedFieldValue = file;
      }
    }
    else
    {
      //Undo file delete - Add file back to the file list.
      if (fileFieldValue.indexOf(file) < 0)
      {
      	fileFieldValue = fileFieldValue + "|" + file;
      }

      //Undo file delete - Remove file from the deleted file list
      fileDeletedFieldValue = fileDeletedFieldValue.replace(file, "");
      fileDeletedFieldValue = fileDeletedFieldValue.replace("||", "|");
      if (fileDeletedFieldValue.indexOf("|") == 0){
      	fileDeletedFieldValue = fileDeletedFieldValue.substring(fileDeletedFieldValue.indexOf("|") + 1);
      }
      if (fileDeletedFieldValue.lastIndexOf("|") == (fileDeletedFieldValue.length - 1)){
      	fileDeletedFieldValue = fileDeletedFieldValue.substring(0, fileDeletedFieldValue.length - 1);
      }
    }

    document.getElementById(fieldName + "Hidden").value = fileFieldValue; //assign to hidden
//  alert("fileFieldValueFinal = " + document.getElementById(fieldName + "Hidden").value);
	document.getElementById(fieldName + "Deleted_Hidden").value = fileDeletedFieldValue; //assign to deleted file list (hidden)
//  alert("fileDeletedFieldFinal = " + document.getElementById(fieldName + "Deleted_Hidden").value);
}

function isChanged()
{
	var the_form = document.getElementById('mainForm');

    for (var i = 0; i < the_form.elements.length; i++)
    {
        var element = the_form.elements[i];
        var type = element.type;
        if (type == "checkbox" || type == "radio")
        {
            if (element.checked != element.defaultChecked)
            {
                return true;
            }
        }
        else if (type == "hidden" || type == "password" || type == "text" ||
                 type == "textarea" || type == "file")
        {
            if (element.value != element.defaultValue)
            {
                return true;
            }
        }
        else if (type == "select-one" || type == "select-multiple")
        {
            for (var j = 0; j < element.options.length; j++)
            {
                if (element.options[j].selected !=
                    element.options[j].defaultSelected)
                {
                    return true;
                }
            }
        }
    }
    return false;
}

</script> 
