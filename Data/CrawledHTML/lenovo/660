    <script type="text/javascript">


function setErrorMessage( errorTxt, showMessageOnly )
{
  if( errorTxt == '' )
  {
    document.getElementById('SystemMessageTable').style.display='none';
    showNoMessage();
  }
  else
  {
    document.getElementById('SystemMessageTable').style.display='inline';
    var mpta = document.getElementById('mastheadPageTitleArea');
    if( mpta )
    {
      mpta.style.display='inline';
    }
    if( showMessageOnly )
    {
      showErrorMessage( errorTxt, null, true );
    }
    else
    {
      showErrorMessage( errorTxt, '/SEUILibrary/skins/i2-hightech/images' );
    }
  }
}

function setPositiveMessage( msgTxt )
{
  if( msgTxt == '' )
  {
    document.getElementById('SystemMessageTable').style.display='none';
    showNoMessage();
  }
  else
  {
    document.getElementById('SystemMessageTable').style.display='inline';
    showPositiveMessage( msgTxt, '/SEUILibrary/skins/i2-hightech/images' );
  }
}

function getElementPosition(elemID)
{
  var offsetTrail = document.getElementById(elemID);
  var offsetLeft = 0;
  var offsetTop = 0;
  while (offsetTrail){
  offsetLeft += offsetTrail.offsetLeft;
  offsetTop += offsetTrail.offsetTop;
  offsetTrail = offsetTrail.offsetParent;
  }
  if (navigator.userAgent.indexOf('Mac') != -1 && typeof document.body.leftMargin != 'undefined'){
  offsetLeft += document.body.leftMargin;
  offsetTop += document.body.topMargin;
  }
  return [offsetLeft,offsetTop];
}


function showPromosInPopup(targetWorkflow, category)
{
  var workc = document.getElementById('WorkingCategory');
  var promoc = document.getElementById('PromoCategory');

  if( !promoc || !promoc.value )
  {
    alert( "Sorry, no promotions in this level! ("+workc.value+")");
    return;
  }

  var wURL = '//shop.lenovo.com/SEUILibrary/controller/e/webca/LenovoPortal/en_CA/catalog.workflow:expandcategory?to-popup=yes&current-category-id='+promoc.value;
  var wFeatures = 'height=420, width=800, resizeable=yes, modal=yes, alwaysRaised=yes';
  showInPopup(wURL, wFeatures);
}


function startSolution(targetWorkflow, params)
{
  if(params != null && params != "")
  {
    targetWorkflow += params;
  }

  if(targetWorkflow.indexOf("?") > 0)
  {
    targetWorkflow += '&menu-id='+getMenuId();
  }
  else
  {
    targetWorkflow += '?menu-id='+getMenuId();
  }
  if(targetWorkflow.indexOf("catalog-id") < 0 && targetWorkflow.indexOf("catalog_id") < 0)
  {
    targetWorkflow += '&current-catalog-id=12F0696583E04D86B9B79B0FEC01C087';
  }
  if(targetWorkflow.indexOf("category-id") < 0 && targetWorkflow.indexOf("category_id") < 0)
  {
    targetWorkflow += '&current-category-id=7DCDBCE6F7B544808DEE1B6053CE2BCA';
  }

  loadContent(targetWorkflow);
}


function doLogout()
{
  document.location.href = "https://shop.lenovo.com/SEUILibrary/controller/e/webca/LenovoPortal/en_CA/logoff.workflow:LogoffPortal"
}

function openBPEWorkflow(solution, service, workflow, params)
{
  
  bpeURL = "https://shop.lenovo.com/SEUILibrary/controller/e/webca/LenovoPortal/en_CA/seutil.workflow:OpenBPEWorkflow"+"?Solution="+solution+"&Service="+service+"&Workflow="+workflow+"&Params="+escape(params);
  loadContent(bpeURL);
}

function top_onload()
{
  initializeMenu( "");
  setNumCrumbs();
}

function showSubSiteSelectorPopup()
{
  var addedURL = "https://shop.lenovo.com/SEUILibrary/controller/e/webca/LenovoPortal/en_CA/account.workflow:ShowSubSiteSelector?to-headerpopup=yes";
  window.open(addedURL,"","width=600,height=400,scrollbars=1,resizable=1");
}

function changeCurrentSite( sitePartyID )
{
  if( sitePartyID != '' )
  {
    document.forms.ChangeSiteForm.sitePartyID.value = sitePartyID;
    document.forms.ChangeSiteForm.submit();
  }
}

