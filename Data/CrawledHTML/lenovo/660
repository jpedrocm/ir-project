  <script type="text/javascript">
var gaugeRunning = false;
var styleDimensionSuffix = "px";

// Start gauge with this function. Parameters are optional.
function startGauge(line1text, line2text, textFont)
{
  doBrowserCustomization();
  if(line1text!=null)
  {
    line1 = document.getElementById("gaugeLine1");
    line1.innerHTML = line1text;
  }
  if(textFont!=null && textFont!='')
  {
    line1.style.font = textFont;
  }
  if(line2text!=null)
  {
    line2 = document.getElementById("gaugeLine2");
    line2.innerHTML = line2text;
  }
  if(textFont!=null && textFont!='')
  {
    line2.style.font = textFont;
  }
  gaugeRunning = true;
  //showDimmer();
  showGauge();
  updateGauge();
}
function stopGauge()
{
  gaugeRunning = false;
  closeFloatingPopup(floatingCancelResponse);
  //document.getElementById("gaugelayer").style.display = "none";
  //document.getElementById("dimmerlayer").style.display = "none";
}

function updateGauge()
{
  //updateGaugePosition();
  animateGauge();
  if(gaugeRunning)
  {
    setTimeout('updateGauge()', 20);
  }
}

function showGauge()
{
  floatingPopupResetButtons();
  openFloatingPopup($('#gaugelayer').html(), 'Gauge');
  //document.getElementById("gaugelayer").style.display="inline";
}
var currentGauge = 1;
var gaugeCounter = 0;

function animateGauge()
{
  gaugeCounter++;
  if(gaugeCounter==30)
  {
    currLightGauge = document.getElementById("gauge"+currentGauge+"_light");
    currDarkGauge = document.getElementById("gauge"+currentGauge+"_dark");
    currentGauge++;
    if(currentGauge==7)
    {
      currentGauge = 1;
    }
    nextLightGauge = document.getElementById("gauge"+currentGauge+"_light");
    nextDarkGauge = document.getElementById("gauge"+currentGauge+"_dark");

    currLightGauge.style.display = "none";
    currDarkGauge.style.display = "inline";
    nextLightGauge.style.display = "inline";
    nextDarkGauge.style.display = "none";
    gaugeCounter = 0;
  }
}

function doBrowserCustomization() 
{
  if(isChrome())
  {
  }
  else if(isSafari())
  {
  }
  else if(isOpera())
  {
    // TODO! animation not working
  }
}


var userAgent = navigator.userAgent;
var appVersion = navigator.appVersion;

function isChrome()
{
  return userAgent.indexOf("Chrome")!=-1;
}
function isSafari()
{
  return userAgent.indexOf("Safari")!=-1 && !isChrome();
}
function isOpera()
{
  return userAgent.indexOf("Opera")!=-1;
}
</script> 
