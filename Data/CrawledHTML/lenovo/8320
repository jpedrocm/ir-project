   <script>
var product_notify_me = [
{"url" : "/desktops/ideacentre/y700-series/y710-cube", "productID" : "ideacentre-y710-cube"},
{"url" : "/desktops/ideacentre/aio-y900/aio-y910-27", "productID" : "ideacentre-y910-aio"},
{"url" : "/smartphones/phab-series/phab2/", "productID" : "phab-2"},
{"url" : "/tablets/lenovo/yoga-book/yoga-book-android", "productID" : "yoga-book-a"},
{"url" : "/tablets/lenovo/yoga-book/yoga-book-windows", "productID" : "yoga-book-w"},
{"url" : "/laptops/yoga/900-series/yoga-910", "productID" : "yoga-910"},
{"url" : "/tablets/lenovo/miix-series/miix_510", "productID" : "miix-510"},
{"url" : "/tablets/lenovo/yoga-tablet-series/yoga-tab-3-plus", "productID" : "yoga-tab-3-plus"},
{"url" : "/laptops/ideapad/y900-series/y910-17", "productID" : "ideapad-y910"}
];

$(document).ready(function() {
	$.each(product_notify_me, function(i, item) {
	  if(location.href.indexOf(item.url) > 0 && $("#longscroll-subseries").length > 0 && !$('#tab-li-customize').length ){
		var country = (typeof $('meta[name="Lenovo.Country"]').attr('content') != 'undefined') ? $('meta[name="Lenovo.Country"]').attr('content').toLowerCase() : '';
			var prodDesc = (typeof $('meta[name="ModelNameVariants"]').attr('content') != 'undefined') ? $('meta[name="ModelNameVariants"]').attr('content').toLowerCase() : '';
			var prodLOB = (typeof $('meta[name="ProductType"]').attr('content') != 'undefined') ? $('meta[name="ProductType"]').attr('content').toLowerCase() : '';
			prodLOB = prodLOB.replace("&", "and");
			var prodBrand = (typeof $('meta[name="ProductName"]').attr('content') != 'undefined') ? $('meta[name="ProductName"]').attr('content').toLowerCase() : '';
			var prodFam = (typeof $('meta[name="ModelName"]').attr('content') != 'undefined') ? $('meta[name="ModelName"]').attr('content').toLowerCase() : '';
			var prodSubFam = (typeof $('meta[name="ModelName"]').attr('content') != 'undefined') ? $('meta[name="ModelName"]').attr('content').toLowerCase() : '';
			var prodCD = item.productID;
			
			var brandRE = /.*(?=\s)/;
			if( prodDesc == ""){
			  prodDesc = item.productID
			}
			var qs = "?COUNTRY_ISO2=" + country + "&PRODUCT_DESC=" + prodDesc + "&PRODUCT_LOB=" + prodLOB + "&PRODUCT_BRAND=" + prodBrand.match(brandRE) + "&PRODUCT_FAMILY=" + prodFam + "&PRODUCT_SUB_FAMILY=" + prodSubFam + "&PRODUCT_CD=" + prodCD;

			//add the notify me button to the page
			$(".subseriesHeader .cta-group-action").html('<a href="https://gp.wd.lenovo.com/en/notifyme/v1592' + qs + '" class="button-called-out-alt button-full" id="notify-me" style="background-color:#e61b02; font-size:.8em;">Notify Me When Available</a>');
			$("#notify-me").fancybox({
				'type':'iframe'
			});
	  }
    });
	
});
</script> 
