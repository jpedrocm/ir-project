function getDiscount(msrpPrice, msrpDiscountPrice, msrpSalePrice)
{
    var listvalue = msrpPrice;

    var salevalue = msrpSalePrice;
   if (msrpDiscountPrice == "null" || msrpDiscountPrice == "" || msrpDiscountPrice == 0)
   {
     var percentage = ((listvalue- salevalue) / listvalue) * 100;
     //console.log("percentage:" +percentage);
     var per_round = Math.floor(percentage);
     return per_round;
   }
   else
   {
     var discountvalue = msrpDiscountPrice;
     var percentage = (discountvalue / listvalue) * 100;
     //console.log("percentage1:" +percentage);
     var per_round = Math.floor(percentage);
      return per_round;
   }
}

function getDiscountAmount(msrpPrice, msrpDiscountPrice, msrpSalePrice)
{
    var listvalue = msrpPrice;

    var salevalue = msrpSalePrice;
   if (msrpDiscountPrice == "null" || msrpDiscountPrice == "" || msrpDiscountPrice == 0)
   {
     var amount = listvalue- salevalue;
     var amt_round = Math.floor(amount);
     return amt_round;
   }
   else
   {
     return msrpDiscountPrice;
   }
}

