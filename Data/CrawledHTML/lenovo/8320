  (function(app) {
     var self = app.CONFIGURATOR = (app.CONFIGURATOR || {});
     self.beforeRenderUI = function(map, ojson) {
       renderOverrideModules(renderBeforeRenderUIModules, map, ojson);
       return map;
     };
		 
     if(window.defaultEC != null)
     {
       var origATC = self.addToCart;
       self.addToCart = function(event) {
         if(window.defaultEC != null)
         {
					 console.info("APPLY DEFAULT ECOUPON CODE");
           $.post(long_url + "/coupons.workflow:ApplyOffer", {"ec":decodeURIComponent(window.defaultEC),"target-url":"/us/en/landingpage/yoga-3-pro/done.html"}, function( ) {
             origATC(event);
           });
         }
         else
         {
           origATC(event);
         }
       }
     }

     var origIC = self.openConfigurator;
     self.openConfigurator = function(params) {
		 if(params.ps)
			{
				var psp = params.ps.split(",");
				for(var p=0;p<psp.length;p++)
				{
					psp[p] = encodeURIComponent(decodeURIComponent(psp[p]).replace(/\+/g, "+"));
				}
				params.ps = psp.join(",");
			}
	  if(window.partDefault != null)
       {
         if(params.ps != null)
         {
           params.ps += "," + window.partDefaultPS;
         }
         else
         {
           params.ps = window.partDefaultPS;
         }
       }
       window.NABUILDER.params = params;
       origIC(params);
     }

     self.initCustomize = function(event) {

       var $trigger, params, sb;	  	   
       $trigger = $(this);
	   
       if ($trigger.data("sb") != null) {
         sb = $trigger.data("sb");
       } else {
         sb = $trigger.attr("href").split("?").pop().replace("sb=", "");
       }
       params = {};
       params.sb = sb;
       
		var qsparts = $trigger.attr("href").split("?").pop().split("&");
       var haspssel = "";
       for(var qp=0;qsparts != null && qp<qsparts.length;qp++)
       {
       	  if(qsparts[qp].indexOf("ps=") == 0)
       	  {
       	    if(haspssel.length > 0)
       	    {
       	    	haspssel += "," + qsparts[qp].split("=")[1];
       	   	}
       	   	else
       	   	{
       	    	haspssel = qsparts[qp].split("=")[1];
       	   	}
       	  }
       }

       if(haspssel != null && haspssel != "")
       {
         params.ps = haspssel;
       }
       if(window.warrantyDefault != null)
       {
         if(params.ps != null)
         {
           params.ps += "," + window.warrantyDefaultPS;
         }
         else
         {
           params.ps = window.warrantyDefaultPS;
         }
       }

       

       if ($trigger.hasClass(self.settings.expressShipButtonClass)) {
         params.task = "express";
       }
       self.openConfigurator(params);
       if (event != null) {
         event.preventDefault();
       }
     };

  })(window.LENOVO = window.LENOVO || {});

  

function startGauge(msg)
{
}

$(document).ready(function(){
	if(typeof rolloverCart != "undefined"){
		var listeners = [
			{
				matchURL : "cart.workflow:AddToCart",
				oidParam : "item-id"
			},
			{
				matchURL : "builder.workflow:Exit",
				oidParam : "sb"
			}
		];
		rolloverCart.hookupAjaxListener(listeners);
	}
});

if(window.location.search.indexOf("cc=") >= 0 && typeof rolloverCart != "undefined"){
	var re = /sb=(:.{8}:.{8}:)/
	rolloverCart.disableActions(window.location.search.match(re)[1]);
}

