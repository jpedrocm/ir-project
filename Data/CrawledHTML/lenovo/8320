if (prodSeries.indexOf("href") != -1) {Series = $(prodSeries).find("span").text();} else {Series = prodSeries;}

if((typeof(model) != "undefined") && (typeof(prodtitle) != "undefined")){
if (model.indexOf("Lenovo") != -1) {var Modelno = model.replace("Lenovo","Essential");Modelno = Modelno.replace(str,"");modelName = $.trim(Modelno);
