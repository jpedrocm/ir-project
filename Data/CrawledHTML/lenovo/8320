        }
    });
	
	//Lenovo Pref Card Builder Link
	
	
        if($('meta[name="TaxonomyType"]').attr('content') == "SubSeriesPage") {
			$(document).ajaxSuccess(function (e, xhr, s) {
				if ((s.url.indexOf("workflow:LoadBuilderTemplate") > -1) || (s.url.indexOf("builder.workflow:Navigate") > -1) || (s.url.indexOf("builder.workflow:Update") > -1) ) {
					$('.pricingSummary-cta').after('<div class="lenovo-pref" style="width: 205px;margin: 0px;margin-top: 20px;"><a href="//shop.lenovo.com/us/en/lenovo-pref-card" class="link" target="_blank" style="cursor:pointer;"><img src="//www.lenovo.com/images/OneWebImages/Common/Icons/lenovo-preferred-payment-icon.png" style="float: left;margin-right: 5px;position: relative;top: 7px;height: 30px;"></a><div class="text" style="float: left;font-size: 12px;padding-left: 0px;padding-top: 6px;width: 145px;text-align: left;">Monthly Payment Options<a href="//shop.lenovo.com/us/en/lenovo-pref-card" class="link" target="_blank" style="display: block;bottom: 6px;color: #69BF48;padding-bottom: 2px;padding-left: 3px;font-size: 14px;font-weight:bold;">Apply Now <span style="font-weight: bold;cursor: pointer;">&gt;</span></a></div><div style="clear:both"></div></div>');
				}
			});
        }
   
     $(document).ready(function(){
  	 var footerLinks = $(".footer-bottomBar-nav nav a");
		 footerLinks.each(function(index, item){
		 	if ($(footerLinks[index]).text() === "Sales Terms and Conditions") {
		 	 	$(this).attr('rel', 'modal');
		 	}
		 });
	 });	 
