   <script type="text/javascript">
var bundle_oid = $('meta[name="metaidentifier"]').attr('content');
	
var url = "/us/en/desktops/thinkcentre/e-series/e93z/"; 
var segment_array = url.split( '/' );
var last_segment = segment_array[segment_array.length - 2]; 
url = url.substring(0,url.lastIndexOf(last_segment));
if(url.indexOf("w-series") > 0){
   url = "/us/en/laptops/thinkpad/p-series/";
} else if(url.indexOf("y-series") > 0 ){
   url = "/us/en/laptops/ideapad/y700-series/";
} else if(url.indexOf("z-series") > 0 ){
	url = "/us/en/laptops/ideapad/500-series/";
} else if(url.indexOf("g-series") > 0 ){
    url = "/us/en/laptops/ideapad/300-series/";
} else if(url.indexOf("s-series") > 0 ){
    url="/us/en/laptops/ideapad/100-series/";
} else if(url.indexOf("u-series") > 0 ){
	url="/us/en/laptops/ideapad/500-series/";
}else if(url.indexOf("b-series") > 0 ){
	url="/us/en/laptops/";
}

