var footerjson = {
		"active" : true,
		"icons" : [
{
				"label" : "Laptops",
				"icon" : "//www.lenovo.com/shop/americas/content/img_lib/mobile/dock-icon-laptops.png",
				"url"  : "http://shop.lenovo.com/us/en/laptops",
				"newTab": false,
				"phoneNumber" : ""
			},{
				"label" : "Tablets",
				"icon" : "//www.lenovo.com/shop/americas/content/img_lib/mobile/dock-icon-tablets.png",
