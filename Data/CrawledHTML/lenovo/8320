         <script type="text/javascript">
	function ajaxsearchsubmit(form) {
	var search = encodeURIComponent(form.w.value);
	document.activeElement.blur();
	window.location = "http://search.lenovo.com/search#w=" + search + "&fsearch=1";
	return false
	}

	//Desktop Version	
	$(document).ready(function() {
		$('.search_menu').prepend('<form role="search" class="searchbox" action="https://search.lenovo.com/search" method="get" name="site_search" onsubmit="return ajaxsearchsubmit(this)"><input class="input-text" id="sli_search_1" autocomplete="off" type="search" name="w" placeholder="Search" data-provide="rac"><input type="hidden" name="cc" value="www"><input type="hidden" name="fsearch" value="yes"><input value="en" type="hidden" name="lang"></form>'); 
	
		//Mobile version
		$('.search_menu .link-wrapper').html('<form role="search" class="searchInput" action="https://search.lenovo.com/search" method="get" name="site_search" onsubmit="return ajaxsearchsubmit(this)"><input class="searchInput-text" id="sli_search_1" autocomplete="off" type="search" name="w" placeholder="Enter a search term" data-provide="rac"><input type="hidden" name="cc" value="www"><input type="hidden" name="fsearch" value="yes"><input value="en" type="hidden" name="lang"><input class="button-standard searchInput-button" value="Search" type="submit"></form>');
	});
	</script> 
