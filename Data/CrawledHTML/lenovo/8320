} else if (prodtitle.indexOf("IdeaTab") != -1) {modelName = "IdeaTab " + model;
} else if (Series.indexOf("M Series") != -1) {var model_name = prodname;model_name = model_name.replace("Desktop","");modelName = $.trim(model_name);
} else {model = model.replace(str,"");modelName = $.trim(model);}
}

//Systems recommendation mbox
if(typeof(mboxCreate)!="undefined" ){
	mboxDefine('sub-recs', 'subSeries-Recs');
	mboxUpdate('subSeries-Recs', 'entity.id='+ modelName + '_US');
 }
