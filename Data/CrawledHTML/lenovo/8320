/* Compare Tooltip*/
$(document).ready(function(){
	$("div.comparecheckbox > input").click(function(){
		//if this is the first item that compare has been clicked for, we want to show a message		
		var cmpViewed = sessionStorage.getItem("cmpViewed");
		
		if (typeof cmpViewed == "undefined" || cmpViewed == null){
			//this is first view, show the message
			$(this).parent().append("<div class='cmpMsgCont'><div class='cmpMsg'><p>Start Comparing! You can compare any items you want, even products on other pages! After selecting at least 2 products, click the Compare Products tab on the right.</p></div></div>");
			$("div.cmpMsgCont").animate({"opacity":"1"}, 500, function(){				
				$("body").one("click", function(){
					$("div.cmpMsgCont").animate({"opacity":"0"}, 500, function(){$("div.cmpMsgCont").remove();});					
				});
			});			
			sessionStorage.setItem("cmpViewed", true);		
		}		
	});
});
/* Compare Tooltip*/
