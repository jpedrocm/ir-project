    <!--Begin - Set the Users LoggedIn Cookie- Please leave it --> 
    <script type="text/javascript">
$(function() {
 var strURLLocation = new String (window.location);
   var strSearchString1 = new String ("login.workflow:DoDirectLogin");
   var strSearchString2 = new String ("checkout.workflow:SignIn");
    // check cookie
  var userSignedIn = getCookie("myAccountSignedIn");
   if (strURLLocation.lastIndexOf(strSearchString1) != -1 || strURLLocation.lastIndexOf(strSearchString2) != -1)
	{
	content_onload();
	var logged_para = $("#mastheadPageTitleMessaging a").text();
    var msg = $("#mastheadPageTitle").text();
	if (logged_para == "(Sign out)" || msg == "Checkout")
		{
		  	// set cookie
			setCookie("myAccountSignedIn","TRUE",null);
        }
     }
    var strSearchString3 = new String ("/logoff.workflow:LogoffPortal");
	  if (strURLLocation.lastIndexOf(strSearchString3) != -1)
	   {
	     setCookie("myAccountSignedIn","FALSE",null);
       }
});

function setCookie(c_name,value,exdays)
  {
	  var exdate=new Date();
	  exdate.setDate(exdate.getDate() + exdays);
	  var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
	  document.cookie=c_name + "=" + c_value;
  }

 function getCookie(c_name)
 {
	  	var i,x,y,strcookies=document.cookie.split(";");
	  	for (i=0; i<strcookies.length; i++)
	  	{
	        x=strcookies[i].substr(0,strcookies[i].indexOf("="));
	  	    y=strcookies[i].substr(strcookies[i].indexOf("=")+1);
	   	    x=x.replace(/^\s+|\s+$/g,"");
	  	    if (x==c_name)
	  	    {
	  		   return unescape(y);
	   	    }
	  	 }
  }

</script> 
    <!-- End Users LoggedIn Cookie --> 
