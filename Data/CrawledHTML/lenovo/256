{
  
  var floatingPopupCollapseIcon="//shop.lenovo.com/SEUILibrary/hightech-portal/images/icons/b_collapse.gif";
  return floatingPopupCollapseIcon;
}

function getfloatingPopupExpandIcon()
{
  var floatingPopupExpandIcon="//shop.lenovo.com/SEUILibrary/hightech-portal/images/icons/b_expand.gif";
  return floatingPopupExpandIcon;
}

