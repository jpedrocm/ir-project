  <script type="text/javascript">
var bvItemOid = null;
var bvIdentifier = null;
function bvOverlay(itemoid, bvproductid)
{
  bvItemOid = itemoid;
  bvIdentifier = bvproductid;
  $("#bvOverlay").overlay({
    expose:
    {
      color: '#000',
      opacity: 0.75,
      loadSpeed: 'fast',
      top: "center",
      left: "center"
    },
    closeOnClick: false,

    // load ratings as json and call json->xml convert
    onBeforeLoad: function()
    {
      $("#bvreviewsOvr").html("");
      $.post("//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/wci.workflow:load?page=/WW/site/templates/bazaarvoice/BVTemplateOverLay.html", { 'oid' : bvItemOid, 'bvoid' : bvIdentifier }, function(bvcontent)
      {
		var html = $.parseHTML(bvcontent),
			txt = '<b>' + $(html).find('#title').text() + '</b>';
        $("#bvreviewsOvr").html(bvcontent).find('#title').html(txt);
      });

      // set width on load
      var $this = this.getOverlay();
      if ($(window).width() > 1000) $this.width(900);
      // set height of overlay to viewport height minus top offset (x2) minus padding (top)
      $this.height($(window).height()-(50*2)-parseInt($this.css('padding-top'))-parseInt($this.css('padding-bottom')));
      $this.find('.inner').height($this.height());

      var w = $(window);
      $this.css({
        'left':Math.abs(((w.width() - $this.width()) / 2) + w.scrollLeft())
     });
    },
    top: 50 // if this value changes, change in onBeforeLoad() as well
  });
  $("#bvOverlay").data('overlay').load();
};
</script> 
