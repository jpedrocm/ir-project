    <script type="text/javascript">
	/** Seting Cart count in the cookie for dotcom pages **/
	var cart_domain = location.hostname.split('.').slice(-2).join('.'); // to get leni2.com or lenovo.com
	
	document.cookie="_CartCount="+0+"; domain=." + cart_domain + "; path=/";
	/** END **/
	function appendRowToCartRollover(contentItemsID, item, link, img, description, qty) {


	$('#' + contentItemsID).append(
			$('<li>').append(
				$('<a>').attr('href', link).append(
					$('<img>').attr('class', 'rollovercartItemImg').attr('src', img.replace(/\+/g,' '))
				).append(
					$('<span>').attr('class', 'rollovercartItemName').text(description.replace(/(&nbsp;+|<([^>]+)>|\+)/g,' '))
				).append(
					$('<span>').attr('class', 'rollovercartItemQty').attr('value', qty).text('Quantidade: ' + qty + '')
				)
			)
		);
	}

    function getCookie(b){var c,a,e,d=document.cookie.split(";");for(c=0;c<d.length;c++){a=d[c].substr(0,d[c].indexOf("="));e=d[c].substr(d[c].indexOf("=")+1);a=a.replace(/^\s+|\s+$/g,"");if(a==b){return decodeURIComponent(e)}}}

	function updateCartRollover(cartLinkId, rolloverId, contentItemsID ) {
		var site = 'brweb';
		var cookieCart = (typeof getCookie == "function") ? getCookie('ISSUser' + site).split('|')[4] : '';

		if (cookieCart && cookieCart.length !== 0) {
			var jCart = $.parseJSON(cookieCart);
			var itemCount = 0;
			if (jCart && typeof jCart.items === 'object') {
        var link = 'JavaScript:startSolution(\'//shop.lenovo.com/SEUILibrary/controller/e/brweb/LenovoPortal/pt_BR/cart.workflow:ShowCart?shopping-menu-selected-line=cart\');';
				if ($.isArray(jCart.items)) {
				for (var i=0;i<jCart.items.length;i++) {
					var item = jCart.items[i];
				  if (typeof(item) != "function"){
					  appendRowToCartRollover(contentItemsID, item, link, item.img,item.description,item.qty);
					  itemCount += item.qty;
					}
				}
				} else {
					var item = jCart.items;
					itemCount += item.qty;
					appendRowToCartRollover(contentItemsID, item, link, item.img.replace(/http[s]?:/g,''),item.description,item.qty);
				}

				var cartLink = $(".cart_menu");
				  $.each(cartLink,function(){
					if($(this).find(".cartlink" ).length > 0){
					  $(".cart_wrapper").appendTo($(this));
					  $(this).find(".cartlink" ).addClass("has-submenu");
					  $(this).children().click(function(){$("#rollovercart").css("display","block");
					  });
				   }
				  });

				  var counter = $(".cartlink");
				  $.each(counter,function(){
							$(this).append("<span class='counter'></span>");
							$(this).find('.counter').empty().html(''+itemCount+'').show();
					});
			}
		}
	}

	try {
		updateCartRollover('util-currentcart', 'rollovercart', 'rollovercartContentsItems');
	} catch (error) {
		if (window.debugAllowed && !!window.console) {
			console.warn('Error in updateCartRollover');
			console.error(error.message);
			console.log(error);
		}
	}
</script> 
