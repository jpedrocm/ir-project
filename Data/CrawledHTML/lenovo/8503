/* Markus: custom lazy loader to make sure if user has already scrolled and reloads page the items currently in viewport get shown */
/* also supports crossfade */
var afterLoadTop = new Array();
var markedImages = [];
function crossFadeIn()
{
	var $this = $(this);
	if($this.attr("src"))
	{
		var domImage = $("#" + $this.attr("id"));
		domImage.css("opacity", 0.25);
		domImage.attr("src", $this.attr("src"));
		domImage.attr("data-original", "");
		domImage.fadeTo(300, 1);
	}
}

function markImages() {
	var scrolledPlusViewable = $(window).scrollTop()+$(window).height();
	var lazyImages = $(".lazy");
  	lazyImages.each(function() {
  		var mytop = $(this).offset().top;
  		if(scrolledPlusViewable >= mytop)
  		{
  			markedImages[markedImages.length] = $(this);
  		}
  	});		
}
function aolHandler() {  
  
	//sweep images and check viewport. Don't assign src as that might push items out of viewport that should be visible.  	
  	for(var mi=markedImages.length-1;mi>=0;mi--)
  	{	
  		var $this = markedImages[mi];
  		if($this.attr("data-original"))
		{
			var imgID = "img_" + Math.floor(Math.random()*10000000);
			var cfimg = new Image();
			cfimg.setAttribute("id", imgID);
			$this.attr("id", imgID);
			cfimg.onload = crossFadeIn;
			cfimg.src = $this.attr("data-original");
			$this.attr("data-fade", "");
		}
  		$this.removeClass("lazy");  
		markedImages.splice(mi, 1);
  	}
}
$(document).ready(function() {
	markImages();
	aolHandler();
	
	$(".nav").find("li").on("click", function() { 
		$(this).find(".mhaol").each(function() {
			if($(this).is(":visible") && $(this).attr("data-src"))
			{
				$(this).attr("src", $(this).attr("data-src"));
				$(this).removeAttr("data-src");
			}
			else if($(this).is(":visible") && $(this).attr("data-bgsrc"))
			{
				$(this).css("background-image", "url('" + $(this).attr("data-bgsrc") + "')");
				$(this).removeAttr("data-bgsrc");
			}
			else if($(this).is(":visible") && $(this).attr("data-style"))
			{
				$(this).attr("style", $(this).attr("data-style"));
				$(this).removeAttr("data-style");
			}
		});
	});	
	
});
$(window).on( "scroll", function () { markImages(); aolHandler(); } )
$(window).on( "resize", function () { markImages(); aolHandler(); } )

//need to remove reference to normal masthead css if new json navi is present
if (window.mhjson)
{
	//replace mh async with newnavi
	for(var r=0;r<resourceQueue.length;r++) {
		if(resourceQueue[r] == "/ISS_Static/WW/css/themes/owv4/homepage-combined-async.css")
		{
			resourceQueue[r] = "/ISS_Static/WW/css/themes/owv4/homepage-combined-async-newnavi.css";
			break;
		}
	}

	//TEMPFIX: need to fix support menu links
	var supportmenus = window.mhjson["support-technical"];
	if(supportmenus != null && supportmenus.menus)
	{
		var smenus = supportmenus.menus;
		for(var sm=0;sm<smenus.length;sm++)
		{
			smenus[sm].h = smenus[sm].h.replace(/(shop\.lenovo\.com|condev\.leni2\.com)/g, "support.lenovo.com");
		}
	}
	
	for (var m in mhjson) {
		if (mhjson.hasOwnProperty(m)) {
			try
			{
				var onem = mhjson[m];
				var jss = JSON.stringify(onem);
				jss = jss.replace(/\/\/shop.lenovo.com\/\/shop.lenovo.com/g, "//shop.lenovo.com");
				mhjson[m] = JSON.parse(jss);
			}
			catch(mherror)
			{
				console.warn(mherror);
			}
		}
	}
		
}

function resourceLoader(res) {
		if(res.indexOf(".css") == -1)
		{

			$('<script type="text\/javascript" src="' + res + '"><\/script>').appendTo("head");									
		}
		else
		{
			$('<link href="' + res + '" rel="stylesheet"\/>').appendTo("head");
		}
}

				</script>   
 </body>
</html>
