    <script type="text/javascript">
var visibleChars = 21; //how many chars are shown, then ... are appended
var bridgeCommand = '//shop.lenovo.com/SEUILibrary/controller/e/brweb/LenovoPortal/pt_BR/site.workflow:PreSearch';//JSP file to use in this command is mentioned in common.properties file in "site-search-jsp-path" property
var threshold = 2; //defines the threshold (number of chars in search field) when type ahead search is executed.
var suggestionHeader = "Suggestions"; // the text shown as divider between results and suggestions
var displayTime = 0; //div auto hide time (milliseconds. for example 5000 makes results div hide in 5 seconds). 0 = no auto hide.

function presearch(e)
{
  var q;
  if (!e) var e = window.event;
  if (e.target) q = e.target;
  else if (e.srcElement) q = e.srcElement;
  if (q.nodeType == 3) // defeat Safari bug
  q = q.parentNode;

//check which key was pressed
 var evt = e || window.event;
 var typeAheadList = document.getElementById("presearch");

 if(evt.keyCode==38)
 {
   if(currentTAElement == null)
   {
     highLightTA(typeAheadList.lastChild);
     q.value = typeAheadList.lastChild.lastChild.value;
   }
   else if(currentTAElement == typeAheadList.firstChild)
   {
     lowLightTA(currentTAElement);
     highLightTA(typeAheadList.lastChild);
     q.value = typeAheadList.lastChild.lastChild.value;
   }
   else
   {
     var elem = currentTAElement;
     do
     {
       elem = elem.previousSibling;
     } while(elem != null && elem.tagName != "A");

     if(elem != null)
     {
       lowLightTA(currentTAElement);
       highLightTA(elem);
       q.value = elem.lastChild.value;
     }
   }
   evt.returnValue = false;
   evt.cancelBubble = true;
   return false;
 }
 else if(evt.keyCode==40)
 {
   if(currentTAElement == null)
   {
     highLightTA(typeAheadList.firstChild);
     q.value = typeAheadList.firstChild.lastChild.value;
   }
   else if(currentTAElement == typeAheadList.lastChild)
   {
     lowLightTA(currentTAElement);
     highLightTA(typeAheadList.firstChild);
     q.value = typeAheadList.firstChild.lastChild.value;
   }
   else
   {
     var elem = currentTAElement;
     do
     {
       elem = elem.nextSibling;
     } while(elem != null && elem.tagName != "A");

     if(elem != null)
     {
       lowLightTA(currentTAElement);
       highLightTA(elem);
       q.value = elem.lastChild.value;
     }
   }
   evt.returnValue = false;
   evt.cancelBubble = true;
   return false;
 }

  if(q.value.length < threshold)
  {
    var divi = document.getElementById("presearch");
    divi.innerHTML = "";
    divi.style.display = "none";
    return;
  }
  $.get(bridgeCommand, { taf: this.value, ts: new Date().getTime() }, function(data) {
    var trimData = jQuery.trim(data);
    if(trimData.length > 0)
    {
     eval(trimData);
    }
   });
}
function fastPreSearch(content)
{
  var divi = document.getElementById("presearch");
  if(content.length > 2)
  {
    divi.innerHTML = "";
    try
    {
      var tafArray = content;
      var typeAheads = new Array();
      var suggestions = new Array();
      for(var tc=2;tc<tafArray.length;tc++)
      {
        if(tc%2 == 0)
        {
          var cut = (tafArray[tc].length > visibleChars)?tafArray[tc].substring(0,visibleChars)+"...":tafArray[tc];
          typeAheads[typeAheads.length] = "<a href='#' class='typeahead-lowlight' onmouseover='highLightTA(this, true);' onmouseout='lowLightTA(this)' onclick='return submitSearch(this)' title='";
          typeAheads[typeAheads.length] = tafArray[tc];
          typeAheads[typeAheads.length] = "'>";
          typeAheads[typeAheads.length] = cut;
          typeAheads[typeAheads.length] = "<input type='hidden' name='tafval' value='";
          typeAheads[typeAheads.length] = tafArray[tc];
          typeAheads[typeAheads.length] = "'></a>";
        }
        else
        {
          var strt = tafArray[tc].indexOf("[[[");
          var nd = tafArray[tc].indexOf("]]]", strt);
          if(strt > 0 && nd > 0)
          {
            var sugg = tafArray[tc].substring(strt+2, nd+1);
            sugg = sugg.replace(/\[/g,"");
            sugg = sugg.replace(/\]/g,"");
            var ar = sugg.split(",");
            for(var ac=0;ac<ar.length;ac+=3)
            {
              var cut = (ar[ac].length > visibleChars)?ar[ac].substring(0,visibleChars)+"...":ar[ac];
              suggestions[suggestions.length] = "<a href='#' class='typeahead-lowlight' onmouseover='highLightTA(this, true)' onmouseout='lowLightTA(this)' onclick='return submitSearch(this)' title='";
              suggestions[suggestions.length] = ar[ac];
              suggestions[suggestions.length] = "'>";
              suggestions[suggestions.length] = cut;
              suggestions[suggestions.length] = "<input type='hidden' name='tafval' value='";
              suggestions[suggestions.length] = ar[ac];
              suggestions[suggestions.length] = "'></a>";
            }
          }
        }
      }

      if(typeAheads.length > 0 || suggestions.length > 0)
      {
        var ih = typeAheads.join("");
        if(suggestions.length > 0)
        {
          ih += "<div class='search-typeahead-suggestion-txt'>" + suggestionHeader + "</div>" + suggestions.join("");
        }
        divi.innerHTML = ih;
        divi.style.display="block";
        var tafTimeOut = displayTime;
        if(tafTimeOut > 0)
        {
          tafTimer = setTimeout("hideTaf()", tafTimeOut);
        }
      }
      else
      {
        divi.innerHTML = "";
        divi.style.display="none";
      }
    }
    catch(e)
    {
      //alert("An exception occurred in the script. Error name: " + e.name + ". Error description: " + e.description + ". Error number: " + e.number + ". Error message: " + e.message);
    }
  }
  else
  {
    typeAheadList.innerHTML = "";
    typeAheadList.style.display="none";
  }
}

//create divs needed for the field
//and assign onkeyup to field
if(document.getElementsByName("q").length != 0)
{
  var sfld = document.getElementsByName("q")[0];
  var nd = document.createElement("DIV");
  nd.className = "preSearchWrapper";
  var ind = document.createElement("DIV");
  ind.className = "preSearchContent";
  ind.id = "presearch";
  nd.appendChild(ind);
  sfld.parentNode.insertBefore( nd, sfld.nextSibling );
  sfld.onkeyup = presearch;
}

var currentTAElement = null;

function highLightTA(taE, getQValue)
{
if(taE == null)
{
  return;
}
taE.className = "typeahead-highlight";
currentTAElement = taE;
if(getQValue == true)
{
  document.getElementsByName("q")[0].value = currentTAElement.lastChild.value;
}
}

function lowLightTA(taE)
{
if(currentTAElement != null)
{
  currentTAElement.className = "typeahead-lowlight";
}
if(taE == null)
{
  currentTAElement = null;
  return;
}
taE.className = "typeahead-lowlight";
currentTAElement = null;
}

function submitSearch(tafval)
{
for(var ilc=tafval.childNodes.length-1;ilc>0;ilc--)
{
  if(tafval.childNodes[ilc].name == "tafval")
  {
	document.getElementsByName("q")[0].value = tafval.childNodes[ilc].value;
	document.forms.site_search.submit();
	searchPosting = true;
	var typeAheadList = document.getElementById("presearch");
	typeAheadList.innerHTML = "";
	typeAheadList.parentNode.style.display="none";
	break;
  }
}
return false;
}
</script> 
