				"newTab": false,
				"phoneNumber" : ""
			},{
				"label" : "Tablets",
				"icon" : "//www.lenovo.com/shop/americas/content/img_lib/mobile/dock-icon-tablets.png",
				"url"  : "http://shop.lenovo.com/us/en/tablets",
				"newTab": false,
				"phoneNumber" : ""
			},{
