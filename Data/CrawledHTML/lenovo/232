    <div style="display:none !important;"> 
     <script type="text/javascript">
  var tafTimer = null;
  var searchFieldChanged = false;
  var searchPosting = false;
  function postSearch(frm)
  {
    if(searchFieldChanged == false)
    {
      return false;
    }
    var s = frm.q.value;
    for(var i=0; i<s.length; i++)
    {
      var c = s.charAt(i);
      if((c != ' ') && (c != '\n') && (c != '\t') )
      {
        searchPosting = true;
        return true;
      }
    }
    return false;
  }

  function typeAhead(e, searchField)
  {
  
        var q;
        if (!e) var e = window.event;
        if (e.target) q = e.target;
        else if (e.srcElement) q = e.srcElement;
        if (q.nodeType == 3) // defeat Safari bug
        q = q.parentNode;

      //check which key was pressed
       var evt = e || window.event;
       var typeAheadList = document.getElementById("typeAheadList");

       if(evt.keyCode==38)
       {
         if(currentTAElement == null)
         {
           highLightTA(typeAheadList.lastChild);
           q.value = typeAheadList.lastChild.lastChild.value;
         }
         else if(currentTAElement == typeAheadList.firstChild)
         {
           lowLightTA(currentTAElement);
           highLightTA(typeAheadList.lastChild);
           q.value = typeAheadList.lastChild.lastChild.value;
         }
         else
         {
           var elem = currentTAElement;
           do
           {
             elem = elem.previousSibling;
           } while(elem != null && elem.tagName != "A");

           if(elem != null)
           {
             lowLightTA(currentTAElement);
             highLightTA(elem);
             q.value = elem.lastChild.value;
           }
         }
         evt.returnValue = false;
         evt.cancelBubble = true;
         return false;
       }
       else if(evt.keyCode==40)
       {
         if(currentTAElement == null)
         {
           highLightTA(typeAheadList.firstChild);
           q.value = typeAheadList.firstChild.lastChild.value;
         }
         else if(currentTAElement == typeAheadList.lastChild)
         {
           lowLightTA(currentTAElement);
           highLightTA(typeAheadList.firstChild);
           q.value = typeAheadList.firstChild.lastChild.value;
         }
         else
         {
           var elem = currentTAElement;
           do
           {
             elem = elem.nextSibling;
           } while(elem != null && elem.tagName != "A");

           if(elem != null)
           {
             lowLightTA(currentTAElement);
             highLightTA(elem);
             q.value = elem.lastChild.value;
           }
         }
         evt.returnValue = false;
         evt.cancelBubble = true;
         return false;
       }

      var typeAheadListWrapper = document.getElementById("typeAheadListWrapper");
      if(q.value.length < 1 || searchPosting == true)
      {
        typeAheadList.innerHTML = "";
        typeAheadListWrapper.style.display="none";
        return;
      }

      typeAheadList.innerHTML = "";
      typeAheadListWrapper.style.display="none";

//      move(searchField, typeAheadListWrapper,-2,-2);
//      typeAheadListWrapper.style.top = document.getElementById("firstMenuRowTable").offsetTop + searchField.offsetHeight + 3;
  //    typeAheadListWrapper.style.left = getElementPosition(searchField)[0];
//      typeAheadListWrapper.style.width =searchField.offsetWidth + "px";

      setBridgeCallBack(typeAheadCallback);
      setBridgeErrorCallBack(typeAheadErrorCallback);
      setBridgeCursorChange(false);

      var today = new Date();
      var hn = ["If-Modified-Since"];
      var hv = [today.addHours(2).toString()];
      if(typeof(encodeURIComponent) == "undefined")
      {
        loadBridgeDoc("//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/site.workflow:PreSearch?taf=" + escape(q.value.replace(/\+/g, "%2B").replace(/ /g,"+")), hn, hv);
      }
      else
      {
        loadBridgeDoc("//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/site.workflow:PreSearch?taf=" + encodeURIComponent(q.value.replace(/ /g,"+")), hn, hv);
      }
  
  }
  var currentTAElement = null;

  function highLightTA(taE, getQValue)
  {
    if(taE == null)
    {
      return;
    }
    taE.className = "typeahead-highlight";
    currentTAElement = taE;
    if(getQValue == true)
    {
      document.getElementById("searchform").q.value = currentTAElement.lastChild.value;
    }
  }

  function lowLightTA(taE)
  {
    if(currentTAElement != null)
    {
      currentTAElement.className = "typeahead-lowlight";
    }
    if(taE == null)
    {
      currentTAElement = null;
      return;
    }
    taE.className = "typeahead-lowlight";
    currentTAElement = null;
  }

  function typeAheadCallback(content)
  {
    var typeAheadListWrapper = document.getElementById("typeAheadListWrapper");
    var typeAheadList = document.getElementById("typeAheadList");

    if(searchPosting == true)
    {
      typeAheadList.innerHTML = "";
      typeAheadListWrapper.style.display="none";
    }

    setBridgeCursorChange(true);
    content = content.replace(/^\s+/,"");
    if(content != "")
    {
      typeAheadList.innerHTML = "";
      try
      {
        var tafArray = eval(content);
        var typeAheads = new Array();
        var suggestions = new Array();
        for(var tc=2;tc<tafArray.length;tc++)
        {
          if(tc%2 == 0)
          {
            var cut = (tafArray[tc].length > 22)?tafArray[tc].substring(0,22)+"...":tafArray[tc];
            typeAheads[typeAheads.length] = "<a href='#' class='typeahead-lowlight' onmouseover='highLightTA(this, true);' onmouseout='lowLightTA(this)' onclick='return submitSearch(this)' title='";
            typeAheads[typeAheads.length] = tafArray[tc];
            typeAheads[typeAheads.length] = "'>";
            typeAheads[typeAheads.length] = cut;
            typeAheads[typeAheads.length] = "<input type='hidden' name='tafval' value='";
            typeAheads[typeAheads.length] = tafArray[tc];
            typeAheads[typeAheads.length] = "'></a>";
          }
          else
          {
            var strt = tafArray[tc].indexOf("[[[");
            var nd = tafArray[tc].indexOf("]]]", strt);
            if(strt > 0 && nd > 0)
            {
              var sugg = tafArray[tc].substring(strt+2, nd+1);
              sugg = sugg.replace(/\[/g,"");
              sugg = sugg.replace(/\]/g,"");
              var ar = sugg.split(",");
              for(var ac=0;ac<ar.length;ac+=3)
              {
                var cut = (ar[ac].length > 22)?ar[ac].substring(0,22)+"...":ar[ac];
                suggestions[suggestions.length] = "<a href='#' class='typeahead-lowlight' onmouseover='highLightTA(this, true)' onmouseout='lowLightTA(this)' onclick='return submitSearch(this)' title='";
                suggestions[suggestions.length] = ar[ac];
                suggestions[suggestions.length] = "'>";
                suggestions[suggestions.length] = cut;
                suggestions[suggestions.length] = "<input type='hidden' name='tafval' value='";
                suggestions[suggestions.length] = ar[ac];
                suggestions[suggestions.length] = "'></a>";
              }
            }
          }
        }

        if(typeAheads.length > 0 || suggestions.length > 0)
        {
          var ih = typeAheads.join("");
          if(suggestions.length > 0)
          {
            ih += "<div class='search-typeahead-suggestion-txt'>Suggestions</div>" + suggestions.join("");
          }
          typeAheadList.innerHTML = ih;
          typeAheadListWrapper.style.display="block";
          
          var tafTimeOut = 0;
          if(tafTimeOut > 0)
          {
            tafTimer = setTimeout("hideTaf()", tafTimeOut);
          }
        
        }
        else
        {
          typeAheadList.innerHTML = "";
          typeAheadListWrapper.style.display="none";
        }
      }
      catch(e)
      {
  
      }
    }
    else
    {
      typeAheadList.innerHTML = "";
      typeAheadListWrapper.style.display="none";
    }
    searchPosting = false;
  }
  function typeAheadErrorCallback(err, content)
  {
    var typeAheadListWrapper = document.getElementById("typeAheadListWrapper");
    var typeAheadList = document.getElementById("typeAheadList");

    if(searchPosting == true)
    {
      typeAheadList.innerHTML = "";
      typeAheadListWrapper.style.display="none";
    }
    searchPosting = false;
    setBridgeCursorChange(true);
  }
  function submitSearch(tafval)
  {
    for(var ilc=tafval.childNodes.length-1;ilc>0;ilc--)
    {
      if(tafval.childNodes[ilc].name == "tafval")
      {
        var ccc = "";
        if(ccc != "" && document.forms.searchform.cc)
        {
          document.forms.searchform.cc.value = ccc;
        }
		document.forms.searchform.q.value = tafval.childNodes[ilc].value;
        document.forms.searchform.submit();
        searchPosting = true;
        var typeAheadListWrapper = document.getElementById("typeAheadListWrapper");
        var typeAheadList = document.getElementById("typeAheadList");
        typeAheadList.innerHTML = "";
        typeAheadListWrapper.style.display="none";
        break;
      }
    }
    return false;
  }
  function hideTaf()
  {
    tafTimer = setTimeout("doHideTaf()", 50);
  }
  function cancelTafHide()
  {
    if(tafTimer != null)
    {
      clearTimeout(tafTimer);
      tafTimer = null;
    }
  }
  function doHideTaf()
  {
    if(tafTimer != null)
    {
      document.getElementById("typeAheadList").innerHTML = "";
      document.getElementById("typeAheadListWrapper").style.display = "none";
      tafTimer = null;
    }
  }


  </script> 
     <!-- Search -->
     <!-- Search End --> 
    </div> 
   </div> 
