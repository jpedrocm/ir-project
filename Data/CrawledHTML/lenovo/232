function parseForWin10() {
if(w10LanguagePack.recommends["en"] != null)
{
try
{
	var bt = new Date().getTime();
	for(var mr=0;mr<matchRules.length;mr++)
	{
		$(matchRules[mr]).each(function() {
			if(!$(this).attr("data-win10"))
			{
			$(this).removeClass("family-thinkpad");
			var nextToMe = $(this).next();
			var tryme = $(this).text();
			if(nextToMe.hasClass("configuratorItem"))
			{
				tryme = nextToMe.text();				
			}
			var hsreg = new RegExp(w10LanguagePack.home.reg, "gim");						
			var homeMatch = hsreg.test(tryme);

			var proreg = new RegExp(w10LanguagePack.pro.reg, "gim");						
			var proMatch = proreg.test(tryme);

			var recreg = new RegExp(w10LanguagePack.recommends["en"].rec, "gim");						
			var recMatch = recreg.test(tryme);
			
			//console.warn(tryme + ", homeMatch:" + homeMatch + ", proMatch:" + proMatch + ", recMatch:" + recMatch);
			if((homeMatch && proMatch && !recMatch) ||
			   (homeMatch && !proMatch && !recMatch))
			{
				if(nextToMe.hasClass("configuratorItem") && (homeMatch && proMatch && !recMatch))
				{					
					nextToMe.prepend("<div class='w10-rec' style='display:inline-block; margin-left:3em;'>" + w10LanguagePack.recommends["en"].text + "</div>");									
				}
				else if(!nextToMe.hasClass("configuratorItem"))
				{
					$(this).append("<div class='w10-rec'>" + w10LanguagePack.recommends["en"].text + "</div>");				
				}
			}
			$(this).attr("data-win10", "true");
			}
			
		})
	}
	console.warn(new Date().getTime() - bt);
}
catch(win10err)
{
	console.error(win10err);
}
}
}
parseForWin10();
</script>   
 </body>
</html>
