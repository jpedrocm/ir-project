   <script>
$(".bar_3-leftLink a").text("Filter by specs");
$("document").ready(function(){
	var placeholder = "Type search terms";
	
	if(typeof placeholder != "undefined" && placeholder != null && placeholder != ""){
		$(".general_Menu li.search_menu").find("input[type='search']").attr("placeholder", placeholder);
	}
});	
