   <script type="text/javascript">


function facetInitCallback()
{
	if((sortcriteria = document.getElementById("facet_result_sort")) != null)
	{
	 	if(sortcriteria.options.length > 5)
	 	{
			sortcriteria.remove(5);
			sortcriteria.remove(4);
			sortcriteria.remove(3);
			sortcriteria.remove(0);
		}
	}
}

var saleRoot = $("#facet-area").find("ol[data-name='On sale']");
if(saleRoot.length > 0)
{
	var saleFacets = saleRoot.find("li");
	var business_type = "Public".toLowerCase();
	var siteid = "web".toLowerCase();

	if(business_type == 'public')
	{
		saleFacets.eq(3).remove();
		saleFacets.eq(2).remove();
		saleFacets.eq(1).remove();
	}
	else if(business_type == 'affinity')
	{
		saleFacets.eq(3).remove();
		saleFacets.eq(1).remove();
		saleFacets.eq(0).remove();
		saleFacets.eq(2).find(".expandableMenu-item").contents().filter(function() {		
			if(this.nodeType === 3)
			{
				this.textContent = "On sale now";
			}
		});
	}
	else if(business_type == 'epp')
	{
		saleFacets.eq(3).remove();
		saleFacets.eq(2).remove();
		saleFacets.eq(0).remove();
		saleFacets.eq(1).find(".expandableMenu-item").contents().filter(function() {		
			if(this.nodeType === 3)
			{
				this.textContent = "On sale now";
			}
		});

	}
	else
	{
		saleRoot.remove();
	}
}

/*
if("web" != "web")
{
	//laptops
	var onsalegroup = $("#facet-list-item-11");
	if(onsalegroup == null || onsalegroup.length == 0)
	{
		//desktops
		onsalegroup = $("#facet-list-item-9");
	}
	if(onsalegroup == null || onsalegroup.length == 0)
	{
		//tablets
		onsalegroup = $("#facet-list-item-6");
	}
	if(onsalegroup != null && onsalegroup.length > 0)
	{
		onsalegroup.remove();
	}
}
