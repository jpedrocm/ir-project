   <script>
	$(".tabbedBrowse-productListing-button-container a.button-full").removeClass("button-standard-alt").addClass("button-called-out");

	String.prototype.replaceAll = function(a, b) {
		var string = this;

		while(string.lastIndexOf(a) > -1) {
			string = string.replace(a, b);
		}

		return string;
	};

    $('.tabbedBrowse-productListing-container').css('padding-top','2.5em');

    $('.tabbedBrowse-productListings-scrollerHeader > p').hide();

	function addType(oid, type) {
		oid = oid.replaceAll(":", "_");
		oid = "item" + oid;
		$("ol.tabbedBrowse-productListings li." + oid + " div.tabbedBrowse-productListing-button-container > a.button-called-out > span").text(type);
		//$("ol.tabbedBrowse-productListings li." + oid ).addClass(class);
	}

</script> 
   <script>
var check_URL = ['/laptops/ideapad/','/laptops/lenovo/','/laptops/yoga/','/tablets/lenovo/'];

for (var i = 0; i < check_URL.length; i++) {
   if(window.location.href.indexOf(check_URL[i]) > -1){
		$(".tabbedBrowse-productListings").find(".tabbedBrowse-productListing-container").each(function(){
			if($(this).find('.nextdayship').length > 0){
				var pliheader = $(this).find(".ribbon-header");
				if(pliheader.length > 0)
				{
					pliheader.addClass("nextDay-ship");
					
					//Added the limited time deal ribbon for temporary 
					var oid_class= $(this).attr("class");
					if(oid_class.indexOf("000001C9_00017D80") > 0){
						pliheader.addClass("limited-time-deal");
					}
				
				}
				else
				{			
					$(this ).addClass("nextDay-ship");
				}
			}
		});
   }
}
