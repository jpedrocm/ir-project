    <!-- persistent cart --> 
    <div class="menu_2_wrapper cart_wrapper"> 
     <ul style="overflow: hidden;" class="menu_2 cartrollover"> 
      <li class="menu_level_2"> 
       <div id="rollovercart"> 
        <div id="rollovercartContents" class="cf"> 
         <ul id="rollovercartContentsItems"></ul> 
        </div> 
        <div id="rollovercartFooter" class="cf"> 
         <a id="rollovercartViewCart" href="JavaScript:startSolution('//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/cart.workflow:ShowCart?shopping-menu-selected-line=cart');" class="button-standard button-full button-called-out"> <span>View cart</span> </a> 
        </div> 
       </div> </li> 
     </ul> 
    </div> 
    <script type="text/javascript">
	var rolloverCart = (function(){
		var $rolloverItemContainer = $('#rollovercartContentsItems');
		var $cartMenu = $(".cart_menu");
		var $cartLink = $(".cartlink");
		var $cartWrapper = $("div.menu_2_wrapper.cart_wrapper");
		var $cartCounter = $("li.cart_menu .counter");
		var $cartMarker = $("li.cart_menu .marker");	
		var unbound = false;
		var cartLink = "JavaScript:startSolution('//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/cart.workflow:ShowCart?shopping-menu-selected-line=cart');";		
		var site = 'ISSUserweb'; 
		var cartData = $.parseJSON(sessionStorage.getItem(site));
		var ajaxListeners = [];
				
		const maxRolloverItems = 5
		const maxQty = 0;
		const maxItemQty = parseInt(50);

		var hookupAjaxListener = function(listeners){
			ajaxListeners = listeners
		
			$(document).ajaxComplete(function(e, xhr, settings){				
				for(var x=0; x < ajaxListeners.length; x++)
				if(settings.url.indexOf(ajaxListeners[x].matchURL) >= 0){
					var re= new RegExp( ajaxListeners[x].oidParam + "=(:.{8}:.{8}:)");
					var oid = "";
									
					if (typeof settings.data != "undefined" && settings.data != "")
					{
						oid = decodeURIComponent(settings.data).match(re)[1]
					}
					
					if(oid == ""){
						var oid = settings.url.match(re)[1];
					}
					
					if(oid != ""){
						addItem(oid);
					}
				}
			});
		
		}
		
		
		/*****************************************************************************************************
			 * appendRowToCartRollover: Adds an item to the rollover cart
			 *
			 * @param item: The item to be added to the rollover cart
		******************************************************************************************************/		
		var appendRowToCartRollover = function(item) {
			var cartURL = (item.customize != "")? 'href="' + item.customize + '"': 'href="' + cartLink + '"' ;
			var updateBtnAction = item.qtyBtn.replace(/&#034;/g, '"').replace(/&#039;/g, "'").replace(/&gt;/g, '>').replace(/&lt;/g, '<');
			if(item.oid ==':000001CC:000093CD:' || item.oid ==':000001CC:000093D1:' || item.oid ==':000001CC:000093D4:' || item.oid ==':000001CC:00008DCC:' || item.oid ==':000001CC:00008DCA:'){
				var qtyHTML="";
				var divider="";
			}
			else{
				var qtyHTML = '<form id="updateQntyForm" data-oid="' + item.oid +'" class="updateQty"><label for="cart-item-pricing-and-quantity-form-field">Qty:</label>' + item.update.replace(/&#034;/g, '"').replace(/&#039;/g, "'").replace(/&gt;/g, '>').replace(/&lt;/g, '<');
			qtyHTML += (item.update.indexOf("input") == -1)? '</form>' : '<a id="cart-item-pricing-and-quantity-form-button" class="rolloverCartUpdateBtn" href="#" onclick="' + updateBtnAction + '">Update</a></form>';
				var divider='<span class="divider">|</span>';
			
			}
			var itemHTML = '<li><a data-quick="' + item.itemQS + '" data-itemtype="' + item.itemType + '" data-customize="' + item.customize + '" ' + cartURL + '><img class="rollovercartItemImg" src="' + item.img + '"/></a><div class="rolloverCartDesc"><a data-quick="' + item.itemQS + '" data-itemtype="' + item.itemType + '" data-customize="' + item.customize + '" ' + cartURL + '><span class="rollovercartItemName">' + item.description + '</span></a>' + qtyHTML  + divider+ '<div class="rollovercartRem"><a href="#" onclick="rolloverCart.removeItem(&quot;' + item.remURL + '&quot;, &quot;' + item.oid + '&quot;); return false;">Remove</a></div></div></li>';
		
			$rolloverItemContainer.append(itemHTML);		
		};
		
		/*****************************************************************************************************
			 * addItem: Adds an item to the virtual cart without posting to the cart
			 *
			 * @param prodOID: oid of the item being removed
			 * @param showModal: indicator of whether or not to show a confirmation message for the item being added, defaults to false
		******************************************************************************************************/				
		var addItem = function(prodOID, showModal){			
			try{
				showModal = (typeof showModal != "undefined" && showModal != null)? showModal : false;
				var products = prodOID;
				if(cartData != null){
					for(var x=0; x<cartData.items.length; x++){
						products += "," + cartData.items[x].oid;							
					}
				}
				
				var OIDurl = (window.long_url)?long_url:"//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US";
				OIDurl += "/wci.workflow:load?page=/WW/ag/templates/cart-checkout/owv3/rollover/cartrollover_additem.html&addedPart=" + prodOID + '&oids=' + products;
				
				//get data updates for the rollover cart and the confirmation message
				$.post(OIDurl, function(data) {
					var oData = $.parseJSON(data);
					var re = /(?!\>)\d(?=\<)/;
					oData.addedItem.qty = oData.addedItem.qty.match(re)[0];							
					
					if(cartData != null){
						var itemFound;
						
						if(oData.addedItem.customize == ""){
							for(var x=0; x<cartData.items.length; x++){
								if(cartData.items[x].oid == oData.addedItem.oid){
									cartData.items[x] = oData.addedItem;									
									itemFound = true;
									break;
								}
							}
						}
						if(!itemFound){
							cartData.items.push(oData.addedItem);
						}
						cartData.itemCount++
					}
					else{
						cartData = {itemCount: 1, items: [oData.addedItem]};
						unbound = true;
					}									
					
					/** Seting Cart count in the cookie for dotcom pages **/
					var cart_domain = location.hostname.split('.').slice(-2).join('.'); // to get leni2.com or lenovo.com
					document.cookie="_CartItemCount="+ cartData.itemCount +"; domain=." + cart_domain + "; path=/";

					sessionStorage.setItem(site, JSON.stringify(cartData));	
					updateCartRollover();	
					
					if(oData.addedItem.customize != ""){
						disableActions(prodOID);
					}
					
					if(showModal){
						showModalWindow(oData.addedItem, oData.items);		
						$.fancybox.hideActivity();
						hideOverlay();							
					}
				});
			}
			catch(overexp)
			{
				console.log(overexp);
				window.location.href = "./cart.workflow:ShowCart";
			}			
		}
		
		/*****************************************************************************************************
			 * removeItem: Removes an item from the cart
			 *
			 * @param remUrl: URL for AJAX call to server  to remove the item
			 * @param oid: oid of the item being removed
		******************************************************************************************************/		
		var removeItem = function(remUrl, oid){
			showOverlay();
			$.fancybox.showActivity();		
			$.post(remUrl, afterRemRolloverCartItem(oid));						
		};
		
		/*****************************************************************************************************
			 * afterRemRolloverCartItem: Updates the UI after the item is removed from the cart
			 *
			 * @param oid: oid of the item being removed
		******************************************************************************************************/				
		var afterRemRolloverCartItem = function(oid){		
			try {
				//remove the deleted item from the json
				for(var x=0; x<cartData.items.length; x++){
					if(cartData.items[x].oid == oid){
						cartData.itemCount = cartData.itemCount - parseInt(cartData.items[x].qty);
						cartData.items.splice(x,1);
					}
				}
				
				/** Seting Cart count in the cookie for dotcom pages **/
				var cart_domain = location.hostname.split('.').slice(-2).join('.'); // to get leni2.com or lenovo.com
				document.cookie="_CartItemCount="+ cartData.itemCount +"; domain=." + cart_domain + "; path=/";
				
				sessionStorage.setItem(site, JSON.stringify(cartData));			
				updateCartRollover();
				hideOverlay();
				$.fancybox.hideActivity();		

			} catch (error) {
				if (window.debugAllowed && !!window.console) {
					console.warn('Error in updateCartRollover');
					console.error(error.message);
					console.log(error);
				}
			}			
		};
		
		/*****************************************************************************************************
			 * refreshData: Refresh the cart data object with latst version from session storage
		******************************************************************************************************/			
		var refreshData = function(){
			cartData = $.parseJSON(sessionStorage.getItem(site));
		}
		
		/*****************************************************************************************************
			 * initVirtualCart: Initialized the virtual cart by updating the click events to overrides used by virtual cart
			 *
			 * @param selector: selector for the buttons that will be swapped with virtual cart
			 * @param opts: parameters used to identify the OID for the item that will be added by virtual cart on button click
		******************************************************************************************************/			
		var initVirtualCart = function(selector, opts ){
			//override the click event behavior for add to cart buttons
			//to not take users straight to cart			
			if($(selector).length > 0){
				$(selector).each(function(){
					var prodOID; 
					if(opts['dataAttr']){
						prodOID= $(this).attr(opts['dataAttr']);					
					}
					else if(opts['linkAttr']){
						var addToCartLink= $(this).attr(opts['linkAttr']);
						prodOID = getQS(opts['qsParam'], addToCartLink);			
					}
					
					if(typeof prodOID != 'undefined'){
						$(this).unbind("onclick").unbind("click").attr('onclick', "rolloverCart.addToCart(this,'" + prodOID + "'); return false;");
					}		
					
				});
			}						
		};
		
		/*****************************************************************************************************
			 * addToCart: Method to add item to cart and call helper methods to display a confirmation message to user
			 *
			 * @param e: sender of the click event (the link tag).
			 * @param prodOID: product id of the product being added to cart.
		******************************************************************************************************/
		var addToCart = function(e, prodOID){
			try{
				showOverlay();
				$.fancybox.showActivity();
				
				if(cartData != null && cartData.items != null){
					//check rollover cart ot make sure max qty of 5 hasnt been reached
					for(var x=0; x<cartData.items.length; x++){
						if(cartData.items[x].oid == prodOID && cartData.items[x].qty >= maxItemQty){
							showError("You may have no more than a total of " + maxItemQty + " of a product in your shopping cart.");
							$.fancybox.hideActivity();
							hideOverlay();	
							return false;
						}				
					}
					
					if(maxQty > 0 && cartData.itemCount >= maxQty){
							showError("Your shopping cart may not contain more than " + maxQty + " items total. Please remove one or more items to add another item to the shopping cart.");
							$.fancybox.hideActivity();
							hideOverlay();			
							return false;						
					}
				}			
				
				//Add item to call vai AJAX call
				$.post(((window.long_url)?long_url:"//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US") + "/cart.workflow:AddToCart?showcart=false&addtocart-item-id=" + prodOID, function(data) {
					try{												
						addItem(prodOID, true);			
						
						e.innerHTML = "<span>Added to Cart</span>";
						$(e).addClass("VcDisabled").attr('onclick', "return false").prop('disabled', true);							
					}
					catch(overexp)
					{
						console.log(overexp);
						window.location.href = "./cart.workflow:ShowCart";
					}
				 });
			}
			catch(overexp)
			{
				console.log(overexp);
				window.location.href = "./cart.workflow:ShowCart";
			}
		};

		/*****************************************************************************************************
			 * showError: Method to display an error message in a fancybox pop-up
			 *
			 * @param msg: The error message to be displayed to the user
		******************************************************************************************************/		
		var showError = function(msg){
			modalHtml = '<div id="atcOverlay" class="cmp-overlay modal error"><div class="inner"><h1>Error:</h1><div class="cartContainer"><div class="errorMsg"><p>' + msg + '</p></div>' ;				
			modalHtml += '<div class="modalButtons"><a class="mdlButton" href="#" class="button shop xxl" onclick="$(\'#atcOverlay\').overlay().close(); return false;"><span>Close</span></a></div></div></div></div>';
			if(document.getElementById("atcOverlay") == null){
				$(modalHtml).appendTo('body');
			}
			else{
				$("#atcOverlay").replaceWith(modalHtml);
			}

			$("#atcOverlay").overlay({
				expose:{
					color: '#000',
					opacity: 0.75,
					loadSpeed: 'fast',
					top: "center",
					left: "center"
				},
				closeOnClick: true
			});
			
			$("#atcOverlay").data('overlay').load();				
		}
				
		/*****************************************************************************************************
			 * showModalWindow: Removes an item from the cart
			 *
			 * @param addedItem: object with data about the item that was added to the cart
			 * @param prices: array of prices for the cart items
		******************************************************************************************************/	
		var showModalWindow = function(addedItem, prices){
			var modalHtml;
			var cartContentsHtml = '';
			var addedItemsCt = 0;
						
			//display the message box with continue shopping and go to cart buttons
			if(cartData.items.length > 1){
				modalHtml = '<div id="atcOverlay" class="cmp-overlay modal"><div class="inner"><h1>You&#039;ve just added the following to your cart:</h1><div class="cartContainer"><div class="newItem">';				
			}
			else{
				modalHtml = '<div id="atcOverlay" class="cmp-overlay modal noGrid"><div class="inner"><h1>You&#039;ve just added the following to your cart:</h1><div class="cartContainer"><div class="newItem noGrid">';
			}
			
			//loop through the products array and put together the html & calculate the total
			for(var x=0; x < cartData.items.length; x++)
			{
				var item = cartData.items[x];
				if(item.oid != addedItem.oid){					
					var itemPrice;
										
					cartContentsHtml += '<li><span class="cartItemName">' + item.description.replace(/\+/g, ' ') + '</span><span class="cartItemQty">Quantity: ' + item.qty + '</span><span class="cartItemPrice">Price: ' + prices[item.oid].total + '</span>';
					addedItemsCt++				
				}
				
				if(addedItemsCt == 3){break;}
			}				
			
			//now put together the final html
			//add item html
			modalHtml += '<img src="' + addedItem.img + '"/><p class="prodDesc">' + addedItem.description + "</p>" + '<p class="prodPrice">Price:' + prices[addedItem.oid].total + "</p>";
			modalHtml += '<p style="margin-top:2.5em; clear:both">Need more than 1? You can increase the quantity in cart.</p>'
			//add modal buttons
			modalHtml += '<div class="modalButtons"><a class="mdlButton" href="//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/cart.workflow:ShowCart?shopping-menu-selected-line=cart" class="button shop xxl"><span>Go To Cart</span></a><a href="#" onclick="$(\'#atcOverlay\').overlay().close(); return false;" class="mdlLink"><span>Continue Shopping</span></a></div></div>';

			if(cartContentsHtml != ''){
				modalHtml += '<div class="cartItems"><p class="cartSummary">Recently Added to Cart</p><div id="cartContents"><ul id="cartContentsItems">' + cartContentsHtml + '</ul></div></div></div>';
			}
			
			modalHtml += '</div>';

			if(document.getElementById("atcOverlay") == null){
				$(modalHtml).appendTo('body');
			}
			else{
				$("#atcOverlay").replaceWith(modalHtml);
			}

			$("#atcOverlay").overlay({
				expose:{
					color: '#000',
					opacity: 0.75,
					loadSpeed: 'fast',
					top: "center",
					left: "center"
				},
				closeOnClick: false
			});
			
			$("#atcOverlay").data('overlay').load();						
		};
		/*****************************************************************************************************
			 * getQS: Helper method to extract data from querystring
			 *
			 * @param key sender of the click event (the link tag).
			 * @param url full url containing querystring to be parsed
			 * @return Value of querystring matching the key
		******************************************************************************************************/
		var getQS = function(key, url){
			var da = document.createElement("A");
			da.setAttribute("href", url);
			var queryString = {};
			da.href.replace(new RegExp("([^?=&]+)(=([^&]*))?", "g"), function($0, $1, $2, $3) { queryString[$1] = $3; });
			return queryString[key];
		};	

						
		/*****************************************************************************************************
		* showOverlay: Helper method to show a semi-transparent overlay. this is done to prevent users from clicking
		*							on things while the item is being added to cart.
		******************************************************************************************************/
		var showOverlay = function(){
			var docHeight = $(document).height();

			$("body").append("<div id='virtCartOverlay'></div>");
			$("#virtCartOverlay").height(docHeight).css({
				'opacity':0.75,
				'position': 'absolute',
				'top':0,
				'left':0,
				'background-color':'#FFF',
				'width': '100%',
				'z-index':999				
				});
		}

		/*****************************************************************************************************
			 * removeOverlay: Helper method to remove the semi-transparent overlay.
		******************************************************************************************************/
		var hideOverlay = function(){
			$("#virtCartOverlay").remove()
		};
				
		var rolloverHoverStart = function(){
			$("#rollovercart").css("display","block");
			$(this).parent().addClass("opened");
			if($(".nav .opened").length > 0 && viewport().width > mobileWidth){
				$(".masthead .overlay").addClass("visible");				
			}
		}
		
		var rolloverHoverEnd = function(){
			$("#rollovercart").css("display","none");		
			$(this).parent().removeClass("opened");	
			if($(".nav .opened").length == 0 || viewport().width < mobileWidth){
				$(".masthead .overlay").removeClass("visible");			
			}					
		}
		
		var showRollover = function($item){
			$("#rollovercart").css("display","block");								
			$item.addClass("opened");
			if($(".nav .opened").length > 0 && viewport().width > mobileWidth){
				$(".masthead .overlay").addClass("visible");				
			}								
		}			
		
		var hideRollover = function($item){
			$("#rollovercart").css("display","none");
			$item.removeClass("opened");	
			if($(".nav .opened").length == 0 || viewport().width < mobileWidth){
				$(".masthead .overlay").removeClass("visible");			
			}				
		}

		
		/*****************************************************************************************************
			 * updateCartRollover: Updates the rollover cart drop down UI
			 
			 * @param rebind: Indicator to rebind the click event to show/hide the drop down. defaults to false
		******************************************************************************************************/				
		var updateCartRollover= function(rebind){			
			var rollovercartItemLinks = [], rollovercartItemImages = []; rollovercartItemName = [];		
			$rolloverItemContainer.children().remove();
			
			rebind = typeof rebind !== 'undefined' ?  rebind : false;
						
			if (cartData && cartData.items.length != 0) {
				var totalQty = 0;
				var visibleQty = 0;

				for (var i=0;i<cartData.items.length;i++) {
					var item = cartData.items[i];
					
					totalQty += parseInt(item.qty);
					
					if (typeof(item) != "function" && i < maxRolloverItems){
						appendRowToCartRollover(item);
						visibleQty += parseInt(item.qty);							
					}					
				}
				
				//if there are more than 5 items, add an additional items element
				if(totalQty != visibleQty){
					var itemHTML = '<li class="otherItems"><span class="rollovercartItemName">Other Items:  ' + (totalQty - visibleQty) + '</span></li>';
					$rolloverItemContainer.append(itemHTML);
				}
			
				cartData.itemCount = totalQty		
				
				
				if($cartMenu.length == 1){
					$(".cart_wrapper").appendTo($cartMenu);
					$cartMenu.hover(function(){showRollover($(this))}, function(){hideRollover($(this))}).find(".cartlink" ).addClass("has-submenu").click(function(){
						if($("#rollovercart")){
							if($("#rollovercart").css("display") == "block"){
								hideRollover($(this).parent());
							}
							else{
								showRollover($(this).parent());
						}
						return false;
					}						
					});
					if($.fn.hoverIntent){
						/*$cartMenu.unbind("mouseenter").unbind("mouseleave").hoverIntent({
							over:rolloverHoverStart,
							out:rolloverHoverEnd,
							timeout:250
						});*/
					}
				}
				else{					
					$.each($cartMenu,function(){
						if($(this).find(".cartlink" ).length > 0 || $(this).find(".cartlink" ).length > 0){
							$(".cart_wrapper").appendTo($(this));
							$(this).find(".cartlink" ).addClass("has-submenu");
							$(this).children().click(function(){$("#rollovercart").css("display","block");});
							/*if($.fn.hoverIntent){
								$(this).unbind("mouseenter").unbind("mouseleave").hoverIntent({
									over:rolloverHoverStart,
									out:rolloverHoverEnd,
									timeout:250
								});														
							}*/
						}
					});
				}
				
				$.each($cartLink,function(){
					if($(this).find(".marker").length == 0){
						$(this).append("<div class='marker'></div>");
					}				
					if($(this).find(".counter").length == 0){
						$(this).append("<span class='counter'></span>");
					}
					$(this).find('.counter').empty().html(''+ cartData.itemCount +'').show();					
					
					if(unbound || rebind){
						$(this).unbind("click").click(function(e){
							$('.nav .menu>li>a.has-submenu').not(this).parent().removeClass('opened');
							if($(this).parent().parent().hasClass('general_Menu')){
								$('.nav .menu>li>a.has-submenu').not(this).parent().parent().removeClass('opened');
							}
							$(this).parent().toggleClass("opened");
							if($(this).parent().hasClass("opened")){
								$("#rollovercart").show();
							}
							else{
								$("#rollovercart").hide();
							}
							return false;							
						});
						/*if($.fn.hoverIntent){
							$(this).parent().unbind("mouseenter").unbind("mouseleave").hoverIntent({
								over:rolloverHoverStart,
								out:rolloverHoverEnd,
								timeout:250
							});
						}*/
						
						
						unbound = false;
					}					
				});			
			}
			else
			{						
				//there are no items in the cart, remove the item count and drop down				
				$cartMenu.removeClass("opened");				
				$cartCounter.remove();
				$cartMarker.remove();			
				$.each($cartLink,function(){							
					$(this).unbind("click").removeClass("has-submenu").find('.counter').remove();
					unbound = true;
				});
				$("#rollovercart").css("display", "none");
			}
						
			$("#updateQntyForm").submit(function(e) {
				showOverlay();
				$.fancybox.showActivity();					
				var qtyString = $(this).find("[name='quote-qty']:last").val().split("|");
				var url = $(this).attr('action');
				var newQty = qtyString[2];
				
				var selector = "#rollovercartContentsItems #cart-item-";
				
				if(qtyString[1].indexOf("[") != -1){
					var re = /(\[)(\:)(.*)(\:)(\])/;
					var matches = qtyString[1].match(re);
					selector += "\\" + matches[1] + "\\" + matches[2] + matches[3] + "\\" + matches[4] + "\\" + matches[5];
				}
				else{
					selector += qtyString[1];
				}							
				
				$(selector).parentsUntil("ul", "li").find(".rolloverError").remove();			
				
				if(newQty > maxItemQty){			
					$(selector).parentsUntil("ul", "li").append("<p class='rolloverError'>You may have no more than a total of " + maxItemQty + " of a product. Please reduce the quantity of your selection.</p>");
					$.fancybox.hideActivity();
					hideOverlay();						
				}
				else if(maxQty > 0 && cartData.itemCount >= maxQty){
					$(selector).parentsUntil("ul", "li").append("<p class='rolloverError'>Your shopping cart may not contain more than " + maxQty + " items total. Please remove one or more items to add another item to the shopping cart.</p>");
					$.fancybox.hideActivity();
					hideOverlay();				
				}
				else{
									
					var updatedOid = $(selector).parent().data("oid");
					$.post(url, $(this).serialize(), function(){
						for(var x=0; x<cartData.items.length; x++){
							if(cartData.items[x].oid == updatedOid ){
								if(newQty == 0){
									cartData.items.splice(x,1);
								}
								else{
									var re = /(.*)(originalValue.*)(size.*)/;								
									var match = cartData.items[x].update.match(re);
									var newUpdate = "originalValue=&#034;" + newQty + "&#034; value=&#034;" + newQty + "&#034;"		
																		
									cartData.items[x].update = match[1] + newUpdate + match[3];
									cartData.items[x].qty = newQty;															
								}								
								break;	
							}
						}
						$("#rollovercartContentsItems input[name='quote-qty']").remove();
																
						sessionStorage.setItem(site, JSON.stringify(cartData));	
						updateCartRollover();
						$.fancybox.hideActivity();
						hideOverlay();								
					});					
				}
				
				e.preventDefault(); // avoid to execute the actual submit of the form.
			});	
			
		};
		
		var disableActions = function(oids){
			$("#rollovercartContentsItems li").each(function(){
				if(typeof oids == "undefined" || oids.indexOf($(this).find("#updateQntyForm").data("oid")) != -1){
					$(this).find("#updateQntyForm, .divider, .rollovercartRem").remove();
				}			
			});
			
			if(typeof oids == "undefined"){
				$("#rollovercartFooter").remove();
			}
		
		};
		
		
		var getCartItems = function(oid){
			var cartItem = false;
			if(cartData != null){
				for(var x=0; x < cartData.items.length; x++){
					if(cartData.items[x].oid == oid){
						cartItem = true;
						
					}
				}
			}
			return cartItem;
		};
						
		return{
			updateCartRollover: updateCartRollover,
			removeItem: removeItem,
			refreshData: refreshData,
			addToCart: addToCart,
			initVirtualCart: initVirtualCart,
			disableActions: disableActions,
			addItem: addItem,
			hookupAjaxListener: hookupAjaxListener,
			getCartItems:getCartItems
		};
	})();

	
	try {
		rolloverCart.updateCartRollover();
		rolloverCart.getCartItems();
	} catch (error) {
		if (window.debugAllowed && !!window.console) {
			console.warn('Error in updateCartRollover');
			console.error(error.message);
			console.log(error);
		}
	}	
</script> 
    <!-- end persistent cart --> 
   </header> 
   <!-- end lenovo custom masthead --> 
   <!-- end custom masthead --> 
   <!-- added on 06-10-15 per Allison for banner --> 
   <script>
  $('.common_Menu > li.phone_menu').after('<li class="chat"><div id="MastheadIconChatSalesLinkContainer" style="margin-top:-0.5em;"><div id="MastheadIconChatSalesLink"><div id="MastheadIconChatSalesLinkInfo"><img src="//www.lenovo.com/shop/americas/content/img_lib/masthead-chat-icon.png" /></div></div></div><li>');
  
  //$( ".product_menu > .menu_2_wrapper > .menu_2" ).after( "<div style='padding:0 15px 15px 15px; margin-top:-5px;'><a href='/us/en/landingpage/discover-lenovo/'><div style='background-color:#ff6b00;color:#fff;text-align:center; padding:5px 0; font-size:1.3em; line-height:1.5em;-webkit-box-shadow: 3px 3px 5px -3px #999;-moz-box-shadow: 3px 3px 5px -3px #999;box-shadow: 3px 3px 5px -3px #999;'>See what new innovations are happening at Lenovo &#8212; today. Learn More > </div></a></div>" );
  
</script> 
   <script>
	var isMobile = false; //initiate as false
	// device detection
	if(/(android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|ipad|iris|kindle|Android|Silk|lge |maemo|midp|mmp|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows (ce|phone)|xda|xiino/i.test(navigator.userAgent) 
	|| /1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i.test(navigator.userAgent.substr(0,4))) isMobile = true;
	if(isMobile){
		var ua = navigator.userAgent.toLowerCase();
		var isAndroid = ua.indexOf("android") > -1;
		if(isAndroid) {
			$("li.phone_menu a").attr("href", "wtai://wp/mc;18552536686");
		}	
		else{		
			$("li.phone_menu a").attr("href", "tel://1-855-253-6686");
		}
	}
	else{
		$("li.phone_menu a").addClass("has-submenu");	
		$("li.phone_menu a").after('<div class="menu_2_wrapper"><ul class=" menu_2"><li class="menu_level_2">Sales: 1-855-253-6686</li></ul></div>');		
	}
	
	$(document).ready(function(){
		// Start rollover new support menu
			$('.support_menu .menu_2_wrapper .menu_2 > li').hover(
			   function(){
					$(this).find(".support-submenu").addClass('rollover') 
						if ($(".support-submenu:gt(2)").hasClass("rollover")){
							$("li.support_menu.opened > div > ul > li:nth-child(3) span.over-title").css("visibility","hidden");
						}else{
							$("li.support_menu.opened > div > ul > li:nth-child(3) span.over-title").css("visibility","visible");
						}
			   },
			   function(){ 
					$(this).find(".support-submenu").removeClass('rollover')
					$("li.support_menu.opened > div > ul > li:nth-child(3) span.over-title").css("visibility","visible");
				}
			)
			
			function equalHeight(group) {
				tallest = 0;
				group.each(function() {
					thisHeight = $(this).height();
					if(thisHeight > tallest) {
						tallest = thisHeight;
					}
				});
				group.height(tallest);
			}
				
			equalHeight($(".over-title"));
			equalHeight($(".support-title"));	

			$("span.over-title:contains('SALES')").css("color","#6abf4a");
			$("span.over-title:contains('ORDER SUPPORT')").css("color","#ff6a00");			
		// End rollover new support menu
		
		$(window).scroll(function() {
			var height = $(window).scrollTop();

			if(height  > 25) {
				//console.log("test scroll");
				$('.nav.scrolled .general_Menu .phone_menu span').text("");
				$('.nav.scrolled .general_Menu .phone_menu span').css("padding-top","18px");
				$('.nav.scrolled .general_Menu .phone_menu .menu_2_wrapper').css({'color': '#2e9dce', "font-size": "0.9em", "width":"200px", "max-width":"200px", "padding-top": "5px"});
				$('.nav.scrolled .general_Menu .phone_menu').addClass("vertical-sm");
				
				$('.nav.scrolled .general_Menu .email_menu span').text("");
				$('.nav.scrolled .general_Menu .email_menu span').css("padding-top","18px");
				$('.nav.scrolled .general_Menu .email_menu').addClass("vertical-sm");
			}
		});
		
	});
	
</script> 
