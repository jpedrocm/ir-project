function viewin360(){
 $(document).ready(function(){
 //Lenovo 360 Spin
  $.each(lenovo_360, function(i, item) {
	 if($("meta[name='ModelNumber']").attr("content") == item.subseries){
 		 $('#galleria-stage > .galleria-images').append('<a class="lightbox in-360" id="in-360" rel="view360" href="'+item.id_360+'"><img src="//www.lenovo.com/shop/americas/content/img_lib/design_elements/lenovo-view-360-degree-icon.png" alt="View in 360 Spin" /></a>');
		}
	});

	if($("meta[name='ModelNumber']").attr("content") == "Yoga Tab 2 Pro" && $("meta[name='ProductType']").attr("content").indexOf("Tablet") != -1)
	{
	  $('#galleria-stage > .galleria-images').append('<a class="lightbox in-360" href="http://content.hmxmedia.com/superblaze/lenovo/yoga_tablet_2_13A/v2/index.html?IPromoID=LEN337654"><img src="//www.lenovo.com/shop/americas/content/img_lib/design_elements/lenovo-view-360-degree-icon.png" alt="View in 360 Spin" /></a>');
	}
	if($("meta[name='ModelNumber']").attr("content") == "Yoga 2 Pro" && $("meta[name='ProductType']").attr("content").indexOf("Laptop") != -1)
		{
		  $('#galleria-stage > .galleria-images').append('<a class="lightbox in-360" href="/ISS_Static/WW/ag/js/oneweb/subseries/magic360/yoga-2-pro.html?IPromoID=LEN328020"><img src="//www.lenovo.com/shop/americas/content/img_lib/design_elements/lenovo-view-360-degree-icon.png" alt="View in 360 Spin" /></a>');
	}

    	$("a.in-360").fancybox({
			'height'		: '100%',
			'width'			: '75%',
	        'autoScale'     : false,
			'type'			: 'iframe'
	});

    $('#fancybox-frame').css('background','#fff');
  	$('.subseries-gallery div.galleria-image,.galleria-image-nav-right,.galleria-image-nav-left').click(function() {
	    var class_Name = $(this).attr('class');
	       if($(this).hasClass("first")){
	     	 $("#galleria-stage a#in-360, #galleria-stage a.in-360").css("z-index", "1");
	   	   }else{
	   		 $("#galleria-stage a#in-360, #galleria-stage a.in-360").css("z-index", "0");
	   	   }
  	});

 });
}

  if(window.Galleria)
  {
    Galleria.ready.callbacks.push(viewin360);
  }
  else
  {
    $(document).ready(function(){
      setTimeout("viewin360()", 300);
    });

  }
 </script> 
