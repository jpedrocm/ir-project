  <script type="text/javascript">
$(document).ready(function() {
	$(".lazy").lazyload({skip_invisible:true,effect : "fadeIn"});
	$(".nav").find("li").on("click", function() { 
		$(this).find(".mhaol").each(function() {
			if($(this).is(":visible") && $(this).attr("data-src"))
			{
				$(this).attr("src", $(this).attr("data-src"));
				$(this).removeAttr("data-src");
			}
			else if($(this).is(":visible") && $(this).attr("data-bgsrc"))
			{
				$(this).css("background-image", "url('" + $(this).attr("data-bgsrc") + "')");
				$(this).removeAttr("data-bgsrc");
			}
			else if($(this).is(":visible") && $(this).attr("data-style"))
			{
				$(this).attr("style", $(this).attr("data-style"));
				$(this).removeAttr("data-style");
			}
		});
	});	
});
