   <script type="text/javascript">
var ctoStore = "Public";
$(document).ready(function(){
var model_subseries = $('meta[name="ProductName"]').attr('content');
var model_No = $('meta[name="ModelNumber"]').attr('content');
var modelOID= $('meta[name="metaidentifier"]').attr('content');
//var sytem_type = $('figcaption').text(), recs_div;
if(location.href.indexOf('/us/en/laptops/') > 0 || model_No == "ThinkPad Helix" ){
 $('#standard-on').after("<div id='sub-recs' style='padding-top:10px;'></div>");
}

var modelName,model = $('meta[name="ModelNumber"]').attr('content'),str = /(New!)|(Best Seller)/ig;
var Series, prodSeries = $('meta[name="ModelName"]').attr('content');
if (prodSeries.indexOf("href") != -1) {Series = $(prodSeries).find("span").text();} else {Series = prodSeries;}

if((typeof(model) != "undefined") && (typeof(prodtitle) != "undefined")){
if (model.indexOf("Lenovo") != -1) {var Modelno = model.replace("Lenovo","Essential");Modelno = Modelno.replace(str,"");modelName = $.trim(Modelno);
} else if (model.indexOf("Twist") != -1) {modelName = "ThinkPad Twist";
} else if (model.indexOf("Yoga") != -1 && model.indexOf("ThinkPad") === -1) {modelName = "Lenovo " + model;
} else if (prodtitle.indexOf("IdeaTab") != -1) {modelName = "IdeaTab " + model;
} else if (Series.indexOf("M Series") != -1) {var model_name = prodname;model_name = model_name.replace("Desktop","");modelName = $.trim(model_name);
} else {model = model.replace(str,"");modelName = $.trim(model);}
}

//Systems recommendation mbox
if(typeof(mboxCreate)!="undefined" ){
	mboxDefine('sub-recs', 'subSeries-Recs');
	mboxUpdate('subSeries-Recs', 'entity.id='+ modelName + '_US');
 }
});
</script> 
