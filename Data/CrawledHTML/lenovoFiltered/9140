  <script type="text/javascript">
var matchRules = [];
matchRules[matchRules.length] = ".DTPRELOAD_OS, .NB_PRELOAD_OS, .EPPRELOAD_OS";
//matchRules[matchRules.length] = ".specvalue-operatingsystem.family-thinkpad, .DTPRELOAD_OS, .NB_PRELOAD_OS, .EPPRELOAD_OS";

var w10LanguagePack = {};
w10LanguagePack.home = {"reg":"windows.?10.*(home|家用版).*", "rep": "Windows&nbsp;10 Home"};
