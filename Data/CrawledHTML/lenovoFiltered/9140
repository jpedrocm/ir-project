function checkoutError(e, chkcontent, chkresults, chkstep)
{  
  var msg = "";
  var chkerrortype = "";
  if(e == null)
  {
    e = "";
  }
  
  if(typeof(e.stack) !== 'undefined') { msg += e.stack; }
  else if(typeof(e.message) !== 'undefined') { msg += e.message; }
  else { msg += e; }

  var reasons = "";
  if(chkresults != null)
  {
    msg = 'Field validation / post errors: ' + msg;
    reasons = chkresults;
    chkerrortype = "validation";
  }
  else
  {
    msg = 'Error parsing step template: ' + msg;
    chkerrortype = "script";
    if(chkcontent.indexOf("!--findme") > 0 || chkcontent.indexOf("WCI HTML") >= 0)
    {
      reasons = chkcontent;
    }
    else
    {
      reasons = "";
    }
  }
  $.post( "//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/wci.workflow:load?page=/WW/ag/templates/error/report.html", 
          { 
            "trace": msg,
            "step":chkstep,
            "time": new Date().toString(),
            "type": chkerrortype,
            "host": $('meta[name="Host"]').attr('content'),
            "reasons":reasons            
          } 
  );
}

</script> 
