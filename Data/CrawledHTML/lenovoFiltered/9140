             <script type="text/javascript">
$(document).ready(function(){
	var oid = "_00000025_00003C83_";
	var shippingHTML = $("div.tabbedBrowse-productListings-wrapper .item" + oid + " .pricingSummary-shipping").html();
	$("#singlesku .pricingSummary-shipping").html(shippingHTML);	
