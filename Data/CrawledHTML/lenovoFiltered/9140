   <script type="text/javascript">
$(".tabbedBrowse-productListings .tabbedBrowse-productListing-footer-button-first").each(function(){
	$(this).click(function(event){
		var prodListing = $(event.target).parentsUntil("tabbedBrowse-productListing")

		var checkBoxObject;
		if(typeof prodListing != 'undefined')
		{
			checkBoxObject = $(prodListing).find("div.presel input");
			var checkBoxValue = decodeURIComponent(checkBoxObject.val());

			if(checkBoxObject.is(":checked") && checkBoxValue != 'HUqw3Sf2yTIZZdwaxTY0MA%3D%3D' && checkBoxValue != 'DREAeZx+aeH6YH0l8wcQdA==' && checkBoxValue != 'OpYiMh/gVUQkm4PdpYKCuOGP6wfrQ/jG')
			{
				if(!$(this).closest(".tabbedBrowse-productListing-container").hasClass("quick-shipping")){
					window.OfficeSelected = true;
				}
			}
		}
	});
});


$(".tabbedBrowse-productListings div.presel").each(function(){
	
	//storing the original value of the call out button url 
	var hrefOriginalValue = $('.tabbedBrowse-productListing-button-container > a').attr('href');
    
	$(this).find('input').click(function(){
		var checkBoxValue = decodeURIComponent($(this).val());
		var ps_item = getModelNumber($(this).attr('id'));

		var hrefValue = $('.tabbedBrowse-productListing-button-container > a[data-sb="'+ps_item+'"]').attr('href');
		
		
	  if($(this).is(":checked"))
	  {
		$('.tabbedBrowse-productListing-button-container > a[data-sb="'+ps_item+'"]').attr('href',hrefValue+'&ps='+ encodeURIComponent(checkBoxValue));
	  }
	  else{
		//split the url to sperate the preselection parameter
		var urlparts= hrefValue.split('?');
		
		if (urlparts.length>=2) {
			//split the preselection parameters
			var pars= urlparts[1].split(/[&;]/g);
			
			var param="ps="+$(this).val();
			
			//reverse iteration
			for (var i= pars.length; i-- > 0;) {
				//if the the parameter value matches on unslections
				if (pars[i] == param) {  
					//remove the parameter from url
					pars.splice(i, 1);
				}
			}
			//join together to form the url and attach the value to the call out button
			url= urlparts[0] + (pars.length > 0 ? '?' + pars.join('&') : "");
			$('.tabbedBrowse-productListing-button-container > a[data-sb="'+ps_item+'"]').attr('href',url);
		}
		//else
		//{
			//$('.tabbedBrowse-productListing-button-container > a[data-sb="'+ps_item+'"]').attr('href',hrefOriginalValue);
		//}
	  }
	});
});


function getModelNumber(ps_item){
	ps_item = ps_item.split('ps_item'); 
	return ps_item[1].replace(/_/g , ":");
	 
}

function showPreselItemDetailsPopup(group, code, name) {
  var cmmsURL = '//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/builder.workflow:ItemDetails?fCode=';
		console.info("cmmsURL: " + cmmsURL);
  cmmsURL += "&GroupID="+group+"&Code="+code+"&Name="+name+"&from=builder&hide_menu_area=true&hide_ad_area=true&ilink=presel";
  cmmsWindow = window.open(cmmsURL, 'cmmsWindow', "scrolling=yes,width=720,height=480,toolbar=yes");
  cmmsWindow.focus();
}

