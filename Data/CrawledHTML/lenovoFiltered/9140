$("#singlesku-cta").on("click", function() {
	$('#longscroll-singlesku').hide();
})

$(".skupresel").on("click", function(){
	if($(this).is(":checked")){
		
		var checkBoxValue = decodeURIComponent($(this).val());
		var pscta = $("#singlesku-cta");	
		var hrefValue = pscta.attr('href');	
		pscta.attr('href',hrefValue+'&ps='+ encodeURIComponent(checkBoxValue));	
	}else{ 
	 
		var checkBoxValue = $(this).val();
		var pscta = $("#singlesku-cta");	
		var hrefValue = pscta.attr('href');	
		var hrefValue = pscta.attr('href'); 
		pscta.attr('href',hrefValue.replace('&ps='+checkBoxValue, ''));
	}
});

