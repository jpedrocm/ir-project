   <input type="hidden" id="hdnPercentage" value=""> 
   <input type="hidden" id="hdnAmt" value=""> 
   <script type="text/javascript">

    var high_per = "";
     //console.log("prices inside the loop:599.00; 539.10;59.90");
      var Discount = getDiscount(599.00,59.90,539.10);			
			var DiscountAmt = getDiscountAmount(599.00,59.90,539.10);
			
      if (document.getElementById('hdnPercentage').value == "" )
      {
         document.getElementById('hdnPercentage').value = Discount;
      }
      else
      {
         var preDiscount = document.getElementById('hdnPercentage').value;
         if (preDiscount < Discount)
             document.getElementById('hdnPercentage').value = Discount;
      }
			
      if (document.getElementById('hdnAmt').value == "" )
      {
         document.getElementById('hdnAmt').value = DiscountAmt;
      }
      else
      {
         var preDiscount = document.getElementById('hdnAmt').value;
         if (preDiscount < DiscountAmt)
             document.getElementById('hdnAmt').value = DiscountAmt;
      }			
 

