function initBVConversations()
{

if($('.cta-group-reviews').length){
var product_ID = $('meta[name="metacategoryidentifier"]').attr('content'); 
$BV.configure('global', { productId : product_ID+'_12F0696583E04D86B9B79B0FEC01C087'});

$(function(){
$('.cta-group-reviews .reviews').each(function(i){
$(this).attr('id', 'containerReviews_'+(i+1)); 
	$('.cta-group-reviews .reviews#containerReviews_1').html('<div id="BVRRSummaryContainer"><div>' + $('#bvInitData').html() + '</div></div>');	
 });
//$('li#tab-li-reviews .tab-content div#bvreviewsWrap').html('<div id="BVRRContainer"></div><br/><h2 class="tabbedBrowse-title">' + ($('meta[name="ModelNumber"]').attr('content')) + '<span class="tabbedBrowse-currentTab">Questions & Answers</span></h2><div id="BVQAContainer"></div>');

$('li#tab-li-reviews #bvreviewsWrap').html('<ul class="reviews-tabs"><li class="bvactive"><div class="tabbedBrowse-currentTab" id="rating-review"><span>Ratings & Reviews</span></div></li><li><div class="tabbedBrowse-currentTab" id="question-answer"><span>Question & Answers</span></div></li></ul><div class="BV-content" id="tab1"><div id="BVRRContainer"></div></div><br/><h2 class="tabbedBrowse-title mobile-qa">' + ($('meta[name="ModelNumber"]').attr('content')) +'<span class="tabbedBrowse-currentTab">Questions & Answers</span></h2><div class="BV-content" id="tab2"><div id="BVQAContainer"></div></div>');


 console.log("Getting into BV tabs");
	$('#tab1').show();
    
	$('.reviews-tabs li div#rating-review').click(function(){
		$('#tab1').show();
		$('#tab2').hide();
		//$('.reviews-tabs li').eq(0).addClass('bvactive');
		$('.reviews-tabs li').removeClass('bvactive');
		$(this).parent().addClass('bvactive')
	 });	
	 $('.reviews-tabs li #question-answer').click(function(){
		$('#tab2').show();
		$('#tab1').hide();
		//$(this).addClass('bvactive');
		$('.reviews-tabs li').removeClass('bvactive');
		$(this).parent().addClass('bvactive')
	 });



 $('#BVRRContainer').prepend($('#bvreviewData').html());
 $('#BVQAContainer').prepend($('#bvcontentData').html());
 $BV.ui( 'rr', 'show_reviews', {
    doShowContent : function () { 
			if($('#tab-li-reviews > #tab-reviews .js-tabTitleh3:visible').length > 0){
				if(!$("#tab-li-reviews").hasClass("expandedContentTab")){
					$('#tab-li-reviews > #tab-reviews .js-tabTitleh3').trigger("click");		
				}		
					$('html, body').animate({
            scrollTop: $("#tab-li-reviews").offset().top + 'px'
        }, 'fast');
					return false;					
			}
			else{ 
				if($("#longscroll-subseries").length > 0){
					console.info("nav: " + $(".tabbed-nav-wrapper").height());
					$('html, body').animate({
            scrollTop: ($("#tab-li-reviews").offset().top - ($(".tabbed-nav-wrapper").height() * 2)) + 'px'
					}, 'fast');
					return false;						
				}
				else{
					$('#tab-li-reviews > #tab-reviews').trigger("click");				
				}
			}
    }
  }); 
  $BV.ui( 'qa', 'show_questions', {
      doShowContent : function () { 
		//$('.reviews-tabs li #question-answer').trigger("click");
			if($("#longscroll-subseries").length > 0){
					$('.reviews-tabs li #question-answer').trigger("click");					
			}
			else{
				$('#tab-li-reviews > #tab-reviews').trigger("click");				
			}		
		
      }
   });
});   
 }  
  if(location.href.indexOf('write-review=1') > 0){
	$BV.ui("rr", "submit_review");
	} 
	
}	

$(function(){
    

});
</script> 
