            <script type="text/javascript">
  var hmdInvodo = false;
  function playInvodo(sku, elementID) {
    if(!hmdInvodo)
      {
        $.getScript( "http://e.invodo.com/4.0/s/lenovo_sm.js", function( data, textStatus, jqxhr ) {
        $("#" + elementID).css('text-align', 'center'); // center the video
      setTimeout("handleInvodo('" + sku + "', '" + elementID + "')", 200);
      hmdInvodo = true;
        });
    }
    else
    {
    $("#" + elementID).css('text-align', 'center'); // center the video
    setTimeout("handleInvodo('" + sku + "', '" + elementID + "')", 200);
      }
  }

  function handleInvodo(sku, elementID) {
    if ( typeof (Invodo) != "undefined") {
      Invodo.Conversion.send("pageView", {
        mpd : sku,
        pd : sku,
        pageName : s.eVar36,
        selector : elementID
      });
      createInvodoIframe(elementID, sku);
    }
  }

  function videoFound(elementID, data) {
    document.getElementById("invodo_ifr").style.display = "block";
  }

  function hmdCloseHandler() {
    if (document.getElementById("invodo_ifr") != null) {
      document.getElementById("invodo_ifr").src = "/ISS_Static/site/blank.html";
    }
  }

  function createInvodoIframe(elem, sku) {
    if (document.getElementById("invodo_ifr") != null) {
      $("#invodo_ifr").remove();
    }
    document.getElementById(elem).innerHTML = "";
    var ifr = document.createElement("IFRAME");
    ifr.setAttribute("id", "invodo_ifr");
    ifr.setAttribute("style", "border:0px;");
    ifr.setAttribute("width", "320");
    ifr.setAttribute("height", "180");
    ifr.setAttribute("scrolling", "no");
    ifr.style.display = "none";
    var canonUrl = $('link[rel="canonical"]').filter(":first").attr("href"); //requires canonical URL definition on the page
    document.getElementById(elem).setAttribute("style", "width:320px; margin:0px auto; display:block;");
    document.getElementById(elem).appendChild(ifr);
    document.getElementById(elem).style.display = "block";
    var ifrBase = "/ISS_Static/WW/omniture/r17/globalcontainer/invodo_hmd_player.html";
    ifrBase += "?sku=" + encodeURI(sku) + "&canonUrl=" + encodeURI(canonUrl);
    ifrBase += "&pageName=" + encodeURI(s.eVar36);
    ifrBase += "&elementid=" + encodeURI(elem);
    ifr.src = ifrBase;
  }

  function addPNOIDMapping(pn) {//maps cto/mtm number to category oid
    getWindowPropInstance().setProperty(pn, "94330295E03F180812A3117013C04BD8");
  }
</script> 
