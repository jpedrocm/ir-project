	$('.popLink').fancybox({
				padding: '15px',
				margin: 0,
				titleShow: false,
				showNavArrows: false,
				autoScale: false,
				transitionIn: 'fade',
				transitionOut: 'fade',
				overlayColor: '#000',
				overlayOpacity: '.75',
				centerOnScroll: true
			});
});

$(window).one("scroll", function(){
	var maxHeight = Math.max.apply(null, $(".presel").map(function ()
	{
			return $(this).height();
	}).get());

	if(maxHeight > 0)
	{
		$('.presel').height(maxHeight);
	}

});


</script> 
