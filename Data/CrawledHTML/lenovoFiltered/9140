.lenovo-pref .link:hover {text-decoration:underline;}
</style> 
         <script>
    
    //Lenovo Pref Card Series Link

    $(document).ready(function () {
        if($('meta[name="TaxonomyType"]').attr('content') == "SeriesPage") {
