         <script type="text/javascript">
function initEmailSignup() {

	   //$('li.email_menu > a').attr('rel','modal');   
		$('li.email_menu .menu_2_wrapper .menu_2 > li a').fancybox({
			'type':'iframe',			
			onComplete: function(){
			   $('#fancybox-content').css({'height':'650px','background':'#fff'});
			   $('#fancybox-close').css({'right':'-30px'});
			   $('li.email_menu').removeClass("opened");
			}
		});
}
$(document).ready(function() {
	if(document.querySelector("#facetedBrowseWrapper") == null && document.querySelector(".espot-sidebar-list-large") == null && document.querySelector(".sidebarNav") == null)
	{
		$('.bar_3-leftLink').css('display','none');
	}

	$('#truste').replaceWith('<div style="text-align:right;"><a href="//privacy.truste.com/privacy-seal/validation?rid=3de9ef3e-221f-48c5-827e-399a67246cef" title="TRUSTe Privacy Certification" target="_blank"><img style="border: none" src="//privacy-policy.truste.com/privacy-seal/seal?rid=41d28302-f79e-4c18-86d1-171a1b7e887a" alt="TRUSTe Privacy Certification"/></a></div>');

	//Re-branding Free shipping and overlay
	$('.bar_3-logos').find('.left a, #freeship > a').removeClass('lightbox').addClass('free_shipping');
	$('.bar_3-logos').find('.left > a, #freeship > a').attr('href','/ISS_Static/WW/wci3/us/en/deals/info/freeshipping_overlay.html');
	$('.bar_3-logos').find('.left > a >img, #freeship > a >img').attr('src','//www.lenovo.com/images/OneWebImages/Common/NEW-free-shipping-offer-en_US.png');
	$('.bar_3-logos').find('.left > a, #freeship > a').attr('rel','modal');
	
	//$('.bar_3-logos').find('.left .free_shipping').after('<a class="free-return" href="/ISS_Static/WW/wci3/us/en/deals/info/freereturn_overlay.html"><img src="//www.lenovo.com/images/OneWebImages/Common/NEW-free-returns-offer-en_US.png" /></a>');
		
	
		initEmailSignup();		
	
	
		$('.newsletterInput').submit(function() {
		     var cus_email = $('.newsletterInput .newsletterInput-text').val();		
            var url = $(this).attr("action") + "&email=" + (encodeURIComponent(cus_email));			
            $.fancybox({
                href: url,
                'type': 'iframe',
				onComplete: function(){
				$('#fancybox-content').css({'height':'650px','background':'#fff'});
				$('#fancybox-close').css({'right':'-30px'});
				}
            }); 
			return false;			
        });
	
	//update text for mobile narrow your search
	$("#facet-area .text.short-label").text("See Results");
	
	//add class to discover lenovo banner so it can be hidden on mobile	
	$("li.product_menu ul.menu_2").next().addClass("discover");
 });
</script> 
