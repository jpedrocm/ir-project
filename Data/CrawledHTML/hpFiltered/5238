  <script>
			function signinRedirect() {
				window.location = 'LogonForm?storeId=10151&langId=-1&catalogId=10051&inkTonerCategoryId=88337';
			}

			var len = subcategory.length;
			var TopParent = '';
			var sectionname;
			var pageName;
			var subsection;
			var pagelevelvalue;
			var tempsubsection = subcategory1[len-2];
			
			if (TopParent == null || TopParent == '') {
				sectionname = 'Laptops';
				pageName = 'Laptops';
				subsection = 'Laptops';
				pagelevelvalue = 'clp';
			} else  if (pagelevels == 'mdp') {
				sectionname = '';
				pageName = 'Laptops';
				subsection = '';
				pagelevelvalue = 'mdp';
			} else {
				sectionname = '';
				pageName = 'Laptops';
				subsection = 'Laptops';
				pagelevelvalue = 'mlp';
			}
			if (sectionname == null || sectionname == '') {
				tempsubsection = tempsubsection.toLowerCase();
				if (tempsubsection.indexOf('laptop') != -1) {
					sectionname = 'Laptops';
				} else if (tempsubsection.indexOf('desktop') != -1) {
					sectionname = 'Desktops';
				} else if (tempsubsection.indexOf('printer') != -1) {
					sectionname = 'Printers';
				} else if (tempsubsection.indexOf('tablet') != -1) {
					sectionname = 'Tablets';
				} else if (tempsubsection.indexOf('accessories') != -1) {
					sectionname = 'Accessories';
				} else if (tempsubsection.indexOf('ink') != -1) {
					sectionname = 'Ink and Toner';
				}
			}
			var userId = '-1002';
			var usertype = '';
			var CID = "";
			var businessUnit = getBusinessUnit(sectionname);
			var hp_profileId = '';
			var language = 'en_US';
			var countryname = getcountry(language);
			var websectionId = getwebsectionId(language);
			var aoid = '';
			
			function omniture_Tag(Analyitc_usertype) {
				usertype = Analyitc_usertype;
				categoryTag(sectionname, userId, businessUnit, usertype, pageName, countryname, websectionId, hp_profileId, subsection, pagelevelvalue, CID,aoid);
			}

			HP.Utils.ready( function() {
				//dynamically generated from used molecules	
				HP.Control.init(HP.Tooltip, '.tooltip', {});	
			});
			
			$(document).ready(function() {
				if ('' == 'Y') {
					var Legacy = 'y';
					$.ajax({
						url: getAbsoluteURL() + 'HPLegacyCmd?storeId=10151&catalogId=10051&langId=-1',
						success: function(serviceResponse_json) {
							// Add code here
						}
					});
				}
				shoppingActionsServicesDeclarationJS.setCommonParameters('-1', '10151', '10051');
			});
			
			function numberWithCommas(x) {
				return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
			}
			
			function tabclick(current, tabval) {
				if (typeof trackMetrics != 'undefined') {
					trackMetrics('new.page', {page: {navigation: 'tabNav:' + tabval}});
				}
			}
			
			function compare() {
				if(typeof trackMetrics != 'undefined') {
					trackMetrics( 'new.page', { page: {site:'hhos', name: 'compare products', section: 'compare', page_function:'compare'}} );
				}
			}
		</script> 
