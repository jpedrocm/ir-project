			$(document).ready(function() {
				
				$('#BVFrame').attr('src', $('#BVReviewPageURL').html());

				if(bvFlag.indexOf('false') == -1){ //do want bv
					if(!$('#BVRRContainer #BVRRSourceID').length){
						$('#BVRRContainer').hide(); 
						$('#BVReviewsContainer').show();
					}
				}else{ //do not want bv
					console.log('inside the don"t want BV');
					$('#BVRRContainer').hide(); 
					$('#BVReviewsContainer').show();
				}
				
				if(matchMedia("screen and (min-width: 496px)").matches) {				
					$(".tabsUnderlined li a").each(function(idx, li) {
						var tab = $(li);
						tab.removeClass('active');
						var sId = tab.attr('title');
						$('#'+sId).hide();
						
						if (idx == 0) {
							tab.addClass('active');
							$('#'+sId).show();
						}
					});
				} 
				
				if(window.location.hash){
					// tab name should contain alphabets only and give delay until carousel is initialized for accessories tab
					var tabTitle = window.location.hash.replace(/[\W]/g,'');
					if($('a[title='+tabTitle+']').size()>0){
						setTimeout(function(){
							$('a[title='+tabTitle+']').click();
						},1000);
					}
				}
				
				$('ul.pdp_featured_image_mobile > li').css('display','list-item');		
			});
