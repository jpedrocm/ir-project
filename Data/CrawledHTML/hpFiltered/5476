  <script>
			function signinRedirect() {
				window.location = 'LogonForm?storeId=10151&langId=-1&catalogId=10051&inkTonerCategoryId=231504';
			}

			var len = subcategory.length;
			var TopParent = '88337';
			var sectionname;
			var pageName;
			var subsection;
			var pagelevelvalue;
			var tempsubsection = subcategory1[len-2];
			
			if (TopParent == null || TopParent == '') {
				sectionname = 'All models';
				pageName = 'All models';
				subsection = 'All models';
				pagelevelvalue = 'clp';
			} else  if (pagelevels == 'mdp') {
				sectionname = 'Laptops';
				pageName = 'All models';
				subsection = 'All models';
				pagelevelvalue = 'mdp';
			} else {
				sectionname = 'Laptops';
				pageName = 'All models';
				subsection = 'All models';
				pagelevelvalue = 'mlp';
			}
			if (sectionname == null || sectionname == '') {
				tempsubsection = tempsubsection.toLowerCase();
				if (tempsubsection.indexOf('laptop') != -1) {
					sectionname = 'Laptops';
				} else if (tempsubsection.indexOf('desktop') != -1) {
					sectionname = 'Desktops';
				} else if (tempsubsection.indexOf('printer') != -1) {
					sectionname = 'Printers';
				} else if (tempsubsection.indexOf('tablet') != -1) {
					sectionname = 'Tablets';
				} else if (tempsubsection.indexOf('accessories') != -1) {
					sectionname = 'Accessories';
				} else if (tempsubsection.indexOf('ink') != -1) {
					sectionname = 'Ink and Toner';
				}
			}
			var userId = '-1002';
			var usertype = '';
			var CID = "";
			var businessUnit = getBusinessUnit(sectionname);
			var hp_profileId = '';
			var language = 'en_US';
			var countryname = getcountry(language);
			var websectionId = getwebsectionId(language);
			var aoid = '';
			
			function omniture_Tag(Analyitc_usertype) {
				usertype = Analyitc_usertype;
				categoryTag(sectionname, userId, businessUnit, usertype, pageName, countryname, websectionId, hp_profileId, subsection, pagelevelvalue, CID,aoid);
			}

