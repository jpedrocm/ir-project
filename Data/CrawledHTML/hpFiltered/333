			function compare() {
				if(typeof trackMetrics != 'undefined') {
					trackMetrics( 'new.page', { page: {site:'hhos', name: 'compare products', section: 'compare', page_function:'compare'}} );
				}
			}
		</script> 
  <div id="hpmmdframework_complete"></div> 
  <!-- reactive chat control start --> 
  <div id="MoxieChatLinks" style="color:blue; display:none;"> 
   <!-- BEGIN ChatContentRecommendation.jsp -->
   <!-- END ChatContentRecommendation.jsp --> 
  </div> 
  <script type="text/javascript">
			var MoxieCategoryContent = $('div#MoxieChatLinks').text().trim();
			if (MoxieCategoryContent !== 'undefined') {
				liveHelpURL = liveHelpURL + '&moxiecontent=' + MoxieCategoryContent;
			}
		</script> 
  <!-- reactive chat control end -->   
