          <div id="tab2"> 
           <a onclick="DCG.isitePlayerAPI.pause('#isitetv');" class="tab-toggle touch" href="#tab2"> <i id="mobileTabsIndicatorClosed" class="dcg-icon-chevron-right"></i> <i id="mobileTabsIndicatorOpen" class="dcg-icon-chevron-down"></i> Specification </a> 
           <div class="tab-content tab-tech-specs">
            <div class="content productSheet to-print"> 
             <h2 class="section-title border-b">Technical specifications for LG 49UH850V Smart 3D 4k Ultra HD HDR 49" LED TV</h2> 
             <table class="simpleTable"> 
              <caption>
               SCREEN
              </caption> 
              <tbody> 
               <tr> 
                <th scope="row">Screen technology</th> 
                <td>LED edgelit</td> 
               </tr> 
              </tbody> 
              <tbody> 
               <tr> 
                <th scope="row">4k HDR</th> 
                <td>Yes</td> 
               </tr> 
              </tbody> 
              <tbody> 
               <tr> 
