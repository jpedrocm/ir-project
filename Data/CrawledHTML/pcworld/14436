                                        resp.state.userDetails : undefined;
                                    var productAvailability = typeof resp.state.productAvailability === 'object' ?
                                        resp.state.productAvailability : undefined;
                                    var lastPostCode = resp.state.lastPostCode ?
                                            resp.state.lastPostCode : undefined;

                                    if (window.App && window.App.settings) {
                                        window.App.settings.set("userDetails",
                                                userDetails,
                                                window.App.settings.Type.USER);
