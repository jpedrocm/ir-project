                                    var userDetails = typeof resp.state.userDetails === 'object' && resp.state.userDetails.email ?
                                        resp.state.userDetails : undefined;
                                    var productAvailability = typeof resp.state.productAvailability === 'object' ?
                                        resp.state.productAvailability : undefined;
                                    var lastPostCode = resp.state.lastPostCode ?
                                            resp.state.lastPostCode : undefined;

                                    if (window.App && window.App.settings) {
                                        window.App.settings.set("userDetails",
                                                userDetails,
                                                window.App.settings.Type.USER);
                                        window.App.settings.set("productAvailability",
                                                productAvailability,
                                                window.App.settings.Type.USER);
                                        window.App.settings.set("lastPostCode",
                                                lastPostCode,
                                                window.App.settings.Type.USER);
                                        window.App.settings.set("groupcallIsLoading", false, window.App.settings.Type.USER);
                                    } else {
                                        window.GROUPCALL_RESULT = {
                                            userDetails: userDetails,
                                            productAvailability: productAvailability,
                                            lastPostCode: lastPostCode,
                                            groupcallIsLoading: false
                                        };
                                    }

                        },
                        error: function(){
                                if ( typeof R3_COMMON === 'object' ) {
                                        r3()
                                }
                        },
                        type: 'GET'
                })
                

        </script> 
