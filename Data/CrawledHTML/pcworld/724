$(document).ready(function(){

            // Prevent the variables to leak in the global space
            (function(s, events) {
                if (!s) return; // The `s` object does not exists: stop here

                // A flag to prevent the run() function to be called twice
                var contentWritten = false;

                // A counter that will allow to call the run() function after all the
                // event requests are done.
                var ajaxCounter = 0;

                // Write the content
                function run() {
                    if (contentWritten) return;
                     // Don't remove this timeout becouse it crash IE 8
                    setTimeout(function() {
                        s.t()
                    }, 0);

                    contentWritten = true;
                }

                // Ajax calls
                function request(event) {
                    $.ajax({
                        url: event.url,
                        success: function(data) {
                        event.successCallback(data);
                            if (++ajaxCounter >= events.length) run();
                        },
                        error: function() {
                            event.errorCallback();
                            if (++ajaxCounter >= events.length) run();
                        },
                        method: 'GET'
                    });
                }

                // Launch the requests
                for (var i = 0, len = events.length; i < len; i++) {
                    request(events[i]);
                }

            }(this.s, this.scData || []));
            
