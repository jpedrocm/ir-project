                    if (document.addEventListener) {
                        document.addEventListener('ltkAsyncListener', d);
                    } else {
                        e = document.documentElement; e.ltkAsyncProperty = 0; e.attachEvent('onpropertychange', function (e) {
                        if (e.propertyName == 'ltkAsyncProperty'){d();}});
