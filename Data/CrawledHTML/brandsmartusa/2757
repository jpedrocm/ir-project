        function pageShown(evt) {
            var compareChecks = document.getElementsByName("compareCheck");
            for (var i=0; i<compareChecks.length; i++)
            {
                var compareCheck = compareChecks[i];
                var prodId = compareCheck.getAttribute("prodId");
                if(''.indexOf(''+prodId) == -1) {
                    compareCheck.checked = false;
                }
            }
        }

