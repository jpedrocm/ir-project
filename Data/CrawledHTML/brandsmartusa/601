          function onPageNumEnter(event, pageNumber) {
            if(event && event.keyCode == 13){
              event.preventDefault();
              var pageN = parseInt(pageNumber);
              if (!isNaN(pageN) && pageN <= 1) {
                goPage(pageN);
              }
              return false;
            }
          }
          function goPage(pageNumber) {
            var baseNavStage = '/kitchen+appliances/cooking+packages/_/N-103262+102866';
            var pNum = pageNumber-1;
            if (pNum < 0) pNum = 0
            var startReqNum = pNum * 20;
            var NoParamNew = 'No='+startReqNum;
            var NoParamOld = 'No=0';
            if (baseNavStage.indexOf(NoParamOld) >= 0)
              location = baseNavStage.replace(NoParamOld, NoParamNew)
            else
              location = baseNavStage + (baseNavStage.indexOf('?') == -1 ? '?' : '&') + NoParamNew;
          }
