  <script type="text/javascript">
		$(document).ready(function() {
			$('#contact-subjects option[value=""]').attr('selected', 'selected');
			$('#contact-subjects').change(function(){
					var selected = $("#contact-subjects").val();
					$("#contact-subjects-forms").load("includes/contact-us/" + selected + ".jsp");
					closeErrorMessage();
			 });
			$('#contact-subjects').change();
			$('#message').focus(function() {
				var defMessage = $("#defaultMessage");
				if ( typeof defMessage != "undefined" ) {
					if ( defMessage.val() == this.value) {
						this.value = '';
					}
				}
			}
			).blur(function() {
				var defMessage = $("#defaultMessage");
				if ( typeof defMessage != "undefined" ) {
					if ( '' == this.value) {
						this.value = defMessage.val();
					}
				}
			});
		});
	</script> 
