			} else {
				document.getElementById("wc-power-page").addEventListener()
				$("#wc-power-page").bind('DOMNodeInserted', function(event) {
					alert(event.type);
					if (event.type == 'DOMNodeInserted') {
						$("#menu-manu").show();
						$("#manu").show();
					}
				});
			}
*/
		};

		function countdown(){
			if($("#wc-power-page").text().trim().length > 0 ||
				$("#overview").text().trim().length > 0 ||
				$("#ccs-inline-content").text().trim().length > 0)
			{
				$("#menu-manu").show();
				$("#manu").show();
				timeLeft = 1;
			}
			timeLeft -= 1;
			if (timeLeft) {
				setTimeout(countdown, 1000);
			}
		}

countdown();
	</script> 
