							eAmnt = truncateAmt;
						$(eList).each( function() {
							var maxTextLen = eAmnt,
								textOld = $(this).text().trim(),
								noDots = (textOld.indexOf('...') <= 0);
							if (noDots && textOld.length > maxTextLen) {
