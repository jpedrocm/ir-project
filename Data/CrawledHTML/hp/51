  <script type="text/javascript">
	
	var chatUrl = 'https://h30665.www3.hp.com/netagent/cimlogin.aspx?questid=246C34C5-5B3F-42D0-9EEC-D51634B0EE3F&portid=942E5E82-25F9-4DFD-9A71-537876996383';
	var callUrl = 'https://h30665.www3.hp.com/netagent/cimlogin.aspx?questid=A54BA411-122B-4D0B-8B15-26E72DE88844&portid=21844431-8DFC-4978-B17E-AA1D3636C748';
		

	$('#lp_link').click(function(event) {
    	openHelpWindow(chatUrl);
    });
    
    $('#lh_call').click(function(event) {
    	openHelpWindow(callUrl);
    });
	
	var talCustProp = '';
	$(document).ready(function() {
		talCustProp = getHelpParams('#:Prop!');
	});
	
	function openHelpWindow(helpUrl) {
		helpUrl += getHelpParams('&') + '&nareferer=' + escape(document.location);
		window.open(helpUrl, "popupWindow", "width=625,height=750,scrollbars=yes");
	}

	function getHelpParams(delimiter) {
		var params = '';
		var cartOrderId = $.cookie('WC_CartOrderId_10151');
		if ($.trim(cartOrderId)){
			var cartCookie = $.cookie('WC_CartTotal_' + cartOrderId);
			var content = cartCookie  ? cartCookie.split(/;/) : new Array();
			if(content.length > 1) {
				params += delimiter + 'CartTotal=' + content[1];
			}
		}
		return params;
	}

	function setCookieviewTopSellers(pLabel, pVal, pMinutes) {
		var tExpDate = new Date();
		tExpDate.setTime(tExpDate.getTime() + (pMinutes*60*1000));
		document.cookie = pLabel + "=" + pVal + ((pMinutes == null) ? "" : ";expires=" + tExpDate.toGMTString()) + ";domain=.hp.com; path=/";
	}
	
	function getParameterByName(name) {
	    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
	    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
	        results = regex.exec(location.search);
	    return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
	}

	window.SCREENREADING_LABELS = {
        'open_menu_prefix': "Open ",
        'open_menu_suffix': " Menu",
        'close_menu_prefix': "Close ",
        'close_menu_suffix': " Menu",

        'opened_state_menu_prefix': "Menu ",
        'opened_state_menu_suffix': " is opened",
        'closed_state_menu_prefix': "Menu ",
        'closed_state_menu_suffix': " is closed",

        'opened_state_submenu_suffix': "Press Right Arrow or Space to open",
        'selected_country_state': "Selected",

        'open_popup_prefix': "Open ",
        'open_popup_suffix': " Dialog",
        'close_popup_prefix': "Close ",
        'close_popup_suffix': " Dialog",

        "expand_prefix": "Expand ",
        "expand_suffix": "list",
        "collapse_prefix": "Collapse",
        "collapse_suffix": "list",
        "search_box_suggestion_title": "Use arrow keys to select a search suggestion"
    };
    
	
		HP.hfInit({
		    countrySelectorURL: "http://www8.hp.com/caas/header-footer/us/en/default/3.2.r/footer/country-selector?contentType=jsonp",
		    countryCode:  "us",
		    languageCode: "en",
		    imageGridTopMenu:false,
		    
	        autocompleteParams: [
	        {
	            "type": "hho-store",
	            "title": "HP Store",
	            "inProgressHTML": "Searching HP Store ...",
