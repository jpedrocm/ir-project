	            "count": 5,
	            "actionURL": "/us/en//SearchDisplay?storeId=10151&catalogId=10051&langId=-1&searchTerm=@@term@@&beginIndex=0&pageSize=50&pageView=grid&resultCatEntryType=2&sType=SimpleSearch&searchSource=Q",
	            "url": "/us/en//SearchComponentAutoSuggestView?coreName=MC_10001_CatalogEntry_en_US&serverURL=/us/en//solr\/MC_10001_CatalogEntry_en_US&langId=-1&storeId=10151&catalogId=10051&showHeader=true&lang=en",
