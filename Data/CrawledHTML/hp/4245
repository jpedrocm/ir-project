           <script type="text/javascript">
	var _searchBasedNavigationFacetContext = new Array();

	

	$(document).ready(function () {
					var facetSelected = [];
						
					

					facets = $(".content_section").find(".facet_set");
					
					
					
						facets.each(function(index, element) {
						
						
						
						facetid = $(this).attr("value");
						facetvalues=$(this).find(".filter_checkbox");
						
                                          
						facetvalues.slice(4,facetvalues.length).css("display","none");
						
						if(facetvalues.length > 4){
							
							
							$(this).find(".morelink").css("display","block");
						}
						
						facetvalues.each(function(index, element) {
							
							if($(this).find("input:checked").length > 0){
								$(this).addClass("chkd");
								
								

							}
						});

						if($.inArray(facetid,facetSelected) != -1) {
  							  
							if($(this).find(".morelink").css("display") == 'block'){
								
								
								
								facetfirst = $(this).find("#firstsection_"+facetid);
								checkedfacets = $(this).find(".filter_checkbox");	
								
								checkedfacets.sort(function(a,b) {
									if ((!$(a).hasClass("chkd")) && ($(b).hasClass("chkd")))
									{
										return 1;
									}
									else if (($(a).hasClass("chkd")) && (!$(b).hasClass("chkd")))
									{
										return -1;
									}
									return 0; 
								});
								
								checkedfacets.each(function(index, element) {
									if(m < 4){
										$(this).css("display","block");  
				 						$(facetfirst).find("#firstlistsection_"+facetid).append($(this));
										$(facetfirst).find("#firstlistsection_"+facetid).append('<div class="clear"></div>');
										}
									else if(m >= 4){
										$(this).css("display","none");
										$(facetfirst).find("#firstlistsection_"+facetid).append($(this));
										$(facetfirst).find("#firstlistsection_"+facetid).append('<div class="clear"></div>');
										}
									m++;
									});


							}
							else{
								
							}
						}else{
							

						}
						
    						
				      		 });

					/*facetmore = $(".content_section").find("#facetmore").clone(true);
					facetmore .each(function(index, element) { 
					
						if($(this).val() == 'false'){
								
								
								SearchBasedNavigationDisplayJS.toggleShowMoreNew($(this).attr('name'));
							}
						});*/
                                  });
</script> 
           <link rel="stylesheet" href="/wcsstore/HPStorefrontAssetStore/css/search-results.css"> 
           <script type="text/javascript">
Facetstring="";
var indexvalue;
var check;
var valuestring;
var valueindex;
function FacetTracking(section,value,id)
{
check=document.getElementById(id).checked;

indexvalue=Facetstring.indexOf(section);
if(check)
{
  if(indexvalue!=-1)
   {
    var length=section.length;
var pos=indexvalue+length;
var totallength=Facetstring.length;
Facetstring=Facetstring.substring(0,pos+2)+value+","+Facetstring.substring(pos+2,totallength);
 
   }
  else
   {
Facetstring=Facetstring+section+":"+"["+value+"]";

   }
}

else if(!check)
{
      orgvalue=value+",";
      valueindex=Facetstring.indexOf(value);
      valuelength=value.length;
      totalvalue1=valueindex+valuelength;
      
      
      
      if(Facetstring.charAt(totalvalue1)==',')
      {
       
       Facetstring=Facetstring.replace(orgvalue,"");

      }
      else
      {
      Facetstring=Facetstring.replace(value,"");
      }
     
      
    }
       
    return Facetstring;

}

</script> 
