      <script>
	function PopupDocument(pageURL, title,w,h) {
		var left = (screen.width/2)-(w/2);
		var top = (screen.height/2)-(h/2);
		var targetWin = window.open (pageURL, title, 'toolbar=no, location=no, directories=no, status=no, menubar=no,scrollbars=no, resizable=no, copyhistory=no, width='+w+', height='+h+', top='+top+', left='+left);
		return false;
	} 

	$(document).ready(function() {
		$(document).on('click', '.productSpecsLink', function(event) {
			$($(this).find('.productSpecs')[0]).show();
			$($(this).find('.productSpecsContent')[0]).scrollTop(0);
		});

		$(document).on('click', '.productSpecs, .closeSpecsBtn', function(event) {
			event.stopPropagation()
			event.preventDefault()
			$('.productSpecs').hide();
		});

		$(document).on('click', '.productSpecsContent', function(event) {
			event.stopPropagation()
			event.preventDefault()
		});
	});
</script> 
      <link rel="stylesheet" href="/wcsstore/HPStorefrontAssetStore/css/v2/searchCompare.css"> 
