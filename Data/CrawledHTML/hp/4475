					mDefaultSwatch = $(this).index();
			}
			swatchSwitcher_mobile.li.each(function (i) {
	    				$('img', this).attr('src', swatchSwitcher_mobile.swatch["swatch"+mDefaultSwatch]["slide"+i]);
					});
			});
	
	
