   <script type="text/javascript">
function countdown(element, days, hours, minutes, seconds,addtocartclass) {
    
    var el = document.getElementById(element);
    
    if(days != 'closed')
    {
	     var interval = setInterval(function()
	     {
	        if(seconds == 0) 
	        {
	            if(minutes == 0) 
	            {
	            	if(hours == 0)
	            	{
	            		if(days == 0)
	            		{
			                (el.innerHTML = "Deal over");
			                
					    	  if(element.indexOf('future') != -1)
						        {
						        	el.innerHTML = "Deal active!";
						        }  
						        else{
						        	$(addtocartclass).css('display','none');
						        }  
			                clearInterval(interval);
			                return;
			            }
						else 
			            {
				            days--;
				            hours = 23;
				            minutes = 59;
			    	        seconds = 59;
		    	    	}		             
	                }
	                else 
		            {
			            hours--;
			            minutes = 59;
		    	        seconds = 59;
	    	    	}
	            }
	            else 
	            {
		            minutes--;
	    	        seconds = 59;
	        	}
	        } 
	
			if(days > 0) {
	            var days_text = days + 'd';
	        } else {
	            var days_text = '';
	        }
	        
			if(hours > 0) {
	            var hours_text = hours + 'h';
	        } else {
	            var hours_text = '';
	        }
	        
	        if(minutes > 0) {
	            var minute_text = minutes + 'm';
	        } else {
	            var minute_text = '';
	        }
	        
	        if(element.indexOf('future') != -1)
	        {
	        	var preText = 'Deal starts in: ';
	        }
	        else
	        {
	        	var preText = 'Deal ends in: ';
	        }
	        
	        el.innerHTML = preText + days_text + ' ' + hours_text + ' ' + minute_text + ' ' + seconds + 's';
	        seconds--;
	    }, 1000);
    }
    else
    {
    	el.innerHTML = "Deal over";
    	  if(element.indexOf('future') != -1)
	        {
	        	el.innerHTML = "Deal active!";
	        }
			else{
	        	$(addtocartclass).css('display','none');
	        } 
    	
    }
}

$(document).ready(function(){
var catentids = PLPCatentryIdList;
for (i = 0; i < catentids.length; i++) {
	var countdownCurrentDisplayId = 'countdown_ongoing_' + catentids[i];
	var countdownFutureDisplayId = 'countdown_future_' + catentids[i];
   	
   	//Can have both ongoing and future deals for same product. Make sure both scenarios are captured.
   	countDownTime1 = $('#' + countdownCurrentDisplayId).html();
   	countDownTime2 = $('#' + countdownFutureDisplayId).html();
   	var addtocart = '.InventoryStatusAddToCart_'+catentids[i];
   	  	
   	if( (countDownTime1 != null) && (typeof countDownTime1 != 'undefined') && (countDownTime1.trim() != ''))
   	{
   		//turn on display
   		$('#' + countdownCurrentDisplayId).css('display','block');
   		
   		//Convert time to standard format
   		var a=countDownTime1.split(" ");
		var d=a[0].split("-");
		var t=a[1].split(":");
		countDownTime1= new Date(d[0],(d[1]-1),d[2],t[0],t[1],t[2]);
   		
		//var t1 = new Date(countDownTime1 + ' UTC'); //Timezone from Server is in UTC
		// "mm/dd/yyyy hh:mm:ss" 
		var dateformatted = d[1] + "/" + d[2] + "/" + d[0] + " " + t[0] + ":" + t[1] + ":" + t[2] ;
		var t1 = new Date(dateformatted + ' UTC')
		var t2 = new Date();
			
		// get total seconds between the times
		var delta = (t1 - t2) / 1000;
		
		if(delta > 0) //Deal still active
		{
			// calculate (and subtract) whole days
			var days = Math.floor(delta / 86400);
			delta -= days * 86400;
			
			// calculate (and subtract) whole hours
			var hours = Math.floor(delta / 3600) % 24;
			delta -= hours * 3600;
			
			// calculate (and subtract) whole minutes
			var minutes = Math.floor(delta / 60) % 60;
			delta -= minutes * 60;
			
			// what's left is seconds
			var seconds = Math.floor(delta % 60);  // in theory the modulus is not required
			countdown(countdownCurrentDisplayId, days, hours, minutes, seconds,addtocart);
		}
		else //Deal expired
		{
			countdown(countdownCurrentDisplayId, 'closed', hours, minutes, seconds,addtocart);
		}
	}
	if( (countDownTime2 != null) && (typeof countDownTime2 != 'undefined') && (countDownTime2.trim() != ''))
   	{
   		//turn on display
   		$('#' + countdownFutureDisplayId).css('display','block');  //Timezone from Server is in UTC
   		
   		//Convert time to standard format 
   		var a=countDownTime2.split(" ");
		var d=a[0].split("-");
		var t=a[1].split(":");
		countDownTime2= new Date(d[0],(d[1]-1),d[2],t[0],t[1],t[2]);
		  
		//var t1 = new Date(countDownTime2 + ' UTC');
		// "mm/dd/yyyy hh:mm:ss" 
		var dateformatted = d[1] + "/" + d[2] + "/" + d[0] + " " + t[0] + ":" + t[1] + ":" + t[2] ;
		var t1 = new Date(dateformatted + ' UTC')
		var t2 = new Date();
		
		// get total seconds between the times
		var delta = Math.abs(t1 - t2) / 1000;
		
		// calculate (and subtract) whole days
		var days = Math.floor(delta / 86400);
		delta -= days * 86400;
		
		// calculate (and subtract) whole hours
		var hours = Math.floor(delta / 3600) % 24;
		delta -= hours * 3600;
		
		// calculate (and subtract) whole minutes
		var minutes = Math.floor(delta / 60) % 60;
		delta -= minutes * 60;
		
		// what's left is seconds
		var seconds = Math.floor(delta % 60);  // in theory the modulus is not required
		countdown(countdownFutureDisplayId, days, hours, minutes, seconds,addtocart);
	}
 	if (document.getElementById(catentids[i] + "|price") != null) {
                var priceamt = document.getElementById(catentids[i] + "|price").value;
                var priceamt = priceamt.split('|');
                if (priceamt != "") {
                    if (priceamt.length == 2) {
                        if (priceamt[0] != "Price as configured") {
                            x = document.getElementsByName(catentids[i] + "wholeprice");
                            for (j = 0; j < x.length; j++) {
                                x[j].innerHTML = priceamt[0];
                            }
                        } else {
                            x = document.getElementsByName(catentids[i] + "configPriceHolder");
                            for (j = 0; j < x.length; j++) {
                                x[j].style.display = 'none';
                            }
                            x = document.getElementsByName(catentids[i] + "wholepriceConfig");
                            for (j = 0; j < x.length; j++) {
                                x[j].innerHTML = priceamt[0];
                            }
                        }
                    } else if (priceamt.length == 3) {
                        x = document.getElementsByName(catentids[i] + "wholeprice");
                        for (j = 0; j < x.length; j++) {
                            x[j].innerHTML = priceamt[0];
                        }
                        x = document.getElementsByName(catentids[i] + "decimalprice");
                        for (j = 0; j < x.length; j++) {
                            x[j].innerHTML = priceamt[1];
                        }
                    } else if (priceamt.length == 4) {
                        x = document.getElementsByName(catentids[i] + "startingat");
                        for (j = 0; j < x.length; j++) {
                            x[j].innerHTML = priceamt[0];
                        }
						if(priceamt[1] != "" && priceamt[2] != ""){
	                        x = document.getElementsByName(catentids[i] + "wholeprice");
	                        for (j = 0; j < x.length; j++) {
	                            x[j].innerHTML = priceamt[1];
	                        }
	                        x = document.getElementsByName(catentids[i] + "decimalprice");
	                        for (j = 0; j < x.length; j++) {
	                            x[j].innerHTML = priceamt[2];
	                        }
						}else{
							x = document.getElementsByName(catentids[i] + "wholeprice");
							for (j = 0; j < x.length; j++) {
								x[j].innerHTML = priceamt[0];
							}
							x = document.getElementsByName(catentids[i] + "decimalprice");
							for (j = 0; j < x.length; j++) {
								x[j].innerHTML = priceamt[1];
							}
						}
                    } 
                } else {
                    x = document.getElementsByName(catentids[i] + "priceDiv");
                    for (j = 0; j < x.length; j++) {
                        x[j].style.display = 'none';
                    }
                }
 	} 	
 	if (document.getElementById(catentids[i] + "|promoPrice") != 'undefined' && document.getElementById(catentids[i] + "|promoPrice") != null) {
                var promoPrice = document.getElementById(catentids[i] + "|promoPrice").value;                
                var promoPriceSplitVal = promoPrice.split('.');   
                var originalPrice = document.getElementById(catentids[i] + "|originalPrice").value;
                var origPriceSplitVal = originalPrice.split('.');                
				var baseDiv = document.getElementsByName("base_"+catentids[i]);				
				var oldPriceDiv = document.getElementsByName("oldPriceFlow_"+catentids[i]);				
				var pstDiv = document.getElementsByName("PST_"+catentids[i]);
				var baseDecimalDiv = document.getElementsByName("baseDecimal_"+catentids[i]);
				var promoDiv = document.getElementsByName("promo_"+catentids[i]);
				var promoDecimalDiv = document.getElementsByName("promoDecimal_"+catentids[i]);	
				for ( var j = 0; j < pstDiv.length; j++) {
                   pstDiv[j].style.display = 'block';
				}
				for ( var j = 0; j < oldPriceDiv.length; j++) {
                    oldPriceDiv[j].style.display = 'none';
				}
				for ( var j = 0; j < baseDiv.length; j++) {
					baseDiv[j].innerHTML = origPriceSplitVal[0];
				}
				if (origPriceSplitVal[1] == '00' || origPriceSplitVal[1] == null) {					
					for ( var j = 0; j < baseDecimalDiv.length; j++) {
						baseDecimalDiv[j].innerHTML = '00';
					}
				} else if (origPriceSplitVal[1].length == 1) {					
					for ( var j = 0; j < baseDecimalDiv.length; j++) {
							baseDecimalDiv[j].innerHTML = origPriceSplitVal[1] + '0';
					}
				} else {					
					for ( var j = 0; j < baseDecimalDiv.length; j++) {
							baseDecimalDiv[j].innerHTML = origPriceSplitVal[1];
					}		
				}
				for ( var j = 0; j < promoDiv.length; j++) {
					promoDiv[j].innerHTML = promoPriceSplitVal[0];
				}
				if (promoPriceSplitVal[1] == '00' || promoPriceSplitVal[1] == null) {					
					for ( var j = 0; j < promoDecimalDiv.length; j++) {
						promoDecimalDiv[j].innerHTML = '00';
					}
				} else if (promoPriceSplitVal[1].length == 1) {					
					for ( var j = 0; j < promoDecimalDiv.length; j++) {
							promoDecimalDiv[j].innerHTML = promoPriceSplitVal[1] + '0';
					}
				} else {					
					for ( var j = 0; j < promoDecimalDiv.length; j++) {
							promoDecimalDiv[j].innerHTML = promoPriceSplitVal[1];
					}		
				}                
 	}
}


	if(PLPCatentryIdList.length > 0)
	{
		checkInventory();
	}

});


function checkInventory()
{
	$.ajax
	({
		url: 'CLPStdView?catalogId=10051&langId=-1&storeId=10151&itemIdList=' + PLPCatentryIdList.toString(), 
	    success: function(invData) 
	    {
			//invData = { "skus": [{ "id": "10316", "prodId": "10315", "partNumber": "F0H05UA", "status": "Available", "thresholdValue ": "",	"belowThreshold": "false", "availableQuantity": "963.0"},{ "id": "246064", "prodId": "246064", "partNumber": "F0H05UA", "status": "Available", "thresholdValue ": "",	"belowThreshold": "false", "availableQuantity": "0.0"}] };
			data = $.parseJSON(invData);
		   	$.each(data.skus, function(i, item) 
		   	{
		       	status = item.status;
		       	availQty = item.availableQuantity;

		       	if($($('#countdown_ongoing_' + item.id)[0]).text() != "Deal over"){
		        	if( (status.localeCompare("Available") != 0) || (availQty < 1))    
		        	{
		        		//out of stock
		        		$('#InventoryStatusAddToCart_' + item.id).css('display','none');
		        		$('#InventoryStatusOOS_' + item.id).css('display','block');
		        		$('#progress_n_rdm_' + item.id).css('display','none');
		        		$('#outofstockimg_' + item.id).css('display','block');
		        		$('#outofstockimgtxt_' + item.id).html('Out of Stock');
		        		if($('#outofstockimgtxt_' + item.id).length != 0){
							$('#oosspan_'+ item.id).css('opacity',.25);
						}
		        	}
		        	else{
		        		//In stock
		        		$('#InventoryStatusAddToCart_' + item.id).css('display','block');
		        		$('#InventoryStatusOOS_' + item.id).css('display','none');
		        		$('#progress_n_rdm_' + item.id).css('display','block');
		        		$('#outofstockimg_' + item.id).css('display','none');
		        		$('#oosspan_'+ item.id).css('opacity',1);
		        	}
		        }
		    });
	    }
	});
}




