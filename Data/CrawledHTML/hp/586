    var soldOutPromoIds = [];
	
	var hide="visible";
	if(document.hasFocus()==false){
		hide="blur";
	}
	var focusBackRefreshInv = 0;
	var focusBackRefreshRdmpt = 0;
    $( document ).ready(function() 
    {	
		checkBrowserStatus();
    	getRedeemPercentAjax(); //initial load
    	var interval = setInterval(function() 
		{	
			if(hide=='focus'||hide=='visible'){
				checkInventory(); 
			}
			else{
				focusBackRefreshInv = 1;
			}
		}, 903000);
    	var interval2 = setInterval(function() 
		{
			if(hide=='focus'||hide=='visible'){
					getRedeemPercentAjax(); 
			}
			else{
				focusBackRefreshRdmpt = 1;
			}
		   
		}, 900000);
		
		var browserRefInt = setInterval(function() 
		{
		   location.reload();
		}, 3600000);
    });
	
	function reloadCallInvoke(){
		if (focusBackRefreshRdmpt==1){
			getRedeemPercentAjax(); 
			focusBackRefreshRdmpt=0;
		}
		if (focusBackRefreshInv==1){
			checkInventory();
			focusBackRefreshInv=0;
		}
		
	}
    
	function checkBrowserStatus()
    {
			var hidden = "hidden";

		  // Standards:
		  if (hidden in document)
			document.addEventListener("visibilitychange", onchange);
		  else if ((hidden = "mozHidden") in document)
			document.addEventListener("mozvisibilitychange", onchange);
		  else if ((hidden = "webkitHidden") in document)
			document.addEventListener("webkitvisibilitychange", onchange);
		  else if ((hidden = "msHidden") in document)
			document.addEventListener("msvisibilitychange", onchange);
		  // IE 9 and lower:
		  else if ("onfocusin" in document)
			document.onfocusin = document.onfocusout = onchange;
		  // All others:
		  else
			window.onpageshow = window.onpagehide
			= window.onfocus = window.onblur = onchange;

		  function onchange (evt) {
			var v = "visible", h = "hidden",
				evtMap = {
				  focus:v, focusin:v, pageshow:v, blur:h, focusout:h, pagehide:h
				};

			evt = evt || window.event;
			if (evt.type in evtMap){
			  document.body.className = evtMap[evt.type];
			  hide = evtMap[evt.type];
			  console.log(hide);
			  }
			else{
			  document.body.className = this[hidden] ? hide ="hidden" : hide ="visible";
			  console.log(hide);
			  }
			  if(hide=="visible"){
				  reloadCallInvoke();
			  }
	  }

		$(window).blur(function(){
		  console.log("blur");
		  hide="blur";
		});
		$(window).focus(function(){
		  console.log("focus");
		  hide="focus";
		  reloadCallInvoke();
		});
		
	}
	
	
    function getRedeemPercentAjax()
    {	
    	getInputForRedeemStats();
    	if(fd.length>20){
	    	$.ajax({
				      url: "HPFlashDealsStatsCmd?storeId=10151&v=2",
				      type:"POST",
				      data:{"requestData":fd},
				      async: true
			    }).done(function(data) {
				    if(data.length!=0){
				        //console.log(data);
				        var obj = JSON.parse(data);
				        for(i=0;i<obj.length; i++)
					        {
					        	//alert(obj[i]['RDMDPCT'] + ' ' + obj[i]['PROMOTION_ID']);
					        	var intpercent = (Math.round( obj[i]['RDMDPCT'] )) + '%';
						        	if(Math.round( obj[i]['RDMDPCT'] )>=20 && Math.round( obj[i]['RDMDPCT'] )<99){
							        	var intpercentRemaining = 100-(Math.round( obj[i]['RDMDPCT'] ))+'%';
							        	var div_classname = '#redeem_ongoing_' + obj[i]['PROMOTION_ID'];
							        	var div_classname_parent = '#progress_' + obj[i]['PROMOTION_ID']+ '_parent';
							        	var div_classname_disp = 'div.redeem_ongoing_' + obj[i]['PROMOTION_ID'] + '_disp';	
							        	var div_classname_progress = '#redeem_ongoing_text_' + obj[i]['PROMOTION_ID'];	
							        	//$(div_classname).attr('aria-valuenow',intpercent);
							        	$(div_classname).css('width',intpercent);
							        	$(div_classname_parent).css('display','block');
							        	$(div_classname_progress).html('Only '+intpercentRemaining +' remaining!');
							  	        $(div_classname_progress).css('display','block');
							  	        
						  	        }
						  	        else if (Math.round( obj[i]['RDMDPCT'] )>=100){
						  	        	soldOutPromoIds.push(obj[i]['PROMOTION_ID']);
							        	var intpercentRemaining = '0%';
							        	var div_classname = '#redeem_ongoing_' + obj[i]['PROMOTION_ID'];
							        	var div_classname_parent = '#progress_' + obj[i]['PROMOTION_ID']+ '_parent';
							        	var div_classname_disp = 'div.redeem_ongoing_' + obj[i]['PROMOTION_ID'] + '_disp';	
							        	var div_classname_progress = '#redeem_ongoing_text_' + obj[i]['PROMOTION_ID'];	
							        	//$(div_classname).attr('aria-valuenow',intpercent);
							        	$(div_classname).css('width',intpercent);
							        	$(div_classname_parent).css('display','none');
							        	$(div_classname_progress).html('Sold Out');
							  	        $(div_classname_progress).css('display','block');
							  	        var div_prg_n_ct = '#prg_n_count_'+obj[i]['PROMOTION_ID'];
							  	        $(div_prg_n_ct).children('[id^=countdown_ongoing]').css('display','none');
							  	        var add_cart_parent_div = '#add_cart_parent_'+obj[i]['PROMOTION_ID'];
							  	        $(add_cart_parent_div).css('display','none');
							  	        $('#outofstockimg_' + obj[i]['PROMOTION_ID']).css('display','block');
							  	        $('#outofstockimgtxt_' + obj[i]['PROMOTION_ID']).html('Sold Out');
										$('#img_'+ obj[i]['PROMOTION_ID']).css('opacity',.25);
									}
					        }
				        }
			    });
		    }
    }
    
    function getInputForRedeemStats(){
	    var objJSON = JSON.parse(fd);
    	for(var i in objJSON)
		{		
		    var promoid = objJSON[i]['PX_PROMOTION_ID']; 
		    if($.inArray(promoid, soldOutPromoIds) > -1){
		     	objJSON.splice(i, 1);
		    }
		}
		fd = JSON.stringify(objJSON);
    }
</script> 
