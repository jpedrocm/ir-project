  <script id="postload" class="mobify">var postload = function () {
    var capturing = window.Mobify && window.Mobify.capturing || false;

    if (capturing) {
       

        // Grab reference to a newly created document
        Mobify.Capture.init(function(capture){
            var capturedDoc = capture.capturedDoc;
            
            // Render source DOM to document
            capture.renderCapturedDoc();
        });

    }
}; postload();
</script> 
  <script>
function setDetails()
{

var firstName=document.getElementById("firstName");
firstName.setAttribute('value',document.getElementById('name').value);
var LastName=document.getElementById("lastName");
LastName.setAttribute('value',document.getElementById('last_name').value);
var fromemailId=document.getElementById("fromemailId");
fromemailId.setAttribute('value',document.getElementById('email_field').value);
var phoneNumber=document.getElementById("phoneNumber");
var phoneconcat=document.getElementById('paymentphone1').value+"-"+document.getElementById('paymentphone2').value+"-"+document.getElementById('paymentphone3').value;
if(document.getElementById('phone_2').value.length > 0)
{
	phoneconcat=phoneconcat + " Ext:" + document.getElementById('phone_2').value;
}
phoneNumber.setAttribute('value',phoneconcat);
var country=document.getElementById("country");
country.setAttribute('value',document.getElementById('country_field').value);
var orderNumber=document.getElementById("orderNumber");
orderNumber.setAttribute('value',document.getElementById('order_number_field').value);
var body=document.getElementById("body");
body.setAttribute('value',document.getElementById('question').value);
var subject=document.getElementById("subject");
var subject1=document.getElementById("subject1");
var mainsubject=document.getElementById("mainsubject");
var valueofsubject=document.getElementById('contact_to');
var valueofsub=document.getElementById('dept_issue');
subject1.setAttribute('value',valueofsub.options[valueofsub.selectedIndex].text);
mainsubject.setAttribute('value',valueofsubject.options[valueofsubject.selectedIndex].text);
subject.setAttribute('value',valueofsub.options[valueofsub.selectedIndex].text);

var firstname=document.getElementById('name').value;
var lastname=document.getElementById('last_name').value;
var subject=document.getElementById('contact_to').value;
var email=document.getElementById('email_field').value;
var comments=document.getElementById('question').value;
var deptissue=document.getElementById('dept_issue').value;
var validvalue=validateFront();

if(validvalue==true)
{

document.LiveChat.submit();

}
else
{

}
}
