  <script>
	var pagelevels='mdp';

	function preloadImg(b, e, a) {
		if (!b) return alert("Error: No images declared for preload."), !1;
		!0 != b instanceof Array && (b = [b]);
		jQuery(b).each(function(a) {
			var g = jQuery("<img />").attr("src", b[a]);
			g.load(function() {
				jQuery("body").append(
					'<div id="temp-img" style="position:absolute; left:-9999em;"></div>'
				);
				jQuery("#temp-img").append(g);
				var d = jQuery("#temp-img img").height();
				jQuery("#temp-img").remove();
				if (b.length - 1 == a && e) return e(d)
			});
		})
	}

	function PopupCenter(pageURL, title,w,h) {
		var left = (screen.width/2)-(w/2);
		var top = (screen.height/2)-(h/2);
		var targetWin = window.open (pageURL, title, 'toolbar=no, location=no, directories=no, status=no, menubar=no,scrollbars=no, resizable=no, copyhistory=no, width='+w+', height='+h+', top='+top+', left='+left);
	}

	(function (b) {
		b.fn.animatebg = function (e) {
			var a = {
				startFromDistance : 130,
				fps : 37.5
			};
			e && b.extend(a, e);
			return this.each(function () {
				var c = b(this);
				setTimeout(function () {
					function e(b, f) {
						c.css("background-position", "center -" + b * d + "px");
						b != f - 1 && setTimeout(function () {
							newCurrent = b + 1;
							e(newCurrent, f)
						}, 1E3 / a.fps);
					}
					var d = c.height(),
					h = c.offset().top - a.startFromDistance;
					preloadImg(c.attr("data-sprite"), function (a) {
						c.css("background-image", "url(" + c.attr("data-sprite") + ")");
						var f = a / d,
						k = !1;
						b(window).scroll(function () {
							var a = b(window).scrollTop();
							a >= h ? k || (k = !0, e(0, f)) : a >= h - 200 && !0 == k && (k = !1, c.css("background-position", "center 0"))
						})
					})
				}, 1E3);
			});
		};
	})(jQuery);

	function getUrlParameters(parameter, staticURL, decode){
		var currLocation = (staticURL.length)? staticURL : window.location.search, parArr = currLocation.split("?")[1].split("&"), returnBool = true;
		for(var i = 0; i < parArr.length; i++){
			parr = parArr[i].split("=");
			if(parr[0] == parameter){
				return (decode) ? decodeURIComponent(parr[1]) : parr[1];
				returnBool = true;
			}
			else{
				returnBool = false;
			}
		}
		if(!returnBool) return false;
	}
	
	function showFEA() {
		$('#backLinkDiv').parent("._nav_back_mobile_wrapper").hide();
		$("#vao").removeClass("active");
		$("#viewalloption").hide();
		$("#fea").addClass("active");
		$("#features").show();
		tabEventTrackMetrics("tab:features", "features");
	}

	function showVAO() {
		$('#backLinkDiv').parent("._nav_back_mobile_wrapper").hide();
		$("#fea").removeClass("active");
		$("#features").hide();
		$("#vao").addClass("active");
		$("#viewalloption").show();
		bundleFunc();
		tabEventTrackMetrics("tab:viewalloption", "viewalloption");
	}
	
	function bundleFunc() {
		$('.bundle.image_pager').each(function () {
			var $this = $(this);
			if ($this.attr('id') == null) {
				var idx = $this.index();
				$this.attr('id', 'bund' + idx);
				var prev = $this.find('.pager-prev').attr('id', 'pager-prev' + idx);
				var next = $this.find('.pager-next').attr('id', 'pager-next' + idx);
				
				setTimeout(function () {
					$('#bImg_' + $this.data('id') + '.main_image_slider').bxSlider({
						pagerCustom : '#bund' + idx + ' .subslider',
						mode : 'fade',
						controls : false,
					});
				}, 100);
				
				setTimeout(function () {
					$this.find('.subslider').bxSlider({
						minSlides: 3,
						maxSlides: 3,
						slideWidth: 68,
						slideMargin: 8,
						infiniteLoop: false,
						hideControlOnEnd: true,
						pager: false,
						nextSelector: next,
						prevSelector: prev,
					});
				}, 100);
			}
		});
	}
	
	function getScrollLeft(el) {
		if (el != null && el.position() != null) { 
			return el.position().left + el.width() - 50;
		} else {
			return 0;
		}
	}

	function getScrollTop(el) {
		if (el != null && el.position() != null) { 
			return el.position().top + el.height() + 5;
		} else {
			return 0;
		}
	}
	
	function setToolTipPosition() {
		$('#ModelRatingContainer .ratings.tooltip').css('left', getScrollLeft($('#ModelRatingContainer .ratings.tooltip_main')));
		$('#ModelRatingContainer .ratings.tooltip').css('top', getScrollTop($('#ModelRatingContainer .ratings.tooltip_main')));
		$('#ModelRatingContainer .reviews.tooltip').css('left', getScrollLeft($('#ModelRatingContainer .reviews.tooltip_main')));
		$('#ModelRatingContainer .reviews.tooltip').css('top', getScrollTop($('#ModelRatingContainer .reviews.tooltip_main')));
	}
	
	$(document).ready(function() {
		$('#ModelRatingContainer').html($('#ModelRatingData').html());
		$("div.compare_checkbox").hide();
		$("#features").hide();
		$("#viewalloption").hide();
		
			$("#interested").hide();
			$(".sliderheader").hide();
		
		$('.i_want_to_be_always_visible').waypoint('sticky');
		$("#vao").click(function(event) { showVAO(); });
		$("#fea").click(function(event) {
			 showFEA(); 
		});
		$('span._back_to_main').click(function() {
			$('#backLinkDiv').parent("._nav_back_mobile_wrapper").show();			
		});
		$("#vdetails").click(function(event) { $("#vao").click(); });
		$(window).on('resize', function() {
			setToolTipPosition();
			if ($(window).width() > 496 && $('.tabs_slider').attr('data-tab') == 0) {
				if ($("#fea").hasClass('active')) {
					showFEA();
				} else if ($("#vao").hasClass('active')) {
					showVAO();
				}
			}
		});
		setToolTipPosition();
		if ($(window).width() < 497) {
			setTimeout(function() {
				$('.tabs_slider').attr('data-tab', 1);
				if (window.location.hash.indexOf('Tab=vao') > 0) {
					$(window).scrollTop(0);
				}
			}, 00);
		}
		if (window.location.hash.indexOf('Tab=vao') > 0) {
			showVAO();
		} else {
			 showFEA(); 
		}
		var myUrl = window.location.href;
		if (myUrl.indexOf("?") != -1){
			var searchTerm = getUrlParameters("searchTerm", "", true);
			if (searchTerm != false){
				var srpToMdpFacetValues="";
				var tempSrpToMdpFacetValues = getUrlParameters("mdpFacetValues", "", true);
				if (tempSrpToMdpFacetValues != false && tempSrpToMdpFacetValues != null){
					if (tempSrpToMdpFacetValues.indexOf("_") != -1){
						var srpToMdpFacetValues = ", " + tempSrpToMdpFacetValues.replace(/_/g, ', ');
					} else {
						var srpToMdpFacetValues = ", " + tempSrpToMdpFacetValues;
					}
					if (srpToMdpFacetValues.indexOf("\"") != -1){
						var srpToMdpFacetValues = srpToMdpFacetValues.replace(/\"/g, ' ');
					}
					if (srpToMdpFacetValues.indexOf("\+") != -1) {
						var srpToMdpFacetValues = srpToMdpFacetValues.replace(/\+/g, ' ');
					}
				}
				$("#srpToMdpFacetValueUpdate").html("Based on your search results: " + searchTerm + "" + srpToMdpFacetValues);
				$("#srpToMdpFacetValueUpdate").parent().show();
			}
		}
		if ($('#viewalloption').is(':visible')) { bundleFunc(); }
	});
</script> 
  <script>
	function takecategoryId() {
		document.cookie = 'breadCrumbModelId=308537; path=/;domain=.hp.com';
	}
</script> 
