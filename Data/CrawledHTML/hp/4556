	//start reactive chat
	var chatFlag = '';
	var phoneFlag = '';
	var emailFlag = '';
	var livehelpFlag = '';
	var contactusFlag = '';
	
	function removeChatLinks() {
		//first check for livehelp & contactus
		//if contactus is there then check for  li links
		if (typeof MoxieCategoryContent !== 'undefined') {
			var moxieChat = MoxieCategoryContent.substring((MoxieCategoryContent.indexOf(':') + 1), MoxieCategoryContent.length);
			var  chatValues = moxieChat.split(',');
			for (x in chatValues) {
				var kv = chatValues[x].split('=');
				if (kv[0].trim() === 'chat'.trim()) {
					chatFlag = kv[1].trim();
				} else if (kv[0].trim() === 'email'.trim()) {
					emailFlag = kv[1].trim();
				} else if (kv[0].trim() === 'phone'.trim()) {
					phoneFlag = kv[1].trim();
				} else if (kv[0].trim() === 'livehelp'.trim()) {
					livehelpFlag = kv[1].trim();
				} else if (kv[0].trim() === 'contactus'.trim()) {
					contactusFlag = kv[1].trim();
				}
			}
			// if chat, call & email are 0 remove livehelp & contact us
			if ((chatFlag !== 'undefined' && chatFlag === '0') && (phoneFlag !== 'undefined' && phoneFlag === '0') && (emailFlag !== 'undefined' && emailFlag === '0')) {
				contactusFlag = '0';
				livehelpFlag = '0';
			}
		}
	}

	function moxieChatLinks() {
		try {
			if (livehelpFlag !== 'undefined' && livehelpFlag === '0') {
				if ($('nav .topmenu ul.right li a.js_overlay_trigger').parent().length > 0) {
					$('nav .topmenu ul.right li a.js_overlay_trigger').parent()[0].remove();
				}
			}
			if (contactusFlag !== 'undefined' && contactusFlag === '0') {
				if ($('#lp_chat').length > 0) {
					$('#lp_chat').remove();
				}
			} else if (contactusFlag !== 'undefined' && contactusFlag === '1') {
				if (phoneFlag !== 'undefined' && phoneFlag === '0') {
					$('ul.live_help_list li a#lh_call').parent().remove();
				}
				if (chatFlag !== 'undefined' && chatFlag === '0') {
					$('ul.live_help_list li a#lp_link').parent().remove();
				}
				if (emailFlag !== 'undefined' && emailFlag === '0') {
					$('ul.live_help_list li:last').remove();
				}
			}
		} catch (e) {}
	}

	var liveHelpURL = '/us/en/LiveHelpView?storeId=10151&langId=-1&catalogId10051';
	var searchDisplayUrl = '/us/en/SearchDisplay';
	
	$(document).ready( function() {
		liveHelpURL = liveHelpURL + '&pagelevel=' + pagelevel;
		if (typeof picturefill == 'function') {
			picturefill();
		}
