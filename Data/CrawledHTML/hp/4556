			function numberWithCommas(x) {
				return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
			}
			
			function tabclick(current, tabval) {
				if (typeof trackMetrics != 'undefined') {
					trackMetrics('new.page', {page: {navigation: 'tabNav:' + tabval}});
				}
			}
			
			function compare() {
				if(typeof trackMetrics != 'undefined') {
					trackMetrics( 'new.page', { page: {site:'hhos', name: 'compare products', section: 'compare', page_function:'compare'}} );
				}
			}
