  <script language="JavaScript">
            hpmmd.page.name='ShippingAPOorFPO';
            var userId='-1002';
            var userType='';
            var hp_profileId='';
            var language='en_US';
            var CID='';
            var aoid='';
            hpmmd.browser.hpshopping2 = aoid;
            var moxievalues = '';
            var MoxieCategoryContent = '';

            function omniture_Tag(Analyitc_userType) {
                userType = Analyitc_userType;
                setCommonUserParams(userId, userType, hp_profileId);
            }

            $(document).ready(function() {
                var nonCategoryContent =  $('div#MoxieChatLinks').text().trim();
                if (nonCategoryContent !== 'undefined' && nonCategoryContent.length > 0) {
                    var nonCategoryArray = nonCategoryContent.split(';');
                    for (x in nonCategoryArray) {
                        var kv = nonCategoryArray[x].split(':');
                        if (kv[0].trim() === 'PlainContent') {
                            moxievalues = kv[1].trim();
                            break;
                        }
                    }
                }
                MoxieCategoryContent = moxievalues;
                if (MoxieCategoryContent !== 'undefined') {
                    liveHelpURL = liveHelpURL + '&moxiecontent=' + MoxieCategoryContent;
                }
            });
        </script> 
