          <section class="sales-2 tab-content column">
           <div class="row">
            <div class="small-12 column copy">
             <h2>Laptop</h2>
             <div class="row eq" data-equalizer>
              <div class="large-4 medium-6 small-12 columns panel" data-equalizer-watch data-itemid="1126663">
               <div class="panel-inner">
                <a href="http://store.hp.com/us/en/pdp/hp-spectre-x360---13-4118nr-(energy-star)">
                 <div class="img-holder text-center">
                  <img src="http://ssl-product-images.www8-hp.com/digmedialib/prodimg/lowres/c04957018.png">
