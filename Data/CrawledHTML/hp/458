  <script type="text/javascript">
	// Convert the WCParam object which contains request properties into javascript object
	var WCParamJS = {
		'storeId': '10151',
		'catalogId': '10051',
		'langId': '-1',
		'pageView': '',
		'orderBy': '',
		'orderByContent': '',
