    	openHelpWindow(callUrl);
    });
	
	var talCustProp = '';
	$(document).ready(function() {
		talCustProp = getHelpParams('#:Prop!');
	});
	
	function openHelpWindow(helpUrl) {
		helpUrl += getHelpParams('&') + '&nareferer=' + escape(document.location);
		window.open(helpUrl, "popupWindow", "width=625,height=750,scrollbars=yes");
	}

	function getHelpParams(delimiter) {
		var params = '';
		var cartOrderId = $.cookie('WC_CartOrderId_10151');
		if ($.trim(cartOrderId)){
			var cartCookie = $.cookie('WC_CartTotal_' + cartOrderId);
			var content = cartCookie  ? cartCookie.split(/;/) : new Array();
			if(content.length > 1) {
				params += delimiter + 'CartTotal=' + content[1];
			}
		}
		return params;
	}

	function setCookieviewTopSellers(pLabel, pVal, pMinutes) {
		var tExpDate = new Date();
		tExpDate.setTime(tExpDate.getTime() + (pMinutes*60*1000));
		document.cookie = pLabel + "=" + pVal + ((pMinutes == null) ? "" : ";expires=" + tExpDate.toGMTString()) + ";domain=.hp.com; path=/";
	}
	
	function getParameterByName(name) {
	    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
	    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
	        results = regex.exec(location.search);
	    return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
	}

