  <script>
			function signinRedirect() {
				window.location = 'LogonForm?storeId=10151&langId=-1&catalogId=10051&inkTonerCategoryId=183004';
			}

			var len = subcategory.length;
			var TopParent = '88340';
			var sectionname;
			var pageName;
			var subsection;
			var pagelevelvalue;
			var tempsubsection = subcategory1[len-2];
			
			if (TopParent == null || TopParent == '') {
				sectionname = 'HP LaserJet Enterprise Flow Multi-function';
				pageName = 'HP LaserJet Enterprise Flow Multi-function';
				subsection = 'HP LaserJet Enterprise Flow Multi-function';
				pagelevelvalue = 'clp';
			} else  if (pagelevels == 'mdp') {
				sectionname = 'Printers';
