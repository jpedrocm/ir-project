	var mainFacets = [
	"dte_facet_OS",
	"dte_facet_processornew",
	"dte_facet_facet_scrnsizeus"
	];
	var slideToggleTime = 100;
	var shownFacetItemSize = 5;
	var facetSetClass = 'facetSet';
	
	/* For  Price range facet  */
	var priceRangeFacetPosn ='';
	var minPrice = Math.floor(164.99);
	var maxPrice = Math.ceil(3599.0);
	var step = 10;
	
	var rangeMin = minPrice;
	var rangeMax = maxPrice;
	var isRangeSelected = false; /* To mark if there is price range selection */
	var facetUrlSeq = new Array();
	var facetUrl="";
	var tempDispSeq="";
	var facetUrlSequence="";
	var displaySeq="";
	
	function decodeHtml(input) {
		return $('<div/>').html(input).text();
	}

	function initializePriceSlider(){
		/*
		 * Slider
		 */
		$('#price-slider').slider({
		    range: true,
		    min: minPrice,
		    max: maxPrice,
		    step: step,
		    values: [rangeMin, rangeMax],
		    slide: function (event, ui) {
	       		/* indicates the price range is selected */
	       		isRangeSelected = true ;
	       		/* update the price slider html values */
				updateMinMaxPrice(ui.values[0], ui.values[1]);
		    },
		    stop: function(event, ui) {
              	/* Do the processing only if the value has changed */
		    	if(rangeMin != ui.values[0] || rangeMax != ui.values[1]){
	                /**  updating the global variables to restrict the price range in the response */
		    		rangeMin = ui.values[0];
		       		rangeMax = ui.values[1];
	                
	                /* Do the facet processing */
					changeFacets(false);
				}
            }
		});
		/* update the price slider html values */
		updateMinMaxPrice(rangeMin, rangeMax);
	}
	
	/** updates the HTML with values  */
	function updateMinMaxPrice(min,max){
		/* Labels */
		$("#min_price").html(formatPrice("$" + min));
		$("#max_price").html(formatPrice("$" + max));
		
		/* Do not set the price range unless the user had selected it 
			This is to prevent other facets to fail in cases where PST job is not set .
		 */
	    if(isRangeSelected == true ){
	    	if (min == minPrice)
	    		$("#min_price").find('#minPrice').remove;
	    	else
				$("#min_price").append($('<input/>', { 'id': 'minPrice', 'type': 'hidden', 'name': 'minPrice', 'value': min }))

	    	if (max == maxPrice)
	    		$("#max_price").find('#maxPrice').remove;
	    	else
				$("#max_price").append($('<input/>', { 'id': 'maxPrice', 'type': 'hidden', 'name': 'maxPrice', 'value': max }));
		}
	}
	
	/* Formats the price with commas */
	function formatPrice(nStr)
	{
	    nStr += '';
	    x = nStr.split('.');
	    x1 = x[0];
	    x2 = x.length > 1 ? '.' + x[1] : '';
	    var rgx = /(\d+)(\d{3})/;
	    while (rgx.test(x1)) {
	        x1 = x1.replace(rgx, '$1' + ',' + '$2');
	    }
	    return x1 + x2;
	}
	
	
	function updateSort(searchResults) {
		if (searchResults && searchResults.orderBy) {
			$('#sortForm [name=orderBy]').val(searchResults.orderBy);
		} else {
			$('#sortForm [name=orderBy]').val('');
		}
		Foundation.libs.forms.refresh_custom_select($('#sortForm.custom select[name=orderBy]'), true);

		//update clones
		$('.sortFormClone select[name=orderBy]').val($('#sortForm select[name=orderBy]').val());
		Foundation.libs.forms.refresh_custom_select($('.sortFormClone.custom select[name=orderBy]'), true);

		$('.sortFormClone :radio[name=orderBy]').prop('checked', false);
		$('.sortFormClone :radio[name=orderBy]').each(function(){
			if ($(this).val() == $('#sortForm select[name=orderBy]').val())
				$(this).prop('checked', true);
		});
	}

	function isMainFacet(facetFieldName, facetAttrId) {
		var mainFacet = false;
		mainFacets.forEach( function(item, index){
			if (item.trim() == facetFieldName.trim())
			{
				mainFacet = true;
			}else if(facetAttrId && facetAttrId.length > 0 
						&& item.trim() == facetAttrId.trim()) {
				/*Check if attribute identifier is mapped  */
				mainFacet = true;		
			
			}
		});
		return mainFacet;
	}

	function renderFacets(searchResults) {
		if (searchResults && searchResults.facets) {
			facetUrlSeq.length=0;
			facetUrl="";
			facetUrlSequence="";
			displaySeq="";
			tempDispSeq = "";
			var facetArray = searchResults.facets;
			if ((!Array.isArray && Object.prototype.toString.call(facetArray) === '[object Array]') || Array.isArray(facetArray)) {
				var facetForm = $('#facetForm');
				facetForm.empty();

				var facetsViewEle = $('<div/>', {'id': 'facetsResultsView'});
				facetsViewEle.appendTo(facetForm);

				var mainFacetEle = $('<div/>', {'class': 'mainFacets'});
				var secFacetEle = $('<div/>', {'class': 'secFacets more'});

				var facetCrumbs = $('.facetCrumbs');
				facetCrumbs.empty();
				
				var tempDispSeqArr = new Array();
				var facetMap = new Map();
				var facetSeq = '1:Screen Size;2:Processor;3:Operating System;4:Touch Screen;5:Colors;';
				var facetArr = facetSeq.split(";");
				for(var i=0; i < facetArr.length-1; i++) {
					var facetSeqArr=facetArr[i].split(":");
					facetMap.set(facetSeqArr[1].toLowerCase(),facetSeqArr[0]);
				}
				
				var facetDescMap = new Map();
				var facetDesc = 'ads_f40501_ntk_cs:scrs;ads_f41501_ntk_cs:touch;ads_f50001_ntk_cs:dtscrnsize;ads_f44501_ntk_cs:numusr;ads_f51001_ntk_cs:disp;ads_f46501_ntk_cs:prntclr;ads_f30001_ntk_cs:ordr;ads_f52001_ntk_cs:grph;x_onsale:Deal;ads_f47501_ntk_cs:feat;ads_f31001_ntk_cs:tonertyp;ads_f48501_ntk_cs:ltwt;ads_f32001_ntk_cs:opsy;ads_f49501_ntk_cs:mem;ads_f28501_ntk_cs:cat;ads_f35001_ntk_cs:form;ads_f50501_ntk_cs:aiotch;ads_f51501_ntk_cs:stor;ads_f30501_ntk_cs:inktyp;ads_f52501_ntk_cs:acccat;ads_f31501_ntk_cs:papertyp;ads_f32501_ntk_cs:pr;ads_f11501_ntk_cs:func;ads_f13501_ntk_cs:feat;ads_f42001_ntk_cs:ssd;ads_f37501_ntk_cs:bizcat;ads_f38501_ntk_cs:segm;ads_f45001_ntk_cs:connct;ads_f46001_ntk_cs:prntbrnd;ads_f47001_ntk_cs:prntdup;ads_f48001_ntk_cs:prnttyp;ads_f49001_ntk_cs:proc;';
				var facetDescArray = facetDesc.split(";");
				for(var i=0; i < facetDescArray.length-1; i++) {
					var facetDescArr=facetDescArray[i].split(":");
					facetDescMap.set(facetDescArr[0],facetDescArr[1]);
				}
				
				var facetColCnt = 0;
				for(var i=0; i < facetArray.length; i++) {
					var facet = facetArray[i];
					var facetName = facet[0];
					var facetItems = facet[1];
					var allowMultiSel = facet[2];
					var maxValToDisplay = facet[3];
					var allValuesReturned = facet[4];
					var facetFieldName = facet[5];
					var facetAttrId = facet[6];

					var sectionEle = $('<div/>', {'id': 'facet_' + facetFieldName, 'class': facetSetClass});
					sectionEle.addClass("facetAttr_"+facetAttrId);

					var headingEle = $('<h3/>', {'id': 'facetName_' + facetFieldName, 'class': 'facetName', 'text': facetName});
					headingEle.appendTo(sectionEle);

					var ulEle = $('<ul/>', {'id': 'facetItemList_' + facetFieldName, 'class': 'facetItemList'});
					ulEle.appendTo(sectionEle);
					
					var facetCrumbsVals = '';

					for(var j=0; j < facetItems.length; j++) {
						var facetItem = facetItems[j];
						var facetItemLabel = decodeURIComponent(facetItem[0]);
						var facetItemUrl = decodeURIComponent(facetItem[0]).replace(/[^a-zA-Z0-9]+/g,'-');
						var facetItemCount = facetItem[1];
						var facetItemValue = facetItem[2];
						var facetItemId = facetItem[3];
						var facetItemSelected = facetItem[4];
						var facetItemImg = facetItem[5];

						var liEle = $('<li/>', {'id': 'facetItem_' + facetItemId, 'title': 'Click for ' + facetItemLabel + ' ' + facetName + ' products', 'class': 'facetItem'});
						liEle.appendTo(ulEle);

						var lblEle = $('<label/>');
						lblEle.appendTo(liEle);

						var checkBoxEle = $('<input/>', {'id': 'facetItem_' + facetItemId, 'type': 'checkbox', 'name': 'facet', 'class': 'facetItemChkBox', 'value': facetItemValue});
						try {
							if (parseInt(facetItemCount) < 1) {
								checkBoxEle.prop('disabled', true);
							}
						} catch(error) {
						}

						if (facetItemSelected == 'true') {
							checkBoxEle.prop('checked', true);
							sectionEle.addClass('open');
							facetCrumbsVals = facetCrumbsVals + (facetCrumbsVals.length > 0 ? ", "+facetItemLabel:facetItemLabel);
							if(facetDescMap.get(facetFieldName) != undefined) {
								var facetDescUrl=facetDescMap.get(facetFieldName);
								var facetNameLower = facetName.toLowerCase();
								if(facetMap.get(facetNameLower) != undefined) {								
									if(tempDispSeqArr[facetMap.get(facetNameLower)] != undefined){
										tempDispSeqArr[facetMap.get(facetNameLower)] = tempDispSeqArr[facetMap.get(facetNameLower)]+facetItemLabel+" ";
									}else{
										tempDispSeqArr[facetMap.get(facetNameLower)] = facetItemLabel+" ";
									}
									if(facetUrlSeq[facetMap.get(facetNameLower)] != undefined) {
										facetUrlSeq[facetMap.get(facetNameLower)]=facetUrlSeq[facetMap.get(facetNameLower)]+","+facetItemUrl;
									} else {
										facetUrlSeq[facetMap.get(facetNameLower)]=facetDescUrl+"="+facetItemUrl;
									}
								} else {
									tempDispSeq = tempDispSeq+facetItemLabel+" ";
									if(facetUrl.length>0 && facetUrl[facetUrl.length-1]!=";") {
										facetUrl=facetUrl+","+facetItemUrl;
									} else {
										facetUrl=facetUrl+facetDescUrl+"="+facetItemUrl;
									}
								}
							} else{
								if(facetMap.get(facetNameLower) != undefined) {
									if(tempDispSeqArr[facetMap.get(facetNameLower)] != undefined){
									tempDispSeqArr[facetMap.get(facetNameLower)] = tempDispSeqArr[facetMap.get(facetNameLower)]+facetItemLabel+" ";
								}else{
									tempDispSeqArr[facetMap.get(facetNameLower)] = facetItemLabel+" ";
								}
								} else {
									tempDispSeq = tempDispSeq+facetItemLabel+" ";
								}							
							}
						}
						checkBoxEle.appendTo(lblEle);

						var spanEle = $('<span/>', {'class': 'facetItemLbl', 'html': facetItemLabel});
						spanEle.appendTo(lblEle);

						if (facetItemImg) {
							var imgEle = $('<img/>', {'class': 'facetItemImg', 'src': facetItemImg});
							spanEle.prepend(imgEle);
						}

						if (facetItemCount > 0) {
							spanEle = $('<span/>', {'class': 'facetItemCnt', 'html': facetItemCount});
							spanEle.appendTo(lblEle);
						}
					}
					if(facetUrl.substr(facetUrl.length-1)!=";" && facetUrl.length>0){
						facetUrl=facetUrl+";";
					}

					var facetItems = sectionEle.find('.facetItem');
					if (facetItems.length > 0) {
						var mainFacet = isMainFacet(facetFieldName, facetAttrId);
						if (!mainFacet && facetItems.length > shownFacetItemSize && facetItems.find('.facetItemChkBox:checked').length <= 0) {
							facetItems.each(function(index){
								if (index >= (shownFacetItemSize))
									$(this).addClass('more');
							});
							sectionEle.append($('<div/>', {'class': 'facetItemShow more', 'html': 'More...'}));
						}
						sectionEle.appendTo(mainFacet ? mainFacetEle:secFacetEle);
					}

					if (facetCrumbs.length > 0 && facetCrumbsVals.length > 0) {
						if (facetCrumbs.html().length > 0) {
							facetCrumbs.append(", ");
						} else {
							facetCrumbs.append(": ");
						}
						facetCrumbs.append(facetName + " (" + facetCrumbsVals + ")");
					}
					
					/*  Attach the price slider either on the configured position or as the last facet  add at the end if the position exceeds the total count*/
					if( 
						( priceRangeFacetPosn.length !=0 && (i+1 == priceRangeFacetPosn-1) )
							|| ( (priceRangeFacetPosn.length ==0  || priceRangeFacetPosn > facetArray.length ) && (i+1 == facetArray.length) ) 
							){
							
							/* Price range facet html */
							var priceSlider = $('<div/>', {'class': 'priceSlider'});
							priceSlider.append($('<div/>', { 'id': 'price-slider'}));
							priceSlider.append($('<div/>', { 'id': 'min_price', 'class': 'facetItemLbl left'}));
							priceSlider.append($('<div/>', { 'id': 'max_price', 'class': 'facetItemLbl right'}));
							priceSlider.append($('<div/>', { 'class': 'clear'}));
		
							var sectionEle = $('<div/>', {'id': 'facet_PriceRange' , 'class': facetSetClass});
							sectionEle.append($('<h3/>', {'class': 'facetName', 'text': 'PRICE RANGE'}));
							sectionEle.append(($('<div/>', {'class': 'facetItemList'})).append(priceSlider));
							sectionEle.appendTo(isMainFacet('price_USD','price_USD') ? mainFacetEle:secFacetEle);
		
							if( '' != '' || '' != '' ) {
								isRangeSelected = true;
								rangeMin= ('' != '') ? '':0;
								rangeMax = ('' != '') ? '':8000;
							}
							if(rangeMin!=minPrice || rangeMax!=maxPrice) {
								if(facetMap.get("Price Range") == undefined) {
									facetUrl=facetUrl+"Price="+rangeMin.toString()+"-"+rangeMax.toString()+";";
								} else{
									facetUrlSeq[facetMap.get("Price Range")]="Price="+rangeMin.toString()+"-"+rangeMax.toString();
								}
							}
					}
				}
				for(var i=0;i<facetUrlSeq.length;i++) {
					if(facetUrlSeq[i]!=undefined) {
						facetUrlSequence=facetUrlSequence+facetUrlSeq[i]+";";
					}
				}
				for(var i=0;i<tempDispSeqArr.length;i++) {
					if(tempDispSeqArr[i]!=undefined) {
						displaySeq=displaySeq+tempDispSeqArr[i]+" ";
					}
				}
				displaySeq = displaySeq+tempDispSeq;
				displaySeq = decodeURI(displaySeq.replace('&amp;','&').replace('&lt;','<').replace('&gt;','>'));
				facetUrlSequence=facetUrlSequence+facetUrl;
				var plpUrl = window.location.href.split("?");
				var vwaPos = plpUrl[0].split("vwa/");
				if(vwaPos[1].indexOf("/") != -1) {
					plpUrl[0] = vwaPos[0]+'vwa/'+vwaPos[1].substring(0,vwaPos[1].indexOf("/"));
				} else {
					plpUrl[0] = vwaPos[0]+'vwa/'+vwaPos[1];
				}
				plpUrl[1] = (plpUrl[1] != undefined) ? "?"+plpUrl[1] : "";
				if(facetUrlSequence != "") {
					facetUrlSequence = plpUrl[0]+"/"+facetUrlSequence;
					facetUrlSequence = facetUrlSequence.substring(0,facetUrlSequence.length-1);
					$("#seoUrl").text(facetUrlSequence);					
					window.history.replaceState( {} , window.location.href, facetUrlSequence+plpUrl[1]);	
					var metaTag = 'Free shipping. Buy direct from HP. See customer reviews and comparisons for' + ' ' +displaySeq +'Laptops'+'. Upgrades and savings on select products.';
					//console.log(metaTag);
					if("" != "" && "" != null) {
						document.title = '';
					} else {
						document.title = 'Shop'+' '+displaySeq + 'Laptops | HP® Official Store';	
					} 			
					$('meta[name=description]').remove();
					if("" != "" && "" != null) {
						metaTag="";
					}				
					$('head').append($('<meta/>', {'name' : 'description', 'content':metaTag}));				
					if("" != "" && "" != null){
						document.getElementById("PLPPageTitle").innerHTML=""
					} else {
						document.getElementById("PLPPageTitle").innerHTML=displaySeq +' '+'Laptops';
					}
				} else {
					if("" != "" && "" != null){
						document.title = "";
					} else {
						document.title ="Laptops | HP® Official Store";	
					}
					$('meta[name=description]').remove();
					if("" != "" && "" != null) {
						var metaTag="";
						$('head').append($('<meta/>', {'name' : 'description', 'content':metaTag}));						
					}
					if("" != "" && "" != null) {
						document.getElementById("PLPPageTitle").innerHTML=''
					} else {
						document.getElementById("PLPPageTitle").innerHTML='View All Laptops';
					}
					$("#seoUrl").text("");
					window.history.replaceState( {} , window.location.href, plpUrl[0]+plpUrl[1]);
				}
				var facetsCtrlEle = $('<div/>', {'class': 'facetsControl'});
				if (secFacetEle.find('.facetSet').length > 0) {
					facetsCtrlEle.append($('<div/>', {'class': 'secFacetShow more', 'html': 'More filters'}));
					if (secFacetEle.find('.facetSet').find('.facetItemChkBox:checked').length > 0)
						$('.secFacetShow').trigger('click');
				}
				facetsCtrlEle.append($('<div/>', {'class': 'clearFacets', 'html': 'Clear selections'}));

				facetsCtrlEle.appendTo(facetsViewEle);
				mainFacetEle.appendTo(facetsViewEle);
				secFacetEle.appendTo(facetsViewEle);
				/* Initialize Price range and set the limits */
				initializePriceSlider();
			}
		}
	}

	function changeFacets(reset) {
		$('html').addClass('refreshing');
		$.ajax({
			url: getAbsoluteURL() + "Finder",
			data: reset ? $('#catalogForm').serialize(): decodeURIComponent($('#catalogForm, #facetForm, #sortForm').serialize()),
			success: function(data) {
				try {
					searchResults = $.parseJSON($('<div/>').html(data).find('#searchResults').text());
					
					/*  reset price slider */
					if (reset) {
						rangeMin = minPrice; rangeMax = maxPrice;
						isRangeSelected = false; /* To handle no price scenarios */
					}
					
					updateSort(searchResults);
					renderFacets(searchResults);
					renderPaging(searchResults);

					$('#searchResultView').html($('<div/>').html(data).find('#searchProducts').html());
					initSlider();
					$.hpsfData.refreshPriceInventoryStoreData();
					
					if(Number(searchResults.totalProducts) == 0){
						$('#searchResultView').text("Filter selections have 0 results");
					} 

					$('html, body').animate({scrollTop: $('#facetForm').offset().top});
				} catch (error) {
				}
			},
			error: function(data) {
				alert("System error! Please kindly refresh the page and try again.");
			},
			complete: function() {
				$('html').removeClass('refreshing');
				$('html').removeClass('mobileFacetOpen');
			}
		});
	}

	function renderPaging(searchResults) {
		if (searchResults) {
			var pageForm = $('#pageForm');
			pageForm.empty();

			var pagesEles = $('.pageNos');
			pagesEles.empty();

			$('.loadMorePages').hide();

			var totalProductsCount = Number(searchResults.totalProducts);
			$('.searchTotal').text(totalProductsCount + " products");
			
			
			if (totalProductsCount > 0) {
				var beginIndex = Number(searchResults.beginIndex);
				var pageSize = Number(searchResults.pageSize);
				$('#catalogForm input[name=pageSize]').val(pageSize);

				var totalPages = Math.ceil(totalProductsCount/pageSize);
				var resultPageNo = Math.ceil((beginIndex+1)/pageSize);

				if (totalPages >= 1) {
					if (resultPageNo > 1) {
						pageForm.append($('<input/>', {'id':"page_first", type:"radio", name:"beginIndex", value:"0"}));
						pageForm.append($('<input/>', {'id':"page_prev", type:"radio", name:"beginIndex", value:(resultPageNo-2)*pageSize}));

						pagesEles.each(function(){
							$(this).append($('<label/>', {'class': "pageNo", 'html':"&lt;&lt;", 'for':"page_first"}));
							$(this).append($('<label/>', {'class': "pageNo", 'html':"&lt;", 'for':"page_prev"}));
						});
					}

					var startPgCount = resultPageNo-3 < 1? 1:resultPageNo-3; 
					var endPgCount = resultPageNo+3 > totalPages? totalPages:resultPageNo+3; 
					for (var i= startPgCount; i<=endPgCount; i++) {
						var radioEle = $('<input/>', {'id': "page_"+i, type:"radio", name:"beginIndex", value:(i-1)*pageSize});
						if (resultPageNo == i)
							radioEle.prop('checked', true);
						radioEle.appendTo(pageForm);

						var lblEle = $('<label/>', {'class': "pageNo", 'html':i, 'for':"page_"+i});
						if (resultPageNo == i)
							lblEle.addClass('checked');
						pagesEles.each(function(){$(this).append(lblEle.clone())});
					}

					if (resultPageNo<totalPages) {
						pageForm.append($('<input/>', {'id':"page_next", type:"radio", name:"beginIndex", value:resultPageNo*pageSize}));
						pageForm.append($('<input/>', {'id':"page_more", type:"radio", name:"beginIndex", value:resultPageNo*pageSize}));
						pageForm.append($('<input/>', {'id':"page_last", type:"radio", name:"beginIndex", value:(totalPages-1)*pageSize}));

						pagesEles.each(function(){
							$(this).append($('<label/>', {'class': "pageNo", 'html':"&gt", 'for':"page_next"}));
							$(this).append($('<label/>', {'class': "pageNo", 'html':"&gt;&gt;", 'for':"page_last"}));
						});

						$('.loadMorePages').show();
					}
				}
			}
		}
	}
	function changePage(append) {
		$('html').addClass('refreshing');
		$.ajax({
			url: getAbsoluteURL() + "Finder",
			data: decodeURIComponent($('#catalogForm, #facetForm, #pageForm, #sortForm').serialize()+'&disableFacets=true'),
			success: function(data) {
				try {
					searchResults = $.parseJSON($('<div/>').html(data).find('#searchResults').text());
					updateSort(searchResults);
					// renderFacets(searchResults);
					renderPaging(searchResults);

					if (append) {
						$('#searchResultView').append($('<div/>').html(data).find('#searchProducts').html());
						var firstNewPdtId = $('<div/>').html(data).find('.productCard').first().prop('id');
						$('html, body').animate({scrollTop: $('#'+firstNewPdtId).closest('.productWrapper').offset().top});
					} else {
						$('#searchResultView').html($('<div/>').html(data).find('#searchProducts').html());
						$('html, body').animate({scrollTop: $('#searchResults').offset().top});
					}
					initSlider();
					$.hpsfData.refreshPriceInventoryStoreData();
				} catch (error) {
				}
			},
			error: function(data) {
				alert("System error! Please kindly refresh the page and try again.");
			},
			complete: function() {
				$('html').removeClass('refreshing');
				$('html').removeClass('mobileFacetOpen');
			}
		});
	}
	
	function initSlider() {
		$('.bundle.image_pager').each(function () {
			var $this = $(this);
			if ($this.attr('id') == null) {
				var idx = $this.index();
				$this.attr('id', 'bund' + idx);
				var prev = $this.find('.pager-prev').attr('id', 'pager-prev' + idx);
				var next = $this.find('.pager-next').attr('id', 'pager-next' + idx);
				
				$('#bImg_' + $this.data('id') + '.main_image_slider').bxSlider({
					pagerCustom : '#bund' + idx + ' .subslider',
					mode : 'fade',
					controls : false,
				});
				
				$this.find('.subslider').bxSlider({
					minSlides: 3,
					maxSlides: 3,
					slideWidth: 68,
					slideMargin: 8,
					infiniteLoop: false,
					hideControlOnEnd: true,
					pager: false,
					nextSelector: next,
					prevSelector: prev,
				});
			}
		});
	}

	$(document).ready(function() {
		$(document).hptooltip({items:'.hasTooltip'});
		
		$('#facetForm').on('click', '.facetName', function() {
			$(this).parent().toggleClass('open');
		});

		$('#facetForm').on('click', '.facetItemShow', function() {
			var facetEle = $(this).closest('.facetSet');
			$(this).toggleClass('more');
			if ($(this).hasClass('more')) {
				$(this).html('More...');
				var facetItems = facetEle.find('.facetItem');
				facetItems.each(function(index){
					if (index >= (shownFacetItemSize))
						$(this).addClass('more');
				});
			} else {
				$(this).html('...Less');
				facetEle.find('.facetItem').removeClass('more');
			}
		});

		$('#facetForm').on('click', '.secFacetShow', function() {
			var secFacetEle = $('.secFacets');
			$(this).toggleClass('more');
			if ($(this).hasClass('more')) {
				$(this).html('More filters');
				secFacetEle.addClass('more');
				$('html, body').animate({scrollTop: $('#facetForm').offset().top});
			} else {
				$(this).html('Less filters');
				secFacetEle.removeClass('more');
			}
		});

		$('#facetForm').on('change', '.facetItemChkBox', function(){
			changeFacets(false);
			
			// send analytics event for omniture
			if(window.sendFacetSelectionEvent) {
			    var facetCategory = $(this).parents('.facetSet').children('.facetName').text().trim();
			    var facetValue = $(this).next('.facetItemLbl').text().trim();
			    var facetId = $(this).attr('id');
			    
			    sendFacetSelectionEvent(facetCategory, facetValue, facetId);
			}

		});

		$('#facetForm').on('click', '.facetItemImg', function(event){ //ie bug fix
			event.preventDefault();
			$($(this).closest('.facetItem label')).click();
		});

		$('#facetForm').on('click', '.clearFacets', function(){
			changeFacets(true);
		});

		$('#pageForm').on('change', 'input[name=beginIndex]', function(){
			if ($('#pageForm input[name=beginIndex]:checked').prop('id') == 'page_more')
				changePage(true);
			else
				changePage();
		});
		$('#sortForm').on('change', '[name=orderBy]', function(){
			var selectedValue = $(this).val();
			if(selectedValue != -1){
				changeFacets();
			}
		});

		$(document).on('change', '.sortFormClone [name=orderBy]', function(){
			if ($(this).is(':radio:checked') || $(this).is('select')) {
				var selectedValue = $(this).val();
				if(selectedValue && selectedValue != -1){
					$('#sortForm [name=orderBy]').val(selectedValue);
					$('#sortForm [name=orderBy]').trigger('change');
				}
			}
		});

		updateSort(searchResults);
		renderFacets(searchResults);
		renderPaging(searchResults);
		initSlider();
	});
</script> 
  <link rel="stylesheet" href="/wcsstore/HPStorefrontAssetStore/css/v2/searchCompare.css"> 
