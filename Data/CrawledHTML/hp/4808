			},
			params: {
				langId	: '-1',
				storeId	: '10151',
				userType  : 'G',
				catalogId : '10051',
				categoryId : $('#headerCategoryId').val(),
				currencySymbol : '$',
				dealsRedirect : getParameterByName('dl')
			}
		};
						
		$('form#searchHP').attr('action',searchDisplayUrl);
		$('form#searchHP input#searchBox').attr('name','searchTerm');
