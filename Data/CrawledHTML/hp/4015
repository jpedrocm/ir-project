  <script type="text/javascript">
	//script for swatches
	$(document).ready(function() {
		var swatchSwitcher = swatchSwitcher || {};
		swatchSwitcher.li = $('.pdp_featured_image li');
		swatchSwitcher.pager = $('#featured_image_pager li');
		swatchSwitcher.overlayPager = $('.thumps2 li');
		
		swatchSwitcher.swatch = {
			
		}
		
		swatchSwitcher.swatchPager = {  
			
		}
		var defaultSwatch = 0;
		$(".swatches_list li").each(function(){
			if($(this).hasClass("selectedSwatch")){
					defaultSwatch = $(this).index();
			}
			swatchSwitcher.li.each(function (i) {
					$('img', this).attr('src', swatchSwitcher.swatch["swatch"+(defaultSwatch)]["slide"+(i)]);
				});
			swatchSwitcher.pager.each(function (i) {
					$('img', this).attr('src', swatchSwitcher.swatchPager["swatch"+(defaultSwatch)]["slide"+(i)]);
				});
			swatchSwitcher.overlayPager.each(function (i) {
					$('a', this).attr('href', swatchSwitcher.swatch["swatch"+(defaultSwatch)]["slide"+(i)]);
					$('img', this).attr('src', swatchSwitcher.swatchPager["swatch"+(defaultSwatch)]["slide"+(i)]);
				});
		});

		swatchSwitcher.clickEvents = function() {
			$('.swatch').click(function() {
				$(".selectedSwatch").removeClass("selectedSwatch");
				$(this).parent('li').addClass("selectedSwatch");
				var currentIndex = $(this).parent('li').index();
				swatchSwitcher.li.each(function (i) {
					$('img', this).attr('src', swatchSwitcher.swatch["swatch"+(currentIndex)]["slide"+(i)]);
				});
				swatchSwitcher.pager.each(function (i) {
					$('img', this).attr('src', swatchSwitcher.swatchPager["swatch"+(currentIndex)]["slide"+(i)]);
				});
				swatchSwitcher.overlayPager.each(function (i) {
					$('a', this).attr('href', swatchSwitcher.swatch["swatch"+(currentIndex)]["slide"+(i)]);
					$('img', this).attr('src', swatchSwitcher.swatchPager["swatch"+(currentIndex)]["slide"+(i)]);
				});
			});
		};
		
		swatchSwitcher.clickEvents();
		
		//execute this logic to enable mobile color swatches if available
		if($('div#swatches_mobile').size() > 0) {
			var swatchSwitcher_mobile = swatchSwitcher_mobile || {};
			swatchSwitcher_mobile.li = $('.pdp_featured_image_mobile li');
	
	    	
		
			swatchSwitcher_mobile.swatch = {
				
			}
			
			var mDefaultSwatch = 0;
			$(".swatches_list_mobile li").each(function(){
			if($(this).hasClass("selectedSwatch_mbl")){
					mDefaultSwatch = $(this).index();
			}
			swatchSwitcher_mobile.li.each(function (i) {
	    				$('img', this).attr('src', swatchSwitcher_mobile.swatch["swatch"+mDefaultSwatch]["slide"+i]);
					});
			});
	
	
			swatchSwitcher_mobile.clickEvents = function(){
	    		$('.swatch_mbl').click(function(){    
					$(".selectedSwatch_mbl").removeClass("selectedSwatch_mbl");
					$(this).parent('li').addClass("selectedSwatch_mbl");
					// update selected swatches for desktop UI so that existing CTO js can get the correct color value
					$('a#'+$(this).attr("id")+".swatch").click();
	        
					var currentIndex_mbl = $(this).parent('li').index();
	
					swatchSwitcher_mobile.li.each(function (i) {
	    				$('img', this).attr('src', swatchSwitcher_mobile.swatch["swatch"+currentIndex_mbl]["slide"+i]);
					});
	
					/* price will be updated by pricing ajax services
					var priceBySwatch_mbl = swatchSwitcher_mobile.swatch["swatch"+(currentIndex_mbl+1)]["price"],
					priceAmount_mbl = priceBySwatch_mbl.split('.')[0],
					priceCents_mbl = priceBySwatch_mbl.split('.')[1];
					$('.update_by_swatch_mbl').html('<sup>$</sup>'+priceAmount_mbl+'.<sup>'+priceCents_mbl+'</sup>');
					*/        
				});
			};
	
			swatchSwitcher_mobile.clickEvents();
		}	
	});
</script> 
  <script src="/wcsstore/HPStorefrontAssetStore/js/pdp_zoom.js"></script> 
  <script type="text/javascript">
			var hierarchyList = new Array();		
			var productName = 'HP Spectre x360 Convertible Laptop - 13t  touch';
			var userId = "-1002";
			var sku = encodeURI("M6W88AV_1");
			var sectionValue = "Laptops";
			var pagefunction = "";
			var finalList = "HP Spectre x360~HP Spectre~By brand~Laptops";
			var splitList=finalList.split("~");
			var language="en_US";	
			var countryname=getcountry(language);
			var websectionId=getwebsectionId(language);
			var productIdValue='983652';
			var hp_profileId = '';
			var bvFlag = 'true';
			var MoxiePrice = $('div#ChatMaxPrice').text().trim();
			var aoid = '';
			var EOLProducts="";
			for(var i=0;i<splitList.length;i++) {
				hierarchyList.push(finalList.split("~")[i]);
			}
            if("false" || "false") {
			    pagefunction="PDP";		
			} else {
				pagefunction="CTO";
			}
			
			function tabClick(sId) {
				if(!(typeof trackMetrics=='undefined')) {
					trackMetrics( "new.page", {page: {"name": "tab:" + sId, "tabname": sId, events:['product.view']}});
				}
			}

			function omniture_Tag(Analytic_usertype) {
				var pdpPrice = $.hpsfUI.getPDPPriceByPageName("pdp-main");
				PDPstdTag(productName,userId,sku,pdpPrice,hierarchyList,sectionValue,Analytic_usertype,pagefunction,countryname,hp_profileId,aoid,EOLProducts);
			}
			
			function moxieProductPrice() { 
				if(typeof talCustProp !== 'undefined'){
					var totalprice=$.hpsfUI.getPDPPriceByPageName("pdp-main");
					totalprice = totalprice.replace(",", "");
					talCustProp = "#:Prop!ProductPrice="  + Math.round(totalprice);
				}
				//removes livehelp and contactUs links
				removeMoxieLinks(totalprice);
			}
			
			function removeMoxieLinks(pPrice) {
				var flag = false;
				if(typeof talCustProp !== 'undefined' &&  MoxiePrice !== 'undefined') {
					var productPrice = parseInt(pPrice);
					var moxieLimit = parseInt(MoxiePrice);
					if (productPrice < moxieLimit){
						var flag = true;
					}

					if(typeof $('nav .topmenu ul.right li a.js_overlay_trigger') !== 'undefined' && flag == true ) {
						if($('nav .topmenu ul.right li a.js_overlay_trigger').parent().length > 0) {
							$('nav .topmenu ul.right li a.js_overlay_trigger').parent()[0].remove();
						}
						if( $('#lp_chat').length > 0 ) {
							$('#lp_chat').remove();
						}
					}
				}
			}	
			
			function getNumReviews() {
				var numReviews = $('.BVRRCount .BVRRNumber').first().text();
				if (numReviews > 6) {
				  $('#firstAmount').text(6);
				  $('#totalAmtReviews').text(numReviews);
				} else{
				  $('#firstAmount').text(numReviews);
				  $('#totalAmtReviews').text(numReviews);
				}
			}
			
			function getWriteURL(){
				var writeURL = $('#BVRRRatingSummaryLinkWriteFirstID a').attr('href');
				if(writeURL == undefined){
					writeURL = $('#BVRRRatingSummaryLinkWriteID a').attr('href');
				}
				$('#writeReviewBtn').attr('href', writeURL);
			}
			
			function getNumOutOf() {
				var numReviews = $('.BVRRCount .BVRRNumber').first().text();
				console.log('Num of reviews is ' + numReviews);
				if(numReviews > 6){
				  $('#firstAmount').text(6);
				  $('#totalAmtReviews').text(numReviews);
				} else if(numReviews == ""){
				  $('#numOutOfReviews').hide();
				} else{
				   $('#firstAmount').text(numReviews);
				  $('#totalAmtReviews').text(numReviews);
				}
			}

			function summaryStars() {
				if(bvFlag.indexOf('false') == -1 && !$('#BVSEOSummaryStars #BVRRRatingOverall_Rating_Summary_2').length) {
					var $starSpan = $('#BVRRRatingOverall_Rating_Summary_2').clone();
					$('#BVSEOSummaryStars').prepend($starSpan);  
	
					getWriteURL();
					getNumOutOf();
					try {
						$("[id^=BVRRReviewsSoiSectionID] span[itemprop='name']").each(function() { 
							$(this).html('"' + ($(this).html().trim()) + '"');
						});

						$('div[itemprop="review"]').each(function() {
						     var reviewId = $(this).attr('id');
						     var iframe_content_id = '#BVRRDisplayContentReviewID_' + reviewId + ' .BVRRReviewDisplayStyle5BodyClient';
						     var reviewIdHash = '#' + reviewId;
						     if($(iframe_content_id).html().trim().length){
						     	$(reviewIdHash).html($(reviewIdHash).html() + '<div style="margin-left: -160px;">' + $(iframe_content_id).html() + ' </div>');
						     }
						});
					}
					catch(err) {
						// Log error, if needed
					}
				}
			}
			
			
			HP.Utils.ready( function() {
				//dynamically generated from used molecules
				HP.Control.init(HP.Tooltip, '.tooltip', {});
				HP.Control.init(HP.LightBox, '.js_overlay_trigger', {addToPopupManager     : false});
			});
			
			$.fn.stars = function() {
			    return $(this).each(function() {
			        // Get the value
			        var val = parseFloat($(this).html());
			        // Make sure that the value is in 0 - 5 range, multiply to get width
			        var size = Math.max(0, (Math.min(5, val))) * 16;
			        // Create stars holder
			        var $span = $('<span />').width(size);
			        // Replace the numerical value with stars
			        $(this).parent().html($span);
			    });
			}
			
			function renderAgentMsgInPDP(isCCFlow){
				if(isCCFlow){
            		$('.agentMsg').css("display", "block");
            		$('.agentMsgMob').css("display", "block");
            	}else{
            		$('.agentMsg').css("display", "none");
            		$('.agentMsgMob').css("display", "none");
					addMoxieChat();
            	}
			}
			
			function addMoxieChat(){
                 var s = document.createElement("script");
                 s.src = "https://h30664.www3.hp.com/netagent/proactive/proactive.aspx";
                 document.body.appendChild(s);
            }

			
			
			document.domain = 'hp.com';
			
			$(document).ready(function() {
				
				$('#BVFrame').attr('src', $('#BVReviewPageURL').html());

				if(bvFlag.indexOf('false') == -1){ //do want bv
					if(!$('#BVRRContainer #BVRRSourceID').length){
						$('#BVRRContainer').hide(); 
						$('#BVReviewsContainer').show();
					}
				}else{ //do not want bv
					console.log('inside the don"t want BV');
					$('#BVRRContainer').hide(); 
					$('#BVReviewsContainer').show();
				}
				
				if(matchMedia("screen and (min-width: 496px)").matches) {				
					$(".tabsUnderlined li a").each(function(idx, li) {
						var tab = $(li);
						tab.removeClass('active');
						var sId = tab.attr('title');
						$('#'+sId).hide();
						
						if (idx == 0) {
							tab.addClass('active');
							$('#'+sId).show();
						}
					});
				} 
				
				if(window.location.hash){
					// tab name should contain alphabets only and give delay until carousel is initialized for accessories tab
					var tabTitle = window.location.hash.replace(/[\W]/g,'');
					if($('a[title='+tabTitle+']').size()>0){
						setTimeout(function(){
							$('a[title='+tabTitle+']').click();
						},1000);
					}
				}
				
				$('ul.pdp_featured_image_mobile > li').css('display','list-item');		
			});
		</script> 
  <script>
				$(document).ready(function() {					
					retrieveLatestPromos(10151,10051,-1,983652);	
					var modelIdFromCookie = $.cookie('breadCrumbModelId');	
					var productId=983652;
					var temp='';
					if (modelIdFromCookie == null || modelIdFromCookie == 'null') {
						retrieveBreadCrumbDetails(productId,-1,10151,10051,-1,232005);
						temp = 232005+'_'+productId;
						document.cookie = "breadCrumbModelId=" + temp+";path=/;domain=.hp.com";	
					} else {
						var splitCookie=modelIdFromCookie.split('_');
						if(splitCookie[1] == null) {
							retrieveBreadCrumbDetails(productId,-1,10151,10051,-1,modelIdFromCookie);
							temp = splitCookie[0]+'_'+productId;
							document.cookie = "breadCrumbModelId=" + temp+";path=/;domain=.hp.com";
						} else {
							if(splitCookie[1] == productId) {
								retrieveBreadCrumbDetails(productId,-1,10151,10051,-1,splitCookie[0]);
								temp = splitCookie[0]+'_'+productId;
								document.cookie = "breadCrumbModelId=" + temp+";path=/;domain=.hp.com";
							} else {
								retrieveBreadCrumbDetails(productId,-1,10151,10051,-1,232005);
								temp = 232005+'_'+productId;
								document.cookie = "breadCrumbModelId="  + temp+";path=/;domain=.hp.com";
							}
						}
					}
				});
			</script> 
