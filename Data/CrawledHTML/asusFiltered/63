         <script>
    $(document).ready(function() {
        
        if (!isApplicationPathSite()) {
            if (window.location.pathname.split("/")[1].toLowerCase() == "notebooks_ultrabooks") {
                $("#all-product-line-footer ul img:eq(0)").after("<img class='' src='/media/web_img/McAfee_LiveSafe_Asus_Banne.jpg'/>");
            }
        }        
    });

    function setProductMediaReview() {
        if ($('#ProductMediaReviewZone').hasClass("hide")) {
            $('#ProductMediaReviewZone').removeClass('hide');
            $('#ProductMediaReviewZone').before("<hr class=\"gray\">");
        }
    }

    function setProductDesign() {
        if ($('#ProductDesignZone').hasClass("hide")) {
            $('#ProductDesignZone').removeClass('hide');
            $('#ProductDesignZone').before("<hr class=\"gray\">");
        }
    }

    function setProductRelated() {
        if ($('#Product_BottomSection').hasClass("hide")) {
            $('#Product_BottomSection').removeClass('hide');
            $('#Product_BottomSection').before("<hr class=\"gray\">");

            if ($("div.similar-area").hasClass("hide")) {
                $("div.accessories-area").removeClass("span8-line").addClass("span8");
            }
        }
    }

</script> 
