            <script>
    var intProductForumSysWordingCheckId;
    intProductForumSysWordingCheckId = setInterval("pageProductForumSysWordingCheck()", intSysWordingCheckSpeed);

    function pageProductForumSysWordingCheck() {
        if (sysWordingRead) {
            $("#Forum_Area h1:eq(0)").html(sysWording.Forum);

            clearTimeout(intProductForumSysWordingCheckId);
        }
    }

    var templeteHtml = "<li><a href=\"\"><div><h3></h3><p></p></div></a></li>";

    $(document).ready(function() {
        //var url = (isApplicationPathSite()) ? "/" + getWebsite() + "/GetData.asmx/GetArticlesByTagName" : "/GetData.asmx/GetArticlesByTagName";
        //url = (window.location.host == "athena.asus.com") ? SecureHost + 'www.asus.com' + url : SecureHost + window.location.host + url;
        var url = "/" + getWebsite() + "/forum/utilities/Service.asmx/GetArticlesByTagName";

        $.ajax({
            dataType: 'xml',
            url: url,
            data: { ModelName: $("h1.page-title>span").html() },
            type: 'GET',
            cache: true,
            success: function(xml) {
                if ($("Articles", xml).length > 0) { setProductMediaReview(); $("#Forum_Area").removeClass("hide"); }
                $("Articles", xml).each(function(e) {
                    var tmpHTML = $(templeteHtml);
                    if ($("Articles", xml).length == 2) tmpHTML.find("li").addClass("span6");
                    if ($("Articles", xml).length == 3) tmpHTML.find("li").addClass("span4");
                    tmpHTML.find("a").attr("href", $("ArticleUrl", this).text());
                    tmpHTML.find("h3").html($("Title", this).text());
                    tmpHTML.find("p").html($("ShortDesc", this).text());
                    $("#Forum_Area ul.froum-list").append(tmpHTML);
                });
            },
            error: function(XMLHttpRequest, textStatus, errorThrown) {
                //alert(XMLHttpRequest + '\n' + textStatus + '\n' + errorThrown);
            }
        });
        if ($("#review").hasClass("hide")) { $("#Forum_Area>div").addClass("span12"); $("div.product-media-review-area h1:first").html(""); }
        else $("#Forum_Area>div").addClass("span6 span6-line");
    });
</script> 
