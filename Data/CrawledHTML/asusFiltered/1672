        //        else {
        //            $("#menu_msg .aai-circle-num").css({ 'display': 'block' });
        //            $("#menu_msg .aai-circle-num").text(res.Total);
        //        }
        //    },
        //    error: function () { }
        //});
        //
        //var message_detail = function () {
