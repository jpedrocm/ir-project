     <script>
     var intControlsbottomSysWordingCheckId;
     intControlsbottomSysWordingCheckId = setInterval("controlsbottomSysWordingCheck()", intSysWordingCheckSpeed);

     function controlsbottomSysWordingCheck() {
         if (sysWordingRead) {
