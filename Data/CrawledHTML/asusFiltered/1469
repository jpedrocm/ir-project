        <script>
    var banner_type = "1";
    if ($("#AdvancedSearchBanner a").length > 1) {
        $(".product-home-top-banner").asusTrunBanner({
            arrow_left: ".product-home-top-banner .arrow-left",
            arrow_right: ".product-home-top-banner .arrow-right",
            top_banner_list: ".top-banner-list",
            sec_bar: ".sec-bar",
            sec: 10000, //輪撥秒數
            change_sec: 500, //變化時間
            target_num: 0//第幾個開始
        });
    }

    $(function () {
        $("#AdvancedSearchBanner a").each(function () {
            var src = $(this).css('background-image').replace("url(", "").replace(")", "");
            if ($("#AdvancedSearchBanner a").length == 1) {
                if ($(this).attr("data-color") == "black") {
                    $(".container").addClass("black-style");
                    $("#af-header").addClass("aai-bg-black");
                    $("#asus-api-footer").addClass("aai-bg-black");
                    if ($("#MDAWin10Upgrade")) $("#MDA a.MDA2015").remove(); $("#MDA span.mdatext").after($("#MDAWin10Upgrade").html().replace("_Blue.png", "_White.png"));
                }
                else {
                    if ($("#MDAWin10Upgrade")) $("#MDA a.MDA2015").remove(); $("#MDA span.mdatext").after($("#MDAWin10Upgrade").html());
                }
            }
        });
    });

  var top_banner_resize = function() {
      if ($(window).width() < 720) {
          $(".product-home-top-banner").css('width', '100%');
          $(".product-home-top-banner").css('height', parseInt(($(".product-home-top-banner").width() / 985) * 460));
      } else {
          $(".product-home-top-banner").css('width', '');
          $(".product-home-top-banner").css('height', '');
      }

  }
  $(window).resize(function() {
      top_banner_resize();
  });
  top_banner_resize();
  // $.asusTrunBanner.openBanner();
  // $.asusTrunBanner.hideBanner();
</script> 
