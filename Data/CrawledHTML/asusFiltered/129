              <div class="box-main-tru2life-video"> 
               <video autoplay loop muted preload="auto" width="100%" height="100%" poster="websites/global/productcustomizedTab/1343/images/tru2life-main.jpg"> 
                <source src="websites/global/productcustomizedTab/1343/images/tru2life-main.mp4" type="video/mp4; codecs=&quot;avc1.4D401E, mp4a.40.2&quot;"> 
