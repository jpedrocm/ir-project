          <script language="javascript" type="text/javascript">
    $(document).ready(function () {
        var obj = $("#product-wheretobuy-area");
        var in_tool = false;
        obj.find('.retailers-list').attr("style", "display:none");

        var out_click = function () {
            if (in_tool == false) {
                $('.retailers-list').attr("style", "display:none");
            }
        };

        $('body').on('mousedown', out_click);


        $("#product-wheretobuy-area").find(".wtb-buy-btn").on("click", function () {
            var listEle = $(this).next('.retailers-list');
            listEle.off("hover").show().hover(function () {
                in_tool = true;
            },
            function () {
                in_tool = false;
            });
        });
    });

</script>
