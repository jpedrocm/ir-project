    }

    function setRetailer(link, obj) {
        $("#ShopModal h3").html($(obj).attr("title"));
        var retailer_name = $(obj).parents("div.span3").find("h5").html();
        var image_link = $(obj).parents("div.span3").find("img").html();
        var retailer_text = LinkNotice.replace(new RegExp("@@replaceword@@", "gm"), retailer_name);

        $("#ShopModal p").html(retailer_text);
        $("#ShopModal img").attr("src", image_link);
        $("#ShopModal a:eq(0)").attr("href", link);
        $("#ShopModal a:eq(0)").attr("onclick", "GATrackEvent('buttons', 'clicked', 'where-to-buy-popup-confirmed', this); return false;");
        $("#ShopModal a:eq(1)").attr("onclick", "Sendga('send', 'event', 'buttons', 'clicked', 'where-to-buy-popup-canceled'); return false;");
        $("#ShopModal a:eq(0)").click(function () {
            googleTrackProductfn('WTB_OnlineRetailer', obj);
            $("#ShopModal button.close").click();
        });
    }

    $(document).ready(function ($) {
        $('#ShopModal').appendTo($("body"));
    });

</script> 
