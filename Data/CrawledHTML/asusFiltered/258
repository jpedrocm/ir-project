    function controlsProductWhereListSysWordingCheck() {
        if (sysWordingRead) {
            if ($("#where-to-but-top div.span3").length != 0) { $("#product-wheretobuy-area h2:first").html(sysWording.OnlineRetailers); }
            $("#btn_Retail").html(sysWording.FindonlineRetailers);
            $("#where-search-stores h2:first").html(sysWording.StoresNearYou);
            $("#btn_Locally").html(sysWording.StoresNearYou);
            $("#where-to-but-top div.row-fluid-line a.comparenow").html(sysWording.VisitSite);
            $("#where-search-bar h5").html(sysWording.SearchArea);
            WheretobuyDistance = sysWording.WhereToBuyWording1;
            WheretobuySearchProblem = sysWording.WhereToBuyWording2;
            wtb_StoreName = "<b>" + sysWording.wtb_StoreName + "</b>";
            wtb_Address = "<b>" + sysWording.wtb_Address + "</b>";
            wtb_Phone = "<b>" + sysWording.wtb_Phone + "</b>";
            wtb_Website = "<b>" + sysWording.wtb_Website + "</b>";
            LinkNotice = sysWording.LinkNotice;
            $("#ShopModal div.modal-footer a:eq(0)").html(sysWording.ConfirmMsg);
            $("#ShopModal div.modal-footer a:eq(1)").html(sysWording.Cancel);

            clearTimeout(intControlsProductWhereSysWordingCheckId);
        }
    }

    function setWTBNotice(strWTBNotice) {
