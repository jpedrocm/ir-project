              <!-- item count--> <span class="filter-box-num">(999+)</span> </li> 
             <!-- range facet --> 
             <!-- common facet --> 
             <li> 
              <!-- checkbox and lable --> 
              <div class="filter-box-label"> 
               <label class="form-checkbox" for="4815"> <input type="checkbox" id="4815" name="4800" neg-n-value="100007637 4815" autocomplete="off"> <span class="form-checkbox-title">ShopRunner Eligible</span> </label> 
              </div> 
              <!-- item count--> <span class="filter-box-num">(24)</span> </li> 
             <!-- range facet --> 
            </ul> 
            <!-- show more logic--> 
            <div class="filter-box-bottom"> 
             <!-- show more area--> 
             <span id="" onclick="Biz.SearchPanel2016.Navigation.showMoreLessValues(this, 'Show More', 'Show Less')" class="filter-box-more "> <i class="fa filter-box-more-icon"></i> <span class="filter-box-more-text">Show More</span> </span> 
             <!-- default loading img --> 
             <span class="loader" style="display: none; padding: 0px"> <img alt="Loading Icon" src="http://images10.newegg.com/WebResource/Themes/2005/Nest/none.gif"> </span> 
