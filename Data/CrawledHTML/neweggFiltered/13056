      	      if (adsLoaded) {
      	      try {
      	        jQuery("#csa1Header").show();
      			jQuery("#csa1Feedback").show();
      			jQuery("#csa1").addClass("ad_box wide_ads");
      	      } catch (e) {
      			if(jQuery("#productListVersion").val() == 2016){
      				if(rightside.hasClass("has-side-banner")){
      					if(rightside.find(".row-side") && rightside.find(".row-side").height() > 60){
      						jQuery(".row-side-banners #narrow_ad_unit").parent().hide();
      					}
      					else{
                  jQuery(".row-side-banners #narrow_ad_unit").parent().hide();
      						rightside.removeClass("has-side-banner");
      					}
      				}
      			}
      		  }}
      		  else{
