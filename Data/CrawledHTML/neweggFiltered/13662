            Biz.ProductList.Layout.initLayout();
             
            NEG.domReady(function () {
                var viewType = Web.StateManager.Cookies.get(Web.StateManager.Cookies.Name.PRDLIST, "Layout");
                switch (viewType) {
                    case "l":
                        Biz.ProductList.Layout.listView();
                        break;
                    case "g":
                        Biz.ProductList.Layout.gridView();
                        break;
                }
            });
            
