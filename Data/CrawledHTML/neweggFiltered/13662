      	var rightside = jQuery(".page-content .row-body-inner").find(".row");
      	if(!rightside.hasClass("has-side-banner")){
      		rightside.addClass("has-side-banner");
      	}
            var adblock2 = {
            'verticalSpacing':5,
            'container' : 'csa1',
