              <!-- begin: SortBy--> 
              <div class="list-tool-sortby"> 
               <span class="list-tool-label">Sort by:</span> 
               <label class="form-select"> <select id="Order_top" name="Order" onchange="Biz.ProductList.View.Detailed.changeOrder4SEO(null,'top',true,this,'http://www.newegg.com/Laptop-Cooling-Pads/SubCategory/ID-{SUBCATEGORY}');"> <option value="9" selected>Featured Items</option> <option value="2">Lowest Price</option> <option value="3">Highest Price</option> <option value="15">Best Selling</option> <option value="1">Best Rating</option> <option value="4">Most Reviews</option> <option value="14">Newest to Oldest</option> </select> <span id="product_sort_type" class="form-select-name">Featured Items</span> <i class="fa fa-caret-down"></i> </label> 
              </div> 
              <script>
        Biz.ProductList.NeweggSoldRadioUrl.initSoldBy();
    </script> 
              <!-- end: SortBy--> 
              <!-- begin: ViewPageSize--> 
