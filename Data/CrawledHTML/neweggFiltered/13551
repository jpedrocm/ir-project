              </div> 
              <!-- item count--> <span class="filter-box-num">(47)</span> </li> 
             <!-- range facet --> 
            </ul> 
            <!-- show more logic--> 
            <div class="filter-box-bottom"> 
             <!-- show more area--> 
             <span id="" onclick="Biz.SearchPanel2016.Navigation.showMoreLessValues(this, 'Show More', 'Show Less')" class="filter-box-more "> <i class="fa filter-box-more-icon"></i> <span class="filter-box-more-text">Show More</span> </span> 
             <!-- default loading img --> 
             <span class="loader" style="display: none; padding: 0px"> <img alt="Loading Icon" src="http://images10.newegg.com/WebResource/Themes/2005/Nest/none.gif"> </span> 
             <!-- apply buttom --> 
             <button class="btn btn-tool btn-mini filter-box-apply" style="display: none;" id="fab_4800" name="fabGroup">Apply</button> 
             <!-- popup--> 
            </div> 
