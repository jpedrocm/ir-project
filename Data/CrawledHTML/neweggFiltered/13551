             <li> 
              <!-- checkbox and lable --> 
              <div class="filter-box-label"> 
               <label class="form-checkbox" for="4820"> <input type="checkbox" id="4820" name="4800" neg-n-value="100167749 4820" autocomplete="off"> <span class="form-checkbox-title">Newegg Premier Eligible</span> </label> 
              </div> 
              <!-- item count--> <span class="filter-box-num">(349)</span> </li> 
             <!-- range facet --> 
             <!-- common facet --> 
             <li> 
              <!-- checkbox and lable --> 
              <div class="filter-box-label"> 
               <label class="form-checkbox" for="4817"> <input type="checkbox" id="4817" name="4800" neg-n-value="100167749 4817" autocomplete="off"> <span class="form-checkbox-title">Price Match Guarantee</span> </label> 
              </div> 
              <!-- item count--> <span class="filter-box-num">(143)</span> </li> 
